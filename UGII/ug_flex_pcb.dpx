          <Pref name="NXFPCB_FPEnableAssociative" type="logical" title="Associative" value="true" ver="1926" scope="Part">
            <Description>This option determines whether or not the Flat Pattern should be created associative to the input data.</Description>
            <Toggle off="false"/>
            <Data type="enum">
              <EnumElem name="Yes" value="true"/>
              <EnumElem name="No" value="false"/>
            </Data>
          </Pref>
          <Pref name="NXFPCB_FPFixAtCurrentTimeStamp" type="logical" title="Fix at Current Timestamp" value="true" ver="2206" scope="Part">
            <Description>This option determines whether or not a timestamp should be placed at the last feature in the flexible printed circuit design body when the Flat Pattern feature is created. When this option is on, changes made to the flexible printed circuit design body after the Flat Pattern feature was created do not appear in the Flat Pattern.</Description>
            <Toggle off="false"/>
            <Data type="enum">
              <EnumElem name="Yes" value="true"/>
              <EnumElem name="No" value="false"/>
            </Data>
          </Pref>
          <Pref name="NXFPCB_FPShowInteriorFeatureCurves" type="logical" title="Show Interior Feature Curves" value="true" ver="1926" scope="Part">
            <Description>This option determines whether or not interior feature curves in Flat Pattern features should be shown.</Description>
            <Toggle off="false"/>
            <Data type="enum">
              <EnumElem name="Yes" value="true"/>
