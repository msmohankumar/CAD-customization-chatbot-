<!--

-->
<PrefDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www-cyp.ugsolutions.com/assemblies/pman">
  <Application name="Flexible Printed Circuit Design" csh="PMAN_FPCD" package_suppress="simcenter3d,simviewer,view,mechatronicsplayer,nx2dedit">
    <Category name="General" csh="PMAN_FPCD_General">
      <Tab name="Global Parameters" csh="PMAN_FPCD_General_Global_Parameters">
        <Pref name="NXFPCB_MaterialThickness_EU" type="real" title="Material Thickness" value="0.01" unit="in" ver="1847" scope="Part">
          <Description>This option specifies the global thickness for a Base Flat Segment or a Base Contour Transition feature.</Description>
          <Real/>
        </Pref>
        <Pref name="NXFPCB_MaterialThickness_MU" type="real" title="Material Thickness" value="0.2" unit="mm" ver="1847" scope="Part">
          <Description>This option specifies the global thickness for a Base Flat Segment or a Base Contour Transition feature.</Description>
          <Real/>
        </Pref>
        <Pref name="NXFPCB_BendRadius_EU" type="real" title="Bend Radius" value="0.02" unit="in" ver="1847" scope="Part">
          <Description>This option specifies the global bend radius for Flexible PCB features.</Description>
          <Real/>
        </Pref>
        <Pref name="NXFPCB_BendRadius_MU" type="real" title="Bend Radius" value="0.5" unit="mm" ver="1847" scope="Part">
          <Description>This option specifies the global bend radius for Flexible PCB features.</Description>
          <Real/>
        </Pref>
        <Pref name="NXFPCB_BendReliefDepth_EU" type="real" title="Bend Relief Depth" value="0.005" unit="in" ver="1847" scope="Part">
          <Description>This option specifies the global bend relief depth for Flexible PCB features.</Description>
          <Real/>
        </Pref>
        <Pref name="NXFPCB_BendReliefDepth_MU" type="real" title="Bend Relief Depth" value="0.1" unit="mm" ver="1847" scope="Part">
          <Description>This option specifies the global bend relief depth for Flexible PCB features.</Description>
          <Real/>
        </Pref>
        <Pref name="NXFPCB_BendReliefWidth_EU" type="real" title="Bend Relief Width" value="0.005" unit="in" ver="1847" scope="Part">
          <Description>This option specifies the global bend relief width for Flexible PCB features.</Description>
          <Real/>
        </Pref>
        <Pref name="NXFPCB_BendReliefWidth_MU" type="real" title="Bend Relief Width" value="0.1" unit="mm" ver="1847" scope="Part">
          <Description>This option specifies the global bend relief width for Flexible PCB features.</Description>
          <Real/>
        </Pref>
        <Separator/>
        <Pref name="NXFPCB_NeutralFactor" type="string" title="Neutral Factor" value="0.5" ver="4.0.0" scope="Part">
          <Description>This option specifies the global neutral factor for Flexible PCB features.</Description>
          <Widestring/>
        </Pref>
        <Pref name="NXFPCB_topFace_PartProp_Color" type="int" title="Top Face Color" value="50" ver="7.0.0" scope="Part">
          <Description>This option specifies the color of the top face of the flexible cable.</Description>
          <Color/>
        </Pref>
        <Pref name="NXFPCB_bottomFace_PartProp_Color" type="int" title="Bottom Face Color" value="100" ver="7.0.0" scope="Part">
          <Description>This option specifies the color of the bottom face of the flexible cable.</Description>
          <Color/>
        </Pref>
      </Tab>
      <Tab name="Material Standards" csh="PMAN_FPCD_General_Material_Standards">
        <Group name="Physical Material Library">
          <Pref name="NXFPCD_Enable_Material_Library_Integration" type="logical" title="Integrate Physical Material Library" value="true" ver="1953" scope="Session">
            <Description>This option determines whether or not to integrate a physical material library with the Flexible Printed Circuit Design application.</Description>
            <Toggle off="false"/>
            <Data type="enum">
              <EnumElem name="On" value="true"/>
              <EnumElem name="Off" value="false"/>
            </Data>
          </Pref>
        </Group>
      </Tab>
      <Tab name="Layer Scheme" csh="PMAN_FPCD_General_Layer_Scheme" suppress="FPCD_LayerScheme">
        <Group name="Native NX">
          <Pref name="NXFPCB_layer_scheme_file_MU_WIN" type="string" title="Metric Part" value="layer_scheme_table.txt" ver="1953" scope="Session">
            <Description>This field specifies the name of the layer scheme file. The layer scheme file should be a comma separated text file with an extension txt or csv.</Description>
            <BrowseFileSel type="txt,csv"/>
          </Pref>
          <Pref name="NXFPCB_layer_scheme_file_MU_UNX" type="string" title="Metric Part" value="layer_scheme_table.txt" ver="1953" scope="Session">
            <Description>This field specifies the name of the layer scheme file. The layer scheme file should be a comma separated text file with an extension txt or csv.</Description>
            <BrowseFileSel type="txt,csv"/>
          </Pref>
          <Pref name="NXFPCB_layer_scheme_file_EU_WIN" type="string" title="English Part" value="layer_scheme_table.txt" ver="1953" scope="Session">
            <Description>This field specifies the name of the layer scheme file. The layer scheme file should be a comma separated text file with an extension txt or csv.</Description>
            <BrowseFileSel type="txt,csv"/>
          </Pref>
          <Pref name="NXFPCB_layer_scheme_file_EU_UNX" type="string" title="English Part" value="layer_scheme_table.txt" ver="1953" scope="Session">
            <Description>This field specifies the name of the layer scheme file. The layer scheme file should be a comma separated text file with an extension txt or csv.</Description>
            <BrowseFileSel type="txt,csv"/>
          </Pref>
        </Group>
        <Group name="Teamcenter" suppress="FPCD_LayerSchemeTC">
          <Pref name="NXFPCB_layer_scheme_item_id" type="string" title="Item ID" value="" ver="1953" scope="Session">
            <Description>This field specifies the Item ID of the layer scheme file in Teamcenter. Item ID, Item Revision, and Dataset Name; all three must be specified to locate the file in Teamcenter.</Description>
            <String/>
          </Pref>
          <Pref name="NXFPCB_layer_scheme_item_revision" type="string" title="Item Revision" value="" ver="1953" scope="Session">
            <Description>This field specifies the Item Revision of the layer scheme file in Teamcenter. Item ID, Item Revision, and Dataset Name; all three must be specified to locate the file in Teamcenter.</Description>
            <String/>
          </Pref>
          <Pref name="NXFPCB_layer_scheme_dataset_name" type="string" title="Dataset Name" value="" ver="1953" scope="Session">
            <Description>This field specifies the Dataset Name of the layer scheme file in Teamcenter. Item ID, Item Revision, and Dataset Name; all three must be specified to locate the file in Teamcenter.</Description>
            <String/>
          </Pref>
        </Group>
                <Pref name="NXFPCB_show_edit_layer_scheme_button" type="logical" title="Show Edit Layer Scheme Button" value="false" ver="1953" scope="Session" suppress="FPCD_EditLayerScheme">
                    <Description>This option determines whether or not to show the Edit Layer Scheme button in the Flexible Printed Circuit Design Preferences dialog.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
      </Tab>
    </Category>
    <Category name="Convert" csh="PMAN_FPCD_Convert">
      <SubCategory name="Wrap" csh="PMAN_FPCD_Convert_Wrap">
        <Tab name="All" csh="PMAN_FPCD_Convert_Wrap_All">
          <Group name="Settings">
            <Pref name="NXFPCD_Wrap_HideOriginal" type="logical" title="Hide Original" value="true" ver="1872" scope="Session">
              <Description>This option determines whether or not the original body will be made hidden after the flexible printed circuit model has been created.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="Yes" value="true"/>
                <EnumElem name="No" value="false"/>
              </Data>
            </Pref>
          </Group>
        </Tab>
      </SubCategory>
      <SubCategory name="Rip" csh="PMAN_FPCD_Convert_Rip">
        <Tab name="All" csh="PMAN_FPCD_Convert_Rip_All">
          <Group name="Rip Parameters">
            <Pref name="NXFPCD_EdgeRip_Type" type="logical" title="Use Minimal Relief" value="true" ver="12.0.2" scope="Session">
              <Description>This option determines whether or not to create a rip using the system default rip width.</Description>
              <Toggle off ="false"/>
              <Data type="enum">
                <EnumElem name="Yes" value="true"/>
                <EnumElem name="No" value="false"/>
              </Data>
            </Pref>
            <Pref name="NXFPCD_EdgeRip_RipWidth_EU" type="real" title="Rip Width" value="0.2" unit="in" ver="12.0.2" scope="Session">
              <Description>This option specifies the rip width.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.001575" include="1"/>
              </Data>
            </Pref>
            <Pref name="NXFPCD_EdgeRip_RipWidth_MU" type="real" title="Rip Width" value="1" unit="mm" ver="12.0.2" scope="Session">
              <Description>This option specifies the rip width.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.04" include="1"/>
              </Data>
            </Pref>
            <Pref name="NXFPCD_EdgeRip_EndCapShape" type="int" title="End Cap Shape" value="0" ver="12.0.2" scope="Session">
              <Description>This option specifies the end cap shape type.</Description>
              <RadioBox orientation="horizontal"/>
              <Data type="enum">
                <EnumElem name="Square" bitmap="bend_relief_square" value="0"/>
                <EnumElem name="Round" bitmap="bend_relief_round" value="1"/>
              </Data>
            </Pref>
            <Pref name="NXFPCD_EdgeRip_BlendSharpCorners" type="logical" title="Blend Sharp Corners" value="false" ver="12.0.2" scope="Session">
              <Description>This option determines whether or not to apply a blend at sharp corners.</Description>
              <Toggle off ="false"/>
              <Data type="enum">
                <EnumElem name="Yes" value="true"/>
                <EnumElem name="No" value="false"/>
              </Data>
            </Pref>
            <Pref name="NXFPCD_EdgeRip_CurveRadius_EU" type="real" title="Fillet Radius" value="0.1" unit="in" ver="12.0.2" scope="Session">
              <Description>This option specifies the curve radius.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.001575" include="1"/>
              </Data>
            </Pref>
            <Pref name="NXFPCD_EdgeRip_CurveRadius_MU" type="real" title="Fillet Radius" value="0.5" unit="mm" ver="12.0.2" scope="Session">
              <Description>This option specifies the curve radius.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.04" include="1"/>
              </Data>
            </Pref>
          </Group>
        </Tab>
      </SubCategory>
      <SubCategory name="Cleanup Utility" csh="PMAN_FPCD_Convert_Cleanup_Utility">
        <Tab name="All" csh="PMAN_FPCD_Convert_Cleanup_Utility_All">
          <Group name="Sliver">
            <Pref name="NXFPCD_SliverTol_EU" type="real" title="Sliver Tolerance" value="0.0004" unit="in" ver="1926" scope="Session">
              <Description>This option specifies the tolerance value that should be used by the cleanup utility to remove sliver faces.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.0" include="0"/>
              </Data>
            </Pref>
            <Pref name="NXFPCD_SliverTol_MU" type="real" title="Sliver Tolerance" value="0.01" unit="mm" ver="1926" scope="Session">
              <Description>This option specifies the tolerance value that should be used by the cleanup utility to remove sliver faces.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.0" include="0"/>
              </Data>
            </Pref>
          </Group>
          <Group name="Settings">
            <Pref name="NXFPCD_HideOriginal" type="logical" title="Hide Original" value="true" ver="1926" scope="Session">
              <Description>This option determines whether or not the original body will be made hidden after the cleanup utility has been run.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="Yes" value="true"/>
                <EnumElem name="No" value="false"/>
              </Data>
            </Pref>
          </Group>
        </Tab>
      </SubCategory>
    </Category>
    <Category name="Base" csh="PMAN_FPCD_Base">
      <SubCategory name="Planar Segment" csh="PMAN_FPCD_Base_Planar_Segment">
        <Tab name="Curves" csh="PMAN_FPCD_Base_Planar_Segment_Curves">
          <Group name="Bend Centerline">
            <Pref name="NXFPCB_bendCenterLine_Tab_Enabled" type="logical" title="Display" value="true" ver="12.0.0" scope="Part">
              <Description>This option determines whether bend centerlines are displayed in planar segment sketches.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="Yes" value="true"/>
                <EnumElem name="No" value="false"/>
              </Data>
            </Pref>
            <Pref name="NXFPCB_bendCenterLine_Tab_Color" type="int" title="Color" value="189" ver="12.0.0" scope="Part">
              <Description>This option specifies the color of bend centerlines in planar segment sketches.</Description>
              <Color/>
            </Pref>
            <Pref name="NXFPCB_bendCenterLine_Tab_Font" type="int" title="Style" value="4" ver="1847" scope="Part">
              <Description>This option specifies the style for bend centerlines in planar segment sketches.</Description>
              <GeneralLineFont LineFontOptionMaskString="GeneralLineFonts"/>
            </Pref>
            <Pref name="NXFPCB_bendCenterLine_Tab_Width" type="int" title="Width" value="1" ver="12.0.0" scope="Part">
              <Description>This option specifies the width for bend centerlines in planar segment sketches.</Description>
              <AnnotationLineWidth/>
            </Pref>
          </Group>
          <Group name="Bend Tangent Line">
            <Pref name="NXFPCB_bendTangentLine_Tab_Enabled" type="logical" title="Display" value="true" ver="12.0.0" scope="Part">
              <Description>This option determines whether bend tangent lines are displayed in planar segment sketches.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="Yes" value="true"/>
                <EnumElem name="No" value="false"/>
              </Data>
            </Pref>
            <Pref name="NXFPCB_bendTangentLine_Tab_Color" type="int" title="Color" value="83" ver="12.0.0" scope="Part">
              <Description>This option specifies the color of bend tangent lines in planar segment sketches.</Description>
              <Color/>
            </Pref>
            <Pref name="NXFPCB_bendTangentLine_Tab_Font" type="int" title="Style" value="1" ver="1847" scope="Part">
              <Description>This option specifies the style for bend tangent lines in planar segment sketches.</Description>
              <GeneralLineFont LineFontOptionMaskString="GeneralLineFonts"/>
            </Pref>
            <Pref name="NXFPCB_bendTangentLine_Tab_Width" type="int" title="Width" value="1" ver="12.0.0" scope="Part">
              <Description>This option specifies the width for bend tangent lines in planar segment sketches.</Description>
              <AnnotationLineWidth/>
            </Pref>
          </Group>
        </Tab>
      </SubCategory>
      <SubCategory name="Extension" csh="PMAN_FPCD_Base_Extension">
        <Tab name="All" csh="PMAN_FPCD_Base_Extension_All">
          <Group name="Extension Properties">
            <Pref name="NXFPCB_ExtensionLength_EU" type="real" title="Length" value="3.0" unit="in" ver="1847" scope="Session">
              <Description>This option specifies the Extension length.</Description>
              <Real/>
            </Pref>
            <Pref name="NXFPCB_ExtensionLength_MU" type="real" title="Length" value="10.0" unit="mm" ver="1847" scope="Session">
              <Description>This option specifies the Extension length.</Description>
              <Real/>
            </Pref>
            <Pref name="NXFPCB_ExtensionAngle_EU" type="real" title="Angle" value="90.0" unit="deg" ver="1847" scope="Session">
              <Description>This option specifies the Extension angle.</Description>
              <Real/>
            </Pref>
            <Pref name="NXFPCB_ExtensionAngle_MU" type="real" title="Angle" value="90.0" unit="deg" ver="1847" scope="Session">
              <Description>This option specifies the Extension angle.</Description>
              <Real/>
            </Pref>
            <Pref name="NXFPCB_ExtensionLengthOpt" type="int" title="Length Reference" value="0" ver="12.0.2" scope="Session">
              <Description>This option specifies how the Extension length is measured.</Description>
              <RadioBox orientation="horizontal"/>
              <Data type="enum">
                <EnumElem name="Inside" bitmap="inside_dimension" value="0"/>
                <EnumElem name="Outside" bitmap="outside_dimension" value="1"/>
                <EnumElem name="Web" bitmap="web_length" value="2"/>
                <EnumElem name="Tangent" bitmap="tangent_dimension" value="3"/>
              </Data>
            </Pref>
            <Pref name="NXFPCB_ExtensionInsetOpt" type="int" title="Inset" value="2" ver="12.0.2" scope="Session">
              <Description>This option specifies the Inset of the Extension feature.</Description>
              <RadioBox orientation="horizontal"/>
              <Data type="enum">
                <EnumElem name="Material Inside" bitmap="material_inside" value="0"/>
                <EnumElem name="Material Outside" bitmap="material_outside" value="1"/>
                <EnumElem name="Bend Outside" bitmap="bend_outside" value="2"/>
                <EnumElem name="Material Inside OML" bitmap="material_inside" value="3"/>
              </Data>
            </Pref>
            <Pref name="NXFPCB_ExtensionOffset_EU" type="real" title="Offset" value="0.0" unit="in" ver="1847" scope="Session">
              <Description>This option specifies the Extension offset.</Description>
              <Real/>
            </Pref>
            <Pref name="NXFPCB_ExtensionOffset_MU" type="real" title="Offset" value="0.0" unit="mm" ver="1847" scope="Session">
              <Description>This option specifies the Extension offset.</Description>
              <Real/>
            </Pref>
            <Pref name="NXFPCB_ExtensionEnableGeometryMirrorAndPattern" type="logical" title="Use Geometry Mirror and Pattern" value="false" ver="1926" scope="Session">
              <Description>This option determines whether or not the Extension command should use the mirror and pattern commands to replicate the geometry rather than the feature parameters.</Description>
              <Toggle off ="false"/>
              <Data type="enum">
                <EnumElem name="Yes" value="true"/>
                <EnumElem name="No" value="false"/>
              </Data>
            </Pref>
            <Pref name="NXFPCB_ExtensionEnableMiter" type="logical" title="Miter" value="false" ver="1926" scope="Session">
              <Description>This option determines whether or not miter should be applied to avoid interferences while creating extensions on adjacent edges in a given set.</Description>
              <Toggle off ="false"/>
              <Data type="enum">
                <EnumElem name="On" value="true"/>
                <EnumElem name="Off" value="false"/>
              </Data>
            </Pref>
          </Group>
        </Tab>
      </SubCategory>
      <SubCategory name="Flexible Cable" csh="PMAN_FPCD_Base_Flexible_Cable">
        <Tab name="All" csh="PMAN_FPCD_Base_Flexible_Cable_All">
          <Pref name="NXFPCB_ContactFace" type="int" title="Contact Face" value="0" ver="7.5.0" scope="Session" >
            <Description>This option specifies the contact face.</Description>
            <RadioBox/>
            <Data type="enum">
              <EnumElem name="Top Face" value="0"/>
              <EnumElem name="Bottom Face" value="1"/>
            </Data>
          </Pref>
          <Pref name="NXFPCB_ConductorWidth_EU" type="real" title="Conductor Width" value="0.0625" unit="in" ver="1847" scope="Session">
            <Description>This option specifies the conductor width for the Flexible Cable feature.</Description>
            <Real/>
          </Pref>
          <Pref name="NXFPCB_ConductorWidth_MU" type="real" title="Conductor Width" value="1" unit="mm" ver="1847" scope="Session">
            <Description>This option specifies the conductor width for the Flexible Cable feature.</Description>
            <Real/>
          </Pref>
          <Pref name="NXFPCB_ConductorSpacing_EU" type="real" title="Conductor Spacing" value="0.0625" unit="in" ver="1847" scope="Session">
            <Description>This option specifies the conductor spacing for the Flexible Cable feature.</Description>
            <Real/>
          </Pref>
          <Pref name="NXFPCB_ConductorSpacing_MU" type="real" title="Conductor Spacing" value="1" unit="mm" ver="1847" scope="Session">
            <Description>This option specifies the conductor spacing for the Flexible Cable feature.</Description>
            <Real/>
          </Pref>
          <Pref name="NXFPCB_StrippingLength_EU" type="real" title="Stripping Length" value="0.125" unit="in" ver="1847" scope="Session">
            <Description>This option specifies the stripping length for the Flexible Cable feature.</Description>
            <Real/>
          </Pref>
          <Pref name="NXFPCB_StrippingLength_MU" type="real" title="Stripping Length" value="3" unit="mm" ver="1847" scope="Session">
            <Description>This option specifies the stripping length for the flexible cable feature.</Description>
            <Real/>
          </Pref>
        </Tab>
      </SubCategory>
      <SubCategory name="Cutout" csh="PMAN_FPCD_Base_Cutout">
        <Tab name="All" csh="PMAN_FPCD_Base_Cutout_All">
          <Group name="Cutout Properties">
            <Pref name="NXFPCB_CutoutType" type="int" title="Cut Method" value="0" ver="11.0.0" scope="Session">
              <Description>This option specifies the cut method of Cutout.</Description>
              <RadioBox orientation="horizontal"/>
              <Data type="enum">
                <EnumElem name="Thickness" bitmap="thickness_cut" value="0"/>
                <EnumElem name="Mid-plane" bitmap="midplane_cut" value="1"/>
                <EnumElem name="Nearest Face" bitmap="nearest_face_cut" value="2"/>
              </Data>
            </Pref>
            <Pref name="NXFPCB_CutoutExtentOption" type="int" title="Limits" value="0" ver="4.0.0" scope="Session">
              <Description>This option specifies the limits of the Cutout.</Description>
              <RadioBox orientation="horizontal"/>
              <Data type="enum">
                <EnumElem name="Value" bitmap="finite" value="0"/>
                <EnumElem name="Between" bitmap="from_to_extent" value="1"/>
                <EnumElem name="Until Next" bitmap="through_next" value="2"/>
                <EnumElem name="Through All" bitmap="through_all" value="3"/>
              </Data>
            </Pref>
            <Pref name="NXFPCB_CutoutSymmToggle" type="int" title="Symmetric Depth" value="0" ver="11.0.0" scope="Session">
              <Description>This option determines whether or not the Cutout should be symmetric.</Description>
              <Toggle off="0"/>
              <Data type="enum">
                <EnumElem name="On" value="1"/>
                <EnumElem name="Off" value="0"/>
              </Data>
            </Pref>
            <Pref name="NXFPCB_CutoutExtent_EU" type="real" title="Depth" value="0.2" unit="in" ver="1847" scope="Session">
              <Description>This option specifies the depth of the Cutout feature.</Description>
              <Real/>
            </Pref>
            <Pref name="NXFPCB_CutoutExtent_MU" type="real" title="Depth" value="5.0" unit="mm" ver="1847" scope="Session">
              <Description>This option specifies the depth of the Cutout feature.</Description>
              <Real/>
            </Pref>
          </Group>
        </Tab>
      </SubCategory>
    </Category>
    <Category name="Bend" csh="PMAN_FPCD_Bend">
      <SubCategory name="General" csh="PMAN_FPCD_Bend_General">
        <Tab name="Parameters" csh="PMAN_FPCD_Bend_General_Parameters">
          <Group name="Bend Parameters">
            <Pref name="NXFPCB_UseGlobalBendRadius" type="int" title="Use Global Bend Radius" value="1" ver="4.0.0" scope="Session">
              <Description>This option determines whether or not the global bend radius should be used.</Description>
              <Toggle off="0"/>
              <Data type="enum">
                <EnumElem name="Yes" value="1"/>
                <EnumElem name="No" value="0"/>
              </Data>
            </Pref>
            <Pref name="NXFPCB_UseGlobalNeutralFactor" type="int" title="Use Global Neutral Factor" value="1" ver="4.0.0" scope="Session">
              <Description>This option determines whether or not the global neutral factor should be used.</Description>
              <Toggle off="0"/>
              <Data type="enum">
                <EnumElem name="Yes" value="1"/>
                <EnumElem name="No" value="0"/>
              </Data>
            </Pref>
          </Group>
          <Group name="Relief">
            <Pref name="NXFPCB_BendReliefType" type="int" title="Bend Relief" value="2" ver="4.0.0" scope="Session">
              <Description>This option specifies the type of bend relief to apply to Transition, Contour Transition, Bend, and Jog features.</Description>
              <RadioBox orientation="horizontal"/>
              <Data type="enum">
                <EnumElem name="Square" bitmap="bend_relief_square" value="0"/>
                <EnumElem name="Round" bitmap="bend_relief_round" value="1"/>
                <EnumElem name="None" bitmap="none" value="2"/>
              </Data>
            </Pref>
          </Group>
        </Tab>
      </SubCategory>
      <SubCategory name="Hem" csh="PMAN_FPCD_Bend_Hem">
        <Tab name="All" csh="PMAN_FPCD_Bend_Hem_All">
          <Pref name="NXFPCD_HemTransitionType" type="int" title="Type" value="0" ver="1926" scope="Session">
            <Description>This option specifies the type of Hem.</Description>
            <RadioBox orientation="horizontal"/>
            <Data type="enum">
              <EnumElem name="Closed" bitmap="hem_closed" value="0"/>
              <EnumElem name="Open" bitmap="hem_open" value="1"/>
              <EnumElem name="S-type" bitmap="hem_stype" value="2"/>
              <EnumElem name="Curl" bitmap="hem_curl" value="3"/>
              <EnumElem name="Open Loop" bitmap="hem_open_loop" value="4"/>
              <EnumElem name="Closed Loop" bitmap="hem_closed_loop" value="5"/>
              <EnumElem name="Centered Loop" bitmap="hem_centered_loop" value="6"/>
            </Data>
