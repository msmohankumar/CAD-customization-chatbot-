<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="docum.xsl"?>
<!--

-->
<PrefDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www-cyp.ugsolutions.com/assemblies/pman">
    <Application name="Drafting" csh="PMAN_Drafting" package_suppress="simcenter3d,simviewer,view,mechatronicsplayer">
        <Category name="General/Setup" csh="PMAN_Drafting_General_Setup">
            <Tab name="Standard" csh="PMAN_Drafting_General_Setup_Standard">
                <Pref name="Drafting_Standard" type="string" title="Drafting Standard" value="ISO" ver="5.0.0" scope="Session">
                    <Description>This option allows users to specify a drafting standard used for drafting standard customer defaults.</Description>
                    <DraftingStandard/>
                </Pref>
            </Tab>
            <Tab name="Workflow" csh="PMAN_Drafting_General_Setup_Workflow" package_suppress="nx2dedit">
                <Group name="Stand-alone">
                    <Docum>The options in this group will be used when creating drawings that do not reference any models.</Docum>
                    <Pref name="Drafting_AutoStartInsertSheetDialog" type="logical" title="Always Start Insert Sheet Command" value="true" ver="8.0.0" scope="Session">
                        <Description>This option determines whether or not the Insert Sheet command will be started the first time entering the Drafting application if no drawing sheets exist in the part.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="Drafting_StandaloneAutoStartDrawingViewCreation" type="logical" title="Always Start View Creation" value="false" ver="8.0.0" scope="Session">
                        <Description>This option determines whether or not the Drawing View command will be started after inserting a drawing sheet with no views.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="Drafting_StandaloneAutoStartProjectedView" type="logical" title="Always Start Projected View Command" value="true" ver="8.0.0" scope="Session">
                        <Description>This option determines whether or not the Projected View command will be started after inserting a drawing view.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="Drafting_AutoStartProjectToView" type="logical" title="Always Start Project to View Command" value="true" ver="7.0.0" scope="Session">
                        <Description>This option determines whether or not the Project to View command will be started after inserting a projected drawing view. This option has no effect without an NX Layout license or if the Always Start Projected View Command option is on.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Model-based">
                    <Pref name="Drafting_showNewDrawingDialog" type="int" title="Always Start Insert Sheet Command" value="1" ver="3.0.0" scope="Session">
                        <Description>This option determines whether or not the Insert Sheet command will be started the first time entering the Drafting application if no drawing sheets exist in the part.</Description>
                        <Toggle off="0"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="1"/>
                            <EnumElem name="No" value="0"/>
                        </Data>
                    </Pref>
                    <!--Merged with Drafting_BaseViewOrWizard-->
                    <!--
                    <Pref name="Drafting_AutoStartViewCreation" type="logical" title="Automatically Start View Creation" value="true" ver="7.0.0" scope="Session">
                        <Description>This option determines whether or not a view command will be started after inserting a drawing sheet with no views.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    -->
                    <Pref name="Drafting_BaseViewOrWizard" type="int" title="Always Start" value="1" ver="1899" scope="Session">
                        <Description>This option determines which view command will be started after inserting a sheet with no views.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="View Creation Wizard" value="0"/>
                            <EnumElem name="Base View Command" value="1"/>
                            <EnumElem name="No View Commands" value="2"/>
                        </Data>
                    </Pref>
                    <Pref name="Drafting_AutoStartProjectedView" type="logical" title="Always Start Projected View Command" value="true" ver="5.0.0" scope="Session">
                        <Description>This option determines whether or not the Projected View command will be started after inserting a model view.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="Drafting_CreateComponentOfMasterModelPart" type="logical" title="Create Drafting Component" value="false" ver="6.0.2" scope="Session">
                        <Description>This option determines whether or not drafting components will be created when creating a view of the master model part.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Drawing">
                    <Pref name="Drafting_DrawingSettings" type="int" title="Settings Origination" value="0" ver="5.0.0" scope="Session">
                        <Description>This option determines if a new part created from a template inherits settings from the template part, or the Drafting Standard.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="Drawing Template" value="0"/>
                            <EnumElem name="Drawing Standard"  value="1"/>
                        </Data>
                    </Pref>
                    <Pref name="UG_gridInDrafting" type="int" title="Grid Settings" value="0" ver="6.0.1" scope="Session">
                        <Description>This option determines which grid should be used in Drafting.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="Drafting" value="0"/>
                            <EnumElem name="Sketch" value="1"/>
                            <EnumElem name="Sheet Zone" value="2"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Inherited PMI">
                    <Pref name="Drafting_AllowPMIBidirectionalEdits" type="logical" title="Allow PMI Bidirectional Edits" value="false" ver="6.0.0" scope="Session">
                        <Description>This option determines whether or not model view PMI can be edited while in Drafting.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                  <Pref name="Drafting_PMICustomSymbolSettings" type="int" title="PMI Custom Symbol Settings" value="0" ver="11.0.1" scope="Session">
                    <Description>This option determines whether settings for inherited PMI custom symbols will use drafting preferences or the settings from the PMI custom symbol instance.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                      <EnumElem name="Use Drafting Preferences" value="0"/>
                      <EnumElem name="Use PMI Settings" value="1"/>
                    </Data>
                  </Pref>
                </Group>
                <Group name="Dimension">
                    <Pref name="Drafting_EditSceneDialogDelay" type="int" title="Pause Delay for On-Screen Dialog" value="1" ver="10.0.2" scope="Session">
                        <Description>This option specifies the delay, in increments of 250 milliseconds, between pausing the cursor and when the on-screen dialog is displayed during dimension creation.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="0" value="0"/>
                            <EnumElem name="1" value="1"/>
                            <EnumElem name="2" value="2"/>
                            <EnumElem name="3" value="3"/>
                            <EnumElem name="4" value="4"/>
                            <EnumElem name="5" value="5"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Retained Annotations" csh="PMAN_Drafting_General_Setup_Retained_Annotations" package_suppress="nx2dedit">
                <Group name="Retain Annotation">
                    <Pref name="Drafting_retainAnnotationObjects" type="int" title="Retain Annotations" value="1" ver="3.0.0" scope="Part">
                        <Description>This option determines whether or not drafting objects associated to objects that have been deleted by a modeling change should be retained (not deleted).</Description>
                        <Toggle off="0"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="1"/>
                            <EnumElem name="No" value="0"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Format">
                    <Pref name="Drafting_retainColor" type="int" title="Color" value="193" ver="5.0.0" scope="Part">
                        <Description>This option specifies the color of retained drafting objects.</Description>
                        <ColorOption/>
                        <Data type="color">
                            <EnumElem name="Original" value="0"/>
                        </Data>
                    </Pref>
                    <Pref name="Drafting_retainFont" type="int" title="Style" value="2" ver="1980" scope="Part">
                        <Description>This option specifies the line style for retained drafting objects. The option Original means that the line style will not be changed from what it was before the drafting object became retained.</Description>
                      <GeneralLineFont allowDefaultFont="true" showDefaultAsOriginalFont="true" allowInvisibleFont="true" LineFontOptionMaskString="MBDLineFonts" LineFontDefaultValue="0" LineFontInvisibleValue="-1"/>
                    </Pref>
                    <Pref name="Drafting_retainWidth" type="int" title="Width" value="0" ver="8.5.0" scope="Part">
                        <Description>This option specifies the line width for retained drafting objects. The option Original means that the line width will not be changed from what it was before the drafting object became retained.</Description>
                        <AnnotationLineWidth allowRetainWidth="true" allowWideLineWidth ="true"/>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Welcome Page" csh="PMAN_Drafting_General_Setup_Welcome_Page" package_suppress="nx2dedit">
                <Pref name="Drafting_WelcomePageSetup" type="logical" title="Display Recommended Setup in Welcome Page" value="true" ver="8.0.0" scope="Session">
                    <Description>This option determines whether or not the "Recommended Setup" section will appear in the Drafting Welcome Page.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="Drafting_WelcomeSessionCount" type="int" title="Drafting Welcome Page Duration" value="5" unit="Times" ver="8.0.0" scope="Session">
                    <Description>This option determines the number of NX sessions that will display the Drafting Welcome Page when entering the Drafting application. Setting its value to 0 (zero) will result in it never being displayed.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="0" include="1"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Miscellaneous" csh="PMAN_Drafting_General_Setup_Miscellaneous">
                <Pref name="Drafting_AllowExpressions" type="logical" title="Allow Expressions" value="false" ver="7.5.0" scope="Session">
                    <Description>This option determines whether or not the Expressions editor is available in the Drafting application.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Pref name="Drafting_saveCGMImageWithPart" type="int" title="Save CGM Data in Part" value="1" ver="3.0.0" scope="Part" package_suppress="nx2dedit">
                    <Description>This option determines whether or not CGM data should be saved for retrieval outside the context of NX.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
                <Pref name="Drafting_fontListOption" type="int" title="Fonts to Use" value="1" ver="1953" scope="Session">
                    <Description>This option determines from where font files will be read. System fonts are read from the systems standard font directory and in the directory pointed to by UGII_STANDARD_FONT_DIR. NX Fonts are located in the directory pointed to by UGII_CHARACTER_FONT_DIR.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Standard and NX Fonts" value="0"/>
                        <EnumElem name="Standard Fonts Only" value="1"/>
                        <EnumElem name="NX Fonts Only" value="2"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Text Editor" csh="PMAN_Drafting_General_Setup_Text_Editor" package_suppress="nx2dedit">
                <Pref name="Drafting_AEcharSizeMenu" type="string_list" title="Character Scale Factor" value="&quot;0.25&quot;\&quot;0.5&quot;\&quot;0.75&quot;\&quot;1.0&quot;\&quot;1.25&quot;\&quot;1.5&quot;\&quot;1.75&quot;\&quot;2.0&quot;\&quot;2.25&quot;\&quot;2.5&quot;\&quot;2.75&quot;\&quot;3.0&quot;\&quot;3.25&quot;\&quot;3.5&quot;\&quot;3.75&quot;\&quot;4.0&quot;" ver="3.0.0" scope="Session">
                    <Description>This option specifies the items to appear in the Character Scale Factor option menu in the Text Editor dialog box. The items are different scale factors that can be applied to the text. (The order of the items in the list will be the order of the items in the option menu.)</Description>
                    <MultiLineText height="10"/>
                </Pref>
                <Pref name="Drafting_AEbold" type="int" title="Bold Font Width" value="1" ver="3.0.0" scope="Session">
                    <Description>This option specifies what line width should be used for the Bold text option for NX Fonts.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Normal" bitmap="dui_letter_normal.bmp" value="1"/>
                        <EnumElem name="Thick" bitmap="dui_letter_thick.bmp" value="2"/>
                    </Data>
                </Pref>
                <Pref name="Drafting_AEitalic" type="real" title="Italic Slant Angle" value="20.0" ver="3.0.0" scope="Session">
                    <Description>This option specifies the slant angle that should be used for the Italic text option for NX Fonts.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>                
                <Pref name="Drafting_AEverticalText" type="int" title="Vertical Text" value="0" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not notes are created as vertical text.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
                <Group name="Input Windows">
                    <Pref name="Drafting_AErows" type="int" title="Height" value="5" ver="3.0.0" scope="Session">
                        <Description>This option specifies the visible vertical size (expressed as number of rows) of the text edit window.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="0" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="Drafting_AEcolumns" type="int" title="Width" value="30" ver="3.0.0" scope="Session">
                        <Description>This option specifies the visible horizontal size (expressed as number of columns) of the text edit window.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="0" include="1"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Preview Windows">
                    <Pref name="Drafting_AEpreviewSize" type="int" title="Text Size" value="2" ver="3.0.0" scope="Session">
                        <Description>This option specifies the size of the text in the preview window.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="Actual" value="0"/>
                            <EnumElem name="Fixed Scale" value="1"/>
                            <EnumElem name="Fixed Character Size" value="2"/>
                        </Data>
                        <Docum><Formatted>
Actual - The size corresponds to the actual character size.
Fixed Scale - A scale is applied to the actual character size.
Fixed Character Size - The size is a set character size. This option is very useful when the actual size of the text is very large or very small.</Formatted></Docum>
                    </Pref>
                    <Pref name="Drafting_AEpreviewScale" type="real" title="Scale" value="1.0" ver="3.0.0" scope="Session">
                        <Description>This option specifies the scale applied to the actual character size of the text in the preview window.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="Drafting_AEpreviewCharSize_MU" type="real" title="Character Size" value="3.0" unit="mm" ver="3.0.0" scope="Session">
                        <Description>This option specifies the fixed character size used for the text in the preview window.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="Drafting_AEpreviewCharSize_EU" type="real" title="Character Size" value="0.125" unit="in" ver="3.0.0" scope="Session">
                        <Description>This option specifies the fixed character size used for the text in the preview window.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
        </Category>
        <Category name="Flat Pattern View" csh="PMAN_Drafting_Flat_Pattern_View" package_suppress="nx2dedit">
                <Tab name="Lines" csh="PMAN_Drafting_Flat_Pattern_View_Lines">
                    <Group name="Bend-up Centerline">
                        <Pref name="NXSM_bendUpCenterLine_FlatPatternView_Enabled" type="logical" title="Show" value="true" ver="6.0.0" scope="Part">
                            <Description>This option specifies whether Bend-up Centerlines are shown on flat pattern drawing views.</Description>
                            <Toggle off="false"/>
                            <Data type="enum">
                                <EnumElem name="Yes" value="true"/>
                                <EnumElem name="No" value="false"/>
                            </Data>
                        </Pref>
                        <Pref name="NXSM_bendUpCenterLine_FlatPatternView_Color" type="int" title="Color" value="0" ver="6.0.0" scope="Part">
                            <Description>This option specifies the color of Bend-up Centerlines on flat pattern drawing views.</Description>
                            <ColorOption/>
                            <Data type="color">
                                <EnumElem name="Original" value="0"/>
                            </Data>
                        </Pref>
                        <Pref name="NXSM_bendUpCenterLine_FlatPatternView_Font" type="int" title="Style" value="0" ver="1847" scope="Part">
                            <Description>This option specifies the style for Bend-up Centerlines on flat pattern drawing views.</Description>
                          <GeneralLineFont allowDefaultFont="true" showDefaultAsOriginalFont="true" LineFontOptionMaskString="GeneralLineFonts" LineFontDefaultValue="0"/>
                        </Pref>
                        <Pref name="NXSM_bendUpCenterLine_FlatPatternView_Width" type="int" title="Width" value="0" ver="8.5.0" scope="Part">
                            <Description>This option specifies the width for Bend-up Centerlines on flat pattern drawing views.</Description>
                            <AnnotationLineWidth allowRetainWidth="true" allowWideLineWidth="true"/>
                        </Pref>
                    </Group>
                    <Group name="Bend-down Centerline">
                        <Pref name="NXSM_bendDownCenterLine_FlatPatternView_Enabled" type="logical" title="Show" value="true" ver="6.0.0" scope="Part">
                            <Description>This option specifies whether Bend-down Centerlines are shown on flat pattern drawing views.</Description>
                            <Toggle off="false"/>
                            <Data type="enum">
                                <EnumElem name="Yes" value="true"/>
                                <EnumElem name="No" value="false"/>
                            </Data>
                        </Pref>
                        <Pref name="NXSM_bendDownCenterLine_FlatPatternView_Color" type="int" title="Color" value="0" ver="6.0.0" scope="Part">
                            <Description>This option specifies the color of Bend-down Centerlines on flat pattern drawing views.</Description>
                            <ColorOption/>
                            <Data type="color">
                                <EnumElem name="Original" value="0"/>
                            </Data>
                        </Pref>
                        <Pref name="NXSM_bendDownCenterLine_FlatPatternView_Font" type="int" title="Style" value="0" ver="1847" scope="Part">
                            <Description>This option specifies the line style for Bend-down Centerlines on flat pattern drawing views.</Description>
                          <GeneralLineFont allowDefaultFont="true" showDefaultAsOriginalFont="true" LineFontOptionMaskString="GeneralLineFonts" LineFontDefaultValue="0"/>
                        </Pref>
                        <Pref name="NXSM_bendDownCenterLine_FlatPatternView_Width" type="int" title="Width" value="0" ver="8.5.0" scope="Part">
                            <Description>This option specifies the width for Bend-down Centerlines on flat pattern drawing views.</Description>
                            <AnnotationLineWidth allowRetainWidth="true" allowWideLineWidth="true"/>
                        </Pref>
                    </Group>
                    <Group name="Bend Tangent Line">
                        <Pref name="NXSM_bendTangentLine_FlatPatternView_Enabled" type="logical" title="Show" value="true" ver="5.0.0" scope="Part">
                            <Description>This option specifies whether Bend Tangent Lines are shown on flat pattern drawing views.</Description>
                            <Toggle off="false"/>
                            <Data type="enum">
                                <EnumElem name="Yes" value="true"/>
                                <EnumElem name="No" value="false"/>
                            </Data>
                        </Pref>
                        <Pref name="NXSM_bendTangentLine_FlatPatternView_Color" type="int" title="Color" value="0" ver="6.0.0" scope="Part">
                            <Description>This option specifies the color of Bend Tangent Lines on flat pattern drawing views.</Description>
                            <ColorOption/>
                            <Data type="color">
                                <EnumElem name="Original" value="0"/>
                            </Data>
                        </Pref>
                        <Pref name="NXSM_bendTangentLine_FlatPatternView_Font" type="int" title="Style" value="0" ver="1847" scope="Part">
                            <Description>This option specifies the line style for Bend Tangent Lines on flat pattern drawing views.</Description>
                          <GeneralLineFont allowDefaultFont="true" showDefaultAsOriginalFont="true" LineFontOptionMaskString="GeneralLineFonts" LineFontDefaultValue="0"/>
                        </Pref>
                        <Pref name="NXSM_bendTangentLine_FlatPatternView_Width" type="int" title="Width" value="0" ver="8.5.0" scope="Part">
                            <Description>This option specifies the width for Bend Tangent Lines on flat pattern drawing views.</Description>
                            <AnnotationLineWidth allowRetainWidth="true" allowWideLineWidth="true"/>
                        </Pref>
                    </Group>
                    <Group name="Inner Mold Line">
                        <Pref name="NXSM_bendInnerMoldLine_FlatPatternView_Enabled" type="logical" title="Show" value="true" ver="5.0.0" scope="Part">
                            <Description>This option specifies whether Inner Mold Lines are shown on flat pattern drawing views.</Description>
                            <Toggle off="false"/>
                            <Data type="enum">
                                <EnumElem name="Yes" value="true"/>
                                <EnumElem name="No" value="false"/>
                            </Data>
                        </Pref>
                        <Pref name="NXSM_bendInnerMoldLine_FlatPatternView_Color" type="int" title="Color" value="0" ver="6.0.0" scope="Part">
                            <Description>This option specifies the color of Inner Mold Lines on flat pattern drawing views.</Description>
                            <ColorOption/>
                            <Data type="color">
                                <EnumElem name="Original" value="0"/>
                            </Data>
                        </Pref>
                        <Pref name="NXSM_bendInnerMoldLine_FlatPatternView_Font" type="int" title="Style" value="0" ver="1847" scope="Part">
                            <Description>This option specifies the line style for Inner Mold Lines on flat pattern drawing views.</Description>
                          <GeneralLineFont allowDefaultFont="true" showDefaultAsOriginalFont="true" LineFontOptionMaskString="GeneralLineFonts" LineFontDefaultValue="0"/>
                        </Pref>
                        <Pref name="NXSM_bendInnerMoldLine_FlatPatternView_Width" type="int" title="Width" value="0" ver="8.5.0" scope="Part">
                            <Description>This option specifies the width for Inner Mold Lines on flat pattern drawing views.</Description>
                            <AnnotationLineWidth allowRetainWidth="true" allowWideLineWidth="true"/>
                        </Pref>
                    </Group>
                    <Group name="Outer Mold Line">
                        <Pref name="NXSM_bendOuterMoldLine_FlatPatternView_Enabled" type="logical" title="Show" value="true" ver="5.0.0" scope="Part">
                            <Description>This option specifies whether Outer Mold Lines are shown on flat pattern drawing views.</Description>
                            <Toggle off="false"/>
                            <Data type="enum">
                                <EnumElem name="Yes" value="true"/>
                                <EnumElem name="No" value="false"/>
                            </Data>
                        </Pref>
                        <Pref name="NXSM_bendOuterMoldLine_FlatPatternView_Color" type="int" title="Color" value="0" ver="6.0.0" scope="Part">
                            <Description>This option specifies the color of Outer Mold Lines on flat pattern drawing views.</Description>
                            <ColorOption/>
                            <Data type="color">
                                <EnumElem name="Original" value="0"/>
                            </Data>
                        </Pref>
                        <Pref name="NXSM_bendOuterMoldLine_FlatPatternView_Font" type="int" title="Style" value="0" ver="1847" scope="Part">
                            <Description>This option specifies the line style for Outer Mold Lines on flat pattern drawing views.</Description>
                          <GeneralLineFont allowDefaultFont="true" showDefaultAsOriginalFont="true" LineFontOptionMaskString="GeneralLineFonts" LineFontDefaultValue="0"/>
                        </Pref>
                        <Pref name="NXSM_bendOuterMoldLine_FlatPatternView_Width" type="int" title="Width" value="0" ver="8.5.0" scope="Part">
                            <Description>This option specifies the width for Outer Mold Lines on flat pattern drawing views.</Description>
                            <AnnotationLineWidth allowRetainWidth="true" allowWideLineWidth="true"/>
                        </Pref>
                    </Group>
                    <Group name="Exterior Curves">
                        <Pref name="NXSM_exteriorCurves_FlatPatternView_Color" type="int" title="Color" value="0" ver="6.0.0" scope="Part">
                            <Description>This option specifies the color of Exterior Curves on flat pattern drawing views.</Description>
                            <ColorOption/>
                            <Data type="color">
                                <EnumElem name="Original" value="0"/>
                            </Data>
                        </Pref>
                        <Pref name="NXSM_exteriorCurves_FlatPatternView_Font" type="int" title="Style" value="0" ver="1847" scope="Part">
                            <Description>This option specifies the line style for Exterior Curves on flat pattern drawing views.</Description>
