          <Pref name="PMAN_ShipDrafting_Profile_Sketch_symbols_lib_WIN" type ="string" title ="Custom Symbols Library Definition" value ="$UGII_SHIP_DRAFTING_DIR\data\profile_sketch\symbols\Inverse_bending_line_drawing_symbol.csv" ver="8.5.0" scope="Session">
            <Description>This option specifies the custom symbols definition and library used for Profile Sketch.</Description>
            <BrowseFileSel type="csv"/>
          </Pref>
          <Pref name="PMAN_ShipDrafting_Profile_Sketch_symbols_lib_UNX" type ="string" title ="Custom Symbols Library Definition" value ="$UGII_SHIP_DRAFTING_DIR/data/profile_sketch/symbols/Inverse_bending_line_drawing_symbol.csv" ver="8.5.0" scope="Session">
            <Description>This option specifies the custom symbols definition and library used for Profile Sketch.</Description>
            <BrowseFileSel type="csv"/>
          </Pref>
          <Pref name="PMAN_ShipDrafting_Profile_Sketch_type_code_symbols_lib_WIN" type ="string" title ="Custom Type Code Symbols Library Definition" value ="$UGII_SHIP_DRAFTING_DIR\data\profile_sketch\symbols\Inverse_bending_line_drawing_type_code_symbol.csv" ver="9.0.2" scope="Session">
            <Description>This option specifies the custom type code symbols definition and library used for Profile Sketch.</Description>
            <BrowseFileSel type="csv"/>
          </Pref>
          <Pref name="PMAN_ShipDrafting_Profile_Sketch_type_code_symbols_lib_UNX" type ="string" title ="Custom Type Code Symbols Library Definition" value ="$UGII_SHIP_DRAFTING_DIR/data/profile_sketch/symbols/Inverse_bending_line_drawing_type_code_symbol.csv" ver="9.0.2" scope="Session">
            <Description>This option specifies the custom type code symbols definition and library used for Profile Sketch.</Description>
            <BrowseFileSel type="csv"/>
          </Pref>
          <Pref name="PMAN_ShipDrafting_Profile_Sketch_Hole_WIN" type ="string" title ="Hole Name Mapping File" value ="$UGII_SHIP_DRAFTING_DIR\data\profile_sketch\Profile_sketch_hole_mapping.csv" ver="9.0.1" scope="Session">
            <Description>This option specifies the custom hole mapping file used for Profile Sketch.</Description>
            <BrowseFileSel type="csv"/>
          </Pref>
          <Pref name="PMAN_ShipDrafting_Profile_Sketch_Hole_UNX" type ="string" title ="Hole Name Mapping File" value ="$UGII_SHIP_DRAFTING_DIR/data/profile_sketch/Profile_sketch_hole_mapping.csv" ver="9.0.1" scope="Session">
            <Description>This option specifies the custom hole mapping file used for Profile Sketch.</Description>
            <BrowseFileSel type="csv"/>
          </Pref>
          
          <Pref name="PMAN_ShipDrafting_Profile_Sketch_folder_WIN" type ="string" title="Drawing Files Output Folder" value="$TEMP" ver="8.5.0" scope="Session">
            <Description>This option specifies the folder in which to place Profile Sketch files.</Description>
            <BrowseDirSel/>
          </Pref>
          <Pref name="PMAN_ShipDrafting_Profile_Sketch_folder_UNX" type= "string" title="Drawing Files Output Folder" value="$HOME" ver="8.5.0" scope="Session">
            <Description>This option specifies the folder in which to place Profile Sketch files.</Description>
            <BrowseDirSel/>
          </Pref>
        </Group>
        <Group name="Teamcenter Integration">
          <Pref name="PMAN_ShipDrafting_Profile_Sketch_template_tc" type="string" title="Drawing Templates Configuration File" value="@DB/drawing_template_configure/A" ver="9.0.2" scope="Session">
            <Description>This option specifies the drawing templates configuration file for Profile Sketch.</Description>
            <Widestring/>
          </Pref>
          <Pref name="PMAN_ShipDrafting_Profile_Sketch_symbols_lib_tc" type="string" title="Custom Symbols Library Definition" value ="@DB/Inverse_bending_line_drawing_symbol/A" ver="9.0.2" scope="Session">
            <Description>This option specifies the custom symbols definition and library used for Profile Sketch.</Description>
            <Widestring/>
          </Pref>
          <Pref name="PMAN_ShipDrafting_Profile_Sketch_type_code_symbols_lib_tc" type="string" title="Custom Type Code Symbols Library Definition" value ="@DB/Inverse_bending_line_drawing_type_code_symbol/A" ver="9.0.2" scope="Session">
              <Description>This option specifies the custom type code symbols definition and library used for Profile Sketch.</Description>
              <Widestring/>
          </Pref>
          <Pref name="PMAN_ShipDrafting_Profile_Sketch_Hole_tc" type="string" title="Hole Name Mapping File" value="@DB/Profile_sketch_hole_mapping/A" ver="9.0.2" scope="Session">
            <Description>This option specifies the custom hole mapping file used for Profile Sketch.</Description>
            <Widestring/>
          </Pref>
          <Pref name="PMAN_ShipDrafting_Profile_Sketch_folder_TC" type= "string" title="Drawing Files Output Folder" value="" ver="8.5.0" scope="Session">
            <Description>This option specifies the folder in which to place Profile Sketch files.</Description>
            <Widestring/>
          </Pref>
        </Group>
      </Tab>
      <Tab name="Inverse Bending Line"  csh="PMAN_ShipDrafting_Profile_Sketch_Inverse_Bending_Line">
          <Pref name="PMAN_ShipDrafting_Profile_Sketch_IncludeMaterialAllowance" type="logical" title="Include Material Allowance" value="false" ver="2306" scope="Session">
              <Description>This option determines whether or not to add material allowance to the profile length to determine inverse bending line spacing in profile sketch.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                  <EnumElem name="On" value="true"/>
                  <EnumElem name="Off" value="false"/>
              </Data>
          </Pref>
          <Label label="Spacing - define profile length ranges and spacings."/>
          <Group name="Range 1">
            <Label label="Greater than zero (0) and less than or equal to the following upper limit:"/>
            <Pref name="PMAN_ShipDrafting_Profile_Sketch_IBLSpacingRangeOne_MU" type="real" title="Upper Limit" value="1000" unit="mm" ver="2306" scope="Session">
                <Description>This option specifies the upper limit of the profile length for this range.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                </Data>
            </Pref>
            <Pref name="PMAN_ShipDrafting_Profile_Sketch_IBLSpacingRangeOne_EU" type="real" title="Upper Limit" value="40" unit="in" ver="2306" scope="Session">
                <Description>This option specifies the upper limit of the profile length for this range.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                </Data>
            </Pref>
            <Pref name="PMAN_ShipDrafting_Profile_Sketch_IBLSpacingRangeOne_Spacing_MU" type="real" title="Spacing" value="100" unit="mm" ver="2306" scope="Session">
                <Description>This option specifies the inverse bending line spacing in profile sketch for this profile length range.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                </Data>
            </Pref>
            <Pref name="PMAN_ShipDrafting_Profile_Sketch_IBLSpacingRangeOne_Spacing_EU" type="real" title="Spacing" value="4" unit="in" ver="2306" scope="Session">
                <Description>This option specifies the inverse bending line spacing in profile sketch for this profile length range.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                </Data>
            </Pref>
          </Group>
          <Group name="Range 2">
            <Label label="Greater than range 1 and less than or equal to the following upper limit:"/>
            <Pref name="PMAN_ShipDrafting_Profile_Sketch_IBLSpacingRangeTwo_MU" type="real" title="Upper Limit" value="3000" unit="mm" ver="2306" scope="Session">
                <Description>This option specifies the upper limit of the profile length for this range.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                </Data>
            </Pref>
            <Pref name="PMAN_ShipDrafting_Profile_Sketch_IBLSpacingRangeTwo_EU" type="real" title="Upper Limit" value="120" unit="in" ver="2306" scope="Session">
                <Description>This option specifies the upper limit of the profile length for this range.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                </Data>
            </Pref>
            <Pref name="PMAN_ShipDrafting_Profile_Sketch_IBLSpacingRangeTwo_Spacing_MU" type="real" title="Spacing" value="300" unit="mm" ver="2306" scope="Session">
                <Description>This option specifies the inverse bending line spacing in profile sketch for this profile length range.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                </Data>
            </Pref>
            <Pref name="PMAN_ShipDrafting_Profile_Sketch_IBLSpacingRangeTwo_Spacing_EU" type="real" title="Spacing" value="12" unit="in" ver="2306" scope="Session">
                <Description>This option specifies the inverse bending line spacing in profile sketch for this profile length range.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                </Data>
            </Pref>
          </Group>
          <Group name="Range 3">
            <Label label="Greater than range 2 and less than or equal to the following upper limit:"/>
            <Pref name="PMAN_ShipDrafting_Profile_Sketch_IBLSpacingRangeThree_MU" type="real" title="Upper Limit" value="8000" unit="mm" ver="2306" scope="Session">
                <Description>This option specifies the upper limit of the profile length for this range.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                </Data>
            </Pref>
            <Pref name="PMAN_ShipDrafting_Profile_Sketch_IBLSpacingRangeThree_EU" type="real" title="Upper Limit" value="320" unit="in" ver="2306" scope="Session">
                <Description>This option specifies the upper limit of the profile length for this range.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                </Data>
            </Pref>
            <Pref name="PMAN_ShipDrafting_Profile_Sketch_IBLSpacingRangeThree_Spacing_MU" type="real" title="Spacing" value="600" unit="mm" ver="2306" scope="Session">
                <Description>This option specifies the inverse bending line spacing in profile sketch for this profile length range.</Description>
                <Real/>
                <Data type="range"> 
                    <LowerBound value="0.0" include="0"/>
                </Data>
            </Pref>
            <Pref name="PMAN_ShipDrafting_Profile_Sketch_IBLSpacingRangeThree_Spacing_EU" type="real" title="Spacing" value="24" unit="in" ver="2306" scope="Session">
                <Description>This option specifies the inverse bending line spacing in profile sketch for this profile length range.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                </Data>
            </Pref>
          </Group>
          <Group name="Range 4">
            <Label label="Greater than range 3."/>
            <Pref name="PMAN_ShipDrafting_Profile_Sketch_IBLSpacingRangeFour_Spacing_MU" type="real" title="Spacing" value="800" unit="mm" ver="2306" scope="Session">
                <Description>This option specifies the inverse bending line spacing in profile sketch for this profile length range.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                </Data>
            </Pref>
            <Pref name="PMAN_ShipDrafting_Profile_Sketch_IBLSpacingRangeFour_Spacing_EU" type="real" title="Spacing" value="32" unit="in" ver="2306" scope="Session">
                <Description>This option specifies the inverse bending line spacing in profile sketch for this profile length range.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                </Data>
            </Pref>
          </Group>
      </Tab>
    </Category>
    <Category name="Frame Bar" csh="PMAN_ShipDrafting_Frame_Bar">
      <Tab name="General" csh="PMAN_ShipDrafting_Frame_Bar_General">
        <Pref name="FrameBar_ticDirection" type="int" title="Tic Direction" value="0" ver="8.0.0" scope="Part">
          <Description>This option specifies the tic direction.</Description>
          <RadioBox/>
          <Data type="enum">
            <EnumElem name="Towards View" value="0"/>
            <EnumElem name="Away from View" value="1"/>
          </Data>
        </Pref>
        <Pref name="FrameBar_precision" type="int" title="Precision" value="1" ver="8.0.0" scope="Part">
            <Description>This option specifies the number of decimal places used when a tic label is set to use Element Position.</Description>
            <Integer/>
        </Pref>
        <Pref name="FrameBar_ElementPosition_CoordinateUnit_MU" type="int" title="Unit of Coordinate in Frame Bar Labels" value="1" ver="1847" scope="Session">
            <Description>This option specifies the unit of the coordinate in the frame bar labels.</Description>
            <RadioBox/>
            <Data type="enum">
                <EnumElem name="Meter" value="0"/>
                <EnumElem name="Millimeter" value="1"/>
            </Data>
        </Pref>
          <Pref name="FrameBar_ElementPosition_CoordinateUnit_EU" type="int" title="Unit of Coordinate in Frame Bar Labels" value="1" ver="1847" scope="Session">
              <Description>This option specifies the unit of the coordinate in the frame bar labels.</Description>
              <RadioBox/>
              <Data type="enum">
                  <EnumElem name="Foot" value="0"/>
                  <EnumElem name="Inch" value="1"/>
              </Data>
          </Pref>
          <Group name="Main Line">
              <Pref name="ShipDrafting_FrameBar_MainLine" type="logical" title="Create" value="true" ver="2007" scope="Session">
                  <Description>This option determines whether or not to create the main line when creating the frame bar.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                      <EnumElem name="No" value="false"/>
                      <EnumElem name="Yes" value="true"/>
                  </Data>
              </Pref>
              
              <Pref name="ShipDrafting_FrameBar_MainLineColor" type="int" title="Color" value="134" ver="2007" scope="Session">
                  <Description>This option specifies the color of the main line.</Description>
                  <Color/>
              </Pref>
              <Pref name="ShipDrafting_FrameBar_MainLineFont" type="int" title="Style" value="1" ver="2007" scope="Session">
                  <Description>This option specifies the line style of the main line.</Description>
                  <GeneralLineFont LineFontOptionMaskString="GeneralLineFonts"/>
              </Pref>
              <Pref name="ShipDrafting_FrameBar_MainLineWidth" type="int" title="Width" value="10" ver="2007" scope="Session">
                  <Description>This option specifies the line width of the main line.</Description>
                  <ViewLineWidth allowOriginalWidth="false" allowWideLineWidth="true"/>
              </Pref>
          </Group>
          <Pref name="ShipDrafting_FrameBar_ElementPriority" type="list_sel_reorder_value" title="Priority List"  value="&quot;Aft Perpendicular&quot;\&quot;Midship&quot;\&quot;Forward Perpendicular&quot;\&quot;Centerline&quot;\&quot;Baseline&quot;\&quot;Waterline&quot;\&quot;Bulkheads&quot;\&quot;Intertransverse Frames&quot;\&quot;Transversal Insert Frames&quot;\&quot;Transverse Frames&quot;\&quot;Longitudinal Y Insert Frames&quot;\&quot;Longitudinal Y Frames&quot;\&quot;Decks&quot;\&quot;Longitudinal Z Insert Frames&quot;\&quot;Longitudinal Z Frames&quot;"  ver="2206" scope="Session">
              <Description>This option specifies a list of elements. The one listed first has the highest priority. The priority list determines which plane type is displayed in the Frame Bar if planes are coplanar.</Description>
              <ListSelReorder height="8" reorderable="true" selectable="false"/>
              <Data type="enum">
                  <EnumElem name="Aft Perpendicular"            value="Aft Perpendicular"/>
                  <EnumElem name="Midship"                      value="Midship"/>
                  <EnumElem name="Forward Perpendicular"        value="Forward Perpendicular"/>
                  <EnumElem name="Centerline"                   value="Centerline"/>
                  <EnumElem name="Baseline"                     value="Baseline"/>
                  <EnumElem name="Waterline"                    value="Waterline"/>
                  <EnumElem name="Bulkheads"                    value="Bulkheads"/>
                  <EnumElem name="Intertransverse Frames"       value="Intertransverse Frames"/>
                  <EnumElem name="Transversal Insert Frames"    value="Transversal Insert Frames"/>
                  <EnumElem name="Transverse Frames"            value="Transverse Frames"/>
                  <EnumElem name="Longitudinal Y Insert Frames" value="Longitudinal Y Insert Frames"/>
                  <EnumElem name="Longitudinal Y Frames"        value="Longitudinal Y Frames"/>
                  <EnumElem name="Decks"                        value="Decks"/>
                  <EnumElem name="Longitudinal Z Insert Frames" value="Longitudinal Z Insert Frames"/>
                  <EnumElem name="Longitudinal Z Frames"        value="Longitudinal Z Frames"/>
              </Data>
          </Pref>
      </Tab>
      <Tab name="Aft Perpendicular" csh="PMAN_ShipDrafting_Frame_Bar_Aft_Perpendicular">
        <Pref name="AftPerpendicular_ticDisplay" type="int" title="Tic Display" value="0" ver="8.0.0" scope="Part">
          <Description>This option specifies the aft perpendicular tic display type.</Description>
          <RadioBox/>
          <Data type="enum">
            <EnumElem name="One Side of Main Line" value="0"/>
            <EnumElem name="Both Sides of Main Line" value="1"/>
          </Data>
