<PrefDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www-cyp.ugsolutions.com/assemblies/pman">
  <Application name="Ship Drafting" csh="PMAN_ShipDrafting" package_suppress="simcenter3d,simviewer,view,mechatronicsplayer,nx2dedit">
    <Category name="General" csh="PMAN_ShipDrafting_General">
      <Tab name="General" csh="PMAN_ShipDrafting_General_General">
        <Pref name="ShipDrafting_UseBooklets" type="logical" title="Use Booklets" value="false" ver="10.0.0" scope="Session">
          <Description>This option determines whether or not non-master drawing parts should be created as booklets.</Description>
          <Toggle off="false"/>
          <Data type="enum">
            <EnumElem name="On" value="true"/>
            <EnumElem name="Off" value="false"/>
          </Data>
        </Pref>
        <Group name="Section Drawing View Layout">
          <Pref name="ShipDrafting_SectionsPerDrawing" type="int" title="Sections per Drawing" value="1" ver="10.0.0" scope="Session">
            <Description>This option specifies if single or multiple sections should be created in each non-master drawing part.</Description>
            <OptionMenu/>
            <Data type="enum">
              <EnumElem name="Single" value="0"/>
              <EnumElem name="Multiple" value="1"/>
            </Data>
          </Pref>
          <Pref name="ShipDrafting_ViewsPerSheet" type="int" title="Views per Sheet" value="0" ver="10.0.0" scope="Session">
            <Description>This option specifies the view count per sheet to be created in current drawing part. </Description>
            <OptionMenu/>
            <Data type="enum">
              <EnumElem name="1" value="0"/>
              <EnumElem name="2" value="1"/>
              <EnumElem name="3" value="2"/>
              <EnumElem name="4" value="3"/>
              <EnumElem name="5" value="4"/>
              <EnumElem name="6" value="5"/>
            </Data>
          </Pref>
          <Pref name="ShipDrafting_RowCount" type="int" title="Row Count" value="3" ver="12.0.0" scope="Session">
            <Description>This option specifies the number of rows into which the views should be arranged on the drawing sheet.</Description>
            <Integer/>
            <Data type="range">
              <LowerBound value="1" include="1"/>
              <UpperBound value="12" include="1"/>
            </Data>
          </Pref>
          <Pref name="ShipDrafting_ColumnCount" type="int" title="Column Count" value="3" ver="12.0.0" scope="Session">
            <Description>This option specifies the number of columns into which the views should be arranged on the drawing sheet.</Description>
            <Integer/>
            <Data type="range">
              <LowerBound value="1" include="1"/>
              <UpperBound value="12" include="1"/>
            </Data>
          </Pref>
        </Group>
        <Pref name="ShipDrafting_PreviewUseViewBorder" type="logical" title="Create Boundary for Preview" value="true" ver="11.0.0" scope="Session">
          <Description>This option determines whether or not the view boundary should be created during preview.</Description>
          <Toggle off="false"/>
          <Data type="enum">
            <EnumElem name="Yes" value="true"/>
            <EnumElem name="No" value="false"/>
          </Data>
        </Pref>
        <Pref name="ShipDrafting_EnableBlockSectionDrawing" type="logical" title="Display Multiple Block Sections" value="false" ver="12.0.2" scope="Session">
          <Description>This option specifies whether or not to display the primary block section with its adjacent block sections on the drawing.</Description>
          <Toggle off="false"/>
          <Data type="enum">
            <EnumElem name="Yes" value="true"/>
            <EnumElem name="No" value="false"/>
          </Data>
        </Pref>  
        <Pref name="ShipDrafting_ReferenceCurveMargin_MU" type="real" title="Block Section Boundary Margin" value="500.0" unit="mm" ver="12.0.2" scope="Part">
          <Description>This option specifies how much to extend the view section past the primary block section to show connections with adjacent block sections.</Description>
          <Real/>
          <Data type="range">
             <LowerBound value="0.0" include="0"/>
          </Data>
        </Pref>
        <Pref name="ShipDrafting_ReferenceCurveMargin_EU" type="real" title="Block Section Boundary Margin" value="19.685" unit="in" ver="12.0.2" scope="Part">
          <Description>This option specifies how much to extend the view section past the primary block section to show connections with adjacent block sections.</Description>
          <Real/>
          <Data type="range">
              <LowerBound value="0.0" include="0"/>
          </Data>
        </Pref>        
        <Pref name="ShipDrafting_ShipLineStyles" type="logical" title="Use Ship Line Styles" value="false" ver="1899" scope="Session">
          <Description>This option determines whether or not the Ship line styles will be used.</Description>
          <Toggle off="false"/>
          <Data type="enum">
            <EnumElem name="Yes" value="true"/>
            <EnumElem name="No" value="false"/>
          </Data>
        </Pref>
      </Tab>
      <Tab name="View Scale" csh="PMAN_ShipDrafting_General_View_Scale">
        <Group name="Basic Design">
          <Pref name="ShipDrafting_DeckViewScaleForBasicDesign" type="string" title="Deck View Scale" value="1:200" ver="11.0.0" scope="Session">
            <Description>This option specifies the deck view scale for Basic Design.</Description>
            <String/>
          </Pref>
          <Pref name="ShipDrafting_TransverseBulkheadsViewScaleForBasicDesign" type="string" title="Transverse Bulkhead View Scale" value="1:100" ver="11.0.0" scope="Session">
            <Description>This option specifies the transverse bulkhead view scale for Basic Design.</Description>
            <String/>
          </Pref>
          <Pref name="ShipDrafting_LongitudinalBulkheadsViewScaleForBasicDesign" type="string" title="Longitudinal Bulkhead View Scale" value="1:200" ver="11.0.0" scope="Session">
            <Description>This option specifies the longitudinal bulkhead view scale for Basic Design.</Description>
            <String/>
          </Pref>
        </Group>
        <Group name="Detail Design">
          <Pref name="ShipDrafting_DeckViewScaleForDetailDesign" type="string" title="Deck View Scale" value="1:50" ver="11.0.0" scope="Session">
            <Description>This option specifies the deck view scale for Detail Design.</Description>
            <String/>
          </Pref>
          <Pref name="ShipDrafting_TransverseBulkheadsViewScaleForDetailDesign" type="string" title="Transverse Bulkhead View Scale" value="1:50" ver="11.0.0" scope="Session">
            <Description>This option specifies the transverse bulkhead view scale for Detail Design.</Description>
            <String/>
          </Pref>
          <Pref name="ShipDrafting_LongitudinalBulkheadsViewScaleForDetailDesign" type="string" title="Longitudinal Bulkhead View Scale" value="1:50" ver="11.0.0" scope="Session">
            <Description>This option specifies the longitudinal bulkhead view scale for Detail Design.</Description>
            <String/>
          </Pref>
        </Group>
      </Tab>
    </Category>
    <Category name="Profile Sketch" csh="PMAN_ShipDrafting_Profile_Sketch">
