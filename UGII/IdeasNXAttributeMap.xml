<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<PLMXML xmlns="http://www.plmxml.org/Schemas/PLMXMLSchema" author="IanF" time="15:57:11" date="2003-06-12"
  schemaVersion="5.0">

  <AttributeMapTable sourceApp="I-deas" targetApp="NX">

    <!--  type="system Name" -->
    <Map sourceAttr="z_dmd Item Name"                          targetAttr="I-deas Item Name"        action="map"/>
