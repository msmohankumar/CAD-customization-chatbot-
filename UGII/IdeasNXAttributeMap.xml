    <!--  type="system Access" -->
    <Map sourceAttr="z_dmd Library Status"                     targetAttr="Library Status"          action="exclude"/>
    <Map sourceAttr="z_dmd In Use Lock Owner Name"             targetAttr="In-Use Owner"            action="exclude"/>
