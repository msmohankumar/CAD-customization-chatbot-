                    </Data>
                </Pref>
                <Pref name="DFM_TypicalCutoutMaxCutoutSize_EU" type="real" title="Maximum Cutout Size" value="0.004" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the maximum circumscribed shape for a cutout.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Label label="The following options are used to determine if the trimming shape can be made using only one trimming direction or if a complex cutout (two trimming directions) is needed."/>
                <Pref name="DFM_TypicalCutoutMinRoundDimension_MU" type="real" title="Minimum Round Diameter" value="0.1" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the minimum diameter for a round cutout.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="DFM_TypicalCutoutMinRoundDimension_EU" type="real" title="Minimum Round Diameter" value="0.004" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the minimum diameter for a round cutout.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="DFM_TypicalCutoutMinSquareDimension_MU" type="real" title="Minimum Square Width" value="0.1" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the minimum width for a square cutout.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="DFM_TypicalCutoutMinSquareDimension_EU" type="real" title="Minimum Square Width" value="0.004" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the minimum width for a square cutout.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="DFM_TypicalCutoutMinRectangleDimW_MU" type="real" title="Minimum Rectangle Width" value="0.1" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the minimum width for a rectangular cutout.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="DFM_TypicalCutoutMinRectangleDimW_EU" type="real" title="Minimum Rectangle Width" value="0.004" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the minimum width for a rectangular cutout.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="DFM_TypicalCutoutMinRectangleDimL_MU" type="real" title="Minimum Rectangle Length" value="0.1" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the minimum length for a rectangular cutout.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="DFM_TypicalCutoutMinRectangleDimL_EU" type="real" title="Minimum Rectangle Length" value="0.004" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the minimum length for a rectangular cutout.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="DFM_TypicalCutoutMinOblongDimW_MU" type="real" title="Minimum Oblong Width" value="0.1" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the minimum width for an oblong cutout.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="DFM_TypicalCutoutMinOblongDimW_EU" type="real" title="Minimum Oblong Width" value="0.004" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the minimum width for an oblong cutout.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="DFM_TypicalCutoutMinOblongDimL_MU" type="real" title="Minimum Oblong Length" value="0.1" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the minimum length for an oblong cutout.</Description>
                    <Real/>
                    <Data type="range">
