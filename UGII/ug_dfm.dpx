        <Category name="General" csh="PMAN_Design_for_Manufacturing_General">
            <Tab name="All" csh="PMAN_Design_for_Manufacturing_General_All">
                <Pref name="DFM_StandardsVersionDisplay" type="int" title="Standards Version Display" value="0" ver="3.0.0" scope="Session">
                    <Description>This option specifies how the Standards Version should be displayed.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Do Not Display Messages" value="-1"/>
                        <EnumElem name="Use Native Language Menu Text" value="0"/>
                        <EnumElem name="Display First Line Below" value="1"/>
                        <EnumElem name="Display Both Lines Below" value="2"/>
                    </Data>
                </Pref>
                <Pref name="DFM_AdvisorStandardsVersionLine1" type="string" title="Standards Version First Line" value="File Version:" ver="3.0.0" scope="Session">
                    <Description>This field allows the text for the first line of the standards version to be specified. The number of characters for this text must be between 1 and 42.</Description>
                    <Widestring/>
                </Pref>
                <Pref name="DFM_AdvisorStandardsVersionLine2" type="string" title="Standards Version Second Line" value="---------  2.0 --------" ver="3.0.0" scope="Session">
                    <Description>This field allows the text for the second line of the standards version to be specified. The number of characters for this text must be between 1 and 42.</Description>
                    <Widestring/>
                </Pref>
                <Separator/>
                <Pref name="DFM_ToleranceCharsForDisplay" type="int" title="Tolerance Characters for Display" value="1" ver="3.0.0" scope="Session">
                    <Description>This option specifies the number of characters that is displayed in front of each computed size for the Hole Size and Flanged Hole criteria. A value of 1 means that a G, C, or S is displayed. For example "G 10.0" would be a Gage hole of 10.0. A value of 2 means that Ga, Cr, or St is displayed. A limit of 20 characters is used for the entire field. Therefore, the number of characters specified here should not be more than about 5.</Description>
                    <Integer/>
                </Pref>
                <Pref name="DFM_UseEdgeNamesForObjects" type="int" title="Use Edge Names for Objects" value="1" ver="3.0.0" scope="Session">
                    <Description>DFM feature names will be generated automatically. If the part file is modified, these automatically generated names will probably change as well. This option determines whether or not names attached to the part sheet edges (not tipped data) should be used.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
                <Pref name="DFM_DeleteTempDrawFiles" type="int" title="Delete Temporary Draw Files" value="1" ver="3.0.0" scope="Session">
                    <Description>The Splitting, Wrinkling, Strong, Medium, and Weak Draw analysis creates temporary files to run the draw analysis. This option determines whether or not these files should be deleted. If the files are saved, they can be viewed in the analysis package used.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
                <Pref name="DFM_PressCamDirectionColor" type="int" title="Press Cam Direction Color" value="134" ver="3.0.0" scope="Session">
                    <Description>This option specifies the color of the temporary conehead vector which represents the direction of the pierce, trim, or flange when DFM features are displayed.</Description>
                    <Color/>
                </Pref>
                <Pref name="DFM_AdvisorPierceLimit_MU" type="real" title="Pierce Limit" value="100.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the upper limit for the circumscribed diameters which are to be considered Pierce objects. Circumscribed diameters greater than this value are considered to be Trim objects.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="DFM_AdvisorPierceLimit_EU" type="real" title="Pierce Limit" value="3.9370078" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the upper limit for the circumscribed diameters which are to be considered Pierce objects. Circumscribed diameters greater than this value are considered to be Trim objects.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
            </Tab>
        </Category>
        <Category name="Draw Criteria" csh="PMAN_Design_for_Manufacturing_Draw_Criteria">
            <Tab name="Draw Wall Angle" csh="PMAN_Design_for_Manufacturing_Draw_Criteria_Draw_Wall_Angle">
                <Docum>The purpose of this check is to determine if any backdraft conditions exist for the sheet metal being drawn or if flanges can be formed in the draw die.</Docum>
                <Label label="The following option is used to determine if any backdraft conditions exist for the sheet metal being drawn or if flanges can be formed in the draw die."/>
                <Pref name="DFM_DrawWallAngleRanges" type="string" title="Ranges" value="0.2 0.1" unit="deg" ver="3.0.0" scope="Session">
                    <Description>This option specifies limit values for ranges of angles between the sheet normal and the press direction vector. 2, 3, or 4 angle values can be specified (largest first). The range between each value are then used to apply a color fringe plot on the sheet metal.</Description>
                    <Widestring/>
                </Pref>
                <Label label="Angle &gt; first value: Pass"/>
                <Label label="Second value &lt; Angle &lt; first value: Warning"/>
                <Label label="Angle &lt; second (third and fourth) value: Failure"/>
            </Tab>
            <Tab name="Splitting and Wrinkling" csh="PMAN_Design_for_Manufacturing_Draw_Criteria_Splitting_and_Wrinkling">
                <Label label="The splitting check is used to ensure that splitting does not occur when there is no restraint at the boundary (the holding condition = free). The wrinkling check is used to ensure that wrinkling does not occur when the sheet metal is locked at the boundary (the holding condition = locked)."/>
                <Label label="The following options supply values used for the Forming Limit Diagram curves."/>
                <Pref name="DFM_FLDMargin" type="real" title="Margin" value="10.0" unit="%" ver="3.0.0" scope="Session">
                    <Description>This option specifies the safety factor for the marginal curve below the forming limit curve.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="DFM_FLDMaximumThinning" type="real" title="Maximum Thinning" value="30.0" unit="%" ver="3.0.0" scope="Session">
                    <Description>This option specifies the maximum thinning factor for the curve that represents the minor strain, major strain combinations at which the thickness of the sheet has a constant thinning. Any point above this curve represents elements which have excess thinning.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="DFM_FLDMinimumStrain" type="real" title="Minimum Strain Threshold" value="2.0" unit="%" ver="3.0.0" scope="Session">
                    <Description>This option specifies the value for the minimum strain threshold curve. Sheet metal requires a certain minimum strain in order to hold its shape well. Any point below this curve represents elements with insufficient stretching.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Binder Force" csh="PMAN_Design_for_Manufacturing_Draw_Criteria_Binder_Force">
                <Label label="The following options specify a factor to use between a free binder and a clamped binder."/>
                <Pref name="DFM_StrongBinderForce" type="real" title="Strong" value="0.9" ver="3.0.0" scope="Session">
                    <RealScale precision="2"/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1" name="Free"/>
                        <UpperBound value="1.0" include="1" name="Locked"/>
                    </Data>
                </Pref>
                <Pref name="DFM_MediumBinderForce" type="real" title="Medium" value="0.35" ver="3.0.0" scope="Session">
                    <RealScale precision="2"/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1" name="Free"/>
                        <UpperBound value="1.0" include="1" name="Locked"/>
                    </Data>
                </Pref>
                <Pref name="DFM_WeakBinderForce" type="real" title="Weak" value="0.15" ver="3.0.0" scope="Session">
                    <RealScale precision="2"/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1" name="Free"/>
                        <UpperBound value="1.0" include="1" name="Locked"/>
                    </Data>
                </Pref>
            </Tab>
        </Category>
        <Category name="Trim Criteria" csh="PMAN_Design_for_Manufacturing_Trim_Criteria">
            <Tab name="Scrap Shed Angles" csh="PMAN_Design_for_Manufacturing_Trim_Criteria_Scrap_Shed_Angles">
                <Label label="The following options are used to determine if the external trim curve will cause scrap to become trapped in the manufacturing process."/>
                <Pref name="DFM_ScrapShedStandardAngle" type="real" title="Standard" value="0.1" unit="deg" ver="3.0.0" scope="Session">
                    <Description>This option specifies the Standard angle for each side of the opening (if your standard is specified as an included angle, divide it by 2). If the angle is less than this value (but larger than the Standard Minimum angle), it is a Parallel Scrap condition. If the angle is larger than this value it is a Standard Scrap condition.</Description>
                    <Real/>
                </Pref>
                <Pref name="DFM_ScrapShedStandardAngleMin" type="real" title="Standard Minimum" value="0.0" unit="deg" ver="3.0.0" scope="Session">
                    <Description>This option specifies the Standard Minimum angle for each side of the opening (if your standard is specified as an included angle, divide it by 2). If the angle is less than this value, it is a Trapped Scrap condition.</Description>
                    <Real/>
                </Pref>
            </Tab>
            <Tab name="Trim Relief" csh="PMAN_Design_for_Manufacturing_Trim_Criteria_Trim_Relief">
                <Label label="The following options are used to ensure that an adequate notch at the end points of the flange breakline is present and constructed correctly to reduce the possibility of metal splitting during the flanging operation."/>
                <Pref name="DFM_TrimReliefAngle" type="real" title="Minimum Start Angle" value="60.0" unit="deg" ver="3.0.0" scope="Session">
                    <Description>This option specifies the angle the trimming curve makes with the breakline. If the trim relief starts at less then this angle then the trim relief feature will not pass.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                    <Docum>This value should be less than 90.0 - (Standard Scrap Shed Angle)/2 or the scrap shed check will fail before this check.</Docum>
                </Pref>
                <Pref name="DFM_TrimReliefOffset_MU" type="real" title="Minimum Offset Distance" value="0.1" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the distance from a point on the trimming curve to the end of the breakline (measured perpendicular to its tangent). The point on the trimming curve is the point where its tangent matches the tangent at the end of the breakline (i.e. the two tangents are parallel).</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                    <Docum>If no matching tangents are found and the trim relief starts at the correct angle (see Angle check above), then the trim relief is considered as a pass and the radii check is not performed.</Docum>
                </Pref>
                <Pref name="DFM_TrimReliefOffset_EU" type="real" title="Minimum Offset Distance" value="0.004" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the distance from a point on the trimming curve to the end of the breakline (measured perpendicular to its tangent). The point on the trimming curve is the point where its tangent matches the tangent at the end of the breakline (i.e. the two tangents are parallel).</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                    <Docum>If no matching tangents are found and the trim relief starts at the correct angle (see Angle check above), then the trim relief is considered as a pass and the radii check is not performed.</Docum>
                </Pref>
                <Pref name="DFM_TrimReliefAwayDistance_MU" type="real" title="Away Distance" value="12.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>If the Offset Distance found is greater than the value specified here, then the Offset Distance is considered an automatic pass and the checking of the radii is skipped.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="DFM_TrimReliefAwayDistance_EU" type="real" title="Away Distance" value="0.4724409" unit="in" ver="3.0.0" scope="Session">
                    <Description>If the Offset Distance found is greater than the value specified here, then the Offset Distance is considered an automatic pass and the checking of the radii is skipped.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="DFM_TrimReliefRadiiStandard_MU" type="real" title="Minimum Standard Radii" value="0.1" unit="mm" ver="3.0.0" scope="Session">
                    <Description>The radii checked are those from the flange breakline end point, to the first flat portion of the trimming curve after the trim relief offset point.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="DFM_TrimReliefRadiiStandard_EU" type="real" title="Minimum Standard Radii" value="0.004" unit="in" ver="3.0.0" scope="Session">
                    <Description>The radii checked are those from the flange breakline end point, to the first flat portion of the trimming curve after the trim relief offset point.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="DFM_TrimReliefRadiiDev_MU" type="real" title="Deviation Radii" value="0.1" unit="mm" ver="3.0.0" scope="Session">
                    <Description>Any radii less than this causes higher die costs. The radii checked are those from the flange breakline end point, to the first flat portion of the trimming curve after the trim relief offset point.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="DFM_TrimReliefRadiiDev_EU" type="real" title="Deviation Radii" value="0.004" unit="in" ver="3.0.0" scope="Session">
                    <Description>Any radii less than this causes higher die costs. The radii checked are those from the flange breakline end point, to the first flat portion of the trimming curve after the trim relief offset point.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Trim Cutout Types" csh="PMAN_Design_for_Manufacturing_Trim_Criteria_Trim_Cutout_Types">
                <Docum>The surface tangent direction is a direction 90 degrees to the surface normal and trimming curve tangent. The zero reference direction is a direction 90 degrees to the trimming direction and trimming curve tangent.
A true angle measurement is taken between the surface tangent and zero reference directions at a sufficient number of points along the boundary of the trimming object. (Said another way, the angle between two planes is being computed. One plane is defined by the trimming curve tangent and the surface normal. The other plane is defined by the trimming curve tangent and the trimming direction.) Up and down angles are in reference to the zero reference direction. If the angle measured from the zero reference direction to the surface tangent direction is in the opposite direction of the trimming direction, we have an up angle. If the opposite is true, we have a down angle.</Docum>
                <Label label="The following options are used to determine if the trimming shape can be made using only one trimming direction or if a complex cutout (two trimming directions) is needed."/>
                <Pref name="DFM_TrimCutoutMaxAngleUp" type="real" title="Maximum Up Angle" value="5.0" unit="deg" ver="3.0.0" scope="Session">
                    <Description>This option specifies the maximum allowed Up Angle.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="DFM_TrimCutoutMaxAngleDown" type="real" title="Maximum Down Angle" value="5.0" unit="deg" ver="3.0.0" scope="Session">
                    <Description>This option specifies the maximum allowed Down Angle.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Typical Cutouts" csh="PMAN_Design_for_Manufacturing_Trim_Criteria_Typical_Cutouts">
                <Label label="The following options are used to determine if any cutouts require additional die maintenance and construction costs."/>
                <Pref name="DFM_TypicalCutoutMaxCutoutSize_MU" type="real" title="Maximum Cutout Size" value="0.1" unit="mm" ver="3.0.0" scope="Session">
