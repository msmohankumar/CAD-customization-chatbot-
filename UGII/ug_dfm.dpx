                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="Compare Using Below Values Only" value="0"/>
                        <EnumElem name="Add Springback Angles to Below Values" value="1"/>
                        <EnumElem name="Subtract Springback Angle from Below Values" value="-1"/>
                    </Data>
                </Pref>
                <Pref name="DFM_FlangeBreaklineStandard" type="real" title="Standard Angle" value="85.0" unit="deg" ver="3.0.0" scope="Session">
                    <Description>This option specifies the standard angle. Angles less than this (down to the Deviation Angle) are considered minor deviations.</Description>
                    <Real/>
