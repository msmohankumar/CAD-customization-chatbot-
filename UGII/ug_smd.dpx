                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="SMD_BeadBlendRadius_EU" type="real" title="Blend Radius (r)" value="0.0" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the radius between the placement face and the sides of the U and V Beads.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="SMD_BeadBlendRadius_MU" type="real" title="Blend Radius (r)" value="0.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the radius between the placement face and the sides of the U and V Beads.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="SMD_BeadHeight_EU" type="real" title="Height (H)" value="1.0" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the height of the U and V Beads.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="SMD_BeadHeight_MU" type="real" title="Height (H)" value="20.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the height of the U and V Beads.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="SMD_BeadPlacementWidth_EU" type="real" title="Placement Width (W)" value="0.5" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the width at the base of the U and V Beads.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="SMD_BeadPlacementWidth_MU" type="real" title="Placement Width (W)" value="10.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the width at the base of the U and V Beads.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                </Group>
                <Group name="U Shaped Beads">
                <Pref name="SMD_BeadUAngle" type="real" title="Angle (A)" value="0.0" unit="deg" ver="3.0.0" scope="Session">
                    <Description>This option specifies the wall angle of the U Bead.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="SMD_BeadSecondaryWidth_EU" type="real" title="Secondary Width (w)" value="0.0" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the width at the top of the U Bead.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="SMD_BeadSecondaryWidth_MU" type="real" title="Secondary Width (w)" value="0.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the width at the top of the U Bead.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                </Group>
                <Group name="V Shaped Beads">
                <Pref name="SMD_BeadVAngle" type="real" title="Angle (A)" value="30.0" unit="deg" ver="3.0.0" scope="Session">
                    <Description>This option specifies the wall angle of the V Bead.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="SMD_BeadEndCapAngle" type="real" title="End Cap Angle" value="0.0" unit="deg" ver="3.0.0" scope="Session">
                    <Description>This option specifies the taper angle of the faces placed at the end of the V Bead.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                </Group>
                <Group name="Circular Shaped Beads">
                <Pref name="SMD_BeadCircularRadius_EU" type="real" title="Radius (R)" value="0.5" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the cross section radius of the circular Bead.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="SMD_BeadCircularRadius_MU" type="real" title="Radius (R)" value="10.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the cross section radius of the circular Bead.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="SMD_BeadCircularHeight_EU" type="real" title="Height (H)" value="0.0" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the height of the circular Bead.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="SMD_BeadCircularHeight_MU" type="real" title="Height (H)" value="0.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the height of the circular Bead.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="SMD_BeadEndCapRadius_EU" type="real" title="End Cap Radius" value="0.0" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the radius of the toroidal shape placed at the end of the circular Bead.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="SMD_BeadEndCapRadius_MU" type="real" title="End Cap Radius" value="0.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the radius of the toroidal shape placed at the end of the circular Bead.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                </Group>
                <Pref name="SMD_BeadTolerance_EU" type="real" title="Distance Tolerance" value="0.001" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the distance tolerance in approximating the curves created internally to build the Bead.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="SMD_BeadTolerance_MU" type="real" title="Distance Tolerance" value="0.01" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the distance tolerance in approximating the curves created internally to build the Bead.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="SMD_BeadAngleOption" type="int" title="Angle" value="0" ver="3.0.0" scope="Session">
                    <Description>This option specifies where the angle should be measured from on the U Bead.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="From Normals" value="0"/>
                        <EnumElem name="From Section Axis" value="1"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="SMD_BeadSecWidthOption" type="int" title="Secondary Width From" value="0" ver="3.0.0" scope="Session">
                    <Description>This option specifies how the secondary width should be measured. The Intersection Point option means that it is measured from the theoretical intersections between the top face and the side faces. The Tangent Point option means that it is measured from the tangents between the top and radius faces.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Intersection Point" value="0"/>
                        <EnumElem name="Tangent Point" value="1"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="SMD_BeadEndcapOption" type="int" title="End Cap" value="3" ver="3.0.0" scope="Session">
                    <Description>This option specifies which ends of the Bead should have end caps.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="None" value="0"/>
                        <EnumElem name="Start" value="1"/>
                        <EnumElem name="End" value="2"/>
                        <EnumElem name="Both" value="3"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="SMD_BeadAttachBead" type="int" title="Attach Bead" value="0" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not the Bead should be attached to a sheet or solid body.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                    <Docum>If attaching the Bead to a sheet body, the Bead will sew to the sheet. If attaching the Bead to a solid body, the Bead will either be united to or subtracted from (depending on the direction of the section axis) the solid body.</Docum>
                </Pref>
                <Pref name="SMD_BeadHollowBead" type="int" title="Hollow Bead" value="0" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not (if attaching the Bead to a solid body) the Bead will remove volume from the face opposite the placement face to maintain a constant thickness.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Punch" csh="PMAN_Sheet_Metal_Forming_and_Flattening_Cutouts_Punches_Corners_Punch">
                <Pref name="SMD_PunchPunchtype" type="int" title="Type" value="0" ver="3.0.0" scope="Session">
                    <Description>This option specifies the Punch type to be created.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Emboss" value="0"/>
                        <EnumElem name="Coin" value="1"/>
                        <EnumElem name="Lance" value="2"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="SMD_PunchToptype" type="int" title="Top Type" value="0" ver="3.0.0" scope="Session">
                    <Description>This option specifies the top type for the Punch. Round and Cone top types require a closed, circular profile and can only be placed on a single, planar placement face.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Offset" value="0"/>
                        <EnumElem name="Flat" value="1"/>
                        <EnumElem name="Round" value="2"/>
                        <EnumElem name="Cone" value="3"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="SMD_PunchDepth_EU" type="real" title="Depth" value="1.0" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the distance between the placement face and the floor of the Punch feature, measured from the tool center point along the Punch vector. For Coin type Punches, the depth value cannot be more than the thickness of the body on which the Punch is being applied.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="SMD_PunchDepth_MU" type="real" title="Depth" value="20.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the distance between the placement face and the floor of the Punch feature, measured from the tool center point along the Punch vector. For Coin type Punches, the depth value cannot be more than the thickness of the body on which the Punch is being applied.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="SMD_PunchDieRadius_EU" type="real" title="Die Radius" value="0.0" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the radius of the blend between the placement and "through" faces and the sides of the Punch. This is a fixed value resulting in a constant-radius blend.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="SMD_PunchDieRadius_MU" type="real" title="Die Radius" value="0.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the radius of the blend between the placement and "through" faces and the sides of the Punch. This is a fixed value resulting in a constant-radius blend.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="SMD_PunchTaperAngle" type="real" title="Taper Angle (Emboss)" value="0.0" unit="deg" ver="3.0.0" scope="Session">
                    <Description>This option specifies the constant angle, relative to the Punch direction vector, that determines how the sides of the Punch are angled.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="SMD_PunchPunchRadius_EU" type="real" title="Punch Radius" value="0.0" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the blend radius between the sides of the Punch and the Punch top (apex), or the radius used to construct the very top of the Punch in the case of a closed, circular emboss (e.g. dimple) with a round or cone top.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="SMD_PunchPunchRadius_MU" type="real" title="Punch Radius" value="0.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the blend radius between the sides of the Punch and the Punch top (apex), or the radius used to construct the very top of the Punch in the case of a closed, circular emboss (e.g. dimple) with a round or cone top.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="SMD_PunchConeDepth_EU" type="real" title="Cone Depth" value="0.0" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the depth of the cone top. (This parameter is only required for a Cone top type Punch.)</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="SMD_PunchConeDepth_MU" type="real" title="Cone Depth" value="0.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the depth of the cone top. (This parameter is only required for a Cone top type Punch.)</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="SMD_PunchUseFaceNormalDirection" type="int" title="Face Normal Direction" value="1" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not the Punch should be created normal to the placement face.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="SMD_PunchOutlinetype" type="int" title="Placement Outline" value="0" ver="3.0.0" scope="Session">
                    <Description>This option specifies the placement outline type. The Inside option assigns the Punch opening line as the placement outline, the Outside option assigns the perimeter of the Punch.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Inside" value="0"/>
                        <EnumElem name="Outside" value="1"/>
                    </Data>
                </Pref>
                <Pref name="SMD_PunchUseAutoCentroid" type="int" title="Auto Centroid Creation" value="1" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not an automatic tool center point (centroid) should be created based on a 2D analysis of the placement outline curves.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
                <Pref name="SMD_PunchUseFlangedCutout" type="int" title="Flanged Cutout" value="0" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not a flanged cutout (also known as drawn cutout, flanged hole) should be created in lieu of the emboss.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Hole" csh="PMAN_Sheet_Metal_Forming_and_Flattening_Cutouts_Punches_Corners_Hole">
                <Pref name="SMD_HolePositionMethod" type="int" title="Position Method" value="0" ver="3.0.0" scope="Session">
                    <Description>This option specifies the positioning method for the Hole on the sheet metal.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Edge Offsets" value="0"/>
                        <EnumElem name="Positioning" value="1"/>
                    </Data>
                </Pref>
                <Pref name="SMD_HoleEdge1Offset_EU" type="real" title="Edge 1 Offset" value="0.0" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the offset for the Hole position from the first edge (when the Hole Position Method option is set to Edge Offsets).</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="SMD_HoleEdge1Offset_MU" type="real" title="Edge 1 Offset" value="0.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the offset for the Hole position from the first edge (when the Hole Position Method option is set to Edge Offsets).</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="SMD_HoleOffsetDist2_EU" type="real" title="Edge 2 Offset" value="0.0" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the offset for the Hole position from the second edge (when the Hole Position Method option is set to Edge Offsets).</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="SMD_HoleOffsetDist2_MU" type="real" title="Edge 2 Offset" value="0.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the offset for the Hole position from the second edge (when the Hole Position Method option is set to Edge Offsets).</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="SMD_HoleType" type="int" title="Type" value="0" ver="3.0.0" scope="Session">
                    <Description>This option specifies the type of Hole. The Punch option means that one thickness of the solid body will be pierced. The Through option means that the Hole will go through to a selected face. Finally, the Depth option allows a depth for the Hole to be specified.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Punch" value="0"/>
                        <EnumElem name="Through" value="1"/>
                        <EnumElem name="Depth" value="2"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="SMD_HoleDiameter_EU" type="real" title="Diameter" value="1.0" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the diameter of the Hole.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="SMD_HoleDiameter_MU" type="real" title="Diameter" value="20.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the diameter of the Hole.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="SMD_HoleDepth_EU" type="real" title="Depth" value="1.0" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the depth of the Hole (when the Type option is set to Depth).</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="SMD_HoleDepth_MU" type="real" title="Depth" value="20.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the depth of the Hole (when the Type option is set to Depth).</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="SMD_HoleTipAngle" type="real" title="Tip Angle" value="0.0" unit="deg" ver="3.0.0" scope="Session">
                    <Description>This option specifies the tip angle at the bottom of the Hole (when the Type option is set to Depth).</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                        <UpperBound value="180.0" include="0"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="SMD_HoleFaceNormalDirection" type="int" title="Face Normal Direction" value="1" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not the Hole should be created normal to the placement face.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Slot" csh="PMAN_Sheet_Metal_Forming_and_Flattening_Cutouts_Punches_Corners_Slot">
                <Pref name="SMD_SlotPosMethod" type="int" title="Position Method" value="0" ver="3.0.0" scope="Session">
                    <Description>This option specifies the positioning method for the Slot on the sheet metal.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Edge Offsets" value="0"/>
                        <EnumElem name="Positioning" value="1"/>
                    </Data>
                </Pref>
                <Pref name="SMD_SlotEdge1Offset_EU" type="real" title="Edge 1 Offset" value="0.0" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the offset for the Slot position from the first edge (when the Slot Position Method option is set to Edge Offsets).</Description>
                    <Real/>
                </Pref>
                <Pref name="SMD_SlotEdge1Offset_MU" type="real" title="Edge 1 Offset" value="0.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the offset for the Slot position from the first edge (when the Slot Position Method option is set to Edge Offsets).</Description>
                    <Real/>
                </Pref>
                <Pref name="SMD_SlotEdge2Offset_EU" type="real" title="Edge 2 Offset" value="0.0" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the offset for the Slot position from the second edge (when the Slot Position Method option is set to Edge Offsets).</Description>
                    <Real/>
                </Pref>
                <Pref name="SMD_SlotEdge2Offset_MU" type="real" title="Edge 2 Offset" value="0.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the offset for the Slot position from the second edge (when the Slot Position Method option is set to Edge Offsets).</Description>
                    <Real/>
                </Pref>
                <Separator/>
                <Pref name="SMD_SlotType" type="int" title="Type" value="0" ver="3.0.0" scope="Session">
                    <Description>This option specifies the type of Slot. The Punch option means that one thickness of the solid body will be pierced. The Through option means that the Slot will go through to a selected face. Finally, the Depth option allows a depth for the Slot to be specified.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Punch" value="0"/>
                        <EnumElem name="Through" value="1"/>
                        <EnumElem name="Depth" value="2"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="SMD_SlotLength_EU" type="real" title="Length" value="1.0" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the length of the Slot from arc tangent to arc tangent. The length must be a positive value greater than the width.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="SMD_SlotLength_MU" type="real" title="Length" value="20.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the length of the Slot from arc tangent to arc tangent. The length must be a positive value greater than the width.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="SMD_SlotWidth_EU" type="real" title="Width" value="0.5" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the width of the Slot. The width value must be a positive value less than the length.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="SMD_SlotWidth_MU" type="real" title="Width" value="10.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the width of the Slot. The width value must be a positive value less than the length.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="SMD_SlotDepth_EU" type="real" title="Depth" value="1.0" unit="in" ver="3.0.0" scope="Session">
                    <Description>This option specifies the depth of the Slot (when the Type option is set to Depth).</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="SMD_SlotDepth_MU" type="real" title="Depth" value="20.0" unit="mm" ver="3.0.0" scope="Session">
                    <Description>This option specifies the depth of the Slot (when the Type option is set to Depth).</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="SMD_SlotUseFaceNormalDirection" type="int" title="Face Normal Direction" value="1" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not the Slot should be created normal to the placement face.</Description>
                    <Toggle off="0"/>
