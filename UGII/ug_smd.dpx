        <Category name="Forming Features/Operators" csh="PMAN_Sheet_Metal_Forming_and_Flattening_Forming_Features_Operators">
            <Tab name="Edge Rip" csh="PMAN_Sheet_Metal_Forming_and_Flattening_Forming_Features_Operators_Edge_Rip">
                <Pref name="SMD_EdgeRipGapValue_EU" type="real" title="Gap" value="0.05" unit="in" ver="3.0.0" scope="Session">
