                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Pref name="MW_EjectorPinSingleFeature" type="logical" title="Combine Multiple Ejector Pins in One Trim Feature" value="false" ver="8.5.0" scope="Session">
                    <Description>This option determines whether or not ejector pin post processing should create only one trim body feature with target bodies of multiple selected ejector pins.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Group name="Trim Part Object Layers">
                    <Pref name="MW_TrimPartCoreSurfLayer" type="int" title="Core Surface" value="29" ver="4.0.0" scope="Session">
                        <Description>This option specifies which layer the core surface should be placed on.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="1" include="1"/>
                            <UpperBound value="256" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_TrimPartCavitySurfLayer" type="int" title="Cavity Surface" value="30" ver="4.0.0" scope="Session">
                        <Description>This option specifies which layer the cavity surface should be placed on.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="1" include="1"/>
                            <UpperBound value="256" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_TrimPartCoreMoldSurfLayer" type="int" title="Core Mold Surface" value="31" ver="4.0.0" scope="Session">
                        <Description>This option specifies which layer the core mold surface should be placed on.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="1" include="1"/>
                            <UpperBound value="256" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_TrimPartCavityMoldSurfLayer" type="int" title="Cavity Mold Surface" value="32" ver="4.0.0" scope="Session">
                        <Description>This option specifies which layer the cavity mold surface should be placed on.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="1" include="1"/>
                            <UpperBound value="256" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_TrimPartUserInsertSurfLayer" type="int" title="User Insert Surface" value="33" ver="4.0.0" scope="Session">
                        <Description>This option specifies which layer the user insert surface should be placed on.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="1" include="1"/>
                            <UpperBound value="256" include="1"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Runner" csh="PMAN_Mold_Wizard_Others_Runner">
                <Group name="Guide">
                    <Pref name="MW_RunnerGuideColor" type="int" title="Color" value="31" ver="4.0.0" scope="Session">
                        <Description>This option specifies the color of the runner guide.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="MW_RunnerGuideLayer" type="int" title="Layer" value="171" ver="4.0.0" scope="Session">
                        <Description>This option specifies which layer the runner guide should be placed on.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="1" include="1"/>
                            <UpperBound value="256" include="1"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Solid">
                    <Pref name="MW_RunnerSolidColor" type="int" title="Color" value="6" ver="4.0.0" scope="Session">
                        <Description>This option specifies the color of the runner solid.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="MW_RunnerSolidLayer" type="int" title="Layer" value="72" ver="4.0.0" scope="Session">
                        <Description>This option specifies which layer the runner solid should be placed on.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="1" include="1"/>
                            <UpperBound value="256" include="1"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Cooling" csh="PMAN_Mold_Wizard_Others_Cooling">
                <Pref name="MW_CoolInterferenceCheck" type="logical" title="Interference Check" value="false" ver="4.0.0" scope="Session">
                    <Description>This option determines whether or not the system should automatically check for cooling interference with the cavity or core surface. There is a possible loss of performance using automatic checking.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Off" value="false"/>
                        <EnumElem name="On" value="true"/>
                    </Data>
                </Pref>
                <Group name="Baffle Part Clearances">
                    <Pref name="MW_CoolingBaffleAxialClearance_MU" type="real" title="Axial" value="15.0" unit="mm" ver="4.0.0" scope="Session">
                        <Description>This option specifies the axial clearance value used to determine the length of the cooling hole and baffle.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_CoolingBaffleAxialClearance_EU" type="real" title="Axial" value="0.6" ver="4.0.0" scope="Session" unit="in">
                        <Description>This option specifies the axial clearance value used to determine the length of the cooling hole and baffle.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_CoolingBaffleRadialClearance_MU" type="real" title="Radial" value="10.0" ver="4.0.0" scope="Session" unit="mm">
                        <Description>This option specifies the radial clearance value used to determine the length of the cooling hole and baffle.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_CoolingBaffleRadialClearance_EU" type="real" title="Radial" value="0.4" ver="4.0.0" scope="Session" unit="in">
                        <Description>This option specifies the radial clearance value used to determine the length of the cooling hole and baffle.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Channel">
                    <Pref name="MW_CoolingPartiallyShaded" type="logical" title="Partially Shaded" value="true" ver="7.5.2" scope="Session">
                        <Description>This option determines whether or not partial shading display should be applied to faces and bodies of cooling channel at creation time.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>        
                    </Pref>
                    <Pref name="MW_CoolingChannelLayerName" type="string" title="Layer Category Name" value="CoolChannel" ver="6.0.0" scope="Session">
                        <Description>This field allows a layer category name to be specified for the layer where the cooling channel is placed.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="MW_CoolingEndClearance_MU" type="real" title="Extension Clearance" value="1.0" unit="mm" ver="1899" scope="Session">
                        <Description>This option specifies the extension value for the channel.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_CoolingEndClearance_EU" type="real" title="Extension Clearance" value="0.04" unit="in" ver="1899" scope="Session">
                        <Description>This option specifies the extension value for the channel.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_CoolingAngledClearance_MU" type="real" title="Angled Fitting Clearance" value="0.5" unit="mm" ver="1980" scope="Session">
                        <Description>This option specifies the clearance value to adjust an angled fitting.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_CoolingAngledClearance_EU" type="real" title="Angled Fitting Clearance" value="0.02" unit="in" ver="1980" scope="Session">
                        <Description>This option specifies the clearance value to adjust an angled fitting.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_CoolingUpdateDistanceToBoundaryBody" type="logical" title="Update Distance to Boundary Body" value="true" ver="1953+" scope="Session">
                        <Description>This option determines whether or not a direct channel with "None" type updates the distance value to the boundary body. There is a possible loss of performance using distance update.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Pocket" csh="PMAN_Mold_Wizard_Others_Pocket">
                <Pref name="MW_Pocket_ExpressionSuppControl" type="logical" title="Expression Control" value="false" ver="4.0.0" scope="Session">
                    <Description>This option determines whether or not the subtract feature should be suppressed by expression in the target part.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Mold CSYS" csh="PMAN_Mold_Wizard_Others_Mold_CSYS">
              <Pref name="MW_UPDATE_PARTING_PART_WCS" type="logical" title="Update Parting Part WCS to Linked Datum CSYS" value="true" ver="2007" scope="Session">
                <Description>This option determines whether or not to update the parting part WCS when repositioning the original product component for molding in the Mold CSYS command. When the option is on, the parting part WCS will be same as the linked Datum CSYS.</Description>
                <Toggle off="false"/>
                <Data type="enum">
                  <EnumElem name="No" value="false"/>
                  <EnumElem name="Yes" value="true"/>
                </Data>
              </Pref>
            </Tab>
        </Category>
    </Application>
</PrefDefinition>
