<PrefDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www-cyp.ugsolutions.com/assemblies/pman">
    <Application name="Mold Wizard" csh="PMAN_Mold_Wizard" package_suppress="simcenter3d,simviewer,view,mechatronicsplayer,nx2dedit">
        <Category name="General" csh="PMAN_Mold_Wizard_General">
            <Tab name="General" csh="PMAN_Mold_Wizard_General_General">
                <Pref name="MW_ExcelBackground" type="logical" title="Operate NX while Microsoft Excel is Open" value="true" ver="4.0.0" scope="Session">
                    <Description>This option determines whether or not NX can be operated while Microsoft Excel is open.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Pref name="MW_Load_WAVE_Data" type="logical" title="Load Wave Data" value="false" ver="4.0.0" scope="Session">
                    <Description>This option determines whether or not WAVE data should be loaded.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
              <Group name="Teamcenter">
                <Pref name="MW_PART_ITEM_TYPE" type="string" title="Part Item Type" value="Item" ver="9.0.1" scope="Session">
                  <Description>This option specifies the customized item type of the part in Teamcenter.</Description>
                  <String/>
                </Pref>
                <Pref name="MW_CONFIG_ITEM" type="string" title="Configuration File Item ID and Revision" value="@DB/00005/A" ver="11.0.0" scope="Session">
                  <Description>This option specifies the item ID and revision of the configuration file type in Teamcenter.</Description>
                  <String/>
                </Pref>
              </Group>
            </Tab>
            <Tab name="Project Settings" csh="PMAN_Mold_Wizard_General_Project_Setting">
                <Group name="Project Initialize">
                    <Pref name="MW_ProjectInitialUnit" type="int" title="Project Unit" value="3" ver="2306" scope="Session">
                        <Description>This option specifies the project unit.</Description>
                        <RadioBox/>
                        <Data type="enum">
                            <EnumElem name="Metric" value="1"/>
                            <EnumElem name="English" value="2"/>
                            <EnumElem name="Same as Project Library" value="3"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_ProjectPath_WIN" type="string" title="Project File Path" value="" ver="4.0.0" scope="Session">
                        <Description>This field defines the project file path.</Description>
                        <BrowseDirSel/>
                    </Pref>
                    <Pref name="MW_ProjectPath_UNX" type="string" title="Project File Path" value="" ver="4.0.0" scope="Session">
                        <Description>This field defines the project file path.</Description>
                        <BrowseDirSel/>
                    </Pref>
                  <Pref name="MW_UPDATE_MOLD_CSYS_WORKPIECE_AUTOMATICALLY" type="logical" title="Update Mold CSYS and Workpiece Automatically" value="true" ver="1980" scope="Session">
                    <Description>This option determines whether or not to update the mold CSYS and workpiece automatically when initializing a project.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                      <EnumElem name="No" value="false"/>
                      <EnumElem name="Yes" value="true"/>
                    </Data>
                  </Pref>
                </Group>
            </Tab>
            <Tab name="Part Name Definition" csh="PMAN_Mold_Wizard_General_Part_Name_Definition">
                <Group name="Common">
                    <Pref name="MW_DISPLAY_RENAME_DIALOG" type="logical" title="Display Rename Dialog Box" value="false" ver="6.0.0" scope="Session">
                        <Description>This option determines whether or not the part name mapping dialog box will be displayed when new parts are added to a Mold Wizard project.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_NAME_MAP_TEMPLATE_FILE" type="string" title="Template Name Mapping File" value="map_template_name.xlsx::DEFAULT_MAPPING_TEMPLATE" ver="1926" scope="Session">
                        <Description>This option specifies a customized file to map template name.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="MW_Part_Name_Rule" type="string_list" title="Custom Part Naming Rules" value="&quot;&lt;PROJECT_NAME&gt;_&lt;TEMPLATE_NAME&gt;_???&quot;\&quot;&lt;PROJECT_NAME&gt;_&lt;TEMPLATE_NAME&gt;_&lt;???&gt;&quot;" ver="1926" scope="Session">
                        <Description>This field allows the custom part naming rules to be specified. The part name is formed by combining a series of variables and characters.
<Formatted>1. All alphanumeric characters are valid except \, /, ", *, :, and ?.
2. The "?" character is an incrementing or user defined variable.
The standard variables are:
"PROJECT_NAME"  (set during project initialization)
"TEMPLATE_NAME" (the name of the template part being cloned).</Formatted></Description>
                        <MultiLineText height="6"/>
                    </Pref>
                </Group>
                <Group name="Teamcenter">
                    <Pref name="MW_NAME_IMAN_NUMBER" type="logical" title="Use Teamcenter Number as ??? in above Naming Rule" value="true" ver="6.0.0" scope="Session">
                        <Description>This option determines whether or not the Teamcenter name number should be used.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_IMAN_Name_Method" type="int" title="Teamcenter Part Naming Method" value="2" ver="6.0.0" scope="Session">
                    <Description>This option specifies the part naming method in the Teamcenter environment.
<Formatted>
First option: DB_PART_NAME = DB_PART_NO, which is defined by name rules.

Second option: DB_PART_NO is defined by name rules, DB_PART_NAME is replaced by TEMPLATE_NAME.

Third option: DB_PART_NO is defined by unique Teamcenter number, DB_PART_NAME is defined by name rules.</Formatted></Description>
                        <RadioBox/>
                        <Data type="enum">
                            <EnumElem name="Use Naming Rule for Part Name and Part Number" value="0"/>
                            <EnumElem name="Use Naming Rule for Part Number, Template Name for Part Name" value="1"/>
                            <EnumElem name="Use Teamcenter Number for Part Number, Naming Rule for Part Name" value="2"/>
                        </Data>
                    </Pref>
                </Group>
                <Pref name="MW_PART_FAMILY_LIB_SAVE_DIR_UNX" type="string" title="Part Family Save Directory" value="" ver="4.0.0" scope="Session">
                    <Description>This field defines the directory in which part family data should be saved.</Description>
                    <BrowseDirSel/>
                </Pref>
                <Pref name="MW_PART_FAMILY_LIB_SAVE_DIR_WIN" type="string" title="Part Family Save Directory" value="" ver="4.0.0" scope="Session">
                    <Description>This field defines the directory in which part family data should be saved.</Description>
                    <BrowseDirSel/>
                </Pref>
            </Tab>
            <Tab name="Layer and Category" csh="PMAN_Mold_Wizard_General_Layer_and_Category">
                <Group name="Hidden Objects">
                    <Pref name="MW_HideObjectLayer" type="int" title="Layer" value="255" ver="4.0.0" scope="Session">
                        <Description>This option specifies which layer the hidden objects should be placed on.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="1" include="1"/>
                            <UpperBound value="256" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_HideObjectLayerName" type="string" title="Layer Category Name" value="HiddenObject" ver="6.0.0" scope="Session">
                        <Description>This field allows a layer category name to be specified for the layer where the hidden objects are placed.</Description>
                        <Widestring/>
                    </Pref>
                </Group>
                <Group name="Datum">
                    <Pref name="MW_DatumLayer" type="int" title="Layer" value="100" ver="4.0.0" scope="Session">
                        <Description>This option specifies which layer the datum objects should be placed on.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="1" include="1"/>
                            <UpperBound value="256" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_DatumLayerName" type="string" title="Layer Category Name" value="Datum" ver="4.0.0" scope="Session">
                        <Description>This field allows a layer category name to be specified for the layer where the datum objects are placed.</Description>
                        <Widestring/>
                    </Pref>
                </Group>
            </Tab>
        </Category>
        <Category name="Workpiece" csh="PMAN_Mold_Wizard_Work_Piece">
            <Tab name="General" csh="PMAN_Mold_Wizard_Work_Piece_General">
                <Pref name="MW_WorkPieceOffset_MU" type="real" title="Offset" value="25.0" unit="mm" ver="4.0.0" scope="Session">
                    <Description>This option specifies the offset value of the initial work piece calculated from the bounding box of the product model.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="MW_WorkPieceOffset_EU" type="real" title="Offset" value="1.0" unit="in" ver="4.0.0" scope="Session">
                    <Description>This option specifies the offset value of the initial work piece calculated from the bounding box of the product model.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="MW_WorkPieceMethod" type="int" title="Dimensioning" value="0" ver="4.0.0" scope="Session">
                    <Description>This option specifies the work piece dimensioning method. The Distance Allowance method is centered on the product body. The Reference Point method is centered on the absolute CSYS or a specified point.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="Distance Allowance" value="0"/>
                        <EnumElem name="Reference Point" value="1"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Layer and Category" csh="PMAN_Mold_Wizard_Work_Piece_Layer_and_Category">
                <Group name="Workpiece">
                <Pref name="MW_WorkPieceLayer" type="int" title="Layer" value="20" ver="4.0.0" scope="Session">
                    <Description>This option specifies which layer the work piece should be placed on.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                        <UpperBound value="256" include="1"/>
                    </Data>
                </Pref>
                <Pref name="MW_WorkPieceLayerName" type="string" title="Layer Category Name" value="WorkPiece" ver="4.0.0" scope="Session">
                    <Description>This field allows a layer category name to be specified for the layer where the work piece is placed.</Description>
                    <Widestring/>
                </Pref>
                </Group>
                <Group name="Combined Workpiece">
                <Pref name="MW_CombinedWorkPieceLayer" type="int" title="Layer" value="200" ver="10.0.0" scope="Session">
                    <Description>This option specifies which layer the combined work piece should be placed on.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                        <UpperBound value="256" include="1"/>
                    </Data>
                </Pref>
                <Pref name="MW_CombinedWorkPieceLayerName" type="string" title="Layer Category Name" value="CombinedWorkPiece" ver="10.0.0" scope="Session">
                    <Description>This field allows a layer category name to be specified for the layer where the combined work piece is placed.</Description>
                    <Widestring/>
                </Pref>
                </Group>
            </Tab>
        </Category>
        <Category name="Mold Tools" csh="PMAN_Mold_Wizard_Mold_Tools">
            <Tab name="General" csh="PMAN_Mold_Wizard_Mold_Tools_General">
                <Pref name="MW_ToolsTolerance_MU" type="real" title="Tolerance" value="0.025" unit="mm" ver="4.0.0" scope="Session">
                    <Description>This option specifies the modeling tolerance for the Mold Tools patch command's sheet trim, enlarge face, and traverse loop.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="MW_ToolsTolerance_EU" type="real" title="Tolerance" value="0.001" unit="in" ver="4.0.0" scope="Session">
                    <Description>This option specifies the modeling tolerance for the Mold Tools patch command's sheet trim, enlarge face, and traverse loop.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Group name="Bounding Body">
                    <Pref name="MW_ToolsBoxTranslucency" type="int" title="Translucency" value="50" ver="8.0.0" scope="Session">
                        <Description>The Bounding Body command creates solid boxes (blocks). This option specifies the translucency of the box.</Description>
                        <IntegerScale/>
                        <Data type="range">
                            <LowerBound value="0" include="1"/>
                            <UpperBound value="100" include="1"/>
                        </Data>
                    </Pref>                                
                </Group>    
                <Pref name="MW_ToolsPatchFeatureNum" type="int" title="Maximum Number of Features for Feature Patch" value="150" ver="4.0.0" scope="Session">
                    <Description>This option specifies the maximum number of features allowed in the original product model for feature patch to be active as a patching option.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                    </Data>
                    <Docum>When patching a hole in the product model, a method called feature patch-up is sometimes used. This method creates new features in the original product model. If the original product model has a large number of features, the update processing may be time-consuming.</Docum>
                </Pref>
            </Tab>
            <Tab name="Casting Process Assistant" csh="PMAN_Mold_Wizard_Mold_Tools_Casting_Process_Assistant">
                <Group name="Body Box Offsets">
                    <Pref name="MW_CastingBodyBoxOffsetX1_MU" type="real" title="X1" value="10.0" unit="mm" ver="4.0.0" scope="Session">
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_CastingBodyBoxOffsetX1_EU" type="real" title="X1" value="0.4" unit="in" ver="4.0.0" scope="Session">
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_CastingBodyBoxOffsetX2_MU" type="real" title="X2" value="10.0" unit="mm" ver="4.0.0" scope="Session">
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_CastingBodyBoxOffsetX2_EU" type="real" title="X2" value="0.4" unit="in" ver="4.0.0" scope="Session">
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_CastingBodyBoxOffsetY1_MU" type="real" title="Y1" value="10.0" unit="mm" ver="4.0.0" scope="Session">
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_CastingBodyBoxOffsetY1_EU" type="real" title="Y1" value="0.4" unit="in" ver="4.0.0" scope="Session">
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_CastingBodyBoxOffsetY2_MU" type="real" title="Y2" value="10.0" unit="mm" ver="4.0.0" scope="Session">
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_CastingBodyBoxOffsetY2_EU" type="real" title="Y2" value="0.4" unit="in" ver="4.0.0" scope="Session">
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Step Parting Sheet">
                    <Pref name="MW_CastingStepSheetToolLand_MU" type="real" title="Tool Land" value="5.0" unit="mm" ver="4.0.0" scope="Session">
                        <Real/>
                    </Pref>
                    <Pref name="MW_CastingStepSheetToolLand_EU" type="real" title="Tool Land" value="0.2" unit="in" ver="4.0.0" scope="Session">
                        <Real/>
                    </Pref>
                    <Pref name="MW_CastingStepSheetDraftAngle" type="real" title="Draft Angle" value="5.0" unit="deg" ver="4.0.0" scope="Session">
                        <Real/>
                    </Pref>
                    <Pref name="MW_CastingStepSheetRadius_MU" type="real" title="Radius" value="5.0" unit="mm" ver="4.0.0" scope="Session">
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_CastingStepSheetRadius_EU" type="real" title="Radius" value="0.2" unit="in" ver="4.0.0" scope="Session">
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_CastingStepSheetCornerRadiusOption" type="int" title="Corner Radius Type" value="0" ver="4.0.0" scope="Session">
                        <RadioBox orientation="horizontal"/>
                        <Data type="enum">
                            <EnumElem name="Constant" value="0"/>
                            <EnumElem name="Conical" value="1"/>
                        </Data>
                    </Pref>
                </Group>
                <Pref name="MW_CastingRampAngle" type="real" title="Ramp Parting Sheet Angle" value="5.0" unit="deg" ver="4.0.0" scope="Session">
                    <Real/>
                </Pref>
            </Tab>
            <Tab name="Design Inserts" csh="PMAN_Mold_Wizard_Mold_Tools_Design_Inserts">
                <Pref name="MW_InsertColor" type="int" title="Color" value="11" ver="4.0.0" scope="Session">
                    <Description>This option specifies the color of the insert.</Description>
                    <Color/>
                </Pref>
                <Pref name="MW_InsertLayer" type="int" title="Layer" value="175" ver="4.0.0" scope="Session">
                    <Description>This option specifies which layer the insert should be placed on.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                        <UpperBound value="256" include="1"/>
                    </Data>
                </Pref>
                <Pref name="MW_InsertLayerName" type="string" title="Layer Category Name" value="Subinsert" ver="4.0.0" scope="Session">
                    <Description>This field allows a layer category name to be specified for the layer where the insert objects are placed.</Description>
                    <Widestring/>
                </Pref>
            </Tab>
        </Category>
        <Category name="Parting" csh="PMAN_Mold_Wizard_Parting">
            <Tab name="General" csh="PMAN_Mold_Wizard_Parting_General">
                <Pref name="MW_PartingAutoEdit" type="logical" title="Automatic Update on Product Model Change" value="false" ver="4.0.0" scope="Session">
                    <Description>The Mold Wizard parting module can monitor the product model for changes that affect parting geometry and automatically update the parting lines and surfaces when possible. This option determines whether or not the parting will be edited automatically.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Off" value="false"/>
                        <EnumElem name="On" value="true"/>
                    </Data>
                    <Docum>In some cases, new parting geometry is designed by the system automatically. In some complex cases, it may be preferable to update the parting design manually rather than automatically. Clear this option if you plan to make a complex change to the product.</Docum>
                </Pref>
                <Pref name="MW_PartingAutoProcess" type="logical" title="Automated Parting Process" value="true" ver="4.0.0" scope="Session">
                    <Description>This option determines whether or not the parting process should be done automatically.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Off" value="false"/>
                        <EnumElem name="On" value="true"/>
                    </Data>
                </Pref>
                <Pref name="MW_PartingColorTraverse" type="logical" title="Traverse by Color" value="true" ver="4.0.0" scope="Session">
                    <Description>When defining the parting curves or edges from the product model, this option determines whether or not any curve that has different face colors on either side should be selected. The option allows the parting command to automatically finds all connected curves or edges with the same adjacent colors as the start curve.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Pref name="MW_PartingAssociateSwap" type="logical" title="Associative Swap" value="true" ver="4.0.0" scope="Session">
                    <Description>This option determines whether or not the Swap Product Model command should be associative. When this option is on, it allows the product model used in a mold design project to be replaced with a new version of the model, and still keep full association between the existing mold design features (such as tapers, split faces, parting lines, patch surfaces, parting surfaces, etc) in the mold assembly and new product solid.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Pref name="MW_PartingSwapCompareUIOn" type="logical" title="Swap Compare UI" value="true" ver="4.0.3" scope="Session">
                    <Description>This option determines whether or not to activate Model Compare UI in Swap Model.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Off" value="false"/>
                        <EnumElem name="On" value="true"/>
                    </Data>
                </Pref>
                <Pref name="MW_WaveControlForceWipeout" type="logical" title="Wave Update Control Force Wipe Out" value="false" ver="4.0.3" scope="Session">
                    <Description>This option determines whether or not to allow force wipe out in removing out-of-date linked feature with geometry changes.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Pref name="MW_UseNewSewPartingWorkflow" type="logical" title="Use Sew and Keep Tool Body" value="true" ver="2312" scope="Session">
                    <Description>This option determines whether or not to use the sew parting workflow and keep the tool body. When this option is on, creating additional copies for parting and patch surfaces is not needed.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Group name="Tolerances">
                    <Pref name="MW_PartingLinesTolerance_MU" type="real" title="Parting Lines" value="0.01" ver="4.0.0" scope="Session" unit="mm">
                        <Description>This option specifies the tolerance for finding parting lines (including positive and negative faces), traverse loop, isocline, and split face.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_PartingLinesTolerance_EU" type="real" title="Parting Lines" value="0.0004" ver="4.0.0" scope="Session" unit="in">
                        <Description>This option specifies the tolerance for finding parting lines (including positive and negative faces), traverse loop, isocline, and split face.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_PartingSurfTolerance_MU" type="real" title="Surface" value="0.01" ver="4.0.0" scope="Session" unit="mm">
                        <Description>This option specifies the modeling tolerance for extrude surface, bounded plane, enlarge surface, swept surface, bridge surface, and trim sheet.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_PartingSurfTolerance_EU" type="real" title="Surface" value="0.0004" ver="4.0.0" scope="Session" unit="in">
                        <Description>This option specifies the modeling tolerance for extrude surface, bounded plane, enlarge surface, swept surface, bridge surface, and trim sheet.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_PartingCoreCavityTolerance_MU" type="real" title="Core/Cavity" value="0.1" ver="4.0.0" scope="Session" unit="mm">
                        <Description>This option specifies the modeling tolerance for the cavity and core trim.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MW_PartingCoreCavityTolerance_EU" type="real" title="Core/Cavity" value="0.004" ver="4.0.0" scope="Session" unit="in">
                        <Description>This option specifies the modeling tolerance for the cavity and core trim.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Angles">
                    <Pref name="MW_PartingSmallDraftAngle" type="real" title="Small Draft Angle" value="3.0" unit="deg" ver="4.0.0" scope="Session">
                        <Description>This option specifies the parting small draft angle.</Description>
                        <Real/>
                    </Pref>
                    <Pref name="MW_PartingCrossOverAngle" type="real" title="Cross Over Angle" value="5.0" unit="deg" ver="4.0.0" scope="Session">
                        <Description>This option specifies the parting cross over angle.</Description>
                        <Real/>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Colors" csh="PMAN_Mold_Wizard_Parting_Colors">
                <Label label="The following options specify the colors of different parting objects."/>
                <Pref name="MW_PartingProdColor" type="int" title="Product" value="120" ver="6.0.0" scope="Session">
                    <Color/>
                </Pref>
                <Pref name="MW_PartingLinesColor" type="int" title="Line" value="82" ver="6.0.0" scope="Session">
