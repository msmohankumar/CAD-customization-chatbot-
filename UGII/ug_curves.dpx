<PrefDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www-cyp.ugsolutions.com/assemblies/pman">
    <Application name="Curves" csh="PMAN_Curves" package_suppress="simviewer,view,mechatronicsplayer">
        <Category name="General" csh="PMAN_Curves_General">
            <Tab name="All" csh="PMAN_Curves_General_All">
                <Pref name="Solids_EnableLegacyEditCurveParameters" type="logical" title="Use Legacy Edit Curve Parameters" value="false" ver="7.5.0" scope="Session">
                    <Description>This option determines whether the Edit Curve command on the shortcut menu for a non-associative curve should use the legacy command and have it be the default action when double-clicking on a non-associative curve.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
            </Tab>
        </Category>
        <Category name="Spline" csh="PMAN_Curves_Spline">
            <Tab name="All" csh="PMAN_Curves_Spline_All">
                <Pref name="Model_Spline_Degree" type="int" title="Spline Degree" value="5" ver="3.0.0" scope="Session">
                    <Description>This option specifies the degree of a spline when created using the Studio Spline command.</Description>
                    <IntegerScale/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                        <UpperBound value="24" include="1"/>
                    </Data>
                </Pref>
                <Pref name="Model_Spline_Degree_In_Sketch" type="int" title="Spline Degree in the Sketcher" value="3" ver="3.0.0" scope="Session">
                    <Description>This option specifies the degree of a spline when created using the Studio Spline command while in the Sketch environment.</Description>
                    <IntegerScale/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                        <UpperBound value="24" include="1"/>
                    </Data>
                </Pref>
            </Tab>
        </Category>
        <Category name="Fillet" csh="PMAN_Curves_Fillet">
            <Tab name="All" csh="PMAN_Curves_Fillet_All">
                <Pref name="UG_filletRadius_EU" type="real" title="Radius" value="0.25" unit="in" ver="3.0.0" scope="Part">
                    <Description>This option specifies the fillet radius for the Fillet command in Basic Curves.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="UG_filletRadius_MU" type="real" title="Radius" value="6.0" unit="mm" ver="3.0.0" scope="Part">
                    <Description>This option specifies the fillet radius for the Fillet command in Basic Curves.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
            </Tab>
        </Category>
        <Category name="Trim Curve" csh="PMAN_Curves_Trim_Curve">
            <Tab name="All" csh="PMAN_Curves_Trim_Curve_All">
                <Pref name="Solids_TrimCurve_BoundingObject_Option" type="int" title="Bounding Object Type" value="0" ver="11.0.1" scope="Session">
                    <Description>This option specifies the object type to use as the bounding object.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Selected Object" value="0"/>
                        <EnumElem name="Plane" value="1"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="Solids_TrimCurve_TrimOrDivide_Operation" type="int" title="Trim or Divide Operation" value="0" ver="11.0.1" scope="Session">
                    <Description>This option specifies the operation to be performed on the selected input curves.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Trim" value="0"/>
                        <EnumElem name="Divide" value="1"/>
                    </Data>
                </Pref>
                <Pref name="Solids_TrimCurve_Operation_Direction" type="int" title="Direction" value="0" ver="11.0.1" scope="Session">
                    <Description>This option specifies the method the system will use to find intersections between objects.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Shortest 3D Distance" value="0"/>
                        <EnumElem name="Along Direction" value="1"/>
                    </Data>
                </Pref>
                <Pref name="Solids_TrimCurve_Direction" type="int" title="Specify Direction Option" value="10" ver="11.0.1" scope="Session">
                    <Description>When the Direction option is set to Along Direction, this option specifies the vector option to be used to specify the direction.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Inferred Vector" value="0"/>
                        <EnumElem name="Two Points" value="1"/>
                        <EnumElem name="Curve/Axis Vector" value="3"/>
                        <EnumElem name="On Curve Vector" value="4"/>
                        <EnumElem name="Face/Plane Normal" value="5"/>
                        <EnumElem name="XC-axis" value="8"/>
                        <EnumElem name="YC-axis" value="9"/>
                        <EnumElem name="ZC-axis" value="10"/>
                        <EnumElem name="-XC-axis" value="11"/>
                        <EnumElem name="-YC-axis" value="12"/>
                        <EnumElem name="-ZC-axis" value="13"/>
                        <EnumElem name="View Direction" value="14"/>
                    </Data>
                </Pref>
                <Pref name="Solids_TrimCurve_KeepOrDiscard" type="int" title="Selected Region" value="0" ver="11.0.1" scope="Session">
                    <Description>This option specifies if the selected region should be kept or discarded.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Keep" value="0"/>
                        <EnumElem name="Discard" value="1"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="Solids_TrimCurve_Assoc_InputCurves" type="int" title="Input Curves (associative trim)" value="1" ver="12.0.0" scope="Session">
                    <Description>This option specifies how the input curves are handled after an associative trim is performed.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Keep" value="0"/>
                        <EnumElem name="Hide" value="1"/>
                    </Data>
                </Pref>
                <Pref name="Solids_TrimCurve_NonAssoc_InputCurves" type="int" title="Input Curves (non-associative trim)" value="3" ver="11.0.1" scope="Session">
                    <Description>This option specifies how the input curves are handled after a non-associative trim is performed.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Keep" value="0"/>
                        <EnumElem name="Hide" value="1"/>
                        <EnumElem name="Delete" value="2"/>
                        <EnumElem name="Replace" value="3"/>
                    </Data>
                </Pref>
                <Pref name="Solids_TrimCurve_InputCurves_Dashed" type="logical" title="Make Input Curves Dashed" value="false" ver="11.0.1" scope="Session">
                    <Description>This option specifies whether or not trimmed input curves will be changed to dashed.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="Solids_TrimCurve_CurveExtension" type="int" title="Curve Extension" value="0" ver="12.0.0" scope="Session">
                    <Description>This option specifies how curves should be extended to the bounding objects.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Natural" value="0"/>
                        <EnumElem name="Linear" value="1"/>
                        <EnumElem name="Circular" value="2"/>
                        <EnumElem name="None" value="3"/>
                    </Data>
                </Pref>
                <Pref name="Solids_TrimCurve_ProcessBounding_Object" type="logical" title="Trim Bounding Objects" value="false" ver="11.0.1" scope="Session">
                    <Description>This option determines whether or not the bounding objects should be trimmed as well.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="Solids_TrimCurve_Perform_Extended_Intersection_Calculation" type="logical" title="Extended Intersection Calculation" value="false" ver="11.0.1" scope="Session">
                    <Description>Allows intersection calculation to offer solutions that are not within default tolerance.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="Solids_TrimCurve_SingleSelection" type="logical" title="Single Selection" value="true" ver="12.0.0" scope="Session">
                    <Description>This option determines whether or not a selection input is automatically satisfied after a single selection has been made. Having this option on streamlines the interaction for straightforward curve trimming cases.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
            </Tab>
        </Category>
        <Category name="Curve Length" csh="PMAN_Curves_Curve_Length">
            <Tab name="All" csh="PMAN_Curves_Curve_Length_All">
                <Pref name="Solids_Curve_Length_ExtensionMethod" type="int" title="Extension Method" value="0" ver="4.0.0" scope="Session">
                    <Description>This option specifies if the extension method is incremental or total.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Incremental" value="0"/>
                        <EnumElem name="Total" value="1"/>
                    </Data>
                </Pref>
                <Pref name="Solids_Curve_Length_IncrementalExtensionSide" type="int" title="Incremental Extension Side" value="0" ver="4.0.0" scope="Session">
                    <Description>This option specifies the extension side for the incremental extension method.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Start/End" value="0"/>
                        <EnumElem name="Symmetric" value="1"/>
                    </Data>
                </Pref>
                <Pref name="Solids_Curve_Length_TotalExtensionSide" type="int" title="Total Extension Side" value="0" ver="4.0.0" scope="Session">
                    <Description>This option specifies the extension side for the total extension method.</Description>
