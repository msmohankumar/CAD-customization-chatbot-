                    <Data type="enum">
                        <EnumElem name="Start" value="0"/>
                        <EnumElem name="End" value="1"/>
                        <EnumElem name="Symmetric" value="2"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="Solids_Curve_Length_ExtensionDirection" type="int" title="Extension Direction" value="0" ver="4.0.0" scope="Session">
                    <Description>This option specifies how curves should be extended if the curve length is increased.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Natural" value="0"/>
                        <EnumElem name="Linear" value="1"/>
                        <EnumElem name="Circular" value="2"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="Solids_Curve_Length_NonAssocInputCurves" type="int" title="Input Curves (non-associative curve length)" value="3" ver="4.0.0" scope="Session">
                    <Description>This option specifies how the input curves are handled after a non-associative curve length is performed.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Keep" value="0"/>
                        <EnumElem name="Hide" value="1"/>
                        <EnumElem name="Delete" value="2"/>
                        <EnumElem name="Replace" value="3"/>
                    </Data>
                </Pref>
                <Pref name="Solids_Curve_Length_AssocInputCurves" type="int" title="Input Curves (associative curve length)" value="0" ver="4.0.0" scope="Session">
                    <Description>This option specifies how the input curves are handled after an associative curve length is performed.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Keep" value="0"/>
                        <EnumElem name="Hide" value="1"/>
                    </Data>
                </Pref>
            </Tab>
        </Category>
