                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="X" bitmap="plane_yz.bmp" value="0"/>
                            <EnumElem name="Y" bitmap="plane_xz.bmp" value="1"/>
                            <EnumElem name="Z" bitmap="plane_xy.bmp" value="2"/>
                        </Data>
                    </Pref>
                    <Pref name="UG_viewSectioningSlice" type="int" title="Lock All Planes" value="0" ver="7.5.0" scope="Part">
                        <Description>This option determines whether or not all clipping planes are locked to move together or whether they can be moved independently.</Description>
                        <Toggle off="0"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="1"/>
                            <EnumElem name="No" value="0"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Display Settings">
                    <Pref name="UG_viewSectioningCap" type="int" title="Cap Display" value="1" ver="7.0.0" scope="Part">
                        <Description>This option specifies if and how a section cap is displayed. A cap is an area of solid color drawn onto the cutting surface of the section, creating a "pseudo face" where the section clipping planes intersect the model.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="Off" value="0"/>
                            <EnumElem name="Specify Custom Color" value="1"/>
                            <EnumElem name="Body Color" value="2"/>
                            <EnumElem name="Body Color + Interference" value="3"/>
                            <EnumElem name="Custom Color + Interference" value="4"/>
                        </Data>
                    </Pref>
                    <Pref name="UG_viewSectioningCapColor" type="int" title="Custom Color" value="9" ver="6.0.0" scope="Part">
                        <Description>This option specifies the color of the section cap when Cap Display is set to Specify Custom Color.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="UG_viewSectioningActivePlaneDisplay" type="int" title="Translucent Section Plane" value="1" ver="5.0.0" scope="Session">
                        <Description>This option specifies if the active plane should be displayed as a translucent plane when the cap is off.</Description>
                        <Toggle off="0"/>
                        <Data type="enum">
                            <EnumElem name="Off" value="0"/>
                            <EnumElem name="On" value="1"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Section Curves">
                    <Pref name="UG_viewSectioningShowSectionCurvesPreview" type="logical" title="Show Section Curves Preview" value="false" ver="2306" scope="Session">
                        <Description>This option determines whether or not a temporary preview of the curves created by the location of the section plane is shown.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="UG_viewSectioningCurveColorOption" type="int" title="Color Option" value="1" ver="2306" scope="Session">
                        <Description>This option specifies the color option that should be used for view section curves.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="Body Color" value="0"/>
                            <EnumElem name="Specify Color" value="1"/>
                        </Data>
                    </Pref>
                    <Pref name="UG_viewSectioningCurveColor" type="int" title="Color" value="211" ver="2306" scope="Session">
                        <Description>This option specifies the color of the section curves when the Color Option is set to Specify Color.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="UG_viewSectioningCurveFont" type="int" title="Line Style" value="1" ver="2306" scope="Session">
                        <Description>This option specifies the line style of the section curves.</Description>
                        <GeneralLineFont allowDefaultFont="false" LineFontOptionMaskString="GeneralLineFonts"/>
                    </Pref>
                    <Pref name="UG_viewSectioningCurveWidth" type="int" title="Line Width" value="6" ver="2306" scope="Session">
                        <Description>This option specifies the line width of the section curves.</Description>
                        <ObjectLineWidth allowDefaultWidth="false" allowWideLineWidth="true"/>
                    </Pref>
                    <Pref name="UG_viewSectioningLayerOption" type="int" title="Layer Option" value="0" ver="9.0.0" scope="Session">
                        <Description>This option specifies the layer option that should be used for view section curves.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="Work" value="0"/>
                            <EnumElem name="As Specified" value="1"/>
                        </Data>
                    </Pref>
                    <Pref name="UG_viewSectioningLayer" type="int" title="Layer" value="1" ver="9.0.0" scope="Session">
                        <Description>This option specifies the layer on which associative view section curves should be placed when the Layer Option is set to As Specified.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="1" include="1"/>
                            <UpperBound value="256" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="UG_viewSectioningLoadOnSave" type="int" title="Load Exact Representations when Saving Section Curves" value="0" ver="6.0.4" scope="Session">
                        <Description>This option determines whether or not the 'Save Copy of Section Curves' option in the View Section and Datum Plane Grid commands will cause exact representations to be loaded into memory for any visible lightweight bodies on the section plane. This may increase the time and memory used by the operation, but will ensure fully accurate section curves.</Description>
                        <Toggle off="0"/>
                        <Data type="enum">
                            <EnumElem name="Off" value="0"/>
                            <EnumElem name="On" value="1"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
        </Category>
        <Category name="Part Navigator" csh="PMAN_Gateway_Part_Navigator" package_suppress="simviewer">
            <Tab name="All" csh="PMAN_Gateway_Part_Navigator_All">
                <Pref name="UG_PartNav_TreeMode" type="int" title="Timestamp Order" value="0" ver="5.0.0" scope="Session">
                    <Description>This option determines whether or not the tree in the Part Navigator should be organized and displayed in timestamp order.</Description>
                    <Toggle off="1"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="0"/>
                        <EnumElem name="No" value="1"/>
                    </Data>
                    <Docum>When this option is on, the objects in the tree are shown in the order they were created. When this option is off, the objects are grouped into the bodies that they contribute to. This option also provides more part organization tools (like for example reference sets and groups).</Docum>
                </Pref>
                <Pref name="UG_PartNav_AutomaticScroll" type="int" title="Automatically Scroll to Selection" value="1" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not the Part Navigator automatically scrolls to the entry that matches the graphics window selected feature.</Description>
                    <Toggle off="2"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="2"/>
                    </Data>
                </Pref>
                <Pref name="UG_PartNav_EnableDragDrop" type="int" title="Allow Drag and Drop Reorder" value="1" ver="5.0.0" scope="Session">
                    <Description>This option determines whether or not drag and drop reorder of features is allowed. When this option is off, drag and drop is only allowed when moving objects between reference sets.</Description>
                    <Toggle off="2"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="2"/>
                    </Data>
                </Pref>
                <Pref name="UG_PartNav_DisplayReferenceSetsFolder" type="logical" title="Display Reference Sets Folder" value="false" ver="11.0.1" scope="Session">
                    <Description>This option determines if the Reference Sets folder is displayed when the Part Navigator is in Timestamp Order mode.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="UG_PartNav_MarkParents" type="int" title="Display Parent Objects in Different Font Color" value="1" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not the first-level parent objects of the selected object should be displayed with a different font color in the tree.</Description>
                    <Toggle off="2"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="2"/>
                    </Data>
                </Pref>
                <Pref name="UG_PartNav_ParentFontColor" type="int" title="Parent Objects Color" value="152" ver="1926" scope="Session">
                    <Description>This option specifies the font color of parent objects.</Description>
                    <Color/>
                </Pref>
                <Pref name="UG_PartNav_MarkChildren" type="int" title="Display Child Objects in Different Font Color" value="1" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not the first-level child objects of the selected object should be displayed with a different font color in the tree.</Description>
                    <Toggle off="2"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="2"/>
                    </Data>
                </Pref>
                <Pref name="UG_PartNav_ChildFontColor" type="int" title="Child Objects Color" value="104" ver="1926" scope="Session">
                    <Description>This option specifies the font color of child objects.</Description>
                    <Color/>
                </Pref>
                <Separator/>
                <Pref name="UG_PartNav_NameDisplay" type="int" title="Name Display" value="0" ver="4.0.0" scope="Session">
                    <Description>This option specifies the object name display style that should be used in the tree.</Description>
                     <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="System and User" value="0"/>
                        <EnumElem name="System Only" value="1"/>
                        <EnumElem name="User Replaces System" value="2"/>
                    </Data>
                    <Docum>The option System and User means that both the object type and any user-defined name will be displayed. The option System Only means that only the object type name will be displayed. Finally, the option User Replaces System means that the user-defined name will be displayed if there is any; otherwise the object type name will be displayed.</Docum>
                </Pref>
                <Pref name="UG_PartNav_FeatureCheckBoxAction" type="int" title="Feature Check Box Action" value="1" ver="1847" scope="Session">
                    <Description>This option determines if the action performed by the feature node check box in the Part Navigator is suppress or hide.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Suppress" value="0"/>
                        <EnumElem name="Hide" value="1"/>
                    </Data>
                </Pref>
                <Separator/>
                <Label label="Unused Items Folder"/>
                <Pref name="UG_PartNav_HideOnUse" type="int" title="Hide Objects when Used" value="2" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not objects should be made hidden as they are used and be removed from the Unused item folder.</Description>
                    <Toggle off="2"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="2"/>
                    </Data>
                    <Docum>This option only takes effect when the Timestamp Order option is off.</Docum>
                </Pref>
                <Separator/>
                <Pref name="UG_PartNav_SortMethod" type="int" title="Sort Method" value="2" ver="9.0.0" scope="Session">
                    <Description>This option specifies the sorting method used in the Part Navigator for model views, PMI in model views, or PMI. The method used by the Default option depends on the type of data:<Formatted>
- PMI objects are normally sorted alphabetically by object type.
- Model views which names begin with "#" are listed first, followed by canned views sorted alphabetically, views which names begin with special characters, and user defined views sorted alphabetically.</Formatted></Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Alphabetic" value="0"/>
                        <EnumElem name="Alphanumeric" value="1"/>
                        <EnumElem name="Default" value="2"/>
                    </Data>
                </Pref>
                <Pref name="Solids_MoveCurvesToOtherLayer" type="int" title="Objects/Parents on Invisible Layer - Action on Show" value="2" ver="3.0.0" scope="Session">
                    <Description>This option specifies how objects/parents that are on invisible or non-selectable layers are to be made visible and selectable.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="Move to Work Layer" value="1"/>
                        <EnumElem name="Change Layer to be Selectable" value="2"/>
                    </Data>
                </Pref>
            </Tab>
        </Category>
        <Category name="Assembly Navigator" csh="PMAN_Gateway_Assembly_Navigator" package_suppress="simviewer">
            <Tab name="All" csh="PMAN_Gateway_Assembly_Navigator_All">
                <Pref name="UG_AssyNav_Filtering" type="logical" title="Filter Components" value="false" ver="4.0.0" scope="Session">
                    <Description>This option determines whether or not components in the Assembly Navigator tree will be filtered according to the below filter conditions.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Group name="Filter components that are:">
                <Pref name="UG_AssyNav_FilteringInteropSessionRecipeFiltered" type="logical" title="Excluded by Design Session Recipe Filter" value="true" ver="1980" scope="Session">
                    <Description>This option determines whether or not components that are filtered out of the design session should be excluded from the Assembly Navigator when the navigator is filtered.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="UG_AssyNav_FilteringUnloaded" type="logical" title="Unloaded" value="true" ver="6.0.0" scope="Session">
                    <Description>This option determines whether or not unloaded components should be excluded from the Assembly Navigator when the navigator is filtered.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="UG_AssyNav_FilteringRefset" type="logical" title="Excluded by Reference Set" value="true" ver="6.0.0" scope="Session">
                    <Description>This option determines whether or not components excluded by reference set should be excluded from the Assembly Navigator when the navigator is filtered.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="UG_AssyNav_FilteringBlanked" type="logical" title="Hidden" value="true" ver="6.0.0" scope="Session">
                    <Description>This option determines whether or not hidden components should be excluded from the Assembly Navigator when the navigator is filtered.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="UG_AssyNav_FilteringNotVisuallyReported" type="logical" title="Not being Visually Reported on" value="true" ver="7.5.0" scope="Session">
                    <Description>This option determines whether components that are not being reported on in the Explore Visual Report mode should be excluded from the Assembly Navigator when the navigator is filtered.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                </Group>
                <Pref name="UG_AssyNav_ConstraintFiltering" type="logical" title="Filter Constraints" value="false" ver="6.0.0" scope="Session">
                    <Description>This option determines whether or not constraints in the Assembly Navigator tree will be filtered according to the below filter conditions.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Group name="Filter constraints that are:">
                <Pref name="UG_AssyNav_ConstraintFilteringHidden" type="logical" title="Hidden" value="true" ver="6.0.0" scope="Session">
                    <Description>This option determines whether or not hidden constraints should be excluded from the Assembly Navigator when the navigator is filtered.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="UG_AssyNav_ConstraintFilteringInherited" type="logical" title="Inherited" value="false" ver="6.0.0" scope="Session">
                    <Description>This option determines whether or not inherited override constraints should be excluded from the Assembly Navigator when the navigator is filtered.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                </Group>
                <Pref name="UG_AssyNav_FilteringShowMore" type="logical" title="Display &quot;More&quot; Indicators" value="true" ver="4.0.0" scope="Session">
                    <Description>This option determines whether or not "More" indicators are displayed in the Assembly Navigator when any components are filtered. The "More" indicators indicate that there are components that have been filtered at the assembly levels in which they are displayed. Double-clicking a "More" indicator will display all the components in the Assembly Navigator at that assembly level.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Separator/>
              <Pref name="UG_AssyNav_PackComponents" type="logical" title="Pack Components" value="true" ver="9.0.0" scope="Session">
                <Description>This option determines whether or not multiple instances of the same part are shown as packed components when they first appear in the Assembly Navigator after the assembly is loaded.</Description>
                <Toggle off="false"/>
                <Data type="enum">
                  <EnumElem name="Yes" value="true"/>
                  <EnumElem name="No" value="false"/>
                </Data>
              </Pref>
              <Separator/> 
              <Pref name="UG_AssyNav_ComponentSortOrder" type="int" title="Component Sort Order" value="0" ver="10.0.0" scope="Session">
