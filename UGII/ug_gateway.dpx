                    <Description>This field specifies the directory to be used when recording or playing back macro files.</Description>
                    <BrowseDirSel/>
                </Pref>
                <Pref name="UG_initialGripDir_UNX" type="string" title="GRIP Program Directory" value="" ver="3.0.0" scope="Session">
