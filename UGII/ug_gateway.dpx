                    </Pref>
                    <Pref name="Shape_Region_Library_storage_WIN" type="string" title="Shape Region Library Definition Folder" value="${UGII_BASE_DIR}\ugii\region_recognition\NXShapeRegionLibraryML" ver="2312" scope="Session">
                        <Description>This field specifies the path to the shape region object library files.</Description>
                        <BrowseDirSel/>
                    </Pref>
                    <Pref name="Shape_Region_Library_storage_UNX" type="string" title="Shape Region Library Definition Folder" value="${UGII_BASE_DIR}/ugii/region_recognition/NXShapeRegionLibraryML" ver="2312" scope="Session">
                        <Description>This field specifies the path to the shape region object library files.</Description>
                        <BrowseDirSel/>
                    </Pref>
                </Group>
                <Group name="Prediction">
                <Pref name="Selection_Prediction_Activate" type="logical" title="Selection Prediction" value="true" ver="2007" scope="Session">
                  <Description>This option determines whether or not to display the Select Predicted Objects command.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="Off" value="false"/>
                    <EnumElem name="On" value="true"/>
                  </Data>
                </Pref>
                <Pref name="Selection_Prediction_storage_WIN" type="string" title="Selection Prediction Folder"
                value="${UGII_BASE_DIR}\ugii\selection_prediction" ver="2007" scope="Session">
                  <Description>This field specifies the directory where the selection prediction files are located. The files are used by the Select Predicted Object machine learning algorithm to predict selection objects.</Description>
                  <BrowseDirSel/>
                </Pref>
                <Pref name="Selection_Prediction_storage_UNX" type="string" title="Selection Prediction Folder"
                value="${UGII_BASE_DIR}/ugii/selection_prediction" ver="2007" scope="Session">
                  <Description>This field specifies the directory where the selection prediction files are located. The files are used by the Select Predicted Object machine learning algorithm to predict selection objects.</Description>
                  <BrowseDirSel/>
                </Pref>
              </Group>
            </Tab>
