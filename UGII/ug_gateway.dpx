                        </Data>
                    </Pref>
                    <Pref name="UG_viewSectioningPlaneCsys" type="int" title="Orientation" value="0" ver="11.0.1" scope="Session">
                        <Description>This option specifies the orientation of the section plane.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="Absolute" value="0"/>
                            <EnumElem name="WCS" value="1"/>
                        </Data>
                    </Pref>
                    <Pref name="UG_viewSectioningPlaneNormal" type="int" title="Plane" value="2" ver="11.0.1" scope="Session">
