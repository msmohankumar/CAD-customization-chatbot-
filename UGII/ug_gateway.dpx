        <Category name="JT Files" csh="PMAN_Gateway_JT_Files" package_suppress="simviewer,mechatronicsplayer">
            <Tab name="Import Layers" csh="PMAN_Gateway_JT_Files_Import_Layers">
                <Pref name="Assemblies_ImportTeamCenterVisData_Wireframe_RefGeom" type="logical" title="Import Wireframe and Reference Geometry from JT Files" value="true" ver="3.0.2" scope="Session">
                    <Description>This option determines whether or not wireframe curves &amp; points and reference geometry should be imported when opening JT files in NX.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_ImportTeamCenterVisData_PMI" type="logical" title="Import PMI from JT Files" value="true" ver="4.0.2" scope="Session">
                    <Description>This option determines whether or not Product and Manufacturing Information (PMI) should be imported when opening JT files in NX.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="Gateway_JT_import_wireframe_layer" type="int" title="Wireframe Layer" value="1" ver="5.0.0" scope="Session">
                    <Description>This option specifies what layer wireframe curves &amp; points will be placed on when opening JT files in NX.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                        <UpperBound value="256" include="1"/>
                    </Data>
                </Pref>
                <Pref name="Gateway_JT_import_refgeom_layer" type="int" title="Reference Geometry Layer" value="1" ver="5.0.0" scope="Session">
                    <Description>This option specifies what layer reference geometry will be placed on when opening JT files in NX.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                        <UpperBound value="256" include="1"/>
                    </Data>
                </Pref>
                <Pref name="Gateway_JT_import_model_geometry_layer_option" type="logical" title="Use Work Layer" value="true" ver="6.0.2" scope="Session">
                    <Description>This option determines whether or not the work layer should be used for model geometry objects (solids, sheets, faceted bodies) when opening JT files in NX. When this option is off, the objects are placed on the layer as specified by the Model Geometry Layer option.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Pref name="Gateway_JT_import_model_geometry_layer" type="int" title="Model Geometry Layer" value="1" ver="6.0.2" scope="Session">
                    <Description>This option specifies what layer model geometry objects (solids, sheets, faceted bodies) will be placed on when opening JT files in NX. This will only have effect if the Use Work Layer option is off.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                        <UpperBound value="256" include="1"/>
                    </Data>
                </Pref>
              </Tab>
              <Tab name="Import Content" csh="PMAN_Gateway_JT_Files_Import_Content">
                 <Pref name="Gateway_JT_ForeignInitialBrep" type="int" title="Initially Extract Exact Data" value="0" ver="6.0.0" scope="Session">
                    <Description>This option determines whether or not exact data should automatically be read from JT files during the initial extraction, and if so, from what type of JT files.<Formatted>
All JT Files: Exact data will be read from any type of JT file, if available.
All JT Files Except Files with JT B-rep: Exact data will be read from all JT files except if the JT files contain JT B-rep.
None: Facet data will be read.</Formatted></Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="All JT Files" value="1"/>
                        <EnumElem name="All JT Files Except Files with JT B-rep" value="0"/>
                        <EnumElem name="None" value="2"/>
                    </Data>
                </Pref>
                <Pref name="Gateway_JT_LoadPmiAndRefGeom" type="int" title="PMI" value="2" ver="2007" scope="Session">
                    <Description>This option determines when PMI is loaded from JT files. For lightweight loading, PMI loading can be deferred until the exact data is loaded, but only if the &quot;Import Assembly Structure from JT Files&quot; option is off.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="Always Load" value="0"/>
                        <EnumElem name="Never Load" value="1"/>
                        <EnumElem name="Only Load with Exact Data" value="2"/>
                    </Data>
                </Pref>
                 <Pref name="Gateway_JT_ReferenceSet" type="int" title="Reference Set Definition" value="0" ver="8.0.1" scope="Session">
                    <Description>This option determines what method is used to assign objects in the JT file to reference sets in NX. Reference sets can either be specified by Multi-CAD attributes or by using JT Layer Filters.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="Use Multi-CAD Reference Sets" value="0"/>
                        <EnumElem name="Use Layer Filter Reference Sets" value="1"/>
                    </Data>
                 </Pref>
                 <Pref name="Gateway_JT_import_structure" type="logical" title="Import Assembly Structure from JT Files" value="true" ver="4.0.2" scope="Session">
                    <Description>This option determines whether or not the assembly structure should be imported when opening JT files in NX.

This option applies to native NX only.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>              
                <Pref name="UGMGR_ImportJtOverrides" type="logical" title="Import Assembly-level JT Geometry Overrides" value="false" ver="6.0.2" scope="Session">
                  <Description>This option determines whether or not assembly-level JT geometry overrides are imported from Teamcenter into NX when a multi CAD assembly is loaded.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                      <EnumElem name="Off" value="false"/>
                      <EnumElem name="On" value="true"/>
                  </Data>
                </Pref>
                <Pref name="Gateway_JT_brep_simplification" type="int" title="Face Simplification for JT B-rep" value="0" ver="4.0.2" scope="Session">
                    <Description>This option determines whether or not the geometry should be simplified (for example planar faces recognized as planes) when solids are extracted from the JT file. Simplified geometry is needed, for example, if the part needs to be positioned using constraints, but will make the extract operation take longer.

Note: When the JT file contains XT B-rep, the geometry will already be in its simplest form (i.e. this option is ignored).</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="None (fastest)" value="0"/>
                        <EnumElem name="Basic Simplification" value="1"/>
                        <EnumElem name="Enhanced Simplification" value="2"/>
                        <EnumElem name="Enhanced Simplification and Sew Sheets (slowest)" value="3"/>
                    </Data>
                </Pref>
                <Pref name="Gateway_JT_MasterPrefix" type="string" title="Master Data Prefix" value="j0" ver="8.5.0" scope="Session">
                    <Description>This field specifies the prefix for master data attributes in the JT files.  Attributes with this prefix will be locked when created in NX to prevent changing the values specified by the master JT file.</Description>
                    <Widestring/>
                </Pref>
            </Tab>
            <Tab name="Export" csh="PMAN_Gateway_JT_Files_Export">
                <Pref name="Assemblies_SaveTeamCenterVisData" type="int" title="Save JT Data" value="0" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not a JT file should be saved when parts are saved.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="JT Derived Parts" csh="PMAN_Gateway_JT_Files_JT_Derived_Parts">
                <Pref name="Gateway_JT_ForeignAutoUpdate" type="logical" title="Automatically Update Out-of-date JT Derived Parts" value="false" ver="6.0.0" scope="Session">
                    <Description>When loading a part derived from a JT file, or an assembly containing components derived from JT files, this option determines whether or not these parts should update automatically if the source JT files have been modified. When this option is off, no automatic update is performed and all updates must be made manually.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="Gateway_JT_brep_associativity" type="int" title="Associative Updates" value="1" ver="4.0.2" scope="Session">
                    <Description>This option determines whether or not extra tracking information should be added when the solids/sheets are extracted from the JT file.

If the NX part file is saved and is later required to update when the source JT file changes, this tracking information is required to associatively update downstream features or other references to the update solids/sheets.

If the solids/sheets are only going to be used for the duration of the NX session, not adding this tracking information will make the extract operation faster.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="Off (faster)" value="0"/>
                        <EnumElem name="On (slower)" value="1"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Teamcenter Integration" csh="PMAN_Gateway_JT_Files_Teamcenter_Integration">
                <Pref name="UGMGR_ForeignAutoRelinkJT" type="string" title="Update JT Derived Parts from Source in Current Revision" value="on" ver="4.0.1" scope="Session">
                    <Description>This option determines whether to use the source JT file under the current item revision when updating from an out-of-date JT part. This may be different from the original JT part if a Revise or Save As operation has taken place in Teamcenter.</Description>
                    <Toggle off="off"/>
                    <Data type="enum">
                        <EnumElem name="No" value="off"/>
                        <EnumElem name="Yes" value="on"/>
                    </Data>
                    <Docum>This option determines whether or not to update from the source JT file under the current item revision when performing an update. This may be different from the original JT part if a Revise or Save As operation has taken place in Teamcenter. When on, the NX part will be re-linked to the source JT dataset under the current item revision before the update. Otherwise the original source JT dataset will be used even if this exists only in an old item or revision.</Docum>
                </Pref>
                <Pref name="UGMGR_DeleteSourceJTonSave" type="string" title="Delete Source JT on Save" value="off" ver="5.0.0" scope="Session">
                    <Description>This option determines whether to delete the source JT file when a JT derived part is saved.

Note that this option only applies to JT files attached to item revisions by a rendering relation in Teamcenter.</Description>
                    <Toggle off="off"/>
                    <Data type="enum">
                        <EnumElem name="No" value="off"/>
                        <EnumElem name="Yes" value="on"/>
                    </Data>
                    <Docum>This option determines whether or not to delete the source JT file when a JT derived part is saved during a Save or Save As operation. When on, NX will attempt to delete the source JT part in Teamcenter during any save which creates an NX dataset. Otherwise, the original source JT dataset is left intact. Note that this option only applies to JT files attached to item revisions by a rendering relation in Teamcenter.</Docum>
                </Pref>
                <Pref name="UGMGR_JTCreationMode" type="int" title="Synchronous JT Creation in File Save" value="1" ver="8.5.0" scope="Session">
                  <Description>This option specifies whether or not the JT file should be created synchronously with NX part save. When this option is off, the JT file creation will take place as a batch translation in the background and so reducing the overall time to save. Note that this option applies only when the Save JT Data option is on.</Description>
                  <Toggle off="0"/>
                  <Data type="enum">
                    <EnumElem name="No" value="0"/>
                    <EnumElem name="Yes" value="1"/>
                  </Data>
                </Pref>
            </Tab>
        </Category>
