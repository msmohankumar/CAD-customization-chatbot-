                  <OptionMenu/>
                  <Data type="enum">
                      <EnumElem name="Chronological" value="0"/>
                      <EnumElem name="Alphanumeric" value="1"/>
                      <EnumElem name="Alphabetic" value="2"/>
                      <EnumElem name="Sequential" value="3"/>
                  </Data>
              </Pref>
              <Separator/>
                <Pref name="UG_AssyNav_CompOnInvisibleLayer" type="int" title="Component on Invisible Layer - Action on Show" value="2" ver="4.0.4" scope="Session">
                    <Description>This option specifies how a component that is on an invisible layer is to be made visible and selectable.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="Do No Layer Setting Action" value="0"/>
                        <EnumElem name="Move Component to Work Layer" value="1"/>
                        <EnumElem name="Change Layer to be Selectable" value="2"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="UG_AssyNav_ConstraintNameDisplay" type="int" title="Constraint Name Display" value="0" ver="8.0.1" scope="Session">
                    <Description>This option specifies the constraint name display style that should be used in the Assembly Navigator and Constraint Navigator tree.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="System and User" value="0"/>
                        <EnumElem name="User and System" value="1"/>
                        <EnumElem name="System Only" value="2"/>
                        <EnumElem name="User Replaces System" value="3"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Teamcenter Integration" csh="PMAN_Gateway_Assembly_Navigator_TeamcenterIntegration">
                <Group name="4GD">
                <Pref name="UG_AssyNav_4GD_ShowAssemblyStructure" type="logical" title="Show Assembly Structure" value="true" ver="12.0.0" scope="Session">
                    <Description>This option determines whether or not a subset's presented parents should be shown in the Assembly Navigator.<Formatted>
A presented parent is a design component that is itself not a member of the subset and is only shown because it is on a branch to a subset member.</Formatted></Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="UG_AssyNav_4GD_SubsetPartitionViewStyle" type="int" title="Subset Partition View Style" value="2" ver="9.0.0" scope="Session">
                    <Description>This option determines if a subset's contents should be organized by partitions according to the subset's viewed partition scheme in the Assembly Navigator, and if so how.<Formatted>
