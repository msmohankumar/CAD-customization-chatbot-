        <Category name="View Operations" csh="PMAN_Gateway_View_Operations" package_suppress="simviewer">
            <Tab name="View Operations" csh="PMAN_Gateway_View_Operations_View_Operations">
                <Group name="Mouse Wheel Scrolling">
                    <Pref name="UG_mouse_wheel_scroll" type="int" title="Direction" value="0" ver="10.0.0" scope="Session">
                        <Description>This option specifies the mouse direction to zoom in when using the mouse wheel, MB1+MB2, or Ctrl+MB2.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="Forward to Zoom In" value="0"/>
                            <EnumElem name="Backward to Zoom In" value="1"/>
