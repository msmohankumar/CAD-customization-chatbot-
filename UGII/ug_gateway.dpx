                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="None" value="0"/>
                        <EnumElem name="Flat" value="1"/>
                        <EnumElem name="Hierarchical" value="2"/>
                    </Data>
                </Pref>
                </Group>
            </Tab>
