<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="docum.xsl"?>
<!--

-->
<PrefDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www-cyp.ugsolutions.com/assemblies/pman">
    <Application name="Sheet Metal" csh="PMAN_Sheet_Metal" package_suppress="simcenter3d,simviewer,view,mechatronicsplayer,nx2dedit">
        <Category name="General" csh="PMAN_Sheet_Metal_General">
          <SubCategory name="Part Properties" csh="PMAN_Sheet_Metal_General_Part_Properties">
            <Tab name="Global Parameters" csh="PMAN_Sheet_Metal_General_Part_Properties_Global_Parameters">
                <Pref name="NXSM_MaterialThickness_EU" type="real" title="Material Thickness" value="0.1" unit="in" ver="1847" scope="Part">
                    <Description>This option specifies the global thickness for a Base Tab, a Base Contour Flange, or Sheet Metal From Solid feature.</Description>
                    <Real/>
                </Pref>
                <Pref name="NXSM_MaterialThickness_MU" type="real" title="Material Thickness" value="3.0" unit="mm" ver="1847" scope="Part">
                    <Description>This option specifies the global thickness for a Base Tab, a Base Contour Flange, Base Lofted Flange, or Sheet Metal From Solid feature.</Description>
                    <Real/>
                </Pref>
                <Pref name="NXSM_UseGlobalMaterialThickness" type="int" title="Use Global Material Thickness" value="1" ver="6.0.0" scope="Part">
                    <Description>This option determines whether or not the global material thickness should be used.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
                <Pref name="NXSM_BendRadius_EU" type="real" title="Bend Radius" value="0.1" unit="in" ver="1847" scope="Part">
                    <Description>This option specifies the global bend radius for sheet metal features.</Description>
                    <Real/>
                </Pref>
                <Pref name="NXSM_BendRadius_MU" type="real" title="Bend Radius" value="3.0" unit="mm" ver="1847" scope="Part">
                    <Description>This option specifies the global bend radius for sheet metal features.</Description>
                    <Real/>
                </Pref>
                <Pref name="NXSM_BendReliefDepth_EU" type="real" title="Bend Relief Depth" value="0.1" unit="in" ver="1847" scope="Part">
                    <Description>This option specifies the global bend relief depth for sheet metal features.</Description>
                    <Real/>
                </Pref>
                <Pref name="NXSM_BendReliefDepth_MU" type="real" title="Bend Relief Depth" value="3.0" unit="mm" ver="1847" scope="Part">
                    <Description>This option specifies the global bend relief depth for sheet metal features.</Description>
                    <Real/>
                </Pref>
                <Pref name="NXSM_BendReliefWidth_EU" type="real" title="Bend Relief Width" value="0.1" unit="in" ver="1847" scope="Part">
                    <Description>This option specifies the global bend relief width for sheet metal features.</Description>
                    <Real/>
                </Pref>
                <Pref name="NXSM_BendReliefWidth_MU" type="real" title="Bend Relief Width" value="3.0" unit="mm" ver="1847" scope="Part">
                    <Description>This option specifies the global bend relief width for sheet metal features.</Description>
                    <Real/>
                </Pref>
                <Pref name="NXSM_BendDefinitionMethod" type="int" title="Bend Definition Method" value="0" ver="12.0.0" scope="Part">
                    <Description>This option specifies the global bend definition method for sheet metal features.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="Neutral Factor Value" value="0"/>
                        <EnumElem name="Bend Table" value="1"/>
                        <EnumElem name="Bend Allowance Formula" value="2"/>
                        <EnumElem name="Bend Deduction Formula" value="3"/>
                        <EnumElem name="Bend Formula DIN6935" value="4"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="NXSM_NeutralFactor" type="string" title="Neutral Factor" value="0.33" ver="3.0.0" scope="Part">
                    <Description>This option specifies the global neutral factor for sheet metal features.</Description>
                    <Widestring/>
                </Pref>
                <Pref name="NXSM_BendAllowanceFormula" type="string" title="Bend Allowance Formula" value="(Radius+(Thickness*0.44))*rad(Angle)" ver="5.0.0" scope="Part">
                    <Description>This option specifies the global bend allowance formula for sheet metal features.</Description>
                    <Widestring/>
                </Pref>
                <Pref name="NXSM_BendDeductionFormula" type="string" title="Bend Deduction Formula" value="2*(tan(Angle/2)*(Radius+Thickness))-(rad(Angle)*(Radius+ (0.44 * Thickness)))" ver="1926" scope="Part">
                  <Description>This option specifies the global bend deduction formula for sheet metal features.</Description>
                  <Widestring/>
                </Pref>
                <Pref name="NXSM_BendTableType" type="int" title="Bend Table Type" value="0" ver="12.0.0" scope="Part">
                  <Description>This option specifies the bend table type used if the bend definition method is set to bend table.</Description>
                  <RadioBox/>
                  <Data type="enum">
                    <EnumElem name="Neutral Factor Table" value="0"/>
                    <EnumElem name="Bend Allowance Table" value="1"/>
                    <EnumElem name="Bend Deduction Table" value="2"/>
                  </Data>
                </Pref>
            </Tab>
            <Tab name="Material Standards" csh="PMAN_Sheet_Metal_General_Part_Properties_Material_Standards">
                <Group name="Native NX">
                  <Pref name="NXSM_standards_file_MU_WIN" type="string" title="Metric Part" value="sheet_metal_material_table.txt" ver="12.0.0" scope="Session">
                    <Description>This field specifies the name of the Sheet Metal Material Standards file. The standards file should be a comma separated text file with an extension txt or csv.</Description>
                    <BrowseFileSel type="txt,csv"/>
                  </Pref>
                  <Pref name="NXSM_standards_file_MU_UNX" type="string" title="Metric Part" value="sheet_metal_material_table.txt" ver="12.0.0" scope="Session">
                    <Description>This field specifies the name of the Sheet Metal Material Standards file. The standards file should be a comma separated text file with an extension txt or csv.</Description>
                    <BrowseFileSel type="txt,csv"/>
                  </Pref>
                  <Pref name="NXSM_standards_file_EU_WIN" type="string" title="English Part" value="sheet_metal_material_table.txt" ver="12.0.0" scope="Session">
                    <Description>This field specifies the name of the Sheet Metal Material Standards file. The standards file should be a comma separated text file with an extension txt or csv.</Description>
                    <BrowseFileSel type="txt,csv"/>
                  </Pref>
                  <Pref name="NXSM_standards_file_EU_UNX" type="string" title="English Part" value="sheet_metal_material_table.txt" ver="12.0.0" scope="Session">
                    <Description>This field specifies the name of the Sheet Metal Material Standards file. The standards file should be a comma separated text file with an extension txt or csv.</Description>
                    <BrowseFileSel type="txt,csv"/>
                  </Pref>
                </Group>
                <Group name="Teamcenter">
                <Pref name="NXSM_nxmgr_item_id_MU" type="string" title="Item ID" value="" ver="1953+" scope="Session">
                    <Description>This field specifies the Item ID of the Sheet Metal Material Standards File in Teamcenter. Item ID, Item Revision, and Dataset Name; all three must be specified to locate the file in Teamcenter.</Description>
                    <Widestring/>
                </Pref>
                <Pref name="NXSM_nxmgr_item_revision_MU" type="string" title="Item Revision" value="" ver="1953+" scope="Session">
                    <Description>This field specifies the Item Revision of the Sheet Metal Material Standards file in Teamcenter. Item ID, Item Revision, and Dataset Name; all three must be specified to locate the file in Teamcenter.</Description>
                    <Widestring/>
                </Pref>
                <Pref name="NXSM_nxmgr_dataset_name_MU" type="string" title="Dataset Name" value="" ver="1953+" scope="Session">
                    <Description>This field specifies the Dataset Name of the Sheet Metal Material Standards file in Teamcenter. Item ID, Item Revision, and Dataset Name; all three must be specified to locate the file in Teamcenter.</Description>
                    <Widestring/>
                </Pref>
                <Pref name="NXSM_nxmgr_item_id_EU" type="string" title="Item ID" value="" ver="1953+" scope="Session">
                    <Description>This field specifies the Item ID of the Sheet Metal Material Standards File in Teamcenter. Item ID, Item Revision, and Dataset Name; all three must be specified to locate the file in Teamcenter.</Description>
                    <Widestring/>
                </Pref>
                <Pref name="NXSM_nxmgr_item_revision_EU" type="string" title="Item Revision" value="" ver="1953+" scope="Session">
                    <Description>This field specifies the Item Revision of the Sheet Metal Material Standards file in Teamcenter. Item ID, Item Revision, and Dataset Name; all three must be specified to locate the file in Teamcenter.</Description>
                    <Widestring/>
                </Pref>
                <Pref name="NXSM_nxmgr_dataset_name_EU" type="string" title="Dataset Name" value="" ver="1953+" scope="Session">
                    <Description>This field specifies the Dataset Name of the Sheet Metal Material Standards file in Teamcenter. Item ID, Item Revision, and Dataset Name; all three must be specified to locate the file in Teamcenter.</Description>
                    <Widestring/>
                </Pref>
                </Group>
                <Group name="Physical Material Library">
                <Pref name="NXSM_Enable_Material_Library_Integration" type="logical" title="Integrate Physical Material Library" value="true" ver="1953" scope="Session">
                    <Description>This option determines whether or not to integrate a physical material library with the Sheet Metal application.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                      <EnumElem name="On" value="true"/>
                      <EnumElem name="Off" value="false"/>
                    </Data>
                </Pref>
                </Group>
            </Tab>
          </SubCategory>
          <SubCategory name="Sheet Metal Validation" csh="PMAN_Sheet_Metal_General_Sheet_Metal_Validation">
            <Tab name="Validation Parameters" csh="PMAN_Sheet_Metal_General_Sheet_Metal_Validation_Validation_Parameters">
              <Pref name="NXSM_Validation_Min_Punch_Clearance_EU" type="real" title="Minimum Tool Clearance" value="0.5" unit="in" ver="1847" scope="Part">
                <Description>This option specifies the global minimum tool clearance for sheet metal validation.</Description>
                <Real/>
              </Pref>
              <Pref name="NXSM_Validation_Min_Punch_Clearance_MU" type="real" title="Minimum Tool Clearance" value="5.0" unit="mm" ver="1847" scope="Part">
                <Description>This option specifies the global minimum tool clearance for sheet metal validation.</Description>
                <Real/>
              </Pref>
              <Pref name="NXSM_Validation_Min_Web_Length_EU" type="real" title="Minimum Web Length" value="0.5" unit="in" ver="1847" scope="Part">
                <Description>This option specifies the global minimum web length for sheet metal validation.</Description>
                <Real/>
              </Pref>
              <Pref name="NXSM_Validation_Min_Web_Length_MU" type="real" title="Minimum Web length" value="5.0" unit="mm" ver="1847" scope="Part">
                <Description>This option specifies the global minimum web length for sheet metal validation.</Description>
                <Real/>
              </Pref>
            </Tab>
          </SubCategory>
        </Category>
        <Category name="Convert" csh="PMAN_Sheet_Metal_Convert">
          <SubCategory name="Sheet Metal from Solid" csh="PMAN_Sheet_Metal_Convert_Sheet_Metal_from_Solid">
            <Tab name="All" csh="PMAN_Sheet_Metal_Convert_Sheet_Metal_from_Solid_All">
              <Group name="Settings">
                <Pref name="NXSM_SheetMetalfromSolidHideOriginal" type="logical" title="Hide Original" value="true" ver="1872" scope="Session">
                  <Description>This option determines whether or not the original body will be made hidden after the sheet metal model has been created.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="Yes" value="true"/>
                    <EnumElem name="No" value="false"/>
                  </Data>
                </Pref>
              </Group>
            </Tab>
          </SubCategory>
          <SubCategory name="Rip" csh="PMAN_Sheet_Metal_Convert_Rip">
            <Tab name="All" csh="PMAN_Sheet_Metal_Convert_Rip_All">
              <Group name="Rip Parameters">
              <Pref name="NXSM_EdgeRip_Type" type="logical" title="Use Minimal Relief" value="true" ver="12.0.2" scope="Session">
                <Description>This option determines whether or not to create a rip using the system default rip width.</Description>
                <Toggle off ="false"/>
                <Data type="enum">
                  <EnumElem name="Yes" value="true"/>
                  <EnumElem name="No" value="false"/>
                </Data>
              </Pref>
              <Pref name="NXSM_EdgeRip_RipWidth_EU" type="real" title="Rip Width" value="0.2" unit="in" ver="12.0.2" scope="Session">
                <Description>This option specifies the rip width.</Description>
                <Real/>
                <Data type="range">
                  <LowerBound value="0.001575" include="1"/>
                </Data>
              </Pref>
              <Pref name="NXSM_EdgeRip_RipWidth_MU" type="real" title="Rip Width" value="1" unit="mm" ver="12.0.2" scope="Session">
                <Description>This option specifies the rip width.</Description>
                <Real/>
                <Data type="range">
                  <LowerBound value="0.04" include="1"/>
                </Data>
              </Pref>
              <Pref name="NXSM_EdgeRip_EndCapShape" type="int" title="End Cap Shape" value="0" ver="12.0.2" scope="Session">
                <Description>This option specifies the end cap shape type.</Description>
                <RadioBox orientation="horizontal"/>
                <Data type="enum">
                  <EnumElem name="Square" bitmap="bend_relief_square" value="0"/>
                  <EnumElem name="Round" bitmap="bend_relief_round" value="1"/>
                </Data>
              </Pref>
              <Pref name="NXSM_EdgeRip_BlendSharpCorners" type="logical" title="Blend Sharp Corners" value="false" ver="12.0.2" scope="Session">
                <Description>This option determines whether or not to apply a blend at sharp corners.</Description>
                <Toggle off ="false"/>
                <Data type="enum">
                  <EnumElem name="Yes" value="true"/>
                  <EnumElem name="No" value="false"/>
                </Data>
              </Pref>
              <Pref name="NXSM_EdgeRip_CurveRadius_EU" type="real" title="Fillet Radius" value="0.1" unit="in" ver="12.0.2" scope="Session">
                <Description>This option specifies the curve radius.</Description>
                <Real/>
                <Data type="range">
                  <LowerBound value="0.001575" include="1"/>
                </Data>
              </Pref>
              <Pref name="NXSM_EdgeRip_CurveRadius_MU" type="real" title="Fillet Radius" value="0.5" unit="mm" ver="12.0.2" scope="Session">
                <Description>This option specifies the curve radius.</Description>
                <Real/>
                <Data type="range">
                  <LowerBound value="0.04" include="1"/>
                </Data>
              </Pref>
              </Group>
            </Tab>
          </SubCategory>
          <SubCategory name="Cleanup Utility" csh="PMAN_Sheet_Metal_Convert_Cleanup_Utility">
            <Tab name="All" csh="PMAN_Sheet_Metal_Convert_Cleanup_Utility_All">
              <Group name="Sliver">
              <Pref name="NXSM_SliverTol_EU" type="real" title="Sliver Tolerance" value="0.0004" unit="in" ver="7.5.0" scope="Session">
                <Description>This option specifies the tolerance value for the Sheet Metal Cleanup utility to remove sliver faces.</Description>
                <Real/>
                <Data type="range">
                  <LowerBound value="0.0" include="0"/>
                </Data>
              </Pref>
              <Pref name="NXSM_SliverTol_MU" type="real" title="Sliver Tolerance" value="0.01" unit="mm" ver="7.5.0" scope="Session">
                <Description>This option specifies the tolerance value for the Sheet Metal Cleanup utility to remove sliver faces.</Description>
                <Real/>
                <Data type="range">
                  <LowerBound value="0.0" include="0"/>
                </Data>
              </Pref>
              </Group>
              <Group name="Settings">
              <Pref name="NXSM_HideOriginal" type="int" title="Hide Original" value="1" ver="7.5.0" scope="Session">
                <Description>This option determines whether or not the original body will be made hidden after the sheet metal cleanup utility has been run.</Description>
                <Toggle off="0"/>
                <Data type="enum">
                  <EnumElem name="Yes" value="1"/>
                  <EnumElem name="No" value="0"/>
                </Data>
              </Pref>
              </Group>
            </Tab>
          </SubCategory>
        </Category>
        <Category name="Base" csh="PMAN_Sheet_Metal_Base">
          <SubCategory name="Tab" csh="PMAN_Sheet_Metal_Base_Tab">
            <Tab name="Curves" csh="PMAN_Sheet_Metal_Base_Tab_Curves">
              <Group name="Bend Centerline">
                <Pref name="NXSM_bendCenterLine_Tab_Enabled" type="logical" title="Display" value="true" ver="12.0.0" scope="Part">
                  <Description>This option determines whether bend centerlines are displayed in tab sketches.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="Yes" value="true"/>
                    <EnumElem name="No" value="false"/>
                  </Data>
                </Pref>
                <Pref name="NXSM_bendCenterLine_Tab_Color" type="int" title="Color" value="189" ver="12.0.0" scope="Part">
                  <Description>This option specifies the color of bend centerlines in tab sketches.</Description>
                  <Color/>
                </Pref>
                <Pref name="NXSM_bendCenterLine_Tab_Font" type="int" title="Style" value="4" ver="1847" scope="Part">
                  <Description>This option specifies the style for bend centerlines in tab sketches.</Description>
                  <GeneralLineFont LineFontOptionMaskString="GeneralLineFonts"/>
                </Pref>
                <Pref name="NXSM_bendCenterLine_Tab_Width" type="int" title="Width" value="1" ver="12.0.0" scope="Part">
                  <Description>This option specifies the width for bend centerlines in tab sketches.</Description>
                  <AnnotationLineWidth/>
                </Pref>
               </Group>
              <Group name="Bend Tangent Line">
                <Pref name="NXSM_bendTangentLine_Tab_Enabled" type="logical" title="Display" value="true" ver="12.0.0" scope="Part">
                  <Description>This option determines whether bend tangent lines are displayed in tab sketches.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="Yes" value="true"/>
                    <EnumElem name="No" value="false"/>
                  </Data>
                </Pref>
                <Pref name="NXSM_bendTangentLine_Tab_Color" type="int" title="Color" value="83" ver="12.0.0" scope="Part">
                  <Description>This option specifies the color of bend tangent lines in tab sketches.</Description>
                  <Color/>
                </Pref>
                <Pref name="NXSM_bendTangentLine_Tab_Font" type="int" title="Style" value="1" ver="1847" scope="Part">
                  <Description>This option specifies the style for bend tangent lines in tab sketches.</Description>
                  <GeneralLineFont LineFontOptionMaskString="GeneralLineFonts"/>
                </Pref>
                <Pref name="NXSM_bendTangentLine_Tab_Width" type="int" title="Width" value="1" ver="12.0.0" scope="Part">
                  <Description>This option specifies the width for bend tangent lines in tab sketches.</Description>
                  <AnnotationLineWidth/>
                </Pref>
              </Group>
            </Tab>
          </SubCategory>
          <SubCategory name="Flange" csh="PMAN_Sheet_Metal_Base_Flange">
            <Tab name="All" csh="PMAN_Sheet_Metal_Base_Flange_All">
              <Group name="Flange Properties">
                <Pref name="NXSM_FlangeLength_EU" type="real" title="Length" value="1.0" unit="in" ver="1847" scope="Session">
                  <Description>This option specifies the Flange length.</Description>
                  <Real/>
                </Pref>
                <Pref name="NXSM_FlangeLength_MU" type="real" title="Length" value="20.0" unit="mm" ver="1847" scope="Session">
                  <Description>This option specifies the Flange length.</Description>
                  <Real/>
                </Pref>
                <Pref name="NXSM_FlangeAngle_EU" type="real" title="Angle" value="90.0" unit="deg" ver="1847" scope="Session">
                  <Description>This option specifies the Flange angle.</Description>
                  <Real/>
                </Pref>
                <Pref name="NXSM_FlangeAngle_MU" type="real" title="Angle" value="90.0" unit="deg" ver="1847" scope="Session">
                  <Description>This option specifies the Flange angle.</Description>
                  <Real/>
                </Pref>
                <Pref name="NXSM_FlangeLengthOpt" type="int" title="Length Reference" value="0" ver="12.0.2" scope="Session">
                  <Description>This option specifies how the Flange length is measured.</Description>
                  <RadioBox orientation="horizontal"/>
                  <Data type="enum">
                    <EnumElem name="Inside" bitmap="inside_dimension" value="0"/>
                    <EnumElem name="Outside" bitmap="outside_dimension" value="1"/>
                    <EnumElem name="Web" bitmap="web_length" value="2"/>
                    <EnumElem name="Tangent" bitmap="tangent_dimension" value="3"/>
                  </Data>
                </Pref>
                <Pref name="NXSM_FlangeInsetOpt" type="int" title="Inset" value="0" ver="12.0.2" scope="Session">
                  <Description>This option specifies the Inset of the Flange feature.</Description>
                  <RadioBox orientation="horizontal"/>
                  <Data type="enum">
                    <EnumElem name="Material Inside" bitmap="material_inside" value="0"/>
                    <EnumElem name="Material Outside" bitmap="material_outside" value="1"/>
                    <EnumElem name="Bend Outside" bitmap="bend_outside" value="2"/>
                    <EnumElem name="Material Inside OML" bitmap="material_inside" value="3"/>
                  </Data>
                </Pref>
                <Pref name="NXSM_FlangeOffset_EU" type="real" title="Offset" value="0.0" unit="in" ver="1847" scope="Session">
                  <Description>This option specifies the Flange offset.</Description>
                  <Real/>
                </Pref>
                <Pref name="NXSM_FlangeOffset_MU" type="real" title="Offset" value="0.0" unit="mm" ver="1847" scope="Session">
                  <Description>This option specifies the Flange offset.</Description>
                  <Real/>
                </Pref>
                <Pref name="NXSM_FlangeEnableGeometryMirrorAndPattern" type="logical" title="Use Geometry Mirror and Pattern" value="false" ver="1926" scope="Session">
                  <Description>This option determines whether or not the Flange command should use the mirror and pattern commands to replicate the geometry rather than the feature parameters.</Description>
                  <Toggle off ="false"/>
                  <Data type="enum">
                    <EnumElem name="Yes" value="true"/>
                    <EnumElem name="No" value="false"/>
                  </Data>
                </Pref>
                <Pref name="NXSM_FlangeEnableMiter" type="logical" title="Miter" value="false" ver="1926" scope="Session">
                  <Description>This option determines whether or not miter should be applied to avoid interferences while creating flanges on adjacent edges in a given set.</Description>
                  <Toggle off ="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
              </Group>
            </Tab>
          </SubCategory>
          <SubCategory name="Normal Cutout" csh="PMAN_Sheet_Metal_Base_Normal_Cutout">
            <Tab name="All" csh="PMAN_Sheet_Metal_Base_Normal_Cutout_All">
              <Group name="Cutout Properties">
                <Pref name="NXSM_NormalCutoutType" type="int" title="Cut Method" value="0" ver="11.0.0" scope="Session">
                  <Description>This option specifies the cut method of Normal Cutout.</Description>
                  <RadioBox orientation="horizontal"/>
                  <Data type="enum">
                    <EnumElem name="Thickness" bitmap="thickness_cut" value="0"/>
                    <EnumElem name="Mid-plane" bitmap="midplane_cut" value="1"/>
                    <EnumElem name="Nearest Face" bitmap="nearest_face_cut" value="2"/>
                  </Data>
                </Pref>
                <Pref name="NXSM_NormalCutoutExtentOption" type="int" title="Limits" value="0" ver="4.0.0" scope="Session">
                  <Description>This option specifies the limits of the Normal Cutout.</Description>
                  <RadioBox orientation="horizontal"/>
                  <Data type="enum">
                    <EnumElem name="Value" bitmap="finite" value="0"/>
                    <EnumElem name="Between" bitmap="from_to_extent" value="1"/>
                    <EnumElem name="Until Next" bitmap="through_next" value="2"/>
                    <EnumElem name="Through All" bitmap="through_all" value="3"/>
                  </Data>
                </Pref>
                <Pref name="NXSM_SymmToggle" type="int" title="Symmetric Depth" value="0" ver="11.0.0" scope="Session">
                  <Description>This option determines whether or not the Cutout should be symmetric.</Description>
                  <Toggle off="0"/>
                  <Data type="enum">
                    <EnumElem name="On" value="1"/>
                    <EnumElem name="Off" value="0"/>
                  </Data>
                </Pref>
                <Pref name="NXSM_Extent_EU" type="real" title="Depth" value="0.2" unit="in" ver="1847" scope="Session">
                  <Description>This option specifies the depth of the Normal Cutout feature.</Description>
                  <Real/>
                </Pref>
                <Pref name="NXSM_Extent_MU" type="real" title="Depth" value="5.0" unit="mm" ver="1847" scope="Session">
                  <Description>This option specifies the depth of the Normal Cutout feature.</Description>
                  <Real/>
                </Pref>
              </Group>
            </Tab>
          </SubCategory>
        </Category>
        <Category name="Bend" csh="PMAN_Sheet_Metal_Bend">
          <SubCategory name="General" csh="PMAN_Sheet_Metal_Bend_General">
            <Tab name="Parameters" csh="PMAN_Sheet_Metal_Bend_General_Parameters">
              <Group name="Bend Parameters">
                <Pref name="NXSM_UseGlobalBendRadius" type="int" title="Use Global Bend Radius" value="1" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not the global bend radius should be used.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
                <Pref name="NXSM_UseGlobalNeutralFactor" type="int" title="Use Global Neutral Factor" value="1" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not the global neutral factor should be used.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
              </Group>
              <Group name="Relief">
                <Pref name="NXSM_BendReliefType" type="int" title="Bend Relief" value="0" ver="4.0.0" scope="Session">
                    <Description>This option specifies the type of bend relief to apply to Flange, Contour Flange, Bend, and Jog features.</Description>
