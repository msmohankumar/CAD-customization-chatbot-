        <Category name="Flat Pattern" csh="PMAN_Sheet_Metal_Flat_Pattern">
          <SubCategory name="Flat Pattern" csh="PMAN_Sheet_Metal_Flat_Pattern_Flat_Pattern">
            <Tab name="Treatments" csh="PMAN_Sheet_Metal_Flat_Pattern_Flat_Pattern_Treatments">
              <Group name="Treatment Options">
                <Pref name="NXSM_FPOrientation" type="int" title="Orientation Method" value="0" ver="2206" scope="Part">
                  <Description>This option specifies the orientation method to use when creating Flat Solid features. Use Default orientation to leave flat solid body at same place after unfolding. Use Select Edge orientation to align selected edge with the X-axis of absolute CSYS. Use Specify CSYS to align specified CSYS to absolute CSYS.</Description>
                  <RadioBox orientation="horizontal"/>
                  <Data type="enum">
                    <EnumElem name="Default" value="0"/>
                    <EnumElem name="Select Edge" value="1"/>
                    <EnumElem name="Specify CSYS" value="2"/>
                  </Data>
                </Pref>
                <Pref name="NXSM_FPCornerOutTreatment" type="int" title="Outside Corner Treatments" value="0" ver="3.0.0" scope="Part">
                  <Description>This option specifies the type of corner treatments to be applied to convex corners when creating Flat Solid features.</Description>
                  <RadioBox orientation="horizontal"/>
                  <Data type="enum">
                    <EnumElem name="None" value="0"/>
                    <EnumElem name="Chamfer" value="1"/>
                    <EnumElem name="Radius" value="2"/>
                  </Data>
                </Pref>
                <Pref name="NXSM_FPCornerOutTreatmentValue_EU" type="real" title="Outside Value" value="0.003937" unit="in" ver="1847" scope="Part">
                  <Description>This option specifies the value for corner treatments to be applied to convex corners when creating Flat Solid features.</Description>
                  <Real/>
                </Pref>
                <Pref name="NXSM_FPCornerOutTreatmentValue_MU" type="real" title="Outside Value" value="0.1" unit="mm" ver="1847" scope="Part">
                  <Description>This option specifies the value for corner treatments to be applied to convex corners when creating Flat Solid features.</Description>
                  <Real/>
                </Pref>
                <Pref name="NXSM_FPCornerInTreatment" type="int" title="Inside Corner Treatments" value="0" ver="3.0.0" scope="Part">
                  <Description>This option specifies the type of corner treatments to be applied to concave corners when creating Flat Solid features.</Description>
                  <RadioBox orientation="horizontal"/>
                  <Data type="enum">
                    <EnumElem name="None" value="0"/>
                    <EnumElem name="Chamfer" value="1"/>
                    <EnumElem name="Radius" value="2"/>
                  </Data>
                </Pref>
                <Pref name="NXSM_FPCornerInTreatmentValue_EU" type="real" title="Inside Value" value="0.003937" unit="in" ver="1847" scope="Part">
                  <Description>This option specifies the value for corner treatments to be applied to concave corners when creating Flat Solid features.</Description>
                  <Real/>
                </Pref>
                <Pref name="NXSM_FPCornerInTreatmentValue_MU" type="real" title="Inside Value" value="0.1" unit="mm" ver="1847" scope="Part">
                  <Description>This option specifies the value for corner treatments to be applied to concave corners when creating Flat Solid features.</Description>
                  <Real/>
                </Pref>
                <Pref name="NXSM_FPHoleTreatment" type="int" title="Hole Treatments" value="0" ver="1847" scope="Part">
                  <Description>This option specifies the type of treatments to be applied to circular cutouts when creating Flat Pattern features.</Description>
                  <RadioBox/>
                  <Data type="enum">
                    <EnumElem name="None" value="0"/>
                    <EnumElem name="Center Mark" value="1"/>
                    <EnumElem name="Hole Curve and Center Mark" value="2"/>
                  </Data>
                </Pref>
                <Pref name="NXSM_FPHoleTreatmentValue_EU" type="real" title="Hole Diameter" value="0.003937" unit="in" ver="1847" scope="Part">
                  <Description>This option specifies the diameter value for hole treatments to be applied to circular cutouts when creating Flat Pattern features.</Description>
                  <Real/>
                </Pref>
                <Pref name="NXSM_FPHoleTreatmentValue_MU" type="real" title="Hole Diameter" value="0.1" unit="mm" ver="1847" scope="Part">
                  <Description>This option specifies the diameter value for hole treatments to be applied to circular cutouts when creating Flat Pattern features.</Description>
                  <Real/>
                </Pref>
              </Group>
              <Group name="Flat Pattern Simplify">
                <Pref name="NXSM_FPSimplify" type="int" title="Simplify B-splines" value="0" ver="3.0.0" scope="Part">
                  <Description>This option determines whether or not the B-splines should be simplified in a Flat Pattern.</Description>
                  <Toggle off="0"/>
                  <Data type="enum">
                    <EnumElem name="Yes" value="1"/>
                    <EnumElem name="No" value="0"/>
                  </Data>
                </Pref>
                <Pref name="NXSM_FPMinimumArc_EU" type="real" title="Minimum Arc" value="0.003937" unit="in" ver="3.0.0" scope="Part">
                  <Description>This option specifies the tolerance value for the radius of curvature on the B-splines to be simplified to arcs in a Flat Pattern.</Description>
                  <Real/>
                  <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                  </Data>
                </Pref>
                <Pref name="NXSM_FPMinimumArc_MU" type="real" title="Minimum Arc" value="0.1" unit="mm" ver="3.0.0" scope="Part">
                  <Description>This option specifies the tolerance value for the radius of curvature on the B-splines to be simplified to arcs in a Flat Pattern.</Description>
                  <Real/>
                  <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                  </Data>
                </Pref>
                <Pref name="NXSM_FPDeviationalTol_EU" type="real" title="Deviational Tolerance" value="0.003937" unit="in" ver="3.0.0" scope="Part">
                  <Description>This option specifies the deviational tolerance value for the tangent along B-splines to be simplified to lines in a Flat Pattern.</Description>
                  <Real/>
                  <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                  </Data>
                </Pref>
                <Pref name="NXSM_FPDeviationalTol_MU" type="real" title="Deviational Tolerance" value="0.1" unit="mm" ver="3.0.0" scope="Part">
                  <Description>This option specifies the deviational tolerance value for the tangent along B-splines to be simplified to lines in a Flat Pattern.</Description>
                  <Real/>
                  <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                  </Data>
                </Pref>
              </Group>
              <Pref name="NXSM_FPEnableAssociative" type="logical" title="Associative" value="true" ver="1926" scope="Part">
                <Description>This option determines whether or not the Flat Pattern should be created associative to the input data.</Description>
                <Toggle off="false"/>
                <Data type="enum">
                  <EnumElem name="Yes" value="true"/>
                  <EnumElem name="No" value="false"/>
                </Data>
              </Pref>
              <Pref name="NXSM_FPFixAtCurrentTimeStamp" type="logical" title="Fix at Current Timestamp" value="true" ver="2206" scope="Part">
                <Description>This option determines whether or not a timestamp should be placed at the last feature in the sheet metal body when the Flat Pattern feature is created. When this option is on, changes made to the sheet metal body after the Flat Pattern feature was created do not appear in the Flat Pattern.</Description>
                <Toggle off="false"/>
                <Data type="enum">
                  <EnumElem name="Yes" value="true"/>
                  <EnumElem name="No" value="false"/>
                </Data>
              </Pref>              
              <Pref name="NXSM_FPEnableShowInteriorFeatureCurves" type="logical" title="Show Interior Feature Curves" value="true" ver="1926" scope="Part">
                <Description>This option determines whether or not interior feature curves in Flat Pattern features should be shown.</Description>
                <Toggle off="false"/>
                <Data type="enum">
                  <EnumElem name="Yes" value="true"/>
                  <EnumElem name="No" value="false"/>
                </Data>
              </Pref>
              <Pref name="NXSM_FPRemoveBendReliefs" type="int" title="Remove System-generated Bend Reliefs" value="0" ver="3.0.0" scope="Part">
                <Description>This option determines whether or not system-generated bend reliefs in Flat Solid features should be removed.</Description>
                <Toggle off="0"/>
                <Data type="enum">
                  <EnumElem name="Yes" value="1"/>
                  <EnumElem name="No" value="0"/>
                </Data>
              </Pref>
              <Pref name="NXSM_FPMaintainHoles" type="int" title="Maintain Circular Shape for Holes in Flat Patterns" value="1" ver="6.0.0" scope="Part">
                <Description>This option determines whether or not circular shape for holes should be maintained in flat patterns.</Description>
                <Toggle off="0"/>
                <Data type="enum">
                  <EnumElem name="Yes" value="1"/>
                  <EnumElem name="No" value="0"/>
                </Data>
              </Pref>
              <Pref name="NXSM_FPLouverCutoutCurves" type="logical" title="Show Louver Cutout Curves as Interior Cutout" value="true" ver="2206" scope="Part">
                <Description>This option determines whether or not Louver feature cutout curves should classify as interior cutout curves in flat patterns.</Description>
                <Toggle off="false"/>
                <Data type="enum">
                  <EnumElem name="Yes" value="true"/>
                  <EnumElem name="No" value="false"/>
                </Data>
              </Pref>
              <Pref name="NXSM_FPKeepFlatSolidExternal" type="logical" title="Keep Flat Solid External" value="false" ver="2312" scope="Part">
                <Description>This option determines whether or not the flat solid should be kept external while creating the flat pattern feature.</Description>
                <Toggle off="false"/>
                <Data type="enum">
                    <EnumElem name="Yes" value="true"/>
                    <EnumElem name="No" value="false"/>
                </Data>
