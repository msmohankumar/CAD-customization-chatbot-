                        </Data>
                    </Pref>
                    <Pref name="PDCW_CastingBodyBoxOffsetX2_MU" type="real" title="X2" value="10.0" unit="mm" ver="1899" scope="Session">
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="PDCW_CastingBodyBoxOffsetX2_EU" type="real" title="X2" value="0.4" unit="in" ver="1899" scope="Session">
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="PDCW_CastingBodyBoxOffsetY1_MU" type="real" title="Y1" value="10.0" unit="mm" ver="1899" scope="Session">
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="PDCW_CastingBodyBoxOffsetY1_EU" type="real" title="Y1" value="0.4" unit="in" ver="1899" scope="Session">
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="PDCW_CastingBodyBoxOffsetY2_MU" type="real" title="Y2" value="10.0" unit="mm" ver="1899" scope="Session">
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="PDCW_CastingBodyBoxOffsetY2_EU" type="real" title="Y2" value="0.4" unit="in" ver="1899" scope="Session">
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Step Parting Sheet">
                    <Pref name="PDCW_CastingStepSheetToolLand_MU" type="real" title="Tool Land" value="5.0" unit="mm" ver="1899" scope="Session">
                        <Real/>
                    </Pref>
                    <Pref name="PDCW_CastingStepSheetToolLand_EU" type="real" title="Tool Land" value="0.2" unit="in" ver="1899" scope="Session">
                        <Real/>
                    </Pref>
                    <Pref name="PDCW_CastingStepSheetDraftAngle" type="real" title="Draft Angle" value="5.0" unit="deg" ver="1899" scope="Session">
                        <Real/>
                    </Pref>
