                        <Integer/>
                        <Data type="range">
                            <LowerBound value="1" include="1"/>
                            <UpperBound value="256" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="PDCW_DatumLayerName" type="string" title="Layer Category Name" value="Datum" ver="1899" scope="Session">
                        <Description>This field allows a layer category name to be specified for the layer where the datum objects are placed.</Description>
                        <Widestring/>
                    </Pref>
                </Group>
            </Tab>
        </Category>
        <Category name="Workpiece" csh="PMAN_PDCW_Work_Piece">
            <Tab name="General" csh="PMAN_PDCW_Work_Piece_General">
                <Pref name="PDCW_WorkPieceOffset_MU" type="real" title="Offset" value="25.0" unit="mm" ver="1899" scope="Session">
                    <Description>This option specifies the offset value of the initial work piece calculated from the bounding box of the product model.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="PDCW_WorkPieceOffset_EU" type="real" title="Offset" value="1.0" unit="in" ver="1899" scope="Session">
                    <Description>This option specifies the offset value of the initial work piece calculated from the bounding box of the product model.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="PDCW_WorkPieceMethod" type="int" title="Dimensioning" value="0" ver="1899" scope="Session">
                    <Description>This option specifies the work piece dimensioning method. The Distance Allowance method is centered on the product body. The Reference Point method is centered on the absolute CSYS or a specified point.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="Distance Allowance" value="0"/>
                        <EnumElem name="Reference Point" value="1"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Layer and Category" csh="PMAN_PDCW_Work_Piece_Layer_and_Category">
                <Group name="Workpiece">
                <Pref name="PDCW_WorkPieceLayer" type="int" title="Layer" value="20" ver="1899" scope="Session">
                    <Description>This option specifies which layer the work piece should be placed on.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                        <UpperBound value="256" include="1"/>
                    </Data>
                </Pref>
                <Pref name="PDCW_WorkPieceLayerName" type="string" title="Layer Category Name" value="WorkPiece" ver="1899" scope="Session">
                    <Description>This field allows a layer category name to be specified for the layer where the work piece is placed.</Description>
                    <Widestring/>
                </Pref>
                </Group>
                <Group name="Combined Workpiece">
                <Pref name="PDCW_CombinedWorkPieceLayer" type="int" title="Layer" value="200" ver="1899" scope="Session">
                    <Description>This option specifies which layer the combined work piece should be placed on.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                        <UpperBound value="256" include="1"/>
                    </Data>
                </Pref>
                <Pref name="PDCW_CombinedWorkPieceLayerName" type="string" title="Layer Category Name" value="CombinedWorkPiece" ver="1899" scope="Session">
                    <Description>This field allows a layer category name to be specified for the layer where the combined work piece is placed.</Description>
                    <Widestring/>
                </Pref>
                </Group>
            </Tab>
        </Category>
        <Category name="Mold Tools" csh="PMAN_PDCW_Mold_Tools">
            <Tab name="General" csh="PMAN_PDCW_Mold_Tools_General">
                <Pref name="PDCW_ToolsTolerance_MU" type="real" title="Tolerance" value="0.025" unit="mm" ver="1899" scope="Session">
                    <Description>This option specifies the modeling tolerance for the Mold Tools patch command's sheet trim, enlarge face, and traverse loop.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="PDCW_ToolsTolerance_EU" type="real" title="Tolerance" value="0.001" unit="in" ver="1899" scope="Session">
                    <Description>This option specifies the modeling tolerance for the Mold Tools patch command's sheet trim, enlarge face, and traverse loop.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Group name="Bounding Body">
                    <Pref name="PDCW_ToolsBoxTranslucency" type="int" title="Translucency" value="50" ver="1899" scope="Session">
                        <Description>The Bounding Body command creates solid boxes (blocks). This option specifies the translucency of the box.</Description>
                        <IntegerScale/>
                        <Data type="range">
                            <LowerBound value="0" include="1"/>
                            <UpperBound value="100" include="1"/>
                        </Data>
                    </Pref>                                
                </Group>    
                <Pref name="PDCW_ToolsPatchFeatureNum" type="int" title="Maximum Number of Features for Feature Patch" value="150" ver="1899" scope="Session">
                    <Description>This option specifies the maximum number of features allowed in the original product model for feature patch to be active as a patching option.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                    </Data>
                    <Docum>When patching a hole in the product model, a method called feature patch-up is sometimes used. This method creates new features in the original product model. If the original product model has a large number of features, the update processing may be time-consuming.</Docum>
                </Pref>
            </Tab>
