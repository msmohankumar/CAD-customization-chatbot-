<PrefDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www-cyp.ugsolutions.com/assemblies/pman">
    <Application name="Pressure Die Casting Wizard" csh="PMAN_PDCW" package_suppress="simcenter3d,simviewer,view,mechatronicsplayer,nx2dedit">
        <Category name="General" csh="PMAN_PDCW_General">
            <Tab name="General" csh="PMAN_PDCW_General_General">
                <Pref name="PDCW_ExcelBackground" type="logical" title="Operate NX while Microsoft Excel is Open" value="true" ver="1899" scope="Session">
                    <Description>This option determines whether or not NX can be operated while Microsoft Excel is open.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Pref name="PDCW_Load_WAVE_Data" type="logical" title="Load Wave Data" value="false" ver="1899" scope="Session">
                    <Description>This option determines whether or not WAVE data should be loaded.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
              <Group name="Teamcenter">
                <Pref name="PDCW_PART_ITEM_TYPE" type="string" title="Part Item Type" value="Item" ver="1899" scope="Session">
                  <Description>This option specifies the customized item type of the part in Teamcenter.</Description>
                  <String/>
                </Pref>
                <Pref name="PDCW_CONFIG_ITEM" type="string" title="Configuration File Item ID and Revision" value="@DB/00005/A" ver="1899" scope="Session">
                  <Description>This option specifies the item ID and revision of the configuration file type in Teamcenter.</Description>
                  <String/>
                </Pref>
              </Group>
            </Tab>
            <Tab name="Project Settings" csh="PMAN_PDCW_General_Project_Setting">
                <Group name="Project Initialize">
                    <Pref name="PDCW_ProjectInitialUnit" type="int" title="Project Unit" value="3" ver="2306" scope="Session">
                        <Description>This option specifies the project unit.</Description>
                        <RadioBox/>
                        <Data type="enum">
                            <EnumElem name="Metric" value="1"/>
                            <EnumElem name="English" value="2"/>
                            <EnumElem name="Same as Project Library" value="3"/>
                        </Data>
                    </Pref>
                    <Pref name="PDCW_ProjectPath_WIN" type="string" title="Project File Path" value="" ver="1899" scope="Session">
                        <Description>This field defines the project file path.</Description>
                        <BrowseDirSel/>
                    </Pref>
                    <Pref name="PDCW_ProjectPath_UNX" type="string" title="Project File Path" value="" ver="1899" scope="Session">
                        <Description>This field defines the project file path.</Description>
                        <BrowseDirSel/>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Part Name Definition" csh="PMAN_PDCW_General_Part_Name_Definition">
                <Group name="Common">
                    <Pref name="PDCW_DISPLAY_RENAME_DIALOG" type="logical" title="Display Rename Dialog Box" value="false" ver="1899" scope="Session">
                        <Description>This option determines whether or not the part name mapping dialog box will be displayed when new parts are added to a Pressure Die Casting Wizard project.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="PDCW_NAME_MAP_TEMPLATE_FILE" type="string" title="Template Name Mapping File" value="map_template_name.txt" ver="1899" scope="Session">
                        <Description>This option specifies a customized file to map template name.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="PDCW_Part_Name_Rule" type="string_list" title="Custom Part Naming Rules" value="&lt;PROJECT_NAME&gt;_&lt;TEMPLATE_NAME&gt;_???" ver="1899" scope="Session">
                        <Description>This field allows the custom part naming rules to be specified. The part name is formed by combining a series of variables and characters.
<Formatted>1. All alphanumeric characters are valid except \, /, ", *, :, and ?.
2. The "?" character is an incrementing or user defined variable.
The standard variables are:
"PROJECT_NAME"  (set during project initialization)
"TEMPLATE_NAME" (the name of the template part being cloned).</Formatted></Description>
                        <MultiLineText height="6"/>
                    </Pref>
                </Group>
                <Group name="Teamcenter">
                    <Pref name="PDCW_NAME_IMAN_NUMBER" type="logical" title="Use Teamcenter Number as ??? in above Naming Rule" value="true" ver="1899" scope="Session">
                        <Description>This option determines whether or not the Teamcenter name number should be used.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="PDCW_IMAN_Name_Method" type="int" title="Teamcenter Part Naming Method" value="2" ver="1899" scope="Session">
                    <Description>This option specifies the part naming method in the Teamcenter environment.
<Formatted>
First option: DB_PART_NAME = DB_PART_NO, which is defined by name rules.

Second option: DB_PART_NO is defined by name rules, DB_PART_NAME is replaced by TEMPLATE_NAME.

Third option: DB_PART_NO is defined by unique Teamcenter number, DB_PART_NAME is defined by name rules.</Formatted></Description>
                        <RadioBox/>
                        <Data type="enum">
                            <EnumElem name="Use Naming Rule for Part Name and Part Number" value="0"/>
                            <EnumElem name="Use Naming Rule for Part Number, Template Name for Part Name" value="1"/>
                            <EnumElem name="Use Teamcenter Number for Part Number, Naming Rule for Part Name" value="2"/>
                        </Data>
                    </Pref>
                </Group>
                <Pref name="PDCW_PART_FAMILY_LIB_SAVE_DIR_UNX" type="string" title="Part Family Save Directory" value="" ver="1899" scope="Session">
                    <Description>This field defines the directory in which part family data should be saved.</Description>
                    <BrowseDirSel/>
                </Pref>
                <Pref name="PDCW_PART_FAMILY_LIB_SAVE_DIR_WIN" type="string" title="Part Family Save Directory" value="" ver="1899" scope="Session">
                    <Description>This field defines the directory in which part family data should be saved.</Description>
                    <BrowseDirSel/>
                </Pref>
            </Tab>
            <Tab name="Layer and Category" csh="PMAN_PDCW_General_Layer_and_Category">
