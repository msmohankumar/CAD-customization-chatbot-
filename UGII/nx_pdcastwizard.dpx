                        <Integer/>
                        <Data type="range">
                            <LowerBound value="1" include="1"/>
                            <UpperBound value="256" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="PDCW_HideObjectLayerName" type="string" title="Layer Category Name" value="HiddenObject" ver="1899" scope="Session">
                        <Description>This field allows a layer category name to be specified for the layer where the hidden objects are placed.</Description>
                        <Widestring/>
                    </Pref>
                </Group>
                <Group name="Datum">
                    <Pref name="PDCW_DatumLayer" type="int" title="Layer" value="100" ver="1899" scope="Session">
