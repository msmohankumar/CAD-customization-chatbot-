            <Tab name="Layers" csh="PMAN_PDCW_Parting_Layers">
                <Label label="The following options specify which layers different parting objects should be placed on."/>
                <Pref name="PDCW_PartingProdLayer" type="int" title="Product" value="1" ver="1899" scope="Session">
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                        <UpperBound value="256" include="1"/>
                    </Data>
                </Pref>
                <Pref name="PDCW_PartingLinesLayer" type="int" title="Line" value="26" ver="1899" scope="Session">
