            <Tab name="Design Inserts" csh="PMAN_PDCW_Mold_Tools_Design_Inserts">
                <Pref name="PDCW_InsertColor" type="int" title="Color" value="11" ver="1899" scope="Session">
                    <Description>This option specifies the color of the insert.</Description>
                    <Color/>
                </Pref>
                <Pref name="PDCW_InsertLayer" type="int" title="Layer" value="175" ver="1899" scope="Session">
                    <Description>This option specifies which layer the insert should be placed on.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                        <UpperBound value="256" include="1"/>
                    </Data>
                </Pref>
                <Pref name="PDCW_InsertLayerName" type="string" title="Layer Category Name" value="Subinsert" ver="1899" scope="Session">
                    <Description>This field allows a layer category name to be specified for the layer where the insert objects are placed.</Description>
