                <Pref name="PMAN_RoutingMechanicalSpecificationLoadOptions" type="int" title="Options - Routing Mechanical" value="0" ver="9.0.0" scope="Session">
                    <Description>This option specifies the filter to apply while loading specifications in Routing Mechanical.</Description>
                    <RadioBox orientation="vertical"/>
                    <Data type="enum">
                        <EnumElem name="All from Active Discipline" value="0"/>
                        <EnumElem name="From Specified Authoring Source" value="1"/>
                        <EnumElem name="From Work Part Runs" value="2"/>
                    </Data>
                </Pref>
                <Pref name="PMAN_RoutingLogicalSpecificationLoadOptions" type="int" title="Options - Routing Logical" value="0" ver="9.0.0" scope="Session">
                    <Description>This option specifies the filter to apply while loading specifications in Routing Logical.</Description>
                    <RadioBox orientation="vertical"/>
                    <Data type="enum">
                        <EnumElem name="All from Active Discipline" value="0"/>
                        <EnumElem name="From Specified Authoring Source" value="1"/>
                        <EnumElem name="From Work Part Runs" value="2"/>
                    </Data>
                </Pref>
                <Pref name="PMAN_RoutingSpecificationAuthoringSource" type="string" title="Authoring Source" value="NX" ver="9.0.1" scope="Session">
                    <Description>This option specifies the authors of the specification to be loaded.</Description>
                    <String/>
                </Pref>
            </Tab>
            <Tab name="Specification Folders" csh="PMAN_Routing_Part_Library_Specifications_Folders">
                <Label label="The following options are used to define the native folder path for the Routing applications."/>
                <Pref name="PMAN_RoutingElectricalSpecificationsFolder_WIN" type="string" title="Routing Electrical" value="$UGII_ROUTE_ELEC_KIT_DIR\specifications\$UGII_ROUTING_KIT_UNITS\" ver="9.0.0" scope="Session">
                    <Description>This option specifies the folder path for specification XML files in Routing Electrical.</Description>
                    <BrowseDirSel/>
                </Pref>
                <Pref name="PMAN_RoutingElectricalSpecificationsFolder_UNX" type="string" title="Routing Electrical" value="$UGII_ROUTE_ELEC_KIT_DIR/specifications/$UGII_ROUTING_KIT_UNITS/" ver="9.0.0" scope="Session">
                    <Description>This option specifies the folder path for specification XML files in Routing Electrical.</Description>
                    <BrowseDirSel/>
                </Pref>
                <Pref name="PMAN_RoutingMechanicalSpecificationsFolder_WIN" type="string" title="Routing Mechanical" value="$UGII_ROUTE_MECH_KIT_DIR\specifications\$UGII_ROUTING_KIT_UNITS\" ver="9.0.0" scope="Session">
                    <Description>This option specifies the folder path for specification XML files in Routing Mechanical.</Description>
                    <BrowseDirSel/>
                </Pref>
                <Pref name="PMAN_RoutingMechanicalSpecificationsFolder_UNX" type="string" title="Routing Mechanical" value="$UGII_ROUTE_MECH_KIT_DIR/specifications/$UGII_ROUTING_KIT_UNITS/" ver="9.0.0" scope="Session">
                    <Description>This option specifies the folder path for specification XML files in Routing Mechanical.</Description>
                    <BrowseDirSel/>
                </Pref>
                <Pref name="PMAN_RoutingLogicalSpecificationsFolder_WIN" type="string" title="Routing Logical" value="$UGII_ROUTE_LGCL_KIT_DIR\specifications\$UGII_ROUTING_KIT_UNITS\" ver="9.0.3" scope="Session">
                    <Description>This option specifies the folder path for specification XML files in Routing Logical.</Description>
                    <BrowseDirSel/>
                </Pref>
                <Pref name="PMAN_RoutingLogicalSpecificationsFolder_UNX" type="string" title="Routing Logical" value="$UGII_ROUTE_LGCL_KIT_DIR/specifications/$UGII_ROUTING_KIT_UNITS/" ver="9.0.3" scope="Session">
                    <Description>This option specifies the folder path for specification XML files in Routing Logical.</Description>
                    <BrowseDirSel/>
                </Pref>
                <Pref name="PMAN_RoutingSpecificationNativeOption" type="logical" title="Use Native Folders when Connected to Teamcenter" value="true" ver="9.0.0" scope="Session" suppress="TCX_LITE">
                    <Description>This option applies only when connected to Teamcenter and determines whether or not specification files are loaded and saved exclusively from/to native folders or from/to Teamcenter. The Teamcenter specification folders are set in the Teamcenter preferences.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
            </Tab>
        </Category>
        <Category name="Electrical" csh="PMAN_Routing_Electrical">
            <Tab name="General" csh="PMAN_Routing_Electrical_General">
                <Group name="Allowable Curves">
                    <Pref name="RTE_ElectricalPathAllowCurvesLine" type="logical" title="Lines" value="true" ver="6.0.0" scope="Session">
                        <Description>This option determines whether or not lines are used when creating a path.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="On" value="true"/>
                            <EnumElem name="Off" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="RTE_ElectricalPathAllowCurvesArc" type="logical" title="Arcs" value="true" ver="6.0.0" scope="Session">
                        <Description>This option determines whether or not arcs are used when creating a path.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="On" value="true"/>
                            <EnumElem name="Off" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="RTE_ElectricalPathAllowCurvesSpline" type="logical" title="Splines" value="true" ver="6.0.0" scope="Session">
                        <Description>This option determines whether or not splines are used when creating a path.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="On" value="true"/>
                            <EnumElem name="Off" value="false"/>
                        </Data>
                    </Pref>
                </Group>
                <Pref name="RTE_ElectricalHarnessName" type="string" title="Harness Name" value="" ver="5.0.0" scope="Session">
                    <Description>This option specifies the harness name supplied to connections that have no specified harness.</Description>
                    <Widestring/>
                </Pref>
                <Separator/>
                <Pref name="RTE_ElectricalCreateTerminals" type="int" title="Create Terminals" value="0" ver="5.0.0" scope="Session">
                    <Description>This option specifies which set of terminal ports to create terminals for when performing an Automatic Create Terminals command from the Component List dialog box.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="All" value="0"/>
                        <EnumElem name="Listed" value="1"/>
                        <EnumElem name="Routed" value="2"/>
                    </Data>
                    <Docum>
                        <Formatted>
All: All the terminal ports for all component multiports will have terminals automatically created.
Listed: Only terminal ports in the connection list will have terminals automatically created.
Routed: Only terminal ports for routed connections will have terminals automatically created.
                        </Formatted>
                    </Docum>
                </Pref>
                <Pref name="RTE_ElectricalDisplaySolidonTerminal" type="logical" title="Display Solid on Terminal Segment" value="true" ver="5.0.0" scope="Session">
                    <Description>This option determines whether or not the wire solid on the terminal segment is displayed.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Group name="Auto Assignment Method">
                    <Pref name="RTE_ElectricalComponentMatchMethod" type="int" title="Match By" value="0" ver="5.0.0" scope="Session">
                        <Description>This option specifies the method by which to automatically assign components in the Component List Navigator.</Description>
                        <RadioBox/>
                        <Data type="enum">
                            <EnumElem name="Attribute" value="2"/>
                            <EnumElem name="Component Name" value="1"/>
                            <EnumElem name="File Name" value="0"/>
                        </Data>
                        <Docum>
                            <Formatted>
Attribute: Compares the Component List Navigator cell value under the Column Heading to the value of the component or port attribute of the given title.
    The column name is the display name of the field in the display format.
    The comparison is case sensitive.

Component Name: Compares the Connector name, Component name, and Equipment name to the name of each component.
    The comparison is case sensitive.

File Name: Compares the part name of a Connector to the file name of a candidate part.
    The comparison is not case sensitive and does not compare the extension.
                            </Formatted>
                        </Docum>
                    </Pref>
                    <Pref name="RTE_ElectricalColumntoMatch" type="string" title="Component List Column Title" value="&lt;NONE&gt;" ver="5.0.0" scope="Session">
                        <Description>This option specifies the Component List Navigator column name in which to look for a match to the Component Attribute value. This option is used when Match By is set to Attribute.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="RTE_ElectricalNametoMatch" type="string" title="Component Attribute Title" value="&lt;NONE&gt;" ver="5.0.0" scope="Session">
                        <Description>This option specifies the component attribute name for which to get the value for matching with the Component List Navigator's specified column's row value for attribute matching. This option is used when Match By is set to Attribute.</Description>
                        <Widestring/>
                    </Pref>
                </Group>
                <Pref name="RTE_ElectricalReportRoutingFailures" type="logical" title="Report Routing Failures" value="true" ver="5.0.0" scope="Session">
                    <Description>This option determines whether or not automatic or manual routing failures are reported to the Information window.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="RTE_ElectricalHighlightWireStock" type="logical" title="Highlight Wire Stock" value="true" ver="5.0.0" scope="Session">
                    <Description>This option determines whether or not wire stock is highlighted when a Connection List Navigator row is selected.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="RTE_ElectricalHighlightWirePath" type="logical" title="Highlight Wire Path" value="false" ver="6.0.0" scope="Session">
                    <Description>This option determines whether or not wire paths are highlighted when a routed segment is selected.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="RTE_ElectricalBundleColor" type="int" title="Bundle Color" value="134" ver="5.0.0" scope="Session">
                    <Description>This option specifies the color of routed bundles.</Description>
                    <Color/>
                </Pref>
                <Pref name="RTE_ElectricalDeleteSRStockOnRoute" type="logical" title="Delete Space Reservation Stock on Auto or Manual Route" value="false" ver="6.0.0" scope="Session">
                    <Description>This option determines whether or not to delete Space Reservation stock upon Auto or Manual route. When this option is off, Space Reservation stock remains over the segments along with other stocks but not as a part of the bundle stock.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="RTE_ElectricalUseParentCableStock" type="logical" title="Use Cable Stock when Wire Stock is Missing" value="false" ver="1953+" scope="Session">
                    <Description>This option determines whether or not to use the stock of the parent cable if the stock of the child wire cannot be found.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="RTE_ElectricalMinimumOverstockWidth_MU" type="real" title="Minimum Overstock Width" value="2.5" unit="mm" ver="6.0.2" scope="Session">
                    <Description>This option specifies the minimum overstock length for overstock that does not have a specified width.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="RTE_ElectricalMinimumOverstockWidth_EU" type="real" title="Minimum Overstock Width" value="0.1" unit="in" ver="6.0.2" scope="Session">
                    <Description>This option specifies the minimum overstock length for overstock that does not have a specified width.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Group name="Wire Cut Length Calculation">
                    <Pref name="RTE_ElectricalCutLengthMultiplier" type="real" title="Cut Length Multiplier" value="10.0" unit="%" ver="1847" scope="Session">
                        <Description>This option specifies how much a wire's length should be increased when calculating the cut length. The cut length is calculated by multiplying the wire's modeled length by the Cut Length Multiplier and then adding the Cut Length Offset.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="RTE_ElectricalCutLengthOffset_MU" type="real" title="Cut Length Offset" value="5.0" unit="mm" ver="1847" scope="Session">
                        <Description>This option specifies the additional length to add to a wire's length when calculating the cut length. The cut length is calculated by multiplying the wire's modeled length by the Cut Length Multiplier and then adding the Cut Length Offset.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="RTE_ElectricalCutLengthOffset_EU" type="real" title="Cut Length Offset" value="0.25" unit="in" ver="1847" scope="Session">
                        <Description>This option specifies the additional length to add to a wire's length when calculating the cut length. The cut length is calculated by multiplying the wire's modeled length by the Cut Length Multiplier and then adding the Cut Length Offset.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Navigators" csh="PMAN_Routing_Electrical_Navigators">
                <Group name="Automatic Route">
                    <Pref name="RTE_ElectricalAutoRoute" type="int" title="Route Level" value="1" ver="5.0.0" scope="Session">
