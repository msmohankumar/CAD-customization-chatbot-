                            <EnumElem name="Pin" value="1"/>
                            <EnumElem name="Component" value="2"/>
                        </Data>
                    </Pref>
                    <Pref name="RTE_ElectricalAutoRouteStockStyle" type="int" title="Stock Style" value="1" ver="5.0.0" scope="Session">
