                    <String/>
                </Pref>
                <Pref name="RTE_BPN_NextBranchID" type="string" title="Next Branch ID" value="(PREVIOUS_BRANCH_ID+1)" ver="8.5.1" scope="Session">
                    <Description>This option specifies the formula with which to calculate the value of the Next Branch ID.</Description>
                    <String/>
                </Pref>
                <Pref name="RTE_BPN_AttributeName" type="string" title="Attribute Name" value="HVAC Duct Branch ID" ver="8.5.1" scope="Session">
                    <Description>This option specifies the name of the attribute that will hold the value of the branch ID.</Description>
                    <String/>
                </Pref>

                <Pref name="RTE_ForceDuctReinforcementType" type="logical" title="Force Duct Reinforcement Type" value="false" ver="8.5.2" scope="Session">
                    <Description>This option determines whether or not the default reinforcement type should be used for all ducts.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="RTE_DuctReinforcementType" type="int" title="Reinforcement Type" value="0" ver="8.5.2" scope="Session">
                    <Description>This option specifies if inside, outside, or no reinforcement should be placed on a duct.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="None"        value="0"/>
                        <EnumElem name="Inside"      value="1"/>
                        <EnumElem name="Outside"     value="2"/>
                    </Data>
                </Pref>
            </Tab>
        </Category>
        <Category name="Logical" csh="PMAN_Routing_Logical">
            <Tab name="General" csh="PMAN_Routing_Logical_General">
                <Group name="Allowable Curves">
                    <Pref name="RTE_LogicalPathAllowCurvesLine" type="logical" title="Lines" value="true" ver="6.0.0" scope="Session">
                        <Description>This option determines whether or not lines are used when creating a path.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="On" value="true"/>
                            <EnumElem name="Off" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="RTE_LogicalPathAllowCurvesArc" type="logical" title="Arcs" value="false" ver="6.0.0" scope="Session">
                        <Description>This option determines whether or not arcs are used when creating a path.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="On" value="true"/>
                            <EnumElem name="Off" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="RTE_LogicalPathAllowCurvesSpline" type="logical" title="Splines" value="false" ver="6.0.0" scope="Session">
                        <Description>This option determines whether or not splines are used when creating a path.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="On" value="true"/>
                            <EnumElem name="Off" value="false"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Line Style Pattern">
                    <Pref name="RTL_LogicalFontLayer" type="int" title="Layer" value="2" ver="8.5.0" scope="Session">
                        <Description>This option specifies the layer on which the line style should be used. This value may be overridden by the Work Layer option in the Add Line Font dialog box.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="1" include="1"/>
                            <UpperBound value="256" include="1"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
        </Category>
        <Category name="Bill of Materials" csh="PMAN_Routing_BOM">
            <Tab name="General" csh="PMAN_Routing_BOM_General">
                <Pref name="RTE_BomDetailLevel" type="int" title="Detail Level" value="0" ver="7.5.0" scope="Session">
                    <Description>This option specifies how fabrications are reported on a Routing Bill of Materials.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Summary" value="0"/>
                        <EnumElem name="Itemized" value="1"/>
                    </Data>
                    <Docum>
                        <Formatted>
Summary: The system will report only components and stock in the current work part.
Itemized: The existence of each routing object is reported individually, regardless of its position in the assembly hierarchy.
                        </Formatted>
                    </Docum>
                </Pref>
                <Pref name="RTE_BomReportCountOfStockPieces" type="logical" title="Report Count of Stock Pieces" value="true" ver="7.5.0" scope="Session">
                    <Description>This option determines whether or not the Quantity column for rows containing stocks or overstocks should report the number of pieces of stock for the "Summary" level of reporting.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="RTE_BomFabricationNumbering" type="int" title="Fabrication Numbering" value="0" ver="7.5.0" scope="Session">
                    <Description>This option specifies the numbering of any fabrications in a Routing Bill of Materials.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Sequential" value="0"/>
                        <EnumElem name="Original" value="1"/>
                    </Data>
                    <Docum>
                        <Formatted>
Sequential: Sequential numbering is used as the callout in the Routing Bill of Materials.
Original: The user supplied fabrication numbering is used as the callout in the Routing Bill of Materials.
                        </Formatted>
                    </Docum>
                </Pref>
                <Pref name="RTE_BomIncludeComponents" type="logical" title="Include Components" value="false" ver="7.5.0" scope="Session">
                    <Description>This option determines whether to include components underneath the Assembly.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="RTE_BomIncludeGaskets" type="logical" title="Include Gaskets, Nuts, and Bolts" value="false" ver="7.5.0" scope="Session">
                    <Description>This option determines whether to include Routing UDOs created to store information about Bolts and Gaskets (under Piping Preferences of Routing Mechanical).</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Group name="Stock Options">
                    <Pref name="RTE_BomIncludeStock" type="logical" title="Include Stock" value="false" ver="7.5.0" scope="Session">
                        <Description>This option determines whether to include mechanical stock such as conduit, filler stock, piping, raceway, etc. (anything assigned using the "Stock" dialog box).</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="RTE_BomIncludeOverstock" type="logical" title="Include Overstock" value="false" ver="7.5.0" scope="Session">
                        <Description>This option determines whether to include anything assigned using the "Overstock" dialog box (except for overstocks that are defined as "Wrapped" overstocks).</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="RTE_BomIncludeWireStock" type="logical" title="Include Wire Stock" value="false" ver="7.5.0" scope="Session">
                        <Description>This option determines whether to include wire stocks from Routing Electrical that have a "PART_NUMBER" attribute.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="RTE_BomStockLength" type="int" title="Length Reporting" value="0" ver="7.5.0" scope="Session">
                        <Description>This option specifies how stock length should be reported.</Description>
                        <RadioBox orientation="horizontal"/>
                        <Data type="enum">
                            <EnumElem name="Single Sum" value="0"/>
                            <EnumElem name="List Each" value="1"/>
                        </Data>
                        <Docum>
                            <Formatted>
Single Sum: Collapses all stocks of a given type into a single line and displays the sum of all lengths in the "Length" column.
List Each: Groups all stocks of a given type and length together (applicable only when the "Level" option is set to "Itemized").
                            </Formatted>
                        </Docum>
                    </Pref>
                </Group>
                <Group name="Template Parts">
                    <Pref name="RTE_BomMetricTemplatePartName_WIN" type="string" title="Metric Template" value="routing_bom_metric.prt" ver="8.0.0" scope="Session">
                        <Description>This option specifies the name of the metric template part to use for the Routing Bill of Materials. If no path is given with the part, the environment variable UGII_ROUTING_BOM_PATH is used as the path to the part.</Description>
                        <BrowseFileSel type="prt"/>
                    </Pref>
                    <Pref name="RTE_BomEnglishTemplatePartName_WIN" type="string" title="English Template" value="routing_bom_english.prt" ver="8.0.0" scope="Session">
                        <Description>This option specifies the name of the inch template part to use for the Routing Bill of Materials. If no path is given with the part, the environment variable UGII_ROUTING_BOM_PATH is used as the path to the part.</Description>
                        <BrowseFileSel type="prt"/>
                    </Pref>
                    <Pref name="RTE_BomMetricTemplatePartName_UNX" type="string" title="Metric Template" value="routing_bom_metric.prt" ver="8.0.0" scope="Session">
                        <Description>This option specifies the name of the metric template part to use for the Routing Bill of Materials. If no path is given with the part, the environment variable UGII_ROUTING_BOM_PATH is used as the path to the part.</Description>
                        <BrowseFileSel type="prt"/>
                    </Pref>
                    <Pref name="RTE_BomEnglishTemplatePartName_UNX" type="string" title="English Template" value="routing_bom_english.prt" ver="8.0.0" scope="Session">
                        <Description>This option specifies the name of the inch template part to use for the Routing Bill of Materials. If no path is given with the part, the environment variable UGII_ROUTING_BOM_PATH is used as the path to the part.</Description>
                        <BrowseFileSel type="prt"/>
                    </Pref>
                </Group>
                <Pref name="RTE_BomWriteToListingWindow" type="logical" title="Write to Information Window" value="false" ver="7.5.0" scope="Session">
                    <Description>This option determines whether or not to write the Bill of Materials information to the Information window when a new BOM is created.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="RTE_BomIgnoreReferenceSets" type="logical" title="Ignore Reference Sets" value="false" ver="2206" scope="Session">
                    <Description>This option determines whether or not the Routing Bill of Materials will ignore reference sets for stock in components.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
            </Tab>
        </Category>
    </Application>
</PrefDefinition>

