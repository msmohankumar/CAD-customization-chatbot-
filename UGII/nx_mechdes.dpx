                    <Description>This option specifies the density of a flexible body.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="MD_FlexibleMaterialDensity_EU" type="real" title="Density" value="0.0433527504" unit="lbm/in^3" ver="2007" scope="Part">
                    <Description>This option specifies the density of a flexible body.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="MD_FlexibleMaterialDamping" type="real" title="Damping" value="0.25" ver="2007" scope="Part">
                    <Description>This option specifies the coefficient of damping forces acting on flexible bodies to reduce their oscillation over time.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                        <UpperBound value="1.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="MD_FlexibleMaterialDynamicFriction" type="real" title="Dynamic Friction" value="0.2" ver="2007" scope="Part">
                    <Description>This option specifies the coefficient of the dynamic friction acting on a flexible body against a surface.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                        <UpperBound value="1.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="MD_FlexibleMaterialLinearStiffness" type="real" title="Linear Stiffness" value="1.0" ver="2007" scope="Part">
                    <Description>This option specifies the coefficient of conservation of distance between adjacent particles. The larger the value, the stronger is the conservation of distance between particles (similar to the stiffness of a spring, a larger value means a stronger, more rigid spring).</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                        <UpperBound value="1.0" include="1"/>
