<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="docum.xsl"?>
<!--

-->
<PrefDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www-cyp.ugsolutions.com/assemblies/pman">
    <Application name="Mechatronics Concept Designer" csh="PMAN_Mechatronics_Designer" package_suppress="simcenter3d,simviewer,view,nx2dedit">
        <Category name="General" csh="PMAN_Mechatronics_Designer_General">
            <Tab name="General" csh="PMAN_Mechatronics_Designer_General_General">
                <Group name="Objects Not Part of Physics">
                    <Pref name="MD_NonPhysicsColor" type="int" title="Color" value="15" ver="1980" scope="Part">
                        <Description>This option specifies the color of objects that are not members of a physics object.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="MD_RichGeomType" type="logical" title="Include Curves, Datums, and PMI" value="true" ver="1980" scope="Part">
                        <Description>This option determines if curves, datums, and PMI that are not part of the physics also assume the Objects Not Part of Physics color when the Rigid Body Color option is on.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Arrangement">
                    <Pref name="MD_ArrangementType" type="int" title="Type" value="0" ver="2206" scope="Part">
                        <Description>This option specifies the capture arrangement type. An isolated arrangement always ignores assembly constraints. A position override in an isolated arrangement exists independently of overrides in other arrangements. A regular arrangement is a standard arrangement which may ignore assembly constraints. A position override in a standard arrangement exists in all other standard arrangements.</Description>
                        <RadioBox orientation="horizontal"/>
                        <Data type="enum">
                            <EnumElem name="Isolated" value="0"/>
                            <EnumElem name="Regular" value="1"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Warn on Mass Ratio of Attachment and Base">
                    <Pref name="MD_RatioLimit" type="real" title="Maximum Limit" value="50.0" ver="1872" scope="Session">
                        <Description>This option specifies the maximum mass ratio limit of attachment and base in joint. A warning message will display when the mass ratio (the larger value of mass divided by the smaller one) is larger than this limit.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="1.0" include="1"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Inverse Kinematics">
                    <Pref name="MD_Inverse_Kinematics_Configuration_File_WIN" type="string" title="Robot Configuration File" value="$UGII_BASE_DIR\mechatronics\configuration\Mechatronics_IK_Robots.xlsx" ver="2312" scope="Part">
                        <Description>This field specifies the full file name of the robot configuration file.</Description>
                        <BrowseFileSel type="xlsx" />
                    </Pref>
                    <Pref name="MD_Inverse_Kinematics_Configuration_File_UNX" type="string" title="Robot Configuration File" value="$UGII_BASE_DIR/mechatronics/configuration/Mechatronics_IK_Robots.xlsx" ver="2312" scope="Part">
                        <Description>This field specifies the full file name of the robot configuration file.</Description>
                        <BrowseFileSel type="xlsx"/>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Gravity and Material" csh="PMAN_Mechatronics_Designer_General_Gravity_and_Material">
                <Group name="Acceleration due to Gravity">
                    <Pref name="MD_DefaultGravityX_MU" type="real" title="Gx" value="0.0" unit="mm/sec^2" ver="7.5.1" scope="Part">
                        <Description>This option specifies the X-component (in absolute coordinates) of the gravitation.</Description>
                        <Real/>
                    </Pref>
                    <Pref name="MD_DefaultGravityX_EU" type="real" title="Gx" value="0.0" unit="in/sec^2" ver="7.5.1" scope="Part">
                        <Description>This option specifies the X-component (in absolute coordinates) of the gravitation.</Description>
                        <Real/>
                    </Pref>
                    <Pref name="MD_DefaultGravityY_MU" type="real" title="Gy" value="0.0" unit="mm/sec^2" ver="7.5.1" scope="Part">
                        <Description>This option specifies the Y-component (in absolute coordinates) of the gravitation.</Description>
                        <Real/>
                    </Pref>
                    <Pref name="MD_DefaultGravityY_EU" type="real" title="Gy" value="0.0" unit="in/sec^2" ver="7.5.1" scope="Part">
                        <Description>This option specifies the Y-component (in absolute coordinates) of the gravitation.</Description>
                        <Real/>
                    </Pref>
                    <Pref name="MD_DefaultGravityZ_MU" type="real" title="Gz" value="-9806.65" unit="mm/sec^2" ver="7.5.1" scope="Part">
                        <Description>This option specifies the Z-component (in absolute coordinates) of the gravitation.</Description>
                        <Real/>
                    </Pref>
                    <Pref name="MD_DefaultGravityZ_EU" type="real" title="Gz" value="-386.08" unit="in/sec^2" ver="7.5.1" scope="Part">
                        <Description>This option specifies the Z-component (in absolute coordinates) of the gravitation.</Description>
                        <Real/>
                    </Pref>
                </Group>
                <Group name="Material Parameters">
                    <Pref name="MD_DynamicsFriction" type="real" title="Dynamic Friction" value="0.7" ver="7.5.1" scope="Part">
                        <Description>This option specifies the coefficient of the dynamic friction of the material. Dynamic friction is the force resisting the motion when a body is moving on a surface.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                    <!---->
                    <Pref name="MD_RollingFriction" type="real" title="Rolling Friction" value="0.0" ver="9.0.0" scope="Part">
                        <Description>This option specifies the coefficient of the rolling friction of the material. Rolling friction is the force resisting the motion when a body is rolling on a surface.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="MD_Restitution" type="real" title="Restitution" value="0.01" ver="7.5.1" scope="Part">
                        <Description>This option specifies the restitution of the material. Restitution is a measure of how much energy is dissipated by a collision impulse. The value is in the range of 0 to 1, where 0 is a fully plastic collision and 1 is a perfectly elastic collision. The restitution is multiplied by the object's velocity to determine how fast the object will move in the opposite direction.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                            <UpperBound value="1.0" include="1"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Damping">
                    <Pref name="MD_LinearDamping_MU" type="real" title="Linear Damping" value="0.05" unit="mN-sec/mm" ver="9.0.0" scope="Part">
                        <Description>This option specifies the linear damping of a rigid body.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="MD_LinearDamping_EU" type="real" title="Linear Damping" value="0.0002855" unit="lbf-sec/in" ver="9.0.0" scope="Part">
                        <Description>This option specifies the linear damping of a rigid body.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="MD_AngularDamping_MU" type="real" title="Angular Damping" value="199999.9997715" unit="mN-mm-sec/rad" ver="1872" scope="Part">
                        <Description>This option specifies the angular damping of a rigid body.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="MD_AngularDamping_EU" type="real" title="Angular Damping" value="1.77015311" unit="lbf-in-sec/rad" ver="1872" scope="Part">
                        <Description>This option specifies the angular damping of a rigid body.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Flicker Sound File">
                    <Pref name="MD_FlickerSoundPath_WIN" type="string" title="Location" value="$UGII_BASE_DIR\mechatronics\configuration\audio" ver="2312" scope="Session">
                         <Description>This field specifies the location of the flicker sound file. </Description>
                         <BrowseDirSel/>
                    </Pref>
                    <Pref name="MD_FlickerSoundPath_UNX" type="string" title="Location" value="$UGII_BASE_DIR/mechatronics/configuration/audio" ver="2312" scope="Session">
                        <Description>This field specifies the location of the flicker sound file.</Description>
                        <BrowseDirSel/>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Physics Engine" csh="PMAN_Mechatronics_Designer_General_Physics_Engine">
                <Pref name="MD_PhysicsEngineType" type="int" title="Engine Type" value="0" ver="2312" scope="Session">
                    <Description>This option specifies which physics engine to use in the simulation.</Description>
                    <RadioBox orientation="horizontal"/>
                  <Data type="enum">
                    <EnumElem name="Bullet" value="0"/>
                    <EnumElem name="PhysX" value="1"/>
                        <EnumElem name="Kineo" value="2"/>
                  </Data>
                </Pref>
                <Group name="Runtime Parameters">
                    <Pref name="MD_CollisionPrecision_MU" type="real" title="Collision Precision" value="0.1" unit="mm" ver="7.5.1" scope="Part">
                        <Description>This option specifies distance between objects that are considered to be colliding. It represents the maximum distance (the gap) between bodies to be in contact.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MD_CollisionPrecision_EU" type="real" title="Collision Precision" value="0.0039" unit="in" ver="7.5.1" scope="Part">
                        <Description>This option specifies distance between objects that are considered to be colliding. It represents the maximum distance (the gap) between bodies to be in contact.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MD_StepTime" type="real" title="Step Time" value="1" unit="ms" ver="7.5.1" scope="Part">
                        <Description>This option specifies the amount of time that is simulated in a single compute step.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.1" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="MD_SpringForceMultiplier" type="real" title="Spring Force Multiplier" value="100" ver="7.5.1" scope="Part">
                        <Description>This option specifies spring force applied when dragging objects in simulation mode.</Description>
                        <Real/>
                    </Pref>
                    <Pref name="MD_CollisionCategory_UNX" type="string" title="Collide Category Specification" value="" ver="7.5.1" scope="Session">
                        <Description>This option specifies which categories should not collide.</Description>
                        <BrowseFileSel type="csv"/>
                    </Pref>
                    <Pref name="MD_CollisionCategory_WIN" type="string" title="Collide Category Specification" value="" ver="7.5.1" scope="Session">
                        <Description>This option specifies which categories should not collide.</Description>
                        <BrowseFileSel type="xls,xlsx,csv"/>
                    </Pref>
                </Group>
                <Group name="Engine Tuning">
                    <Pref name="MD_PhysicsEngineTolerance_MU" type="real" title="Tolerance" value="0.1" unit="mm" ver="7.5.1" scope="Part">
                        <Description>This option specifies the amount of movement that the dynamics solver will allow between joints and constraints.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MD_PhysicsEngineTolerance_EU" type="real" title="Tolerance" value="0.0039" unit="in" ver="7.5.1" scope="Part">
                        <Description>This option specifies the amount of movement that the dynamics solver will allow between joints and constraints.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MD_PhysicsEngineErrorReduction" type="real" title="Error Reduction Factor" value="50.0" ver="7.5.1" scope="Part">
                        <Description>This option specifies how fast the solver converges a constraint and resolves a collision. The larger the value, the relatively bigger impulse will be applied by the solver in each iteration to overcome the position error. This makes it faster to resolve collision interferences and consolidate joints. Note however, the larger this value is, the greater is the risk for instability in the system.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MD_PhysicsEngineMaxIter" type="int" title="Maximum Iterations" value="100" ver="7.5.1" scope="Part">
                        <Description>This option specifies the maximum number of iterations the physics engine will perform in a given time step in the attempt to solve the system.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="1" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="MD_StickyForce_MU" type="real" title="Sticky Force" value="100000" unit="mN" ver="1926" scope="Part">
                        <Description>This option specifies the value of the sticky force in the simulation.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MD_StickyForce_EU" type="real" title="Sticky Force" value="22.4809" unit="lbf" ver="1926" scope="Part">
                        <Description>This option specifies the value of the sticky force in the simulation.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="MD_EnableParallel" type="logical" title="Use Parallel Calculation" value="false" ver="1899" scope="Part">
                        <Description>This option determines whether to use parallel calculation.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="MD_RandomizeSolvingOrder" type="logical" title="Randomize Solving Order" value="false" ver="1926" scope="Part">
                        <Description>This option determines if the Bullet solver will use random order for constraints.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="PhysX Engine Performance">
                    <Pref name="MD_EnablePhysXCcd" type="logical" title="Use Continuous Collision Detection" value="false" ver="2306" scope="Session">
                        <Description>This option determines whether to use continuous collision detection.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="MD_EnablePhysXGpuAcceleration" type="logical" title="Use GPU Acceleration" value="false" ver="2306" scope="Session">
                        <Description>This option determines whether to use CUDA acceleration. Not all features have GPU-accelerated implementations.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="MD_PhysXMultiThreadsNum" type="int" title="Number of Worker Threads" value="4" ver="2306" scope="Session">
                        <Description>This option specifies how many worker threads the CPU dispatcher will have.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="0" include="1"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Inspector" csh="PMAN_Mechatronics_Designer_General_Inspector">
                <Group name="Step Size">
                    <Pref name="MD_StepSize" type="int" title="Sample Every n-th Step" value="30" ver="1872" scope="Session">
                        <Description>This option specifies the sampling interval during the simulation.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="1" include="1"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Graph">
                    <Pref name="MD_GraphZoomInScale" type="real" title="Maximum Zoom In Factor" value="100" ver="2206" scope="Session">
                        <Description>This option specifies the maximum zoom in scale used for the graph in the Runtime Inspector navigator.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="1" include="1"/>
                            <UpperBound value="100" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="MD_GraphZoomOutScale" type="real" title="Minimum Zoom Out Factor" value="0.1" ver="2206" scope="Session">
                        <Description>This option specifies the minimum zoom out scale used for the graph in the Runtime Inspector navigator.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.1" include="1"/>
                            <UpperBound value="0.9" include="1"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Settings">
                    <Pref name="MD_InsDecimalPlaces" type="int" title="Number of Decimal Places" value="6" ver="2206" scope="Session">
                        <Description>This option specifies the number of decimal places for values displayed in the Runtime Inspector navigator.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="1" include="1"/>
                            <UpperBound value="10" include="1"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Runtime" csh="PMAN_Mechatronics_Designer_General_Runtime">
                <Pref name="MD_DefaultSimulateActiveDisplayedPartOnly" type="int" title="Parts to Simulate" value="0" ver="12.0.2" scope="Session">
                    <Description>This option specifies if the simulation should be performed in the active displayed part only or in all relevant displayed parts.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="Active Displayed Part" value="0"/>
                        <EnumElem name="All Displayed Parts" value="1"/>
                    </Data>
                </Pref>
                <Pref name="MD_RuntimeOperationCaptureTime" type="real" title="Operation Capture Time" value="60" unit="sec" ver="8.0.0" scope="Session">
                    <Description>This option specifies the time window within which the Sequence Editor displays the operations during simulation. Any operation data outside this time window is deleted.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="5" include="1"/>
                        <UpperBound value="600" include="1"/>
                    </Data>
                </Pref>
                <Pref name="MD_ForwardStepTime" type="real" title="One Step Forward Time" value="100" unit="ms" ver="10.0.0" scope="Session">
                    <Description>This option specifies the amount of time that is simulated in a forward step.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                    </Data>
                </Pref>
                <Group name="Time Scaling Factor">
                    <Pref name="MD_MinimumTimeScaleFactor" type="real" title="Minimum" value="0.1" ver="10.0.0" scope="Session">
                        <Description>The time scaling factor is used to scale the simulation step time. This option specifies the minimum value allowed for this factor.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.01" include="1"/>
                            <UpperBound value="1.0" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="MD_MaximumTimeScaleFactor" type="real" title="Maximum" value="10.0" ver="10.0.0" scope="Session">
                        <Description>The time scaling factor is used to scale the simulation step time. This option specifies the maximum value allowed for this factor.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="1.0" include="1"/>
                            <UpperBound value="100.0" include="1"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Runtime Warning and Error">
                    <Pref name="MD_Ignore_Runtime_Warning" type="logical" title="Ignore Runtime Warning" value="false" ver="1872" scope="Session">
                        <Description>This option specifies whether to ignore a runtime warning.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="MD_Pause_Runtime_Error" type="logical" title="Pause at Runtime Error" value="false" ver="1872" scope="Session">
                        <Description>This option specifies whether to pause when a runtime error occurs.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Monitor">
                    <Pref name="MD_GraphSampleTime" type="real" title="Graph Sample Time" value="40" unit="ms" ver="2206" scope="Session">
                        <Description>This option specifies the graph sample time used to monitor a measurement during the simulation.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="1" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="MD_BodyToUseForMeasureType" type="int" title="Body Type" value="0" ver="2206" scope="Session">
                        <Description>This option specifies the body type used to measure distance or angle. The facet body type gives better performance.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="Facet Body" value="0"/>
                            <EnumElem name="Solid Body" value="1"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Simulation Recording">
                    <Pref name="MD_RecordingSamplingRate" type="int" title="Sampling Rate" value="5" ver="12.0.2" scope="Session">
                        <Description>This option specifies how many simulation steps one frame will record.</Description>
                        <IntegerScale/>
                        <Data type="range">
                            <LowerBound value="1" include="1"/>
                            <UpperBound value="10" include="1"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Performance">
                    <Pref name="MD_SimplyGeomForPerformance" type="logical" title="Simplify Geometries for Better Display Performance" value="false" ver="1926" scope="Session">
                        <Description>This option specifies whether to simplify the physics geometry to achieve better display performance during the simulation.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="External Data for Position Control">
                    <Pref name="MD_ExternalAlgorithmForPositionControl" type="int" title="Speed Calculation" value="0" ver="1926" scope="Session">
                        <Description>This option specifies if the external data algorithm is calculated from external and current position or two external positions.</Description>
                        <RadioBox/>
                        <Data type="enum">
                            <EnumElem name="External and Current Position" value="0"/>
                            <EnumElem name="Two External Positions" value="1"/>
                        </Data>
                    </Pref>
                </Group>
              <Group name="Unpack FMU File">
                <Pref name="MD_SpecifyUnpackedFMUControl" type="int" title="Location Method" value="0" ver="2212" scope="Session">
                  <Description>This option determines whether to specify an unpacked FMU location, or to use the default location.</Description>
                  <RadioBox orientation="horizontal"/>
                  <Data type="enum">
                    <EnumElem name="Use Default" value="0"/>
                    <EnumElem name="Specify" value="1"/>
                  </Data>
                </Pref>
                <Pref name="MD_FMUUnpackedPath_WIN" type="string" title="Location" value="$UGII_TMP_DIR" ver="2212" scope="Session">
                  <Description>This field specifies the location of the unpacked FMU. This option applies if the "Location Method" option is set to "Specify".</Description>
                  <BrowseDirSel/>
                </Pref>
                <Pref name="MD_FMUUnpackedPath_UNX" type="string" title="Location" value="$UGII_TMP_DIR" ver="2212" scope="Session">
                  <Description>This field specifies the location of the unpacked FMU. This option applies if the "Location Method" option is set to "Specify".</Description>
                  <BrowseDirSel/>
                </Pref>
                <Pref name="MD_RemoveFMUUnpackedFilesControl" type="logical" title="Remove Unpacked FMU Files" value="true" ver="2212" scope="Session">
                  <Description>This option determines whether or not to remove files unpacked from the FMU when the simulation finishes.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="Yes" value="true"/>
                    <EnumElem name="No" value="false"/>
                  </Data>
                </Pref>
              </Group>
              <Group name="EtherNetIP EDS Repository">
                <Pref name="MD_SpecifyEtherNetIPEdsControl" type="int" title="Location Method" value="0" ver="2306" scope="Session">
                  <Description>This option determines whether to specify an EtherNetIP EDS repository location, or to use the default location.</Description>
                  <RadioBox orientation="horizontal"/>
                  <Data type="enum">
                    <EnumElem name="Use Default" value="0"/>
                    <EnumElem name="Specify" value="1"/>
                  </Data>
                </Pref>
                <Pref name="MD_EtherNetIPEds_WIN" type="string" title="Location" value="$UGII_BASE_DIR\mechatronics\RuntimeInterface\ethernetip" ver="2306" scope="Session">
                  <Description>This field specifies the location of the EtherNetIP EDS repository. This option applies if the "Location Method" option is set to "Specify".</Description>
                  <BrowseDirSel/>
                </Pref>
                <Pref name="MD_EtherNetIPEds_UNX" type="string" title="Location" value="$UGII_BASE_DIR/mechatronics/RuntimeInterface/ethernetip" ver="2306" scope="Session">
                  <Description>This field specifies the location of the EtherNetIP EDS repository. This option applies if the "Location Method" option is set to "Specify".</Description>
                  <BrowseDirSel/>
                </Pref>
              </Group>
            </Tab>
        </Category>
        <Category name="Integration Settings" csh="PMAN_Mechatronics_Designer_Integration_Settings">
            <Tab name="SIZER" csh="PMAN_Mechatronics_Designer_Integration_Settings_SIZER">
                <Pref name="MD_SIZER_Integration_CAD_Creator_Run_Method" type="int" title="CAD Creator Run Method" value="0" ver="9.0.0" scope="Session">
                    <Description>This option specifies the run method of CAD Creator software when the motor is imported from SIZER.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="Web Service" value="0"/>
                        <EnumElem name="Local Installation" value="1"/>
                    </Data>
                </Pref>
            </Tab>
