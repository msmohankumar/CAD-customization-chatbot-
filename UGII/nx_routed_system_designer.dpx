<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="docum.xsl"?>
<!--

-->
<PrefDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www-cyp.ugsolutions.com/assemblies/pman">
    <Application name="Routed System Designer" csh="PMAN_RoutedSystemDesigner" package_suppress="simcenter3d,simviewer,view,nx2dedit">
        <Category name="General" csh="PMAN_RoutedSystemDesigner_General">
          <Tab name="General" csh="PMAN_RoutedSystemDesigner_General_General">
            <Pref name="RSD_AutomaticallyLoadRoutingData" type="logical" title="Automatically Load Routing Data" value="false" ver="2312" scope="Session">
              <Description>This option determines whether routing data should be loaded when opening an assembly in a Routed System Designer application.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="Yes" value="true"/>
                <EnumElem name="No" value="false"/>
              </Data>
            </Pref>
          </Tab>
            <Tab name="Paths" csh="PMAN_RoutedSystemDesigner_General_Paths">
                <Group name="Splines">
                    <Pref name="RSD_UseMinimumRadiusAttribute" type="logical" title="Use Minimum Radius Attribute" value="false" ver="1953" scope="Session">
                        <Description>This option determines whether or not the value of the Minimum Curve Check attribute on each path segment should be used when checking the minimum radius.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_RadiusAttributeMethod" type="int" title="Radius Attribute Method" value="1" ver="1953" scope="Session">
                        <Description>This option specifies the radius attribute method.
                            <Formatted>
Radius: The value entered for the Minimum Checking Radius is the final minimum radius value.
Ratio to Diameter: A ratio to be multiplied by the diameter of the stock or space reservation to generate the minimum radius value.
                            </Formatted>
                        </Description>
                        <RadioBox orientation="vertical"/>
                        <Data type="enum">
                            <EnumElem name="Radius" value="1"/>
                            <EnumElem name="Ratio to Diameter" value="2"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_DefaultPortExtensionLength_MU" type="real" title="Port Extension Length" value="1" unit="mm" ver="1953" scope="Session">
                        <Description>This option specifies the spline extension magnitude for ports that do not have an extension defined.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_DefaultPortExtensionLength_EU" type="real" title="Port Extension Length" value="0.04" unit="in" ver="1953" scope="Session">
                        <Description>This option specifies the spline extension magnitude for ports that do not have an extension defined.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_ConstrainedCurvatureFailureColor" type="int" title="Color when Failing Curvature Constraint" value="186" ver="1953" scope="Session">
                        <Description>This option specifies the color of a constrained curvature spline when it fails to meet the specified radius.</Description>
                        <Color/>
                    </Pref>
                </Group>
                <Pref name="RSD_StockOffsetExpression" type="string" title="Stock Offset Expression" value="(MAX_PATH_STOCK_DIA/2.0)" ver="1953" scope="Session">
                    <Description>This option specifies the expression used to offset a control point due to stock on the path.</Description>
                    <Widestring/>
                </Pref>
                <Group name="Jump Path">
                    <Pref name="RSD_JumpPathApplyStyle" type="logical" title="Apply Style" value="true" ver="2312" scope="Session">
                        <Description>This option determines whether or not to show cableway jump paths in the specified color, style, and width.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_JumpPathColor" type="int" title="Color" value="181" ver="2312" scope="Session">
                        <Description>This option specifies the color of cableway jump paths.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="RSD_JumpPathStyle" type="int" title="Style" value="2" ver="2312" scope="Session">
                        <Description>This option specifies the line style for cableway jump paths.</Description>
                        <GeneralLineFont LineFontOptionMaskString="GeneralLineFonts" LineFontDefaultValue="2"/>
                    </Pref>
                    <Pref name="RSD_JumpPathWidth" type="int" title="Width" value="10" ver="2312" scope="Session">
                        <Description>This option specifies the line width for cableway jump paths.</Description>
                        <ObjectLineWidth allowWideLineWidth="true"/>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Stock" csh="PMAN_RoutedSystemDesigner_General_Stock">
                <Group name="New Object Translucency">
                    <Label label="The following options are used to define the translucency of newly created routing objects."/>
                    <Pref name="RSD_StockTranslucency" type="int" title="Stock" value="0" unit="%" ver="1953" scope="Session">
                        <Description>This option specifies the translucency of general routing stock.</Description>
                        <IntegerScale/>
                        <Data type="range">
                            <LowerBound value="0" include="1"/>
                            <UpperBound value="100" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_InsulationTranslucency" type="int" title="Insulation" value="50" unit="%" ver="1953" scope="Session">
                        <Description>This option specifies the translucency of routing insulation.</Description>
                        <IntegerScale/>
                        <Data type="range">
                            <LowerBound value="0" include="1"/>
                            <UpperBound value="100" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_SpaceReservationTranslucency" type="int" title="Space Reservation" value="50" unit="%" ver="1953" scope="Session">
                        <Description>This option specifies the translucency of routing space reservation.</Description>
                        <IntegerScale/>
                        <Data type="range">
                            <LowerBound value="0" include="1"/>
                            <UpperBound value="100" include="1"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Existing Object Translucency">
                    <Pref name="RSD_ExistingObjectTranslucency" type="logical" title="Apply Translucency to Work Part Components" value="false" ver="2312" scope="Session">
                        <Description>This option determines whether or not to apply temporary translucency to routing components in the work part.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                 </Group>
                <Pref name="RSD_SpaceReservationSetBodyDensityToZero" type="logical" title="Set Space Reservation Body Density to Zero" value="true" ver="1953" scope="Session">
                    <Description>This option determines whether or not to set the body density to zero for space reservation.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Group name="Native NX">
                    <Pref name="RSD_StockComponentFolder_WIN" type="string" title="Stock Component Folder" value="" ver="1953" scope="Session">
                        <Description>This option specifies the location of newly created temporary stock components. This may be a directory that does not exist, as the directory will then be created. If the directory cannot be created, the work directory is used. The directory specified may be relative or absolute.</Description>
                        <BrowseDirSel/>
                    </Pref>
                    <Pref name="RSD_StockComponentFolder_UNX" type="string" title="Stock Component Folder" value="" ver="1953" scope="Session">
                        <Description>This option specifies the location of newly created temporary stock components. This may be a directory that does not exist, as the directory will then be created. If the directory cannot be created, the work directory is used. The directory specified may be relative or absolute.</Description>
                        <BrowseDirSel/>
                    </Pref>
                </Group>
                <Group name="Teamcenter Integration">
                    <Pref name="RSD_StockComponentFolder_TC" type="string" title="Stock Component Folder" value="" ver="1953" scope="Session">
                        <Description>This option specifies the location of newly created temporary stock components. This should be an existing folder. If the folder does not exist, the default folder is used.</Description>
                        <Widestring/>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Parts" csh="PMAN_RoutedSystemDesigner_General_Parts">
                <Pref name="RSD_PartsLayerOption" type="int" title="Layer" value="1" ver="1953" scope="Session">
                    <Description>This option specifies the layer option used when adding routing components to an assembly.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="Original" value="1"/>
                        <EnumElem name="Work" value="2"/>
                        <EnumElem name="As Specified" value="3"/>
                    </Data>
                </Pref>
                <Pref name="RSD_PartsUsePrefPortPlacement" type="logical" title="Use Preferred Port Placement" value="false" ver="1953" scope="Session">
                    <Description>If a port is found with a user-specified attribute or name value during Place Part, this option determines whether or not the most favorable solution derived using this port is suggested as the first solution for placing the routing part.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                    <Docum>Having this option on takes extra processing, so is probably best used only if there are many placement possibilities for the parts.</Docum>
                </Pref>
                <Pref name="RSD_PrefPortPlacementMethod" type="int" title="Placement Method" value="0" ver="1953" scope="Session">
                    <Description>This option specifies the method used for the preferred port placement.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="Attribute" value="0"/>
                        <EnumElem name="Name" value="1"/>
                    </Data>
                </Pref>
                <Pref name="RSD_PrefPortPlacementAttributeName" type="string" title="Attribute Name" value="" ver="1953" scope="Session">
                    <Description>When Method is set to Attribute, this option defines the name of the port attribute.</Description>
                    <String/>
                </Pref>
                <Pref name="RSD_PrefPortPlacementAttributeValue" type="string" title="Attribute Value" value="" ver="1953" scope="Session">
                    <Description>When Method is set to Attribute, this option defines the value of the port attribute.</Description>
                    <String/>
                </Pref>
                <Pref name="RSD_PrefPortPlacementName" type="string" title="Port Name" value="" ver="1953" scope="Session">
                    <Description>When Method is set to Name, this option defines the name of the port.</Description>
                    <String/>
                </Pref>
            </Tab>
            <Tab name="Display" csh="PMAN_RoutedSystemDesigner_General_Display">
                <Group name="Port">
                    <Pref name="RSD_DisplayConnectedPorts" type="logical" title="Display Connected Ports" value="false" ver="1953" scope="Session">
                        <Description>This option determines whether or not connected ports are displayed.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_DisplayPortLength_MU" type="real" title="Port Length" value="25.4" unit="mm" ver="1953" scope="Session">
                        <Description>This option specifies the length of ports to display.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_DisplayPortLength_EU" type="real" title="Port Length" value="1.0" unit="in" ver="1953" scope="Session">
                        <Description>This option specifies the length of ports to display.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Design Rule Display">
                    <Pref name="RSD_ViolationObjectsColor" type="int" title="Violation Objects Color" value="-1" ver="1953" scope="Session">
                        <Description>This option specifies the color of violated objects.</Description>
                        <ColorOption/>
                        <Data type="color">
                            <EnumElem name="Default" value="-1"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
        </Category>
        <Category name="Part Library" csh="PMAN_RoutedSystemDesigner_Part_Library">
            <Tab name="Reuse Library" csh="PMAN_RoutedSystemDesigner_Part_Library_Reuse_Library">
                <Group name="Library Configuration">
                    <Pref name="RSD_PartLibraryConfiguration" type="int" title="Part Library Configuration" value="0" ver="1953" scope="Session">
                        <Description>This options specifies if the part library is driven by Teamcenter Classification or PTB files (Native or Teamcenter).</Description>
                        <RadioBox orientation="vertical"/>
                        <Data type="enum">
                            <EnumElem name="Native/Teamcenter PTB" value="0"/>
                            <EnumElem name="Teamcenter Classification" value="1"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_PartLibraryClassificationRoot" type="string" title="Classification Root" value="routing_parts" ver="1953" scope="Session">
                        <Description>This options is valid for Teamcenter Classification mode only. It specifies which node in the classification hierarchy is the root of the routing part library.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="RSD_PartLibraryStartingNodeType" type="int" title="Search" value="1" ver="1953" scope="Session">
                        <Description>This option determines whether to search the Routing Part Library only from the current node or from the root node.</Description>
                        <RadioBox/>
                        <Data type="enum">
                            <EnumElem name="From Current Node" value="0"/>
                            <EnumElem name="From Root Node" value="1"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_EnableSearchingTeamcenterClassificationDirectly" type="logical" title="Provide Search of Teamcenter Classification Directly" value="false" ver="1953" scope="Session">
                        <Description>This option determines whether or not a Search button should be provided to allow searching of attribute values directly from the Teamcenter Classification database. For large Teamcenter Classification libraries, this is faster than querying for and loading all the underlying members. This option applies only when using the Routing Part Library with Teamcenter Classification.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_ClassificationObjectThresholdForSearchingTeamcenterDirectly" type="int" title="Threshold Value to Search Teamcenter Classification Directly" value="200" ver="1953" scope="Session">
                        <Description>When the 'Provide Search of Teamcenter Classification Directly' option is on, this option specifies the number of classification objects that have to be classified under a specific node or its child nodes in the Routing Part Library for a Search button to be provided on the Search Children dialog. The Search button allows a search directly of Teamcenter Classification without having to load all the underlying data for the node or its child nodes. This option applies only when using the Routing Part Library with Teamcenter Classification.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="0" include="1"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Discipline">
                    <Docum>The discipline specified here is used as the current discipline on Routing application load. If the discipline specified here is not present in the Application View XML files, then the current discipline is set to "none".</Docum>
                    <Label label="The following options are used to define the initial discipline for the Routing applications. Disciplines are defined in the Application View XML files."/>
                    <Pref name="RSD_PartLibraryDefaultElecDiscipline" type="string" title="Routing Electrical" value="Wiring" ver="1953" scope="Session">
                        <Description>This option specifies the initial discipline for the Routing Electrical application.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="RSD_PartLibraryDefaultMechDiscipline" type="string" title="Routing Mechanical" value="Piping" ver="1953" scope="Session">
                        <Description>This option specifies the initial discipline for the Routing Mechanical application.</Description>
                        <Widestring/>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Specification Folders" csh="PMAN_RoutedSystemDesigner_Part_Library_Specifications_Folders">
                <Label label="The following options are used to define the native folder path for the Routing applications."/>
                <Pref name="RSD_ElectricalSpecificationsFolder_WIN" type="string" title="Routing Electrical" value="$UGII_ROUTE_ELEC_KIT_DIR\specifications\$UGII_ROUTING_KIT_UNITS\" ver="1953" scope="Session">
                    <Description>This option specifies the folder path for specification XML files in Routing Electrical.</Description>
                    <BrowseDirSel/>
                </Pref>
                <Pref name="RSD_ElectricalSpecificationsFolder_UNX" type="string" title="Routing Electrical" value="$UGII_ROUTE_ELEC_KIT_DIR/specifications/$UGII_ROUTING_KIT_UNITS/" ver="1953" scope="Session">
                    <Description>This option specifies the folder path for specification XML files in Routing Electrical.</Description>
                    <BrowseDirSel/>
                </Pref>
                <Pref name="RSD_MechanicalSpecificationsFolder_WIN" type="string" title="Routing Mechanical" value="$UGII_ROUTE_MECH_KIT_DIR\specifications\$UGII_ROUTING_KIT_UNITS\" ver="1953" scope="Session">
                    <Description>This option specifies the folder path for specification XML files in Routing Mechanical.</Description>
                    <BrowseDirSel/>
                </Pref>
                <Pref name="RSD_MechanicalSpecificationsFolder_UNX" type="string" title="Routing Mechanical" value="$UGII_ROUTE_MECH_KIT_DIR/specifications/$UGII_ROUTING_KIT_UNITS/" ver="1953" scope="Session">
                    <Description>This option specifies the folder path for specification XML files in Routing Mechanical.</Description>
                    <BrowseDirSel/>
                </Pref>
                <Pref name="RSD_SpecificationNativeOption" type="int" title="Folders when Connected to Teamcenter" value="1" ver="1953" scope="Session">
                    <Description>This option applies only when connected to Teamcenter and determines if specification files are loaded and saved exclusively from/to native folders or from/to Teamcenter. The Teamcenter specification folders are set in the Teamcenter preferences.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="Native" value="0"/>
                        <EnumElem name="Teamcenter" value="1"/>
                    </Data>
                </Pref>
            </Tab>
        </Category>
        <Category name="Electrical" csh="PMAN_RoutedSystemDesigner_Electrical">
            <Tab name="General" csh="PMAN_RoutedSystemDesigner_Electrical_General">
                <Pref name="RSD_ElectricalBundleColor" type="int" title="Bundle Color" value="134" ver="1953" scope="Session">
                    <Description>This option specifies the color of routed bundles.</Description>
                    <Color/>
                </Pref>
                <Pref name="RSD_ElectricalDeleteSRStockOnRoute" type="logical" title="Delete Space Reservation Stock on Auto or Manual Route" value="false" ver="1953" scope="Session">
                    <Description>This option determines whether or not to delete Space Reservation stock upon Auto or Manual route. When this option is off, Space Reservation stock remains over the segments along with other stocks but not as a part of the bundle stock.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="RSD_ElectricalUseParentCableStock" type="logical" title="Use Cable Stock when Wire Stock is Missing" value="false" ver="2206" scope="Session">
                    <Description>This option determines whether or not to use the stock of the parent cable if the stock of the child wire cannot be found.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="RSD_ElectricalMinimumInsulationWidth_MU" type="real" title="Minimum Insulation Width" value="2.5" unit="mm" ver="1953" scope="Session">
                    <Description>This option specifies the minimum insulation length for insulation that does not have a specified width.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="RSD_ElectricalMinimumInsulationWidth_EU" type="real" title="Minimum Insulation Width" value="0.1" unit="in" ver="1953" scope="Session">
                    <Description>This option specifies the minimum insulation length for insulation that does not have a specified width.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Group name="Wire Cut Length Calculation">
                    <Pref name="RSD_ElectricalCutLengthMultiplier" type="real" title="Cut Length Multiplier" value="10.0" unit="%" ver="1953" scope="Session">
                        <Description>This option specifies how much a wire's length should be increased when calculating the cut length. The cut length is calculated by starting with the modeled length and adding the modeled length multiplied by the Cut Length Multiplier and then adding the Cut Length Offset.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_ElectricalCutLengthOffset_MU" type="real" title="Cut Length Offset" value="5.0" unit="mm" ver="1953" scope="Session">
                        <Description>This option specifies the additional length to add to a wire's length when calculating the cut length. The cut length is calculated by multiplying the wire's modeled length by the Cut Length Multiplier and then adding the Cut Length Offset.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_ElectricalCutLengthOffset_EU" type="real" title="Cut Length Offset" value="0.25" unit="in" ver="1953" scope="Session">
                        <Description>This option specifies the additional length to add to a wire's length when calculating the cut length. The cut length is calculated by multiplying the wire's modeled length by the Cut Length Multiplier and then adding the Cut Length Offset.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                </Group>
                <Pref name="RSD_ElectricalDefaultJumperLength_MU" type="real" title="Jumper Wire Length" value="25.0" unit="mm" ver="1953" scope="Session">
                      <Description>Automatic Route and Manual Route set the length of jumper wires to this value when the calculated 3D length of the wire is 0 (zero).</Description>
                       <Real/>
                       <Data type="range">
                       <LowerBound value="0.0" include="1"/>
                       </Data>
                </Pref>
                <Pref name="RSD_ElectricalDefaultJumperLength_EU" type="real" title="Jumper Wire Length" value="1.0" unit="in" ver="1953" scope="Session">
                    <Description>DAutomatic Route and Manual Route set the length of jumper wires to this value when the calculated 3D length of the wire is 0 (zero).</Description>
                    <Real/>
                    <Data type="range">
                    <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Navigators" csh="PMAN_RoutedSystemDesigner_Electrical_Navigators">
              <Pref name="RSD_ElectricalHarnessName" type="string" title="Harness Name" value="" ver="1953" scope="Session">
                <Description>This option specifies the harness name supplied to connections that have no specified harness.</Description>
                <Widestring/>
              </Pref>

              <Group name="Auto Assignment Method">
                  <Pref name="RSD_ElectricalComponentMatchMethod" type="list_sel_reorder_value" title="Match By" value="&quot;COMPONENT_NAME&quot;\&quot;ATTRIBUTE&quot;\&quot;FILE_NAME&quot;" ver="1980" scope="Session">
                      <Description>This option specifies the methods by which to automatically assign components in the Component List Navigator. The order in the list indicates the order in which they will be shown in the dialog box.</Description>
                      <ListSelReorder height="4" reorderable="true" selectable="false"/>
                      <Data type="enum">
                          <EnumElem name="Component Name" value="COMPONENT_NAME"/>
                          <EnumElem name="Attribute" value="ATTRIBUTE"/>
                          <EnumElem name="File Name" value="FILE_NAME"/>
                      </Data>
                  <Docum>
                    <Formatted>
Attribute: Compares the Component List Navigator cell value under the Column Heading to the value of the component or port attribute of the given title.
    The column name is the display name of the field in the display format.
    The comparison is case sensitive.

Component Name: Compares the Connector name, Component name, and Equipment name to the name of each component.
    The comparison is case sensitive.

File Name: Compares the part name of a Connector to the file name of a candidate part.
    The comparison is not case sensitive and does not compare the extension.
                    </Formatted>
                  </Docum>
                </Pref>
                <Pref name="RSD_ElectricalColumnToMatch" type="string" title="Component List Column Title" value="&lt;NONE&gt;" ver="1953" scope="Session">
                  <Description>This option specifies the Component List Navigator column name in which to look for a match to the Component Attribute value. This option is used when Match By is set to Attribute.</Description>
                  <Widestring/>
                </Pref>
                <Pref name="RSD_ElectricalNameToMatch" type="string" title="Component Attribute Title" value="&lt;NONE&gt;" ver="1953" scope="Session">
                  <Description>This option specifies the component attribute name for which to get the value for matching with the Component List Navigator's specified column's row value for attribute matching. This option is used when Match By is set to Attribute.</Description>
                  <Widestring/>
                </Pref>
              </Group>

              <Pref name="RSD_ElectricalHighlightWireStock" type="logical" title="Highlight Wire Stock" value="true" ver="1953" scope="Session">
                <Description>This option determines whether or not wire stock is highlighted when a Connection List Navigator row is selected.</Description>
                <Toggle off="false"/>
                <Data type="enum">
                  <EnumElem name="Yes" value="true"/>
                  <EnumElem name="No" value="false"/>
                </Data>
              </Pref>
              <Pref name="RSD_ElectricalHighlightWirePath" type="logical" title="Highlight Wire Path" value="false" ver="1953" scope="Session">
                <Description>This option determines whether or not wire paths are highlighted when a routed segment is selected.</Description>
                <Toggle off="false"/>
                <Data type="enum">
                  <EnumElem name="Yes" value="true"/>
                  <EnumElem name="No" value="false"/>
                </Data>
              </Pref>
              <Group name="Automatic Route">
                    <Pref name="RSD_ElectricalAutoRoute" type="int" title="Route Level" value="1" ver="1953" scope="Session">
                        <Description>This option specifies at what level to route. Component level routing does not require that the connection have pin information.</Description>
                        <RadioBox orientation="horizontal"/>
                        <Data type="enum">
                            <EnumElem name="Pin" value="1"/>
                            <EnumElem name="Component" value="2"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_ElectricalAutoRouteStockStyle" type="int" title="Stock Style" value="1" ver="1953" scope="Session">
                        <Description>This option specifies the stock style to use during automatic routing. Centerline makes automatic routing faster. Simple shows the wire bundles as sweeps.</Description>
                        <RadioBox orientation="horizontal"/>
                        <Data type="enum">
                            <EnumElem name="Centerline" value="0"/>
                            <EnumElem name="Simple" value="1"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_ElectricalAllowEditOfNavigatorData" type="logical" title="Allow Edit of Navigator Column Data" value="true" ver="1953" scope="Session">
                        <Description>This option determines whether or not to allow editing of the data in the Connection and Component List columns.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
