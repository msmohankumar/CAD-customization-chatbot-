                </Pref>
                <Group name="Standard Angles">
                    <Pref name="RSD_FormboardPrimaryAngle" type="real" title="Primary Angle" value="90.0" unit="deg" ver="2212" scope="Session">
                        <Description>This option specifies the primary standard angle.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                            <UpperBound value="180.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_FormboardSecondaryAngle" type="real" title="Secondary Angle" value="45.0" unit="deg" ver="2212" scope="Session">
                        <Description>This option specifies the secondary standard angle (applies only when all of the primary angle locations have been filled with branches).</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                            <UpperBound value="180.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_FormboardTertiaryAngle" type="real" title="Tertiary Angle" value="30.0" unit="deg" ver="2212" scope="Session">
                        <Description>This option specifies the tertiary standard angle (applies only when all of the primary and secondary angle locations have been filled with branches).</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                            <UpperBound value="180.0" include="0"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Random Angles">
                    <Pref name="RSD_FormboardMinRandomAngle" type="real" title="Minimum Angle" value="10.0" unit="deg" ver="2212" scope="Session">
                        <Description>This option specifies the minimum random branch angle.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                            <UpperBound value="180.0" include="0"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_FormboardMaxRandomAngle" type="real" title="Maximum Angle" value="170.0" unit="deg" ver="2212" scope="Session">
                        <Description>This option specifies the maximum random branch angle.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="0"/>
                            <UpperBound value="180.0" include="0"/>
                        </Data>
                    </Pref>
                </Group>
                <Pref name="RSD_FormboardBranchShapeType" type="int" title="Branch Shape" value="0" ver="2212" scope="Session">
                    <Description>This option specifies if branches should be straightened out or angled when flattening.</Description>
