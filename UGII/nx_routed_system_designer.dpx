                <Pref name="RSD_FormboardBranchAngleMethod" type="int" title="Branch Angles" value="0" ver="2212" scope="Session">
                    <Description>This option specifies the method used to calculate the angles of the flattened branches in the Formboard.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="As Designed"        value="0"/>
                        <EnumElem name="Standard Angles"    value="1"/>
