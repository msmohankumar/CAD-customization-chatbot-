                    <BrowseFileSel type="xml"/>
                </Pref>
                <Pref name="RSD_Cableway_Configuration_File_UNX" type="string" title="Configuration File" value="$UGII_ROUTE_MECH_KIT_DIR/appview/RSD_CablewayExport.xml" ver="1980" scope="Session">
                    <Description>This option specifies the name of the configuration file used by the Cableway Export command.</Description>
                    <BrowseFileSel type="xml"/>
                </Pref>
            </Tab>
        </Category>
        <Category name="Bill of Materials" csh="PMAN_RoutedSystemDesigner_BOM">
            <Tab name="General" csh="PMAN_RoutedSystemDesigner_BOM_General">
                <Pref name="RSD_BomDetailLevel" type="int" title="Detail Level" value="0" ver="1953" scope="Session">
                    <Description>This option specifies how fabrications are reported on a Routing Bill of Materials.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Summary" value="0"/>
                        <EnumElem name="Itemized" value="1"/>
                    </Data>
                    <Docum>
                        <Formatted>
Summary: The system will report only components and stock in the current work part.
Itemized: The existence of each routing object is reported individually, regardless of its position in the assembly hierarchy.
                        </Formatted>
                    </Docum>
                </Pref>
                <Pref name="RSD_BomReportCountOfStockPieces" type="logical" title="Report Count of Stock Pieces" value="true" ver="1953" scope="Session">
                    <Description>This option determines whether or not the Quantity column for rows containing stocks or insulations should report the number of pieces of stock for the "Summary" level of reporting.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="RSD_BomFabricationNumbering" type="int" title="Fabrication Numbering" value="0" ver="1953" scope="Session">
                    <Description>This option specifies the numbering of any fabrications in a Routing Bill of Materials.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Sequential" value="0"/>
                        <EnumElem name="Original" value="1"/>
                    </Data>
                    <Docum>
                        <Formatted>
Sequential: Sequential numbering is used as the callout in the Routing Bill of Materials.
Original: The user supplied fabrication numbering is used as the callout in the Routing Bill of Materials.
                        </Formatted>
                    </Docum>
                </Pref>
                <Pref name="RSD_BomIncludeComponents" type="logical" title="Include Components" value="true" ver="1953" scope="Session">
                    <Description>This option determines whether to include components underneath the Assembly.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="RSD_BomIncludeGaskets" type="logical" title="Include Gaskets, Nuts, and Bolts" value="false" ver="1953" scope="Session">
                    <Description>This option determines whether to include Routing UDOs created to store information about Bolts and Gaskets (under Piping Preferences of Routing Mechanical).</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Group name="Stock">
                    <Pref name="RSD_BomIncludeStock" type="logical" title="Include Stock" value="true" ver="1953" scope="Session">
                        <Description>This option determines whether to include mechanical stock such as conduit, filler stock, piping, raceway, etc. (anything assigned using the "Stock" dialog box).</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_BomIncludeInsulations" type="logical" title="Include Insulations" value="true" ver="1953" scope="Session">
                        <Description>This option determines whether to include anything assigned using the "Insulation" dialog box (except for insulations that are defined as "Wrapped" insulations).</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_BomIncludeWireStock" type="logical" title="Include Wire Stock" value="true" ver="1953" scope="Session">
                        <Description>This option determines whether to include wire stock from Routing Electrical that have a "PART_NUMBER" attribute.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="RSD_BomStockLength" type="int" title="Length Reporting" value="0" ver="1953" scope="Session">
                        <Description>This option specifies how stock length should be reported.</Description>
                        <RadioBox orientation="horizontal"/>
                        <Data type="enum">
                            <EnumElem name="Single Sum" value="0"/>
                            <EnumElem name="List Each" value="1"/>
                        </Data>
                        <Docum>
                            <Formatted>
Single Sum: Collapses all stock of a given type into a single line and displays the sum of all lengths in the "Length" column.
List Each: Groups all stock of a given type and length together (applicable only when the "Level" option is set to "Itemized").
                            </Formatted>
                        </Docum>
                    </Pref>
                </Group>
                <Group name="Template Parts">
                    <Pref name="RSD_BomMetricTemplatePartName_WIN" type="string" title="Metric" value="routing_bom_metric.prt" ver="1953" scope="Session">
                        <Description>This option specifies the name of the Metric (millimeter) template part to use for the Routing Bill of Materials. If no path is given with the part, the environment variable UGII_ROUTING_BOM_PATH is used as the path to the part.</Description>
                        <BrowseFileSel type="prt"/>
                    </Pref>
                    <Pref name="RSD_BomMetricTemplatePartName_UNX" type="string" title="Metric" value="routing_bom_metric.prt" ver="1953" scope="Session">
                        <Description>This option specifies the name of the Metric (millimeter) template part to use for the Routing Bill of Materials. If no path is given with the part, the environment variable UGII_ROUTING_BOM_PATH is used as the path to the part.</Description>
                        <BrowseFileSel type="prt"/>
                    </Pref>
                    <Pref name="RSD_BomEnglishTemplatePartName_WIN" type="string" title="English" value="routing_bom_english.prt" ver="1953" scope="Session">
                        <Description>This option specifies the name of the English (inch) template part to use for the Routing Bill of Materials. If no path is given with the part, the environment variable UGII_ROUTING_BOM_PATH is used as the path to the part.</Description>
                        <BrowseFileSel type="prt"/>
                    </Pref>
                    <Pref name="RSD_BomEnglishTemplatePartName_UNX" type="string" title="English" value="routing_bom_english.prt" ver="1953" scope="Session">
                        <Description>This option specifies the name of the English (inch) template part to use for the Routing Bill of Materials. If no path is given with the part, the environment variable UGII_ROUTING_BOM_PATH is used as the path to the part.</Description>
                        <BrowseFileSel type="prt"/>
                    </Pref>
                </Group>
                <Pref name="RSD_BomWriteToListingWindow" type="logical" title="Write to Information Window" value="false" ver="1953" scope="Session">
                    <Description>This option determines whether or not to write the Bill of Materials information to the Information window when a new BOM is created.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
            </Tab>
        </Category>
    </Application>
</PrefDefinition>
