<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="docum.xsl"?>
<!--

-->
<PrefDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www-cyp.ugsolutions.com/assemblies/pman">
    <Application name="Teamcenter Integration" csh="PMAN_TCIN" package_suppress="simviewer,mechatronicsplayer,nx2dedit">
        <Category name="General" csh="PMAN_TCIN_General">
            <Tab name="General" csh="PMAN_TCIN_General_General">
                <Pref name="UGMGR_Export" type="string" title="Export Associated Files" value="on" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not associated files are exported from the database during part load and whether the changed and new associated files (from the export directory) are imported into the database when performing a Save or a Close operation.</Description>
                    <Toggle off="off"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="on"/>
                        <EnumElem name="No" value="off"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_UserRole" type="string" title="Role" value="ALL" ver="3.0.0" scope="Session">
                    <Description>This option specifies which associated files are exported from the database during part load and all the changed and new associated files are imported into the database when performing a Save or a Close operation.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="ALL" value="ALL"/>
                        <EnumElem name="ANALYSIS" value="ANALYSIS"/>
                        <EnumElem name="BASE" value="BASE"/>
                        <EnumElem name="CAM" value="CAM"/>
                        <EnumElem name="DESIGN" value="DESIGN"/>
                        <EnumElem name="DRAFTING" value="DRAFTING"/>
                        <EnumElem name="GATEWAY" value="GATEWAY"/>
                        <EnumElem name="GFEM" value="GFEM"/>
                        <EnumElem name="MECHANISM" value="MECHANISM"/>
                        <EnumElem name="MOLDFLOW" value="MOLDFLOW"/>
                        <EnumElem name="TEAM" value="TEAM"/>
                    </Data>
                    <Docum>This option has no effect when the Export Associated Files option is off.</Docum>
                </Pref>
                <Pref name="UGMGR_PromptAssocFilesOnQuit" type="string" title="Warn on Exit" value="Yes" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not a warning message is displayed on exit if associated files have been modified or created in the session and not saved to Teamcenter because the corresponding part has not been saved.</Description>
                    <Toggle off="No"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="Yes"/>
                        <EnumElem name="No" value="No"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="UGMGR_LockingPolicy" type="string" title="Check-out/Check-in Policy" value="AutoCheckinClose" ver="1847" scope="Session">
                  <Description>This option specifies if parts are checked out on modification (Automatic) or explicitly (Manual), as well as when they are checked in.</Description>
                  <RadioBox orientation="vertical"/>
                  <Data type="enum">
                    <EnumElem name="Automatic Check-out on Modify or Save; Check-in on Save/Close" value="AutoCheckinSave"/>
                    <EnumElem name="Automatic Check-out on Modify or Save; Check-in on Close" value="AutoCheckinClose"/>
                    <EnumElem name="Automatic Check-out on Save; Check-in on Save" value="Manual"/>
                    <EnumElem name="Manual Check-out Required for Save; Manual Check-in" value="ManualPlusPlus"/>
                    <EnumElem name="Manual Check-out Required for Save; Check-in on Close" value="SuperManualPlusPlus"/>
                  </Data>
                </Pref>
                <Pref name="UGMGR_PromptManualCheckOut" type="logical" title="Prompt for Change ID and Comments on Manual Check-out" value="true" ver="2206" scope="Session">
                  <Description>This option determines whether or not a change ID and comment can be added during manual check-out.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="Yes" value="true"/>
                    <EnumElem name="No" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UGMGR_Unlock" type="string" title="Check In Parts on" value="Close" ver="3.0.0" scope="Session">
                    <Description>This option specifies if parts are checked in when the part is closed or saved. (This option only applies when the Check-out/Check-in Policy is set to Automatic.)</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Close" value="Close"/>
                        <EnumElem name="Save" value="Save"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="UGMGR_DisplaySaveasDialog" type="string" title="Save As Non-master Parts Dialog Box" value="display" ver="3.0.0" scope="Session">
                    <Description>This option specifies how non-master parts belonging to a master are processed. Note: This is applicable only when Save As New Item Type is performed on the master part.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="Display Dialog Box" value="display"/>
                        <EnumElem name="Save All Non-masters" value="save_all"/>
                        <EnumElem name="Save No Non-masters" value="save_none"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_CheckNonMastersOnSave" type="string" title="Check Non-masters on Save" value="yes" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not non-master parts should be checked on save.</Description>
                    <Toggle off="no"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="yes"/>
                        <EnumElem name="No" value="no"/>
                    </Data>
                </Pref>
              <Pref name="UGMGR_Include_Parts_Modified_By_Other_Users" type="string" title="Include Parts Modified by Other Users" value="On" ver="6.0.0" scope="Session">
                <Description>This option determines whether or not the Reopen Parts dialog box (File -> Close -> Reopen Selected Parts) will include parts that have been updated in Teamcenter since they were opened in the current session. Also, when this option is on, the File -> Close -> Reopen All Modified Parts command will close and reopen all parts flagged as updated in Teamcenter since they were opened in the current session.</Description>
                <Toggle off="Off"/>
                <Data type="enum">
                  <EnumElem name="On" value="On"/>
                  <EnumElem name="Off" value="Off"/>
                  </Data>
                </Pref>
                <Pref name="UGMGR_DefaultSaveAsAction" type="string" title="Save As Action" value="SAVEAS_NEW_REVISION" ver="6.0.0" scope="Session">
                  <Description>This option specifies the initial value of the Save As Action in the Save As dialog box.</Description>
                  <OptionMenu/>
                  <Data type="enum">
                    <EnumElem name="New Item Revision" value="SAVEAS_NEW_REVISION"/>
                    <EnumElem name="New Item" value="SAVEAS_NEW_ITEM"/>
                  </Data>
                </Pref>

                <Pref name="UGMGR_PublishDrawingRelations" type="logical" title="Publish Drawing Relations on Save" value="true" ver="11.0.1" scope="Session">
                    <Description>This option determines whether to publish the following two drawing relations for master parts with drawing sheets in them.
<Formatted>
1. The "Drawing Of" relation to their only non-"Reference Only" component.
2. The "Drawing Using" relation to their "Reference Only" components.
</Formatted>
                    </Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_PublishExternalFileRelations" type="logical" title="Publish External File Reference Relations in Teamcenter" value="true" ver="1847" scope="Session">
                    <Description>This option determines whether to publish the 'External File Reference' relation when part gets saved into Teamcenter.
                    </Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
              <Pref name="UGMGR_ShowClientCacheWarning" type="logical" title="Show Client Meta Model Cache Warning on Startup" value="true" ver="12.0.2" scope="Session">
                <Description>This option determines whether or not a warning message is displayed at startup when the Teamcenter client meta-model cache is either not populated or failed to download. The Teamcenter administrator can generate the client meta-model cache to improve performance.</Description>
                <Toggle off="false"/>
                <Data type="enum">
                  <EnumElem name="No" value="false"/>
                  <EnumElem name="Yes" value="true"/>
                </Data>
              </Pref>
              <Pref name="UGMGR_AdvSearQueriesSorting" type="logical" title="Show Queries in Alphabetical Order (Advanced Search)" value="true" ver="1899" scope="Session">
                <Description> This option determines whether or not to show the queries in alphabetical order in "Look For" in the Advanced Search dialog.</Description>
                <Toggle off="false"/>
                <Data type="enum">
                  <EnumElem name="No" value="false"/>
                  <EnumElem name="Yes" value="true"/>
                </Data>
              </Pref>
              <Pref name="UGMGR_ExternalRefernecesExportDir_WIN" type="string" title="Export Directory for External File References" value="${UGII_TMP_DIR}\ExternalFileReferences" ver="1847" scope="Session">
                  <Description>This field specifies the directory to which the external files referenced by parts should be downloaded.</Description>
                  <BrowseDirSel/>
              </Pref>
              <Pref name="UGMGR_ExternalRefernecesExportDir_UNX" type="string" title="Export Directory for External File References" value="${UGII_TMP_DIR}/ExternalFileReferences" ver="1847" scope="Session">
                  <Description>This field specifies the directory to which the external files referenced by parts should be downloaded.</Description>
                  <BrowseDirSel/>
              </Pref>
              <Pref name="UGMGR_EnableSessionSharing" type="logical" title="Synchronize Settings from Teamcenter to NX" value="false" ver="1847+" scope="Session">
                    <Description>This option determines whether changes to settings such as group and role will be synchronized from Teamcenter to NX.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
              <Pref name="UGMGR_LoadHonoringRevisionRule" type="logical" title="Honor Precise Term in Revision Rule for Relations" value="true" ver="1980" scope="Session">
                <Description>This option determines whether or not the precise term, when it is defined in the Revision Rule, is used when configuring a relation.</Description>
                <Toggle off="false"/>
                <Data type="enum">
                  <EnumElem name="No" value="false"/>
                  <EnumElem name="Yes" value="true"/>
                </Data>
              </Pref>

            </Tab>
            <Tab name="Assembly" csh="PMAN_TCIN_General_Assembly">
                <Pref name="UGMGR_StructureUpdateOnLoad" type="string" title="Structure Update on Load" value="add_positioned" ver="4.0.0" scope="Session">
                    <Description>This option specifies if changes made to the Teamcenter product structure are reflected in the assembly when it is opened and if so which changes are applied.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Complete" value="complete"/>
                        <EnumElem name="Add Components that have a Position" value="add_positioned"/>
                        <EnumElem name="None" value="none"/>
                    </Data>
                    <Docum><Formatted>'Complete': All components not already present in NX will be added on load, those without transforms being positioned at the origin.
'Add Components that have a Position': Changes to the product structure will be processed, but intervention (via the Manage Pending Components dialog box) will be required for components without valid transforms in the database.
'None': The structure in the database is ignored and the NX structure is used as it is.</Formatted></Docum>
                </Pref>
                <Pref name="UGMGR_StructureUpdateOnSave" type="string" title="Structure Update on Save" value="on" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not any changes made to the assembly are reflected in the Teamcenter product structure when the assembly is saved.</Description>
                    <Toggle off="off"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="on"/>
                        <EnumElem name="No" value="off"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_StructureUpdateOnEmptyBom" type="string" title="Structure Update on Empty BOM" value="on" ver="6.0.0" scope="Session">
                    <Description>This option determines whether or not the assembly structure in NX should be deleted when the Teamcenter BOM is empty.</Description>
                    <Toggle off="off" />
                    <Data type="enum">
                        <EnumElem name="Yes" value="on" />
                        <EnumElem name="No" value="off" />
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="UGMGR_SyncArrangements" type="string" title="Synchronize Assembly Arrangements" value="off" ver="4.0.0" scope="Session">
                    <Description>This option determines whether or not any changes made to the assembly's arrangements are synchronized between NX and Teamcenter when the assembly is loaded or saved. It should be treated as a site setting.</Description>
                    <Toggle off="off"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="on"/>
                        <EnumElem name="No" value="off"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_UpdateTransform" type="string" title="Update Modified Transforms" value="on" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not component repositions performed in the Teamcenter database should be applied when the part is loaded (application may fail if the component position is constrained in NX). This option only applies if Structure Update on Load is on.</Description>
                    <Toggle off="off"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="on"/>
                        <EnumElem name="No" value="off"/>
                    </Data>
                </Pref>
              <Separator/>
              <Pref name="UGMGR_AllowAnyItemtype" type="logical" title="Allow Adding Teamcenter Child Types which Prevent Save" value="false" ver="1847" scope="Session">
                <Description>This option determines whether or not child types which are not specified in the Teamcenter preference TCAllowedChildTypes can be added as assembly components. If these types are added to an assembly, the assembly cannot be saved, but can be used temporarily in the session.</Description>
                <Toggle off="false"/>
                <Data type="enum">
                  <EnumElem name="No" value="false"/>
                  <EnumElem name="Yes" value="true"/>
                </Data>
              </Pref>
              <Separator/>
            </Tab>
          <Tab name="Active Workspace" csh="PMAN_TCIN_General_Active_Workspace">
            <Pref name="ActiveWorkspace_mode" type="string" title="Hosting Location" value="hosted_in_nx" ver="1980" scope="Session">
              <Description>This option specifies if Active Workspace should be hosted and available in the Resource Bar, or if the default browser should be started, and an application connection should be established between NX and Active Workspace.</Description>
              <RadioBox orientation="vertical"/>
              <Data type="enum">
                <EnumElem name="NX Resource Bar" value="hosted_in_nx"/>
                <EnumElem name="Application Connect" value="application_connect"/>
                <EnumElem name="None" value="none"/>
              </Data>
            </Pref>
            <Pref name="ActiveWorkspace_mode_tcxlite" type="string" title="Hosting Location" value="hosted_in_nx" ver="2312" scope="Session">
                <Description>This option specifies if Active Workspace should be hosted and available in the Resource Bar and an application connection should be established between NX and Active Workspace.</Description>
                <RadioBox orientation="vertical"/>
                <Data type="enum">
                    <EnumElem name="NX Resource Bar" value="hosted_in_nx"/>
                    <EnumElem name="None" value="none"/>
                </Data>
            </Pref>
            <Group name="NX Resource Bar">
              <Pref name="UGMGR_DisplayAWOnCloseParts" type="logical" title="Display Active Workspace Tab after Closing Last Part" value="true" ver="1847" scope="Session">
                <Description>
                  This option will open the Active Workspace tab in the resource bar instead of the History tab when the last part is closed. Active Workspace must be installed and configured for this setting to take effect.
                  <Formatted>
                  </Formatted>
                </Description>
                <Toggle off="false"/>
                <Data type="enum">
                  <EnumElem name="No" value="false"/>
                  <EnumElem name="Yes" value="true"/>
                </Data>
              </Pref>
            </Group>
            <Group name="Application Connect" suppress="TCX_LITE">
              <Pref name="Auto_Open_AW_Browser" type="logical" title="Open Browser Automatically" value="true" ver="1980" scope="Session">
                <Description>This option determines whether or not to automatically start the default browser and establish an application connection between NX and Active Workspace.</Description>
                <Toggle off="false"/>
                <Data type="enum">
                  <EnumElem name="No" value="false"/>
                  <EnumElem name="Yes" value="true"/>
                </Data>
              </Pref>
            </Group>
            <Group name="Shape Search">
              <Pref name="Show_Shape_Search_Results_in_AW" type="logical" title="Display Shape Search Results in Active Workspace" value="true" ver="2007" scope="Session">
                <Description>This option determines whether or not to display shape search results in Active Workspace.</Description>
                <Toggle off="false"/>
                <Data type="enum">
                  <EnumElem name="No" value="false"/>
                  <EnumElem name="Yes" value="true"/>
                </Data>
              </Pref>
            </Group>
            <Group name="Start from Active Workspace">
              <Pref name="UGMGR_StartFromAW" type="int" title="Teamcenter Server Connection" value="0" ver="2312" scope="Session">
                <Description>This option specifies which Teamcenter server to connect to.</Description>
                <RadioBox/>
                <Data type="enum">
                  <EnumElem name="Use Existing Standalone Active Workspace Server" value="0"/>
                  <EnumElem name="Create New Independent Server" value="1"/>
                </Data>
              </Pref>
            </Group>
            <Pref name="UGMGR_aw_load_descendants_of_selected_def" type="logical" title="Load Descendants of Selected" value="true" ver="1872" scope="Session">
              <Description>This option determines whether or not all the descendants of the selected assembly should be loaded. Active Workspace must be installed and configured for this setting to take effect.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="No" value="false"/>
                <EnumElem name="Yes" value="true"/>
              </Data>
            </Pref>
          </Tab>
          <Tab name="Parts without NX Datasets" csh="PMAN_TCIN_General_Parts_without_NX_Datasets">
            <Label label="Parts with JT Dataset"/>
              <Pref name="UGMGR_UnpopulatedJTBehaviour" type="string" title="Add NX Datasets" value="never" ver="1872" scope="Session">
                <Description>When a JT derived part is loaded in NX session, this option specifies if the NX dataset is to be saved to the database during a save operation. The part's structure will be written to the database product structure.</Description>
                <OptionMenu/>
                <Data type="enum">
                  <EnumElem name="On Save" value="if_not_writable"/>
                  <EnumElem name="When Modifying Part - Prompt"  value="always"/>
                  <EnumElem name="When Modifying Part - No Prompt"  value="if_modified_no_prompt"/>
                  <EnumElem name="Never"  value="never"/>
                </Data>
                <Docum><Formatted>On Save: Part data will be saved to the database if access controls allows it.
When Modifying Part - Prompt: When modifying a part, a prompt will display asking if an NX dataset should be created on Save.
When Modifying Part - No prompt: When modifying a part, no prompt will display and an NX dataset will be created on Save.
Never: When modifying a part, no prompt will display and an NX dataset will not be created on Save.</Formatted></Docum>
              </Pref>
            <Separator/>
            <Label label="Parts with Other/No Dataset"/>
            <Pref name="UGMGR_UnpopulatedBehaviour" type="string" title="Add NX Datasets" value="if_not_writable" ver="6.0.3" scope="Session">
              <Description>When a part in the database does not have an existing NX dataset, this option specifies if the part is to be saved to the database during a save operation. The part's structure will be written to the database product structure.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="On Save" value="if_not_writable"/>
                <EnumElem name="When Modifying Part - Prompt"  value="always"/>
                <EnumElem name="When Modifying Part - No Prompt"  value="if_modified_no_prompt"/>
                <EnumElem name="Never"  value="never"/>
              </Data>
              <Docum><Formatted>On Save: Part data will be saved to the database if access controls allows it.
When Modifying Part - Prompt: When modifying a part, a prompt will display asking if an NX dataset should be created on Save.
When Modifying Part - No prompt: When modifying a part, no prompt will display and an NX dataset will be created on Save.
Never: When modifying a part, no prompt will display and an NX dataset will not be created on Save.</Formatted></Docum>
            </Pref>
            <Pref name="UGMGR_DisplaySeedDialog" type="string" title="Display Select Template Dialog Box" value="no" ver="2206" scope="Session">
              <Description>This option specifies how to deal with parts created in Teamcenter when they are opened for the first time. The No option means that a default seed part has to be chosen. The Top option means that the Select Seed Part dialog box will be displayed for top level assembly parts only and that a default seed part has to be chosen for the components.</Description>
              <RadioBox orientation="horizontal"/>
              <Data type="enum">
                <EnumElem name="Yes" value="yes"/>
                <EnumElem name="No" value="no"/>
                <EnumElem name="Top" value="top"/>
              </Data>
            </Pref>
            <Pref name="UGMGR_SeedPartName" type="string" title="Default Seed Part" value="None" ver="2206" scope="Session">
              <Description>This option specifies the seed part that will be used as the basis for new part files created if no dialog box is displayed.</Description>
              <String/>
            </Pref>
            <Pref name="UGMGR_ShowWarningNXDatasetAbsent" type="logical" title="Show Warning for Parts without NX Dataset" value="false" ver="1926" scope="Session">
              <Description>This option determines whether or not to display warning for parts without NX dataset.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="No" value="false"/>
                <EnumElem name="Yes" value="true"/>
              </Data>
            </Pref>
            <Separator/>
          </Tab>
            <Tab name="WAVE" csh="PMAN_TCIN_General_WAVE">
                <Pref name="UGMGR_WAVERelations" type="string" title="WAVE Relations" value="ItemRevToItem" ver="3.0.0" scope="Session">
                    <Description>This option specifies how WAVE relations between NX parts should be recorded in the Teamcenter database. This option is used as a site preference; all users of a database should use the same setting.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Item Revision to Item" value="ItemRevToItem"/>
                        <EnumElem name="Dataset to Dataset" value="DatasetToDataset"/>
                    </Data>
                    <Docum>The option Item Revision to Item causes a relation to be created from the Item Revision of the referencing part to the Item of the referenced part. The option Dataset to Dataset means that when a part is saved, the WAVE references from the part will be recorded in relationships connecting the part's dataset to the referenced part's dataset.</Docum>
                </Pref>
            </Tab>
            <Tab name="Feature" csh="PMAN_TCIN_General_Feature">
                <Label label="Publish the chosen feature categories to Teamcenter"/>
                <Pref name="UGMGR_PublishWeldPoints" type="string" title="Spot Weld" value="off" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not weld points will be published to Teamcenter. When on at the time of saving a part containing weld points, the part will be treated as an assembly part with all weld points as child components. This will allow the weld points to participate in the querying, browsing, visualizing, and consuming functionality in Teamcenter.</Description>
                    <Toggle off="off"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="on"/>
                        <EnumElem name="No" value="off"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_PublishDatumPoints" type="string" title="Datum Location Feature" value="off" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not datum points will be published to Teamcenter. When on at the time of saving a part containing datum points, the part will be treated as an assembly part with all datum points as child components. This will allow the datum points to participate in the querying, browsing, visualizing, and consuming functionality in Teamcenter.</Description>
                    <Toggle off="off"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="on"/>
                        <EnumElem name="No" value="off"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_PublishMeasurementPoints" type="string" title="Measurement Point" value="off" ver="2312+" scope="Session">
                  <Description>This option determines whether or not measurement points will be published to Teamcenter. When on at the time of saving a part containing measurement points, the part will be treated as an assembly part with all measurement points as child components. This will allow the measurement points to participate in the querying, browsing, visualizing, and consuming functionality in Teamcenter.</Description>
                  <Toggle off="off"/>
                  <Data type="enum">
                    <EnumElem name="Yes" value="on"/>
                    <EnumElem name="No" value="off"/>
                  </Data>
                </Pref>
                <Pref name="UGMGR_PublishArcWelds" type="string" title="Arc Weld" value="off" ver="8.5.0" scope="Session">
                    <Description>This option determines whether or not arc welds will be published to Teamcenter.</Description>
                    <Toggle off="off"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="on"/>
                        <EnumElem name="No" value="off"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_PublishWeldJoint" type="string" title="Welding Joint" value="off" ver="9.0.0" scope="Session">
                    <Description>This option determines whether or not welding joints will be published to Teamcenter.</Description>
                    <Toggle off="off"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="on"/>
                        <EnumElem name="No" value="off"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_PublishSurfaceWeld" type="string" title="Surface Weld" value="off" ver="9.0.0" scope="Session">
                    <Description>This option determines whether or not surface welds will be published to Teamcenter.</Description>
                    <Toggle off="off"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="on"/>
                        <EnumElem name="No" value="off"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_PublishPointJoins" type="logical" title="Join" value="false" ver="1953" scope="Session">
                    <Description>This option determines whether or not Join features will be published to Teamcenter. If any of the other options are set, no Join feature will be published to any part. Join features published to a part with existing features created by other applications will override the existing features.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_PublishProductInterfaces" type="string" title="Product Interfaces" value="off" ver="2306" scope="Session">
                    <Description>This option specifies if product interface relations and checksum data will be published to Teamcenter. This checksum data allows out-of-date status to be calculated without requiring parts to be fully loaded.</Description>
                    <RadioBox orientation="vertical"/>
                    <Data type="enum">
                        <EnumElem name="None" value="off"/>
                        <EnumElem name="Relations" value="on"/>
                        <EnumElem name="Relations and Out-of-date Status" value="PublishProdIntAndChecksums"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Weld Publishing" csh="PMAN_TCIN_General_Weld_Publishing">
                <Group name="Arc Weld Publishing">
                    <Pref name="UGMGR_ArcWeldPreferenceName_Type" type="string" title="Teamcenter Item Type" value="ArcWeld" ver="9.0.0" scope="Session">
                        <Description>This field specifies Arc Weld class type defined in Teamcenter.</Description>
                        <Widestring/>
                    </Pref>
