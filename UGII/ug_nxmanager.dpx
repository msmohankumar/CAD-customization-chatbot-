                </Pref>
                <Pref name="UGMGR_PartRevPrefix" type="string" title="Part Revision Prefix" value="/" ver="3.0.0" scope="Session">
                    <String/>
                </Pref>
                <Pref name="UGMGR_PartRevPostfix" type="string" title="Part Revision Suffix" value="" ver="3.0.0" scope="Session">
                    <String/>
                </Pref>
            </Tab>
        </Category>
        <Category name="Clone Assembly" csh="PMAN_TCIN_Clone_Assembly" package_suppress="view">
            <Tab name="All" csh="PMAN_TCIN_Clone_Assembly_All">
               <Pref name="UGMGR_DefaultCloningAction" type="string" title="Clone Action" value="Clone" ver="2007" scope="Session">
                    <Description>This option specifies the action to use on the Clone dialog.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Clone" value="Clone"/>
                        <EnumElem name="Revise" value="Revise"/>
                        <EnumElem name="Retain" value="Retain"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_CloneAssignBehavior" type="string" title="Assign Behavior" value="AutoGenerate" ver="2007" scope="Session">
                    <Description>This option specifies the scheme to use for part ID generation.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Part ID Generator" value="AutoGenerate"/>
                        <EnumElem name="Numbering Rule" value="NumberingRule"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_CloneNumberingRule" type="string" title="Numbering Rule" value="WithPrefix" ver="2007" scope="Session">
                    <Description>This option specifies the numbering rule to use for generating a new part ID.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="With Prefix" value="WithPrefix"/>
                        <EnumElem name="With Suffix" value="WithSuffix"/>
                        <EnumElem name="With Replace String" value="WithReplaceString"/>
                        <EnumElem name="With Renumber String" value="WithRenumber"/>
                        <EnumElem name="Mixed" value="MixedRule"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_ClonePrefix" type="string" title="Prefix" value="" ver="2007" scope="Session">
                    <Description>This field specifies the prefix to be applied to the source part ID to generate a new part ID.</Description>
                    <String/>
                </Pref>
                <Pref name="UGMGR_CloneSuffix" type="string" title="Suffix" value="" ver="2007" scope="Session">
                    <Description>This field specifies the suffix to be applied to the source part ID to generate a new part ID.</Description>
                    <String/>
                </Pref>
                <Pref name="UGMGR_CloneStringToReplace" type="string" title="String to Replace" value="" ver="2007" scope="Session">
                    <Description>This field specifies the string in the source part ID to replace, to generate a new part ID.</Description>
                    <String/>
                </Pref>
                <Pref name="UGMGR_CloneReplacementString" type="string" title="Replacement String" value="" ver="2007" scope="Session">
                    <Description>This field specifies the string to replace with, to generate a new part ID.</Description>
                    <String/>
                </Pref>
                <Pref name="UGMGR_CloneRenumberString" type="string" title="Renumber String" value="" ver="2007" scope="Session">
                    <Description>This field specifies the renumber string to generate a new part ID.</Description>
                    <String/>
                </Pref>
                <Pref name="UGMGR_CloneDefaultItemType" type="string" title="Item Type for Alternate ID Assignment" value="" ver="2007" scope="Session">
                    <Description>This field specifies the item type that should be used to populate alternate ID contexts and types in the Clone dialog.</Description>
                    <String/>
                </Pref>
                <Pref name="UGMGR_CloneEnableAutoAssign" type="logical" title="Automatically Assign Attributes to Parts when Added for Clone" value="true" ver="2007" scope="Session">
                    <Description>This option determines whether or not the auto-assignable attributes should be assigned based on the chosen numbering scheme when the part is added to the operation.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_CloneShowTopAssySelectionDlg" type="logical" title="Show Top Assemblies Selection Dialog Box" value="true" ver="2007" scope="Session">
                    <Description>This option determines whether or not to display the top assembly selection dialog box for parts added using a log file. This option is to improve the load performance in case of large assemblies.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_ApplyCloneActionBasedOnItemType" type="string" title="Apply Clone Action Based on Item Type" value="" ver="2007" scope="Session">
                    <Description>This field specifies clone actions which should be assigned to the provided item type. The list should be in the format &apos;ItemType1:Action,ItemType2:Action&apos; and so on, where the item type should be specified with the internal name.</Description>
                    <Widestring/>
                </Pref>
                <Pref name="UGMGR_SkipMaxAllowedWorkRevCheckForClone" type="logical" title="Skip Maximum Working Revision Limit" value="false" ver="2007" scope="Session">
                    <Description>This option determines whether or not to skip the maximum allowed working revision limit validation.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_SkipParentChildTypeValidation" type="logical" title="Skip Parent Child Type Validation" value="false" ver="2007" scope="Session">
                    <Description>This option determines whether or not to skip the parent child type validation.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_ClonePartFamilyMemberTreatmentOption" type="int" title="Treatment of Part Family Member" value="0" ver="2007" scope="Session">
                    <Description>This option specifies how a part family member will be treated.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Retain Existing Family Part" value="0"/>
                        <EnumElem name="Clone as New Non-family Part" value="1"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_EnableMultirevCloning" type="int" title="Multiple Revisions Clone Result" value="0" ver="2007" scope="Session">
                    <Description>This option specifies how multiple revisions of a part should be output in a clone operation.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Multiple Revisions" value="0"/>
                        <EnumElem name="Separate Items" value="1"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_AssignNewItemIdBasedOnSourceItemId" type="logical" title="Assign New Item ID based on Source Item ID" value="false" ver="2007" scope="Session">
                    <Description>This option determines whether or not to assign the new item ID based on the source item ID.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_CloneIgnoreInvalidAttributeValues" type="logical" title="Ignore Invalid Attribute Values" value="false" ver="2212" scope="Session">
                    <Description>This option determines whether or not to ignore invalid attribute values from the source part.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_ReleaseStatusesForNonMasterToSetNoCopyAction" type="string" title="Set &apos;No Copy&apos; Action for Non-masters with Release Status" value="" ver="2007" scope="Session">
                  <Description>This field specifies the release statuses to define initial copy action for non-masters. If any of the specified release statuses are present on the non-master, then it will be applied the 'No Copy' action. This should be a comma-separated list. For example: Released, Approved. The value should be specified with the internal name.</Description>
                  <Widestring/>
                </Pref>
                <Group name="Legacy">
                    <Pref name="UGMGR_CloneSuppressedPanes" type="string" title="Suppress Dialog Box Tabs" value="" ver="3.0.0" scope="Session">
                        <Description>This field specifies the tabs which should be suppressed from the Assembly Clone dialog box. This should be a comma separated list. The names of the panes may be specified in the localized language or in English.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="UGMGR_CloneDefaultRevRule" type="string" title="Revision Rule" value="" ver="3.0.0" scope="Session">
                        <Description>This field specifies the revision rule to be applied when cloning an assembly. If not set then the current revision rule will be used.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="UGMGR_CloneCarryForwardSourceRevisionId" type="logical" title="Use Source Revision ID for Multiple Revisions" value="true" ver="1899" scope="Session">
                        <Description>This option determines whether or not to copy the revision ID from the source parts when there are multiple revisions of the same part in the clone operation.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
        </Category>
        <Category name="Import/Export Assembly" csh="PMAN_TCIN_Import_Export_Assembly" package_suppress="view">
            <Tab name="Import" csh="PMAN_TCIN_Import_Export_Assembly_Import">
                <Pref name="UGMGR_ImportItemType" type="string" title="Item Type" value="" ver="10.0.0" scope="Session">
                    <Description>This field specifies the item type to use for the parts. This option is only used when the "Use Item type in Part File" option is off.</Description>
                    <String/>
                </Pref>
                <Pref name="UGMGR_ImportUseItemTypeFromPartFile" type="logical" title="Use Item type in Part File" value="false" ver="11.0.0" scope="Session">
                    <Description>If an item type exists in the part file, this option determines whether or not it should be used.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_ImportNumberingSource" type="string" title="Numbering Source" value="Generator" ver="10.0.0" scope="Session">
                    <Description>This option specifies the source to use as a basis for part ID generation.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Part ID Generator" value="Generator"/>
                        <EnumElem name="OS Filename" value="OSFilename"/>
                        <EnumElem name="Attribute" value="Attribute"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_ImportConversionRule" type="string" title="Conversion Rule" value="AsIDAndRevision" ver="2206" scope="Session">
                    <Description>This option specifies the conversion rule to use, in conjunction with 'OS Filename' numbering source, to generate a new part ID.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="As ID" value="AsNumber"/>
                        <EnumElem name="As ID and Revision" value="AsIDAndRevision"/>
                        <EnumElem name="With Prefix" value="Prefix"/>
                        <EnumElem name="With Suffix" value="Suffix"/>
                        <EnumElem name="With Replace String" value="Replace"/>
                        <EnumElem name="Mixed" value="MixedRule"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_ImportPrefix" type="string" title="Prefix" value="" ver="10.0.0" scope="Session">
                    <Description>This field specifies the prefix to be applied to the OS file name of the part.</Description>
                    <String/>
                </Pref>
                <Pref name="UGMGR_ImportSuffix" type="string" title="Suffix" value="" ver="10.0.0" scope="Session">
                    <Description>This field specifies the suffix to be applied to the OS file name of the part.</Description>
                    <String/>
                </Pref>
                <Pref name="UGMGR_ImportStringToReplace" type="string" title="String to Replace" value="" ver="10.0.0" scope="Session">
                    <Description>This field specifies the string in the OS file name to replace, to generate a new part ID.</Description>
                    <String/>
                </Pref>
                <Pref name="UGMGR_ImportReplacementString" type="string" title="Replacement String" value="" ver="10.0.0" scope="Session">
                    <Description>This field specifies the string to replace with, to generate a new part ID.</Description>
                    <String/>
                </Pref>
                <Pref name="UGMGR_ImportIDAttribute" type="string" title="ID Attribute" value="${DB_PART_NO}" ver="10.0.2" scope="Session">
                    <Description>This field specifies the attributes whose values, from the part file being operated on, will be used to construct the part ID.</Description>
                    <Widestring/>
                </Pref>
                <Pref name="UGMGR_ImportRevisionAttribute" type="string" title="Revision Attribute" value="${DB_PART_REV}" ver="10.0.2" scope="Session">
                    <Description>This field specifies the attributes whose values, from the part file being operated on, will be used to construct the part revision.</Description>
                    <Widestring/>
                </Pref>
                <Pref name="UGMGR_ImportDefaultName" type="string" title="Default Name" value="${DB_PART_NAME}" ver="10.0.2" scope="Session">
                    <Description>This field specifies the attributes whose values, from the part file being operated on, will be used to construct the part name.</Description>
                    <Widestring/>
                </Pref>
                <Pref name="UGMGR_ImportDefaultDescription" type="string" title="Default Description" value="${DB_PART_DESC}" ver="10.0.2" scope="Session">
                    <Description>This field specifies the attributes whose values, from the part file being operated on, will be used to construct the part description.</Description>
                    <Widestring/>
                </Pref>
                <Pref name="UGMGR_ImportExistingPartAction" type="string" title="Existing Part Action" value="UseExisting" ver="10.0.0" scope="Session">
                    <Description>This option specifies the action to be applied for the part if it already exists in the database.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Overwrite" value="Overwrite"/>
                        <EnumElem name="Use Existing" value="UseExisting"/>
                        <EnumElem name="New Revision" value="NewRevision"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_ImportCheckoutOption" type="string" title="Action on Imported Parts" value="NoChange" ver="1847" scope="Session">
                    <Description>This option specifies if the part should be checked in, checked out, or retain its state after importing it to Teamcenter. The 'No Change' option is not applicable for new parts being created by the import.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Check-in" value="Checkin"/>
                        <EnumElem name="Check-out" value="Checkout"/>
                        <EnumElem name="No Change" value="NoChange"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_ImportDefaultSimItemType" type="string" title="Simulation Item Type" value="" ver="10.0.0" scope="Session">
                    <Description>This field specifies the item type with which Simulation files will be imported.</Description>
                    <String/>
                </Pref>
                <Pref name="UGMGR_ImportDefaultFemItemType" type="string" title="FE Model Item Type" value="" ver="10.0.0" scope="Session">
                    <Description>This field specifies the item type with which FE models will be imported.</Description>
                    <String/>
                </Pref>
                <Pref name="UGMGR_ImportDefaultIdealizedItemType" type="string" title="Idealized Part Item Type" value="" ver="10.0.0" scope="Session">
                    <Description>This field specifies the item type with which Idealized parts will be imported.</Description>
                    <String/>
                </Pref>
                <Group name="Data Conditions">
                    <Pref name="UGMGR_ImportTransientParts" type="logical" title="Import Transient Parts" value="false" ver="1872" scope="Session">
                        <Description>This option determines whether or not to import transient parts. When this option is on, transient parts which have persisted during an export operation, will be imported into Teamcenter.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="UGMGR_HonorItemTypeInName" type="logical" title="Use Item Type from OS File Name in Multi-field Key Environment" value="false" ver="10.0.0" scope="Session">
                        <Description>This option determines whether or not to use the item type extracted from the OS file name of the part, using the autotranslate naming technique. This option is available only in Multi-field Key Environment.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="UGMGR_showTopAssySelectionDlg" type="logical" title="Show Top Assemblies Selection Dialog Box" value="true" ver="10.0.1" scope="Session">
                        <Description>This option determines whether or not to display the top assembly selection dialog box for parts added using a log file or from a folder. This option is to improve the load performance in case of large assemblies.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="UGMGR_AutoImportAutoAssign" type="logical" title="Automatically Assign Attributes to Parts when Added for Import" value="true" ver="10.0.0" scope="Session">
                        <Description>This option determines whether or not the auto-assignable attributes should be assigned based on the chosen naming technique when the part is added to the operation.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="UGMGR_ImportDatasetForUseExist" type="logical" title="Allow Import to Existing Item Revision with No Datasets" value="false" ver="1953" scope="Session">
                        <Description>This option is applicable when the 'Use Existing' action is being applied to a part on import. If an existing Item Revision is found for a part being imported, the Use Existing option would mean that the part will not be imported and the existing Item Revision will be used. This option determines whether or not the part can still be imported to the existing Item Revision if there are no datasets present under the Item Revision.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="UGMGR_SkipMaxAllowedWorkRevisionValidation" type="logical" title="Skip Maximum Working Revision Limit" value="false" ver="1926" scope="Session">
                        <Description>This option determines whether or not to skip the maximum allowed working revision limit validation.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="UGMGR_ImportRetainBVRPrecise" type="logical" title="Retain Precise Structure" value="false" ver="1953" scope="Session">
                        <Description>This option determines if the exported precise assembly should retain its precise structure when it is imported.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="UGMGR_AssignOwnershipToExistingParts" type="logical" title="Change Ownership for Existing Item Revisions" value="false" ver="12.0.2" scope="Session">
                        <Description>This option determines whether to allow modifying the ownership of existing Item Revisions in Teamcenter during an import operation.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Import Action">
                    <Pref name="UGMGR_CloneManageReadOnlyComponents" type="string" title="Apply &apos;Use Existing&apos; Action to All Read-only Parts" value="Yes" ver="3.0.0" scope="Session">
                        <Description>This option determines whether or not a clone-based import operation defaults to the 'Use Existing' action for parts which are read-only in Teamcenter during the import of assemblies with the overwrite action.</Description>
                        <Toggle off="No"/>
                        <Data type="enum">
                            <EnumElem name="No" value="No"/>
                            <EnumElem name="Yes" value="Yes"/>
                        </Data>
                    </Pref>
                    <Pref name="UGMGR_AlwaysUseLatestPartRevision" type="logical" title="Use Latest Revision when Importing into Existing Parts" value="false" ver="1899" scope="Session">
                        <Description>This option determines whether or not the import operation should use the latest part revision of the existing parts.
This option is applicable only if the "Apply 'Use Existing' Action to All Read-only Parts" option is off.
                        </Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                 </Pref>
                    <Pref name="UGMGR_AlwaysUseLatestPartRevision_NewRev" type="logical" title="Apply &apos;New Revision&apos; Action if Latest Revision is Read-only" value="false" ver="1899" scope="Session">
                        <Description>This option determines whether or not an import operation defaults to the 'New Revision' action for parts which are read-only in Teamcenter during the import of assemblies with the overwrite action.
This option is applicable only if the "Use Latest Revision when Importing into Existing Parts" option is being honored.
                        </Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Non-masters" suppress="TCX_LITE">
                    <Pref name="UGMGR_TreatDrawingPart" type="logical" title="Treat Part with Drawing Sheets as Non-master" value="false" ver="10.0.0" scope="Session">
                        <Description>This option determines whether or not to treat parts with drawing sheets as non-master.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="UGMGR_ImportUpdateNonMasterDatasetForNewRevision" type="logical" title="Update Non-master Dataset Name for a New Revision" value="false" ver="1899" scope="Session">
                        <Description>This option determines whether or not to update the non-master dataset name when creating new revision. This will only apply to non-master parts which use the Auto-Translate naming scheme.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="UGMGR_UseRelationFromOsFileName" type="logical" title="Identify Non-masters Based on OS File Name" value="false" ver="1980" scope="Session">
                        <Description>This option determines whether to automatically identify non-masters from the OS file name while applying any numbering scheme. This option will work similarly to the "Based on OS File Name" numbering scheme and use '_' as a delimiter in the OS file name to identify if the OS file is a non-master.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Validation" suppress="TCX_LITE">
                    <Pref name="UGMGR_ValidationMode" type="int" title="Validation Mode" value="0" ver="10.0.0" scope="Session">
                        <Description>This option specifies the validation mode for processing validation results.</Description>
                        <OptionMenu/>
                        <Data type="enum">
