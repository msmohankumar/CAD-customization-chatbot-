            <Tab name="Multiple Revisions" csh="PMAN_TCIN_General_Multiple_Revisions">
                <Pref name="UGMGR_EnableMultipleRevisions" type="logical" title="Allow Multiple Revisions" value="false" ver="6.0.0" scope="Session">
                    <Description>This option determines whether or not Multiple Revisions of a part will be allowed in the NX session. This option should be on for 4GD.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Off" value="false"/>
                        <EnumElem name="On" value="true"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_RevisionChecking" type="string" title="Check Revisions on Load" value="off" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not extended revision conflict checking is active. When this option is on and if revision conflicts exist, a report will be displayed.</Description>
                    <Toggle off="off"/>
                    <Data type="enum">
                        <EnumElem name="Off" value="off"/>
                        <EnumElem name="On" value="on"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_RevisionCheckingOnSave" type="logical" title="Check Revisions on Save" value="true" ver="4.0.0" scope="Session">
                    <Description>This option determines whether or not a warning will be displayed during save if multiple revisions of a part exist in the immediate children of an assembly.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Off" value="false"/>
                        <EnumElem name="On" value="true"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Browse" csh="PMAN_TCIN_General_Browse">
                <Pref name="UGMGR_DetailLevelFilter" type="string" title="Detail Level Browse Filter" value="full" ver="5.0.0" scope="Session" suppress="TCN">
                    <Description>This option specifies the detail level to display when browsing for parts. </Description>
                    <RadioBox orientation="vertical"/>
                    <Data type="enum">
                        <EnumElem name="Full" value="full"/>
                        <EnumElem name="Intermediate" value="intermediate"/>
                        <EnumElem name="Simple" value="simple"/>
                    </Data>
                    <Docum><Formatted>'Full': Display Items, Item Revisions, and Datasets while browsing for parts.
'Intermediate': Display only Items and Datasets while browsing for parts.
'Simple': Display only Datasets while browsing for parts.</Formatted></Docum>
                </Pref>
                <Pref name="UGMGR_RevisionsFilter" type="string" title="Revisions Browse Filter" value="all" ver="5.0.0" scope="Session" suppress="TCN">
                    <Description>This option specifies the revisions to display when browsing for parts. </Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="All" value="all"/>
                        <EnumElem name="Use Current Revision Rule" value="use current revision rule"/>
                        <EnumElem name="Last Modified" value="last modified"/>
                        <EnumElem name="Last Two Modified" value="last two modified"/>
                        <EnumElem name="Last Four Modified" value="last four modified"/>
                        <EnumElem name="Latest Released" value="latest released"/>
                    </Data>
                    <Docum><Formatted>
'All': Display objects for all Item Revisions while browsing for parts.
'Use Current Revision Rule': Display each Item's currently configured object as defined by the current revision rule while browsing for parts.
'Last Modified': Display each Item's last modified object while browsing for parts.
'Last Two Modified': Display each Item's last two modified objects while browsing for parts.
'Last Four Modified': Display each Item's last four modified objects while browsing for parts.
'Latest Released': Display each Item's latest released object while browsing for parts.</Formatted></Docum>
                </Pref>
                <Pref name="UGMGR_DatasetIcons" type="string" title="Determine NX Dataset Type" value="Yes" ver="5.0.0" scope="Session">
                    <Description>This option specifies the dataset icons to use with models and assemblies when browsing for parts. When checked, display dataset-specific icons.</Description>
                    <Toggle off="No"/>
                    <Data type="enum">
                        <EnumElem name="No" value="No"/>
                        <EnumElem name="Yes" value="Yes"/>
                    </Data>
                    <Docum><Formatted>'Yes': Display specific icons next to model and assembly datasets to differentiate them.
'No': Display generic icons next to model and assembly datasets (will always be ugmasters).</Formatted></Docum>
                </Pref>
                <Pref name="UGMGR_QuickSearchAlgorithm" type="string" title="Quick Search" value="Item ID Only" ver="2007" scope="Session">
                    <Description>This option specifies the quick search algorithm to use when browsing for parts. The 'Custom Query' option is used to execute a simple search query defined by the Teamcenter preference 'TC_NX_QuickSearch_Query', which has a single string value. If 'TC_NX_QuickSearch_Query' is not defined, the '__NX_STD_SIMPLE_ITEM_QUERY' query will execute.</Description>
                    <RadioBox orientation="vertical"/>
                    <Data type="enum">
                        <EnumElem name="Item ID Only" value="Item ID Only"/>
                        <EnumElem name="Names and Description" value="Names and Description"/>
                        <EnumElem name="Custom Query" value="Custom Query"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_SearchPageSize" type="int" title="Search Result per Page" value="30" ver="5.0.2" scope="Session">
                    <Description>This option specifies the number of search results to display on a page.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="UGMGR_DefaultContainer" type="string" title="Default Container" value=":Newstuff" ver="9.0.3" scope="Session">
                    <Description>This option specifies the default container in which new parts will be created.</Description>
                    <Widestring/>
                </Pref>
            </Tab>
