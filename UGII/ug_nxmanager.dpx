              <Pref name="UG_ShowChangeNoticeColumn" type="logical" title="Show ECN Column in Save Management Dialog" value="false" ver="2212" scope="Session">
                <Description>This option determines whether to show the ECN (Engineering Change Notice) column in the save management dialog box. The column is shown automatically when ECN is required, and so this option takes effect only when ECN is optional.</Description>
                <Toggle off="false"/>
                <Data type="enum">
                  <EnumElem name="Yes" value="true"/>
                  <EnumElem name="No" value="false"/>
                </Data>
              </Pref>                
              <Group name="Parts with Optional Change Management"  suppress="TCX_LITE">
                <Pref name="UGMGR_UseActiveEcnForOptionalCandidates" type="logical" title="Use Active Engineering Change Notice" value="true" ver="1899" scope="Session">
                  <Description>This option determines whether to inherit active Engineering Change Notice for save candidates with optional Change Management behavior.
This option has no effect if global change context is set in Teamcenter.
                  </Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="Yes" value="true"/>
                    <EnumElem name="No" value="false"/>
                  </Data>
                </Pref>
              </Group>
              <Group name="Material Specification Table">
                  <Pref name="UGMGR_PublishMaterialsTable" type="logical" title="Publish on Save" value="true" ver="2306+" scope="Session">
                      <Description>This option determines whether or not to publish the assigned material for each body when the part is saved to Teamcenter.</Description>
                      <Toggle off="false"/>
                      <Data type="enum">
                          <EnumElem name="No" value="false"/>
                          <EnumElem name="Yes" value="true"/>
                      </Data>
                  </Pref>
              </Group>
            </Tab>
        </Category>
        <Category name="Part File Name Display" csh="PMAN_TCIN_Part_File_Name_Display">
