<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="docum.xsl"?>
<!--

-->
<PrefDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www-cyp.ugsolutions.com/assemblies/pman">
    <Application name="Modeling" csh="PMAN_Modeling" package_suppress="simviewer,view,mechatronicsplayer,nx2dedit">
        <Category name="General" csh="PMAN_Modeling_General">
            <Tab name="General" csh="PMAN_Modeling_General_General">
                <Pref name="Display_LegacyFeatNames" type="int" title="Display Legacy Feature Names" value="0" ver="4.0.0" scope="Session">
                    <Description>This option determines whether or not the feature type is displayed as the feature name in the Part Navigator and other dialog boxes.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="No" value="0"/>
                        <EnumElem name="Yes" value="1"/>
                    </Data>
                    <Docum>When this option is on, the feature type will be displayed in the Part Navigator and other dialog boxes. When this option is off, the new names will be displayed in the Part Navigator and other dialog boxes.</Docum>
                </Pref>
                <Group name="Geometry Construction">
                <Pref name="Solids_solidDensity_EU" type="real" title="Density" value="0.2829" ver="3.0.0" scope="Part">
                    <Description>This option specifies the density assigned to subsequently created solid bodies.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="Solids_solidDensity_MU" type="real" title="Density" value="7830.64" ver="3.0.0" scope="Part">
                    <Description>This option specifies the density assigned to subsequently created solid bodies.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="Solids_solidDensity_SIU" type="real" title="Density" value="7830.64" ver="2206" scope="Part">
                    <Description>This option specifies the density assigned to subsequently created solid bodies.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="Solids_solidDensity_MCU" type="real" title="Density" value="7830.64" ver="2206" scope="Part">
                    <Description>This option specifies the density assigned to subsequently created solid bodies.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="Solids_solidDensityUnits_EU" type="int" title="Density Units" value="1" ver="3.0.0" scope="Part">
                    <Description>This option sets the unit for the density.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="lbm/in3" value="1"/>
                        <EnumElem name="lbm/ft3" value="2"/>
                        <EnumElem name="g/cm3" value="3"/>
                        <EnumElem name="kg/m3" value="4"/>
                    </Data>
                </Pref>
                <Pref name="Solids_solidDensityUnits_MU" type="int" title="Density Units" value="4" ver="3.0.0" scope="Part">
                    <Description>This option sets the unit for the density.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="lbm/in3" value="1"/>
                        <EnumElem name="lbm/ft3" value="2"/>
                        <EnumElem name="g/cm3" value="3"/>
                        <EnumElem name="kg/m3" value="4"/>
                    </Data>
                </Pref>
                  <Pref name="Solids_solidDensityUnits_SIU" type="int" title="Density Units" value="4" ver="2206" scope="Part">
                    <Description>This option sets the unit for the density.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="lbm/in3" value="1"/>
                        <EnumElem name="lbm/ft3" value="2"/>
                        <EnumElem name="g/cm3" value="3"/>
                        <EnumElem name="kg/m3" value="4"/>
                    </Data>
                </Pref>
                <Pref name="Solids_solidDensityUnits_MCU" type="int" title="Density Units" value="4" ver="2206" scope="Part">
                    <Description>This option sets the unit for the density.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="lbm/in3" value="1"/>
                        <EnumElem name="lbm/ft3" value="2"/>
                        <EnumElem name="g/cm3" value="3"/>
                        <EnumElem name="kg/m3" value="4"/>
                    </Data>
                </Pref>
                <Pref name="Solids_MassPropsAccuracy" type="real" title="Mass Analysis Accuracy" value="0.99" ver="3.0.0" scope="Session">
                    <Description>This option specifies the accuracy to be used in mass analyses using solids. The closer to 1.0 the value is, the more precise the results of the calculation will be (and the longer it will take).</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="0.9" value="0.9"/>
                        <EnumElem name="0.99" value="0.99"/>
                        <EnumElem name="0.999" value="0.999"/>
                        <EnumElem name="0.9999" value="0.9999"/>
                        <EnumElem name="0.99999" value="0.99999"/>
                        <EnumElem name="0.999999" value="0.999999"/>
                    </Data>
                </Pref>
                <Pref name="Solids_tolerDist_MU" type="real" title="Distance Tolerance" value="0.0100" unit="mm" ver="9.0.0" scope="Part">
                    <Description>This option specifies the distance (G0) tolerance used for non-exact geometry such as splines and surfaces.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="Solids_tolerDist_EU" type="real" title="Distance Tolerance" value="0.0004" unit="in" ver="9.0.0" scope="Part">
                    <Description>This option specifies the distance (G0) tolerance used for non-exact geometry such as splines and surfaces.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="Solids_tolerDist_SIU" type="real" title="Distance" value="0.00100" unit="m" ver="2206" scope="Part">
                    <Description>This option specifies the distance (G0) tolerance used for non-exact geometry such as splines and surfaces.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="Solids_tolerDist_MCU" type="real" title="Distance" value="0.0100" unit="μm" ver="2206" scope="Part">
                    <Description>This option specifies the distance (G0) tolerance used for non-exact geometry such as splines and surfaces.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="Solids_tolerAngl" type="real" title="Angle Tolerance" value="0.5" unit="deg" ver="3.0.0" scope="Part">
                    <Description>This option specifies the angle (G1) tolerance used for non-exact geometry such as splines and surfaces.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="Solids_tolerG2" type="real" title="Curvature Tolerance" value="0.5" ver="8.0.1" scope="Part">
                    <Description>This option specifies the curvature (G2) tolerance used for non-exact geometry such as splines and surfaces.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="Solids_OptimizeCurve" type="logical" title="Optimize Curve" value="true" ver="12.0.2" scope="Part">
                    <Description>Optimize Curve adjusts output curves in a section or a curve feature to eliminate gaps. This option specifies whether Optimize Curve should be performed. This option only impacts new features.</Description> 
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="Solids_OptimizeCurveDistanceTolFactor" type="real"  title="Optimize Curve Distance Tolerance Factor" value="5.0" ver="11.0.0" scope="Part">
                    <Description>This option specifies the size of the distance gap within which the optimize curve is performed. The size of the gap is defined as the modeling distance tolerance multiplying the factor value.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="1.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="Solids_OptimizeCurveAngleTolFactor" type="real"  title="Optimize Curve Angle Tolerance Factor" value="1.0" ver="12.0.2" scope="Part">
                    <Description>This option specifies the size of the angle gap within which the optimize curve is performed. The size of the gap is defined as the modeling angle tolerance multiplying the factor value.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="1.0" include="1"/>
                    </Data>
                </Pref>                                                    
                </Group>
                <Group name="Scale">
                    <Pref name="Solids_datumcsysScaleFactor" type="real" title="Datum CSYS Display Scale Factor" value="1.25" ver="7.5.0" scope="Part">
                    <Description>This option specifies the display scale factor used for datum coordinate systems.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                </Group>
                <Group name="Display Properties">
                <Pref name="Solids_usePartDefaultsForNewFace" type="int" title="New Face" value="2" ver="3.0.0" scope="Session">
                    <Description>This option specifies how a new face will inherit display properties when a feature of a body updates or is edited. This option does not apply to new faces created by a Boolean operation.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Inherit from Body" value="2"/>
                        <EnumElem name="Inherit Object Preference" value="1"/>
                    </Data>
                </Pref>
                <Pref name="Solids_useToolBodyFaceProperties" type="int" title="Face Modified by Boolean" value="2" ver="3.0.0" scope="Session">
                    <Description>This option specifies how a face of a target body will inherit display properties when modified by a Boolean.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Inherit from Target Body" value="2"/>
                        <EnumElem name="Inherit from Tool Body" value="1"/>
                    </Data>
                </Pref>
                <Pref name="Solids_usePartDefaultsForLinkedExtracted" type="int" title="Link and Extract Geometry" value="2" ver="6.0.0" scope="Session">
                    <Description>This option specifies how geometry produced by link or extract will inherit display properties.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Inherit from Parent" value="2"/>
                        <EnumElem name="Inherit Object Preference" value="1"/>
                    </Data>
                </Pref>
                <Pref name="UG_surfaceGridU" type="int" title="U Grid Lines" value="0" ver="3.0.0" scope="Part">
                    <Description>This option specifies the number of grid curves in the U direction on faces of subsequently created bodies.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="0" include="1"/>
                        <UpperBound value="10000" include="1"/>
                    </Data>
                </Pref>
                <Pref name="UG_surfaceGridV" type="int" title="V Grid Lines" value="0" ver="3.0.0" scope="Part">
                    <Description>This option specifies the number of grid curves in the V direction on faces of subsequently created bodies.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="0" include="1"/>
                        <UpperBound value="10000" include="1"/>
                    </Data>
                </Pref>
                </Group>
                <Group name="Behavior">
                <Pref name="Solids_AutomaticallyMakeSketchesInternal" type="logical" title="Make Sketch Internal to Child Feature" value="false" ver="7.0.0" scope="Session">
                    <Description>This option determines whether or not a sketch feature should be automatically embedded in its child section-based feature.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                </Group>
                <Pref name="UGII_ALLOW_HISTORY_FREE" type="logical" title="Allow Changing to History-Free Modeling Mode" value="true" ver="1872" scope="Session">
                    <Description>This option determines whether or not history-free modeling mode is allowed.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Pref name="TransferAttributesWhenCommandsDivideOrJoinObjects" type="logical" title="Transfer Object Attributes when Commands Divide or Join Objects" value="true" ver="11.0.0" scope="Session">
                    <Description>This option determines whether or not object attributes are transferred or copied from input objects to output objects for commands that divide or join objects. (This option currently applies to the Divide Curve, Divide Face, and Split Body commands.)</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                 </Pref>
                <Pref name="TransferAttributesWhenSplitsTopology" type="logical" title="Transfer Attributes when Bodies, Faces, and Edges are Split" value="false" ver="2312" scope="Session">
                    <Description>This option determines whether or not object attributes are transferred from original bodies, faces, and edges to their splits. </Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                 </Pref>
                 <Pref name="AllowAutomaticWorkPartChange" type="logical" title="Allow Automatic Work Part Change" value="true" ver="1847" scope="Session">
                    <Description>This option determines whether or not the work part will change automatically when the first object is selected in a feature modeling command.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                 </Pref>
                <Pref name="ShowSectionCurveFlowDirection" type="logical" title="Show Section Curve Flow Direction" value="false" ver="1953+" scope="Session">
                    <Description>This option determines whether or not the curve flow direction and origin curve controls are available for section-based feature commands such as Extrude. (Flow direction and origin are always available for flow-sensitive features such as Swept.)</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Edit" csh="PMAN_Modeling_General_Edit">
                <Group name="Feature">
                <Pref name="Solids_DefaultActiononDoubleClick" type="int" title="Feature Double-click Action" value="1" ver="4.0.0" scope="Session">
                    <Description>This option specifies the default action taken when double-clicking on a non-sketch feature in the graphics window or Part Navigator. The default action uses bold font in the MB3 shortcut menu.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Edit Parameters" value="0"/>
                        <EnumElem name="Edit with Rollback" value="1"/>
                    </Data>
                </Pref>
                <Pref name="SketchDBAction" type="int" title="Sketch Double-click Action" value="1" ver="7.5.0" scope="Session">
                    <Description>This option specifies the default action taken when double-clicking on a sketch in the graphics window or Part Navigator. The default action uses bold font in the MB3 shortcut menu.</Description>
                    <OptionMenu/>
                    <Data type = "enum">
                        <EnumElem name="Edit with Rollback" value="0"/>
                        <EnumElem name="Edit" value="1"/>
                    </Data>
                </Pref>
                <Pref name="Solids_NotifyOnDelete" type="int" title="Notify on Delete if Feature has Dependent" value="1" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not a notification message will be displayed when a feature being deleted will affect other features (for example, if the feature being deleted is used as a positioning reference by a second feature).</Description>
                    <Toggle off="2"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="2"/>
                    </Data>
                </Pref>
                <Pref name="Solids_PropagateDelete" type="int" title="Delete Child Feature with Parent" value="1" ver="12.0.0" scope="Session">
                    <Description>This option determines whether or not the child features, including recipe curves, of a feature being deleted will be deleted as well.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="2"/>
                        <EnumElem name="Ask" value="3"/>
                    </Data>
                </Pref>
                <Pref name="Solids_PropagateExpressionSuppress" type="int" title="Suppress by Expression - Suppress Child Features " value="1" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not the child features of a feature being suppressed using suppression expressions will be suppressed as well.</Description>
                    <Toggle off="2"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="2"/>
                    </Data>
                </Pref>
                </Group>
                <Pref name="InternalSketchEditDimension" type="logical" title="Allow Editing of Dimensions of Internal Sketch" value="true" ver="7.5.0" scope="Session">
                    <Description>When a dialog box is active which allows editing of an internal sketch, this option determines whether or not the sketch dimensions will display and be possible to select and edit directly, without entering the Sketch task environment.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Update" csh="PMAN_Modeling_General_Update">
                <Group name="Behavior">
                <Pref name="Delay_Model_Update" type="logical" title="Delay Model Update" value="false" ver="1847" scope="Session">
                    <Description>This option determines whether or not model update is delayed. When model update is delayed an action is required to cause update, such as "Update Model".</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="Delay_Model_Update_Granularity" type="int" title="Model Delay and Update Granularity" value="1" ver="1953" scope="Session">
                    <Description>This option determines if model delay and update apply per group or per feature. Group includes design group and part module.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Group" value="0"/>
                        <EnumElem name="Feature" value="1"/>
                    </Data>
                    <Docum><Formatted>
Group: Dependents from other Design Group or Part Module will get delayed.
Feature: Immediate next dependent will get delayed.</Formatted></Docum>
                </Pref>
                <Pref name="Delay_Product_Interface_Update" type="logical" title="Delay Product Interface Update" value="false" ver="11.0.1" scope="Session">
                    <Description>This option determines whether or not update of product interface objects should be delayed until the "Update Product Interface" command is chosen.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="Delay_Part_Module_Update" type="logical" title="Delay Part Module Update" value="false" ver="9.0.0" scope="Session">
                    <Description>This option determines whether or not update of intrapart Part Modules should be delayed until the "Update Part Modules" command is chosen.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>            
                <Pref name="Solids_DynamicUpdate" type="int" title="Dynamic Update Mode" value="0" ver="3.0.0" scope="Session">
                    <Description>This option specifies how the system dynamically displays the changes to the body as its parent curves, splines, bridge curves, lines, or arcs are being edited using direct manipulation dragging techniques.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="None" value="0"/>
                        <EnumElem name="Incremental" value="1"/>
                        <EnumElem name="Continuous" value="2"/>
                    </Data>
                    <Docum><Formatted>
None: No dynamic display update will take place.
Incremental: The display will be updated when the mouse is released.
Continuous: The display will update while the mouse is moving.</Formatted></Docum>
                </Pref>
                <Pref name="Solids_ImmediateChildren" type="int" title="Dynamics Update Level" value="0" ver="3.0.0" scope="Session">
                    <Description>This option specifies if dynamic update is performed on either the first level child bodies of the parent curve being edited or upon all child bodies of the curve.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="First" value="0"/>
                        <EnumElem name="All" value="1"/>
                    </Data>
                </Pref>
                <Pref name="InterruptUpdate_OnError" type="int" title="Errors" value="2" ver="5.0.0" scope="Session">
                    <Description>This option controls whether or not Edit During Update dialog box will appear when errors are encountered during feature playback.</Description>
                    <Toggle off="2"/>
                    <Data type="enum">
                        <EnumElem name="On" value="1"/>
                        <EnumElem name="Off" value="2"/>
                    </Data>
                    <Docum>This option should be on when the Edit During Update dialog box is required during feature playback. This was the default method in prior releases.</Docum>
                </Pref>
                <Pref name="InterruptUpdate_OnWarning" type="int" title="Warnings" value="2" ver="5.0.0" scope="Session">
                    <Description>This option controls whether or not Edit During Update dialog box will appear when warnings are encountered during feature playback.</Description>
                    <Toggle off="2"/>
                    <Data type="enum">
                        <EnumElem name="On" value="1"/>
                        <EnumElem name="Off" value="2"/>
                    </Data>
                    <Docum>This option should be on when the Edit During Update dialog box is required during feature playback. This was the default method in prior releases.</Docum>
                </Pref>
                <Pref name="Solids_FailOnMissingRefs" type="int" title="Warn on Missing Reference" value="2" ver="3.0.0" scope="Session">
                    <Description>This option controls whether or not warning messages are raised during update if objects that a feature depends upon have been suppressed or modeled away. It is recommended to have this option off while doing general solid modeling.</Description>
                    <Toggle off="2"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="2"/>
                    </Data>
                    <Docum>This option should be on only if it's very important to be notified when feature references are missing (for example within a WAVE control structure). Having this option on may potentially raise lots of warnings that will interrupt feature update. Parts saved in sessions with this option off might exhibit a large number of feature update warnings when updated with this option on. This option does not apply when deleting a feature.</Docum>
                </Pref>
                <Pref name="MakeCurrent_OnError" type="int" title="Make Current Feature on Error during Update" value="2" ver="7.5.0" scope="Session">
                    <Description>This option controls whether or not the update is interrupted and the error feature is made current when errors are encountered during feature playback.</Description>
                    <Toggle off="2"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="2"/>
                    </Data>
                    <Docum>This option should be on when the diagnostic prompt windows are required.</Docum>
                </Pref>
                <Pref name="GenerateFailureReport_OnUpdate" type="int" title="Report Warning and Error after Update" value="1" ver="7.5.0" scope="Session">
                    <Description>This option determines whether or not a feature with a warning or error will be reported after update.</Description>
                    <Toggle off="2"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="2"/>
                    </Data>
                </Pref>
                <Pref name="RollBack_OnInterrupt" type="int" title="Action after Interrupt" value="0" ver="9.0.0" scope="Session">
                    <Description>This option specifies whether a feature update, when it is interrupted, will roll back to the last successfully updated feature and make that feature current, or roll back to the state before the update started.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Make Last Successful Feature Current" value="0"/>
                        <EnumElem name="Undo" value="1"/>
                    </Data>
                </Pref>
                <Pref name="LoadRollbackDataMemory" type="logical" title="Load Rollback Data to Memory" value="false" ver="9.0.1" scope="Session">
                    <Description>This option determines whether or not rollback data, if available in NX, will be loaded to memory instead of disk, up to a limit that is adaptive to the configuration of the hardware.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>           
                </Group>
            </Tab>
            <Tab name="Freeform" csh="PMAN_Modeling_General_Freeform">
                <Group name="Geometry Construction">
                <Pref name="Solids_output_quintic_cv" type="int" title="Curve Fit Method" value="0" ver="4.0.0" scope="Session">
                    <Description>This option specifies the fitting method used when curves must be approximated by splines.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Cubic" value="0"/>
                        <EnumElem name="Quintic" value="1"/>
                        <EnumElem name="Advanced" value="2"/>
                    </Data>
                </Pref>
                <Pref name="Model_Rebuild_Maximum_Degree" type="int" title="Maximum Rebuild Degree" value="7" ver="4.0.0" scope="Session">
                    <Description>This option specifies the maximum degree for surface rebuild.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="3" include="1"/>
                        <UpperBound value="24" include="1"/>
                    </Data>
                </Pref>
                <Pref name="Model_Rebuild_Maximum_Segment" type="int" title="Maximum Rebuild Segments" value="1" ver="4.0.0" scope="Session">
                    <Description>This option specifies the maximum number of segments for surface rebuild.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                        <UpperBound value="999" include="1"/>
                    </Data>
                </Pref>
                <Pref name="Solids_FreeFormConstructionResult" type="int" title="Planar Face Type" value="0" ver="3.0.0" scope="Session">
                    <Description>This option specifies the result of the freeform feature created when using the Through Curves, Through Curve Mesh, Swept, and Ruled commands.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Plane" value="0"/>
                        <EnumElem name="B-surface" value="1"/>
                    </Data>
                </Pref>
                <Pref name="Solids_SurfaceExtension" type="int" title="Surface Extension" value="1" ver="8.5.0" scope="Session">
