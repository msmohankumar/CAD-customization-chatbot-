        <Category name="Human Modeling" csh="PMAN_Modeling_Human_Modeling">
            <Tab name="General" csh="PMAN_Modeling_Human_Modeling_General">
                <Pref name="HM_GENDER" type="int" title="Gender" value="0" ver="5.0.0" scope="Session">
                    <Description>This option specifies the gender of the human to be created.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Male" value="0"/>
                        <EnumElem name="Female" value="1"/>
                    </Data>
                </Pref>
                <Pref name="HM_DATABASE" type="int" title="Database" value="0" ver="5.0.0" scope="Part">
                    <Description>This option specifies the database used to create the human.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="ANSUR" value="0"/>
                        <EnumElem name="NHANES" value="1"/>
                    </Data>
                </Pref>
                <Pref name="HM_STATURE" type="int" title="Stature" value="2" ver="5.0.0" scope="Part">
                    <Description>This option specifies the stature of the human to be created. A percentile means a percentage of the size in the chosen database. Regress means that the human size will be computed. Stature and Weight cannot both be set to Regress.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="99%" value="0"/>
                        <EnumElem name="95%" value="1"/>
                        <EnumElem name="50%" value="2"/>
                        <EnumElem name="5%" value="3"/>
                        <EnumElem name="1%" value="4"/>
                        <EnumElem name="Custom" value="5"/>
                        <EnumElem name="Regress" value="6"/>
                    </Data>
                </Pref>
                <Pref name="HM_WEIGHT" type="int" title="Weight" value="6" ver="5.0.0" scope="Part">
                    <Description>This option specifies the weight of the human to be created. A percentile means a percentage of the weight in the chosen database. Regress means that the human weight will be computed. Stature and Weight cannot both be set to Regress.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="99%" value="0"/>
                        <EnumElem name="95%" value="1"/>
                        <EnumElem name="50%" value="2"/>
                        <EnumElem name="5%" value="3"/>
                        <EnumElem name="1%" value="4"/>
                        <EnumElem name="Custom" value="5"/>
                        <EnumElem name="Regress" value="6"/>
                    </Data>
                </Pref>
                <Pref name="HM_REFERENCE_POINT" type="int" title="Reference Point" value="3" ver="5.0.0" scope="Part">
                    <Description>This option specifies the root site for the human to be created.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Left Eye" value="0"/>
                        <EnumElem name="Right Eye" value="1"/>
                        <EnumElem name="H-point" value="2"/>
                        <EnumElem name="Left Toe" value="3"/>
                        <EnumElem name="Right Toe" value="4"/>
                    </Data>
                </Pref>
                <Pref name="HM_ASSOCIATIVE" type="logical" title="Associative" value="false" ver="5.0.0" scope="Session">
                    <Description>This option determines whether or not the Reference Point should be associative to the human. When this option is on, the human will always update and put its root site at the specified reference point. When this option is off, the reference point will only be used at the creation and ignored in subsequent operations.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Group name="Color">
                <Pref name="HM_HAIR_COLOR" type="int" title="Hair" value="162" ver="6.0.0" scope="Session">
                    <Description>This option specifies the hair color of the human.</Description>
                    <Color/>
                </Pref>
                <Pref name="HM_SKIN_COLOR" type="int" title="Skin" value="74" ver="6.0.0" scope="Session">
                    <Description>This option specifies the skin color of the human.</Description>
                    <Color/>
                </Pref>
                <Pref name="HM_SHIRT_COLOR" type="int" title="Shirt" value="120" ver="6.0.0" scope="Session">
                    <Description>This option specifies the shirt color of the human.</Description>
                    <Color/>
                </Pref>
                <Pref name="HM_PANT_COLOR" type="int" title="Pants" value="191" ver="6.0.0" scope="Session">
                    <Description>This option specifies the pant color of the human.</Description>
                    <Color/>
                </Pref>
                <Pref name="HM_SHOE_COLOR" type="int" title="Shoe" value="173" ver="5.0.0" scope="Session">
                    <Description>This option specifies the shoe color of the human.</Description>
                    <Color/>
                </Pref>
                </Group>
            </Tab>
            <Tab name="Reach Zone" csh="PMAN_Modeling_Human_Modeling_Reach_Zone">
                <Pref name="HM_RZ_MULTIPLE_COLOR" type="logical" title="Multiple Colors" value="true" ver="5.0.0" scope="Session">
                    <Description>This option determines whether or not multiple colors should be shown when creating the reach zone.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Off" value="false"/>
                        <EnumElem name="On" value="true"/>
                    </Data>
                </Pref>
                <Pref name="HM_RZ_COLOR" type="int" title="Color" value="1" ver="5.0.0" scope="Session">
                    <Description>This option specifies the color of the human reach zone.</Description>
                    <Color/>
                </Pref>
                <Pref name="HM_RZ_RESOLUTION" type="int" title="Resolution" value="8" ver="5.0.0" scope="Session">
                    <Description>This option specifies the resolution of the reach zone display. The reach zones are expressed in the form of facet bodies.</Description>
                    <IntegerScale/>
                    <Data type="range">
                        <LowerBound value="2" include="1"/>
                        <UpperBound value="10" include="1"/>
                    </Data>
                </Pref>
                <Group name="Waist Rotation">
                <Pref name="HM_RZ_INCLUDE_WAIST_ROT" type="logical" title="Include Waist Rotation" value="false" ver="5.0.0" scope="Session">
                    <Description>This option determines whether or not the waist rotation should be considered when creating the reach zone. When this option is off, Waist Flex Angle and Waist Lateral Angle are ignored.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Pref name="HM_RZ_FLEX_ANGLE" type="real" title="Waist Flex Angle" value="0.0" ver="5.0.0" scope="Session">
                    <Description>This option specifies the waist flexion angle. This option applies when Include Waist Rotation is on.</Description>
                    <RealScale precision="1"/>
                    <Data type="range">
                        <LowerBound value="-52.0" include="1"/>
                        <UpperBound value="85.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="HM_RZ_LATERAL_ANGLE" type="real" title="Waist Lateral Angle" value="5.0" ver="5.0.0" scope="Session">
                    <Description>This option specifies the waist lateral angle. This option applies when Include Waist Rotation is on.</Description>
                    <RealScale precision="1"/>
                    <Data type="range">
                        <LowerBound value="-40.0" include="1"/>
                        <UpperBound value="40.0" include="1"/>
                    </Data>
                </Pref>
                </Group>
                <Pref name="HM_RZ_ASSOCIATIVE" type="logical" title="Associative" value="true" ver="5.0.0" scope="Session">
                    <Description>This option determines whether or not the created reach zones should be associated to the human.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Posture Prediction" csh="PMAN_Modeling_Human_Modeling_Posture_Prediction">
                <Pref name="HM_PRP_METHOD" type="int" title="Prediction Method" value="0" ver="5.0.0" scope="Part">
                    <Description>This option specifies the method for posture prediction.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="ASPECT H-method" value="0"/>
                        <EnumElem name="Pre-ASPECT H-method" value="1"/>
                    </Data>
                </Pref>
                <Pref name="HM_PRP_SEAT_TYPE" type="int" title="Seat Type" value="0" ver="5.0.0" scope="Part">
                    <Description>This option specifies the seat type for posture prediction.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Fixed" value="0"/>
                        <EnumElem name="Adjustable" value="1"/>
                    </Data>
                </Pref>
                <Pref name="HM_PRP_CUSHION" type="int" title="Cushion" value="1" ver="5.0.0" scope="Part">
                    <Description>This option specifies the cushion for posture prediction.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Soft" value="0"/>
                        <EnumElem name="Medium" value="1"/>
                        <EnumElem name="Firm" value="2"/>
                    </Data>
                </Pref>
                <Pref name="HM_PRP_SHOW_REPORT" type="logical" title="Display Posture Prediction Report" value="false" ver="5.0.0" scope="Session">
                    <Description>This option determines whether or not the posture prediction report should be shown.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
            </Tab>
        </Category>
        <Category name="NX Realize Shape" csh="PMAN_Modeling_NX_Realize_Shape">
          <Tab name="Control Cage Display" csh="PMAN_Modeling_NX_Realize_Shape_Control_Cage_Display">
            <Pref name="SubdivisionModeling_LineColor" type="int" title="Line Color" value="134" ver="10.0.0" scope="Part">
              <Description>This option specifies the color of the control cage edges.</Description>
              <Color/>
            </Pref>
            <Pref name="SubdivisionModeling_LineFont" type="int" title="Line Style (Smooth Edge)" value="5" ver="1847" scope="Part">
              <Description>This option specifies the line style of the control cage smooth edges.</Description>
              <GeneralLineFont LineFontOptionMaskString="GeneralLineFonts"/>
            </Pref>
            <Pref name="SubdivisionModeling_SharpEdgeLineFont" type="int" title="Line Style (Sharp Edge)" value="1" ver="1847" scope="Part">
              <Description>This option specifies the line style of the control cage sharp edges.</Description>
              <GeneralLineFont LineFontOptionMaskString="GeneralLineFonts"/>
            </Pref>
            <Pref name="SubdivisionModeling_LineWidth" type="int" title="Line Width" value="6" ver="9.0.0" scope="Part">
              <Description>This option specifies the line width of the control cage edges.</Description>
              <ObjectLineWidth allowDefaultWidth="false" allowWideLineWidth="true"/>
            </Pref>
            <Pref name="SubdivisionModeling_ShowWeight" type="logical" title="Show Weight" value="false" ver="10.0.0" scope="Part">
              <Description>This option specifies if weights for the edges will be displayed.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="Yes" value="true"/>
                <EnumElem name="No" value="false"/>
              </Data>
            </Pref>
            <Pref name="SubdivisionModeling_VertexColor" type="int" title="Vertex Color" value="134" ver="10.0.0" scope="Part">
              <Description>This option specifies the color of the control cage vertices.</Description>
              <Color/>
            </Pref>
            <Pref name="SubdivisionModeling_VertexStyle" type="int" title="Vertex Style" value="2" ver="10.0.0" scope="Part">
              <Description>This option specifies the shape of the control cage vertices.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="Square" bitmap="square.bmp" value="0"/>
                <EnumElem name="Triangle" bitmap="triangleup.bmp" value="1"/>
                <EnumElem name="Circle" bitmap="circle.bmp" value="2"/>
                <EnumElem name="Plus" bitmap="plus.bmp" value="3"/>
                <EnumElem name="Cross" bitmap="delete.bmp" value="4"/>
              </Data>
            </Pref>
            <Pref name="SubdivisionModeling_VertexSize" type="int" title="Vertex Size" value="0" ver="9.0.0" scope="Part">
              <Description>This option specifies the size of the control cage vertices.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="Small" value="0"/>
                <EnumElem name="Medium" value="1"/>
                <EnumElem name="Large" value="2"/>
              </Data>
            </Pref>
            <Pref name="SubdivisionModeling_ConstraintColor" type="int" title="Constraint Color" value="31" ver="1953" scope="Part">
              <Description>This option specifies the markup color of constrained control cage elements.</Description>
              <Color/>
            </Pref>
            <Pref name="SubdivisionModeling_XRayCage" type="logical" title="X-Ray Cage" value="true" ver="9.0.0" scope="Part">
              <Description>This option specifies if elements of the control cage (hidden by the subdivision body) will shine through the subdivision body.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="On" value="true"/>
                <EnumElem name="Off" value="false"/>
              </Data>
            </Pref>
            <Pref name="SubdivisionModeling_HighlightFaceTranslucency" type="int" title="Highlight Face Translucency" value="50" unit="%" ver="9.0.0" scope="Part">
              <Description>This option specifies the translucency of control cage faces which are shaded temporarily by selection highlight.</Description>
              <IntegerScale/>
              <Data type="range">
                <LowerBound value="0" include="1"/>
                <UpperBound value="100" include="1"/>
              </Data>
            </Pref>
            <Pref name="SubdivisionModeling_AllowBackSideSelection" type="logical" title="Allow Backside Selection" value="true" ver="10.0.0" scope="Session">
              <Description>This option specifies if elements of the control cage hidden behind other elements will be selectable.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="Yes" value="true"/>
                <EnumElem name="No" value="false"/>
              </Data>
            </Pref>
          </Tab>
          <Tab name="Cage Polyline Display" csh="PMAN_Modeling_NX_Realize_Shape_Cage_Polyline_Display">
            <Pref name="SubdivisionModeling_CagePolylineColor" type="int" title="Line Color" value="134" ver="11.0.0" scope="Part">
              <Description>This option specifies the color of the cage polylines.</Description>
              <Color/>
            </Pref>
            <Pref name="SubdivisionModeling_CagePolylineFont" type="int" title="Line Style" value="2" ver="1847" scope="Part">
              <Description>This option specifies the line style of the cage polylines.</Description>
              <GeneralLineFont LineFontOptionMaskString="GeneralLineFonts"/>
            </Pref>
            <Pref name="SubdivisionModeling_CagePolylineWidth" type="int" title="Line Width" value="6" ver="11.0.0" scope="Part">
              <Description>This option specifies the line width of the cage polylines.</Description>
              <ObjectLineWidth allowDefaultWidth="false" allowWideLineWidth="true"/>
            </Pref>
            <Pref name="SubdivisionModeling_CagePolylineVertexColor" type="int" title="Vertex Color" value="134" ver="11.0.0" scope="Part">
              <Description>This option specifies the color of the cage polyline vertices.</Description>
              <Color/>
            </Pref>
            <Pref name="SubdivisionModeling_CagePolylineVertexStyle" type="int" title="Vertex Style" value="0" ver="11.0.0" scope="Part">
              <Description>This option specifies the shape of the cage polyline vertices.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="None" bitmap="none.bmp" value="0"/>
                <EnumElem name="Square" bitmap="square.bmp" value="1"/>
                <EnumElem name="Triangle" bitmap="triangleup.bmp" value="2"/>
                <EnumElem name="Circle" bitmap="circle.bmp" value="3"/>
                <EnumElem name="Plus" bitmap="plus.bmp" value="4"/>
                <EnumElem name="Cross" bitmap="delete.bmp" value="5"/>
              </Data>
            </Pref>
            <Pref name="SubdivisionModeling_CagePolylineVertexSize" type="int" title="Vertex Size" value="0" ver="11.0.0" scope="Part">
              <Description>This option specifies the size of the cage polyline vertices.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="Small" value="0"/>
                <EnumElem name="Medium" value="1"/>
                <EnumElem name="Large" value="2"/>
              </Data>
            </Pref>
          </Tab>
        </Category>
        <Category name="Part Families" csh="PMAN_Modeling_Part_Families">
          <Tab name="All" csh="PMAN_Modeling_Part_Families_All">
            <Pref name="SubdivisionModeling_KeepAllAddInsInSpreadsheet" type="logical" title="Keep All Part Family Add-ins in Spreadsheet" value="false" ver="1953" scope="Session">
              <Description>This option determines whether or not all add-ins are preserved in the spreadsheet.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="Yes" value="true"/>
                <EnumElem name="No" value="false"/>
              </Data>
            </Pref>
            <Pref name="SubdivisionModeling_GeneratePreviewForFamilyMember" type="logical" title="Generate Preview Image for Each Part Family Member" value="false" ver="1953" scope="Session">
              <Description>This option determines whether or not a preview image should be generated for each part family member.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="Yes" value="true"/>
                <EnumElem name="No" value="false"/>
              </Data>
            </Pref>
            <Pref name="SubdivisionModeling_PropagatedChangesToFamilyMember" type="logical" title="Propagate Changes from Template to Members" value="false" ver="2312" scope="Session">
              <Description>This option determines whether or not changes to a part family template should propagate to all existing members open in the session.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="Yes" value="true"/>
                <EnumElem name="No" value="false"/>
              </Data>
            </Pref>
          </Tab>
        </Category>
    </Application>
</PrefDefinition>


