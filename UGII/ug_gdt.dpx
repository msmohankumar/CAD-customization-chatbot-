        <Category name="PMI Section View" csh="PMAN_PMI_Section_View">
            <Tab name="Section View" csh="PMAN_PMI_Section_View_Section_View">
                <Group name="General">
                    <Pref name="PMI_LightweightSectionView_NamePrefix" type="string" title="User Defined Section Name Prefix " value="&quot;Section &quot;" ver="11.0.0" scope="Part">
                        <Description>This option specifies the prefix for the name of a PMI section view when the name of the view is user defined.</Description>
                        <String/>
                    </Pref>
                    <Pref name="PMI_LightweightSectionView_AssociativePlane" type="logical" title="Associative Plane" value="true" ver="11.0.0" scope="Part">
                        <Description>This option determines whether or not an option will be available to make the clipping plane associative to the selected geometry when defining a PMI section view.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="PMI_LightweightSectionView_ClipCoplanarObjects" type="logical" title="Clip Coplanar Objects" value="false" ver="11.0.0" scope="Part">
                        <Description> This option determines whether or not coplanar objects are clipped in plane-only PMI section views.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="PMI_SectionView_ClipWireframeObjects" type="logical" title="Clip Wireframe Objects" value="true" ver="11.0.1" scope="Part">
                        <Description>This option determines whether or not points, PMI regions, sketch curves, wireframe curves, and thread curves will be clipped for PMI section views.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Cap">
                    <Pref name="PMI_LightweightSectionView_Cap" type="int" title="Cap Display" value="1" ver="7.5.0" scope="Part">
                        <Description>This option specifies if and how a section cap is displayed. A cap is an area of solid color drawn onto the cutting surface of the section, creating a "pseudo face" where the section clipping planes intersect the model.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="Off" value="0"/>
                            <EnumElem name="Specify Custom Color" value="1"/>
                            <EnumElem name="Body Color" value="2"/>
                        </Data>
                    </Pref>
                    <Pref name="PMI_LightweightSectionView_CapColor" type="int" title="Custom Cap Color" value="9" ver="7.5.0" scope="Part">
                        <Description>This option specifies the color of the section cap when Cap Display is set to Specify Custom Color.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="PMI_LightweightSectionView_TranslucentSectionPlane" type="int" title="Translucent Section Plane" value="1" ver="7.5.0" scope="Part">
                        <Description>This option specifies if the active plane should be displayed as a translucent plane when the cap is off.</Description>
                        <Toggle off="0"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="1"/>
                            <EnumElem name="No" value="0"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Crosshatch">
                    <Pref name="PMI_LightweightSectionView_DisplayCrosshatch" type="logical" title="Display Crosshatch" value="false" ver="2007" scope="Part">
                        <Description>This option determines whether or not cut faces resulting from the section cut should display crosshatch.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="PMI_LightweightSectionView_PatternDefinedBy" type="int" title="Pattern Defined By" value="0" ver="2007" scope="Part">
                        <Description>This option specifies the pattern definition to be used for displaying crosshatch.</Description>
                        <RadioBox orientation="horizontal"/>
                        <Data type="enum">
                            <EnumElem name="User Specified" value="0"/>
                            <EnumElem name="Material" value="1"/>
                        </Data>
                    </Pref>
                    <Pref name="PMI_LightweightSectionView_AssemblyCrosshatching" type="logical" title="Assembly Crosshatching" value="0" ver="11.0.0" scope="Part">
                        <Description>This option determines whether or not the crosshatching angle for adjacent components in an assembly section view should vary to provide better visualization of each component in the view.</Description>
                        <Toggle off="0"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="1"/>
                            <EnumElem name="No" value="0"/>
                        </Data>
                    </Pref>
                    <Pref name="PMI_LightweightSectionView_restrictCrosshatchAngle" type="logical" title="Restrict Crosshatch to +/- 45 degrees" value="0" ver="9.0.0" scope="Part">
                        <Description>This option determines whether assembly crosshatch angles should be restricted to + or - 45 degrees.</Description>
                        <Toggle off="0" />
                        <Data type="enum">
                            <EnumElem name="Yes" value="1"/>
                            <EnumElem name="No" value="0"/>
                        </Data>
                    </Pref>
                    <Pref name="PMI_LightweightSectionView_xhatchAdjacencyToler_MCU" type="real" title="Crosshatch Adjacency Tolerance" value="1.25" unit="μm" ver="2306" scope="Part">
                        <Description>This option specifies the tolerance that determines the crosshatching angle of adjacent components in an assembly section view. If the adjacent component is at a distance less than the specified tolerance, the system automatically calculates a different crosshatching angle for that component.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="PMI_LightweightSectionView_xhatchAdjacencyToler_MU" type="real" title="Crosshatch Adjacency Tolerance" value="1.25" unit="mm" ver="11.0.0" scope="Part">
                        <Description>This option specifies the tolerance that determines the crosshatching angle of adjacent components in an assembly section view. If the adjacent component is at a distance less than the specified tolerance, the system automatically calculates a different crosshatching angle for that component.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="PMI_LightweightSectionView_xhatchAdjacencyToler_SIU" type="real" title="Crosshatch Adjacency Tolerance" value="1.25" unit="m" ver="2306" scope="Part">
                        <Description>This option specifies the tolerance that determines the crosshatching angle of adjacent components in an assembly section view. If the adjacent component is at a distance less than the specified tolerance, the system automatically calculates a different crosshatching angle for that component.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="0.0" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="PMI_LightweightSectionView_xhatchAdjacencyToler_EU" type="real" title="Crosshatch Adjacency Tolerance" value="0.05" unit="in" ver="11.0.0" scope="Part">
                        <Description>This option specifies the tolerance that determines the crosshatching angle of adjacent components in an assembly section view. If the adjacent component is at a distance less than the specified tolerance, the system automatically calculates a different crosshatching angle for that component.</Description>
