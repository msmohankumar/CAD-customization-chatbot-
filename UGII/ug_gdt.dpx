<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="docum.xsl"?>
<!--

-->
<PrefDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www-cyp.ugsolutions.com/assemblies/pman">
    <Application name="PMI" csh="PMAN_PMI" package_suppress="simviewer,view,mechatronicsplayer,nx2dedit">
        <Category name="PMI General/Setup" csh="PMAN_PMI_PMI_General">
            <Tab name="Miscellaneous" csh="PMAN_PMI_PMI_General_Miscellaneous">         
               <Pref name="PMI_DelayPMILoad" type="logical" title="Delay Component PMI Loading" value="true" ver="2206" scope="Session">
                 <Description>This option determines whether or not to delay loading component PMI when loading an assembly part.</Description>
                 <Toggle off="false"/>
                 <Data type="enum">
                   <EnumElem name="Yes" value="true"/>
                   <EnumElem name="No" value="false"/>
                 </Data>
               </Pref>              
                <Group name="Disclosures">
                  <Pref name="PMI_DiscloseUserDefinedModelViews" type="logical" title="User Defined Model Views" value="false" ver="10.0.3" scope="Part">
                    <Description>This option determines whether or not user defined model views are marked as candidates for inclusion in a disclosure when they are created.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                      <EnumElem name="Off" value="false"/>
                      <EnumElem name="On" value="true"/>
                    </Data>
                  </Pref>
                  <Pref name="PMI_DiscloseCannedModelViews" type="logical" title="Canned Model Views" value="false" ver="10.0.3" scope="Session">
                    <Description>This option determines whether or not canned model views are marked as candidates for inclusion in a disclosure when a part is created.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                      <EnumElem name="Off" value="false"/>
                      <EnumElem name="On" value="true"/>
                    </Data>
                  </Pref>
                  <Pref name="PMI_DisclosurePurpose" type="string" title="Disclosure Purpose" value="&quot;Design Review&quot;" ver="10.0.3" scope="Part">
                    <Description>This option specifies the disclosure purpose for model views when they are marked as publish for disclosure candidates. The "Disclosure Purpose" attribute will not be added to a model view unless its type is included by one of the above options.</Description>
                    <String/>
                  </Pref>
                </Group>

              <Group name="Part Families">
                <Pref name="PMI_SuppressRetainedPMI" type="logical" title="Suppress Retained PMI" value="false" ver="12.0.0" scope="Part">
                  <Description>This option determines whether or not retained PMI in part families' members are suppressed.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="Yes" value="true"/>
                    <EnumElem name="No" value="false"/>
                  </Data>
                </Pref>
                <Pref name="PMI_DeleteEmptyUserDefinedModelViews" type="logical" title="Delete Empty User Defined Model Views" value="false" ver="12.0.0" scope="Part">
                  <Description>This option determines whether or not empty user defined model views in part families' members are deleted.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="Yes" value="true"/>
                    <EnumElem name="No" value="false"/>
                  </Data>
                </Pref>
              </Group>
              <Group name="Neutral File Formats">                
                <Pref name="PMI_CreatePMIFromJT" type="logical" title="Create PMI and Model Views" value="true" ver="1899" scope="Session">
                  <Description>This option determines whether or not to create PMI and model views when reading data from a neutral file format such as a JT or STEP file.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="Yes" value="true"/>
                    <EnumElem name="No" value="false"/>
                  </Data>
                </Pref>
              </Group>
              <Pref name="UG_saveAnn3DDataInPart" type="int" title="Save 3D Annotations" value="0" ver="3.0.0" scope="Part">
                  <Description>This option determines whether or not PMI data should be saved for retrieval outside the context of NX.</Description>
                  <Toggle off="0"/>
                  <Data type="enum">
                      <EnumElem name="Yes" value="1"/>
                      <EnumElem name="No" value="0"/>
                  </Data>
              </Pref>
              <Pref name="PMI_CreateRealPMIFromJT" type="logical" title="Create Real PMI Objects from Neutral File Formats" value="true" ver="7.5.0" scope="Session">
                  <Description>This option determines whether or not NX will try to create real PMI objects when reading data from a neutral file format such as a JT or STEP file.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                      <EnumElem name="Yes" value="true"/>
                      <EnumElem name="No" value="false"/>
                  </Data>
              </Pref>
              <Pref name="PMI_GeometrySharing" type="logical" title="PMI Support for Geometry Sharing" value="false" ver="8.5.0" scope="Part">
                  <Description>This option determines whether or not PMI includes shared geometry when highlighting, querying, or reporting associated objects.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                      <EnumElem name="Yes" value="true"/>
                      <EnumElem name="No" value="false"/>
                  </Data>
              </Pref>
              <Pref name="PMI_RuleWelcomePage_MaxSessionCount" type="int" title="Logic Editor Welcome Page Duration" value="5" unit="Times" ver="1953" scope="Session">
                <Description>This option determines the number of NX sessions that will display the Logic Editor Welcome Page when launching the Logic Editor. Setting its value to 0 (zero) will result in it never being displayed.</Description>
                <Integer/>
                <Data type="range">
                  <LowerBound value="0" include="1"/>
                </Data>
              </Pref>
            </Tab>
            <Tab name="Display" csh="PMAN_PMI_PMI_General_Display">
                <Group name="Create">
                    <Pref name="PMI_ViewIndependent" type="logical" title="Display in all Views" value="false" ver="5.0.0" scope="Part">
                        <Description>This option determines whether or not PMI should be displayed in all views on create.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="PMAN_PMI_General_Prediction_Dir_WIN" type="string" title="Prediction Directory" value="${UGII_BASE_DIR}\mbd\prediction\models" ver="2206" scope="Session">
                        <Description>This field specifies the directory where the PMI prediction files are located. The files are used by the PMI prediction machine learning algorithm to predict PMI annotation plane and model views.</Description>
                        <BrowseDirSel/>
                    </Pref>
                    <Pref name="PMAN_PMI_General_Prediction_Dir_UNX" type="string" title="Prediction Directory" value="${UGII_BASE_DIR}/mbd/prediction/models" ver="2206" scope="Session">
                        <Description>This field specifies the directory where the PMI prediction files are located. The files are used by the PMI prediction machine learning algorithm to predict PMI annotation plane and model views.</Description>
                        <BrowseDirSel/>
                    </Pref>
                </Group>
                <Group name="Annotation Plane">
                    <Pref name="PMI_DisplayAnnotationPlane" type="int" title="Display" value="1" ver="1872" scope="Session">
                        <Description>This option specifies if annotation planes will be displayed while creating and editing PMI.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="All" value="1"/>
                            <EnumElem name="Active Plane Only" value="2"/>
                            <EnumElem name="None" value="3"/>
                        </Data>
                    </Pref>
                    <Pref name="PMI_AnnotationPlane" type="int" title="Plane" value="2" ver="5.0.0" scope="Part">
                        <Description>This option specifies the annotation plane that will be used to display PMI.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="XY Plane of WCS" value="1"/>
                            <EnumElem name="XZ Plane of WCS" value="3"/>
                            <EnumElem name="YZ Plane of WCS" value="4"/>
                            <EnumElem name="Model View Plane" value="2"/>
                        </Data>
                    </Pref>
                    <Pref name="PMI_AnnotationPlaneTranslucency" type="int" title="Translucency" value="60" unit="%" ver="1872" scope="Part">
                        <Description>This option specifies the translucency of the annotation plane preview.</Description>
                        <IntegerScale/>
                        <Data type="range">
                            <LowerBound value="30" include="1"/>
                            <UpperBound value="100" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="PMI_AnnotationPlaneBoundaryColor" type="int" title="Color" value="142" ver="1872" scope="Part">
                        <Description>This option specifies the color of the annotation plane preview.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="PMI_AnnotationPlaneBoundaryFont" type="int" title="Style" value="1" ver="1980" scope="Part">
                        <Description>This option specifies the line style for the active annotation plane preview boundary line.</Description>
                        <GeneralLineFont LineFontOptionMaskString="MBDLineFonts"/>
                    </Pref>
                    <Pref name="PMI_ProjectontoPlane" type="logical" title="Project onto Plane" value="true" ver="1872" scope="Part">
                        <Description>This option determines if a PMI object should be projected onto the specified annotation plane.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="PMI_ShowOrientXpress" type="logical" title="Show OrientXpress" value="false" ver="1872" scope="Session">
                        <Description>This option determines whether OrientXpress will be displayed while creating or editing PMI.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Display">
                    <Pref name="PMI_Display_Shaded_Models" type="logical" title="Display Through Shaded Models" value="true" ver="4.0.0" scope="Part">
                        <Description>This option determines how PMI is displayed in a shaded model. When on, PMI will show through the shaded model.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="PMI_Update_Reading_Direction" type="logical" title="Update Reading Direction after View Rotate" value="true" ver="5.0.0" scope="Session">
                        <Description>This option determines whether or not PMI text reading direction should be updated in response to view rotation.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="PMI_ExcludeCustomSymbolsFromReadingDirection" type="logical" title="Exclude Custom Symbols from Reading Direction Update" value="false" ver="7.5.2" scope="Session">
                        <Description>This option determines whether to exclude custom symbols from PMI reading direction updates.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                  <Pref name="PMI_DisplayBackground" title="PMI Background" type="logical" value="false" ver="1872" scope="Part">
                    <Description>This option determines whether or not to display supported PMI with background.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                      <EnumElem name="Yes" value="true"/>
                      <EnumElem name="No" value="false"/>
                    </Data>
                  </Pref>
                  <Pref name="PMI_Background_Color" type="int" title="PMI Background Color" value="50" ver="1872" scope="Part">
                    <Description>This option specifies the color of the PMI background.</Description>
                    <Color/>
                  </Pref>
                </Group>
                <Group name="Resize Options">
                    <Pref name="PMI_EnableResize" type="logical" title="Allow Resize" value="true" ver="10.0.0" scope="Part">
                        <Description>This option determines whether the resize function for PMI is allowed.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                              <EnumElem name="No" value="false"/>
                              <EnumElem name="Yes" value="true"/>                        
                        </Data>
                    </Pref>
                    <Pref name="PMI_ResizeOnCreate" type="logical" title="Resize on Create" value="false" ver="8.0.0" scope="Part">
                        <Description>This option determines whether PMI are resized when they are created.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="PMI_ResizeOnViewSave" type="logical" title="Resize on View Save" value="false" ver="8.0.0" scope="Part">
                        <Description>This option determines whether PMI are resized during a view save operation.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="PMI_ResizeMethod" type="int" title="Resize Settings" value="1" ver="11.0.0" scope="Part">
                        <Description>This option specifies the method used to resize PMI.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="Relative to Saved View Scale" value="0"/>
                            <EnumElem name="Relative to View Zoom Factor" value="1"/>
                            <EnumElem name="Independently" value="2"/>
                            <EnumElem name="Relative to Part Preferences" value="3"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Objects" csh="PMAN_PMI_PMI_General_Objects">
                <Group name="Datum Label">
                    <Pref name="PMI_DatumStartingLetter" type="string" title="Starting Letter" value="A" ver="4.0.0" scope="Part">
                        <Description>This option specifies the initial letter or letter sequence to use for a PMI datum label. Up to five letters may be specified.</Description>
                        <String/>
                    </Pref>
                </Group>
                <Group name="Coordinate Note">
                    <Pref name="PMI_CoordinateNoteDecimalPlaces" type="int" title="Decimal Places" value="4" ver="6.0.0" scope="Part">
                        <Description>This option specifies the number of decimal places that a coordinate note will display.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="0" include="1"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Advisor" csh="PMAN_PMI_PMI_General_Advisor">
                <Group name="Advisor Settings">
                    <Pref name="PMI_AdvisorCheckingMode" type="int" title="Checking Mode" value="2" ver="1953" scope="Session">
                        <Description>This option specifies the PMI Advisor checking behavior.
<Formatted>
Advise: Checks supported PMI for errors. Displays alerts during creation and edit of PMI on error.
Enforce: Checks supported PMI for errors. Displays alerts and prevents creation and edit of PMI on error.
Off: Deactivates PMI Advisor checking.
</Formatted>
                        </Description>
                      <OptionMenu/>
                      <Data type="enum">
                        <EnumElem name="Advise" value="0"/>
                        <EnumElem name="Enforce" value="1"/>
                        <EnumElem name="Off" value="2"/>
                      </Data>
                    </Pref>
                    <Pref name="PMI_AdvisorActiveStandard" type="int" title="Active Standard" value="0" ver="1953" scope="Session">
                        <Description>This option specifies the standard that the PMI Advisor should be checking PMI against.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                          <EnumElem name="ISO" value="0"/>
                          <EnumElem name="ASME" value="1"/>
                        </Data>
                    </Pref>
                  <Pref name="PMI_AdvisorNavigatorAlerts" type="int" title="Navigator Alerts" value="0" ver="1980" scope="Session">
                    <Description>This option specifies which types of checking failures are displayed in the alert column of the MBD Navigator.
<Formatted>
All: The navigator displays all PMI Advisor failures.
Only Errors: The navigator displays only PMI Advisor errors.
None: Deactivates display of PMI Advisor failures in the navigator.
</Formatted>
                    </Description>
                    <OptionMenu/>
                    <Data type="enum">
                      <EnumElem name="All" value="0"/>
                      <EnumElem name="Only Errors" value="1"/>
                      <EnumElem name="None" value="2"/>
                    </Data>
                  </Pref>
                  <Pref name="PMI_AdvisorAllowCheckConfiguration" type="logical" title="Allow Configuration of Checks" value="false" ver="1980" scope="Session">
                      <Description>This option determines whether or not the PMI Advisor Configuration command allows changes to checking behavior.</Description>
                      <Toggle off="false"/>
                      <Data type="enum">
                          <EnumElem name="Yes" value="true"/>
                          <EnumElem name="No" value="false"/>
                      </Data>
                   </Pref>
                </Group>
                <Group name="Configuration Settings">
                    <Pref name="PMI_AdvisorActiveConfiguration" type="string" title="Active Configuration" value="Default" ver="2206" scope="Session">
                        <Description>This option specifies the active checking configuration. If the one specified does not exist, the Default checking configuration will be used.</Description>
                        <String/>
                    </Pref>
                    <Pref name="PMI_AdvisorConfigurationSaveType" type="int" title="Save Location" value="0" ver="2206" scope="Session">
                        <Description>This option specifies if PMI advisor configurations should be stored in Teamcenter or the local file system.</Description>
                        <RadioBox orientation="horizontal"/>
                            <Data type="enum">
                                <EnumElem name="Local File System" value="0"/>
                                <EnumElem name="Teamcenter" value="1"/>
                            </Data>
                    </Pref>
                    <Pref name="PMI_AdvisorConfigurationSaveLocation_TC" type="string" title="Teamcenter" value="infodba:Advisor Configurations" ver="2206" scope="Session">
                        <Description>This option specifies the location where PMI advisor configurations are saved in Teamcenter.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="PMI_AdvisorConfigurationSaveLocation_UNX" type="string" title="Local File System" value="${UGII_TMP_DIR}" ver="2206" scope="Session">
                        <Description>This option specifies the location where PMI advisor configurations are saved in the local file system.</Description>
                        <BrowseDirSel/>
                    </Pref>
                    <Pref name="PMI_AdvisorConfigurationSaveLocation_WIN" type="string" title="Local File System" value="${UGII_TMP_DIR}" ver="2206" scope="Session">
                        <Description>This option specifies the location where PMI advisor configurations are saved in the local file system.</Description>
                        <BrowseDirSel/>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Effectivity" csh="PMAN_PMI_PMI_General_Effectivity">
                <Pref name="PMI_InferEffectivity" type="logical" title="Infer PMI Effectivity" value="false" ver="8.5.1" scope="Session">
                    <Description>This option determines whether or not the effectivity of Assembly PMI will be inferred based on referenced geometry for Assemblies loaded from Teamcenter.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="PMI_EffectivityOption" type="int" title="Display PMI Associated Only to Primary Geometry" value="1"  ver="8.5.1" scope="Session">
                    <Description>This option provides additional control over the display of PMI associated to primary geometry when Effectivity is being inferred.</Description>
                    <RadioBox orientation="vertical"/>
                    <Data type="enum">
                        <EnumElem name="If All Associated Occurrences are Loaded" value="0"/>
                        <EnumElem name="If Any Associated Occurrences are Loaded" value="1"/>
                    </Data>
                </Pref>
                <Pref name="PMI_InferEffectivityIncludeReferenceComponents" type="logical" title="Display PMI Associated to Components Loaded for Reference" value="false" ver="8.5.1" scope="Session">
                    <Description>This option determines whether or not PMI objects, which are associated to components that have been loaded for reference, are considered for display.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="PMI_EffectivityReferenceOption" type="int" title="Display PMI Associated to Reference Geometry" value="0"  ver="8.5.1" scope="Session">
                    <Description>This option provides additional control over the display of PMI associated to reference geometry when Effectivity is being inferred.</Description>
                    <RadioBox orientation="vertical"/>
                    <Data type="enum">
                        <EnumElem name="If All Associated Occurrences are Loaded" value="0"/>
                        <EnumElem name="If Any Associated Occurrences are Loaded" value="1"/>
                    </Data>
                </Pref>
                <Pref name="PMI_EffectivityListFilteredPMIInPartNavigator" type="logical" title="Display Effectivity Filtered Nodes in Part and MBD Navigators" value="false"  ver="8.5.1" scope="Session">
                    <Description>This option determines whether or not the Effectivity Filtered PMI nodes are displayed in the Part and MBD Navigators.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Parallel to Screen" csh="PMAN_PMI_PMI_General_ParallelToScreen">
                <Group name="Orientation">
                    <Pref name="PMI_DisplayParallelToScreen" title="Display Parallel to Screen" type="logical" value="false" ver="11.0.1" scope="Part">
                        <Description>This option determines whether or not to display supported PMI parallel to screen.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="PMI_LockPanZoom" title="Lock Size and Position" type="logical" value="false" ver="11.0.1" scope="Part">
                        <Description>When panning and zooming, this option determines whether or not to lock the display (size and location) relative to the graphics area for PMI displayed as parallel to screen.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                </Group>
                <Pref name="PMI_ParallelToScreenTypes" type="list_sel_reorder_value" title="Included PMI Types" value="&quot;Notes:1&quot;\&quot;General Notes:1&quot;\&quot;Specific Notes:1&quot;\&quot;Enterprise Identification:1&quot;\&quot;Material Specification:1&quot;\&quot;Part Identification:1&quot;\&quot;Process Specification:1&quot;\&quot;URL Notes:1&quot;\&quot;String Notes:1&quot;\&quot;Number Notes:1&quot;\&quot;Integer Notes:1&quot;\&quot;Balloons:1&quot;\&quot;Custom Symbols:1&quot;\&quot;Government Security Information:1&quot;\&quot;Company Specific Proprietary Information:1&quot;\&quot;Export Control:1&quot;\&quot;Tables:1&quot;\&quot;Coordinate Notes:1&quot;\&quot;Cutting Plane Symbol:1&quot;\&quot;Feature Control Frames:1&quot;" ver="2306" scope="Part">
                    <Description>This list specifies which PMI types should be allowed to display parallel to screen.</Description>
                    <ListSelReorder height="20" reorderable="false" selectable="true"/>
                    <Data type="enum">
                        <EnumElem name="Notes" value="Notes"/>                        
                        <EnumElem name="General Notes" value="General Notes"/>
                        <EnumElem name="Specific Notes" value="Specific Notes"/>
                        <EnumElem name="Enterprise Identification" value="Enterprise Identification"/>
                        <EnumElem name="Material Specification" value="Material Specification"/>
                        <EnumElem name="Part Identification" value="Part Identification"/>
                        <EnumElem name="Process Specification" value="Process Specification"/>
                        <EnumElem name="URL Notes" value="URL Notes"/>
                        <EnumElem name="String Notes" value="String Notes"/>
                        <EnumElem name="Number Notes" value="Number Notes"/>
                        <EnumElem name="Integer Notes" value="Integer Notes"/>
                        <EnumElem name="Balloons" value="Balloons"/>
                        <EnumElem name="Custom Symbols" value="Custom Symbols"/>
                        <EnumElem name="Government Security Information" value="Government Security Information"/>
                        <EnumElem name="Company Specific Proprietary Information" value="Company Specific Proprietary Information"/>
                        <EnumElem name="Export Control" value="Export Control"/>
                        <EnumElem name="Tables" value="Tables"/>
                        <EnumElem name="Coordinate Notes" value="Coordinate Notes"/>
                        <EnumElem name="Cutting Plane Symbol" value="Cutting Plane Symbol"/>      
                        <EnumElem name="Feature Control Frames" value="Feature Control Frames"/>   
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Feature Template" csh="PMAN_PMI_PMI_General_FeatureTemplate">
                <Group name="Settings">
                  <Pref name="PMI_DeleteTemplatePMI" type="logical" title="Delete Feature Template PMI" value="false" ver="2212" scope="Session">
                    <Description>This option determines whether to delete PMI when their owning feature template is deleted.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                      <EnumElem name="Yes" value="true"/>
                      <EnumElem name="No" value="false"/>
                    </Data>
                  </Pref>
                </Group>
            </Tab>
            <Tab name="Context" csh="PMAN_PMI_PMI_General_Context">
                <Group name="Reuse">
                  <Pref name="PMI_AllowCreateContext" type="logical" title="Create Context" value="false" ver="2212" scope="Session">
                    <Description>This option determines whether or not to automatically create a context for linked PMIs, such as WAVE-linked PMI, PMI Mirror, and PMI Reuse.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                      <EnumElem name="Yes" value="true"/>
                      <EnumElem name="No" value="false"/>
                    </Data>
                  </Pref>
                  <Pref name="PMI_ReuseContextFreezeUpdate" type="logical" title="Freeze Update" value="false" ver="2212" scope="Session">
                    <Description>This option determines whether or not to freeze update of linked PMI associated with a context on changes to source PMI.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                      <EnumElem name="Yes" value="true"/>
                      <EnumElem name="No" value="false"/>
                    </Data>
                  </Pref>
                  <Pref name="PMI_ReuseContextAllowEdits" type="logical" title="Allow Edits" value="false" ver="2212" scope="Session">
                    <Description>This option determines whether or not to allow edits to linked PMI associated with a context. It also determines if update should freeze on changes to source PMI.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                      <EnumElem name="Yes" value="true"/>
                      <EnumElem name="No" value="false"/>
                    </Data>
                  </Pref>
                </Group>
                <Group name="Arrangement">
                  <Pref name="PMI_AllowContextSuppressionOnArrangement" type="int" title="Suppress Context" value="0" ver="2312" scope="Session">
                    <Description>This option specifies which contexts to suppress when the active arrangement changes.</Description>
                    <RadioBox/>
                    <Data type="enum">
                      <EnumElem name="Related to Inactive Arrangements" value="0"/>
                      <EnumElem name="None" value="1"/>
                    </Data>
                  </Pref>
                </Group>
            </Tab>
        </Category>
        <Category name="PMI Supplemental Geometry" csh="PMAN_PMI_Supplemental_Geometry">
