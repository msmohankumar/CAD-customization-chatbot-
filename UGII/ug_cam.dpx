                    <Pref name="CAM_create_feature_process_template_UNX" type="string" title="Template" value="" ver="8.0.0" scope="Session">
                        <Description>This option specifies which template to use for template based process creation. If no path is given with the part, the environment variable UGII_CAM_TEMPLATE_PART_DIR is used as the path to the part.</Description>
                        <BrowseFileSel type="prt"/>
                    </Pref>
                    <Pref name="CAM_create_feature_process_template_WIN" type="string" title="Template" value="" ver="8.0.0" scope="Session">
                        <Description>This option specifies which template to use for template based process creation. If no path is given with the part, the environment variable UGII_CAM_TEMPLATE_PART_DIR is used as the path to the part.</Description>
                        <BrowseFileSel type="prt"/>
                    </Pref>
                    <Pref name="CAM_feature_group_process_features" type="int" title="Feature Process Grouping" value="2" ver="8.0.0" scope="Session">
                        <Description>This option determines how feature processes are created and features are merged. Use Existing will reuse existing feature processes if possible. If no applicable process is found a new feature process is created. Always Create New will always create a new feature process and try to merge features into this process. Per Feature will create a new feature process per feature.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="Always Create New" value="0"/>
                            <EnumElem name="Per Feature" value="1"/>
                            <EnumElem name="Use Existing" value="2"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Find Features">
                    <Pref name="CAM_feature_find_type" type="int" title="Type" value="1" ver="1872" scope="Session">
                        <Description>This option specifies the method used to find new features.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="Feature Identification" value="0"/>
                            <EnumElem name="Parametric Recognition" value="1"/>                        
                        </Data>
                    </Pref>
                    <Pref name="CAM_feature_bodies_to_search" type="int" title="Geometry to Search" value="0" ver="6.0.0" scope="Session">
                        <Description>This option specifies the geometry to search when finding features.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="Workpiece" value="0"/>
                            <EnumElem name="All Geometry" value="1"/>
                            <EnumElem name="Specify" value="2"/>
                        </Data>
                    </Pref>
                    <Pref name="CAM_feature_use_feature_name_as_type" type="int" title="Use Feature Name as Type" value="1" ver="6.0.0" scope="Session">
                        <Description>This option determines whether or not to use the feature name as type.</Description>
                        <Toggle off="0"/>
                        <Data type="enum">
                            <EnumElem name="No" value="0"/>
                            <EnumElem name="Yes" value="1"/>
                        </Data>
                    </Pref>
                    <Pref name="CAM_feature_assign_color_attribute" type="int" title="Assign Color Attribute" value="0" ver="6.0.0" scope="Session">
                        <Description>This option determines whether or not to assign the color attribute to the features.</Description>
                        <Toggle off="0"/>
                        <Data type="enum">
                            <EnumElem name="No" value="0"/>
                            <EnumElem name="Yes" value="1"/>
                        </Data>
                    </Pref>
                  <Pref name="CAM_feature_add_cad_attributes" type="int" title="Add Modeling Feature Attributes" value="0" ver="7.5.2" scope="Session">
                    <Description>This option determines whether or not to append attributes defined on modeling features to the recognized features.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                      <EnumElem name="No" value="0"/>
                      <EnumElem name="Yes" value="1"/>
                    </Data>
                  </Pref>
                  <Pref name="CAM_feature_map_features" type="int" title="Map Features" value="0" ver="6.0.0" scope="Session">
                        <Description>This option determines whether or not to map the features when the features are created.</Description>
                        <Toggle off="0"/>
                        <Data type="enum">
                            <EnumElem name="No" value="0"/>
                            <EnumElem name="Yes" value="1"/>
                        </Data>
                  </Pref>
                  <Pref name="CAM_feature_recognition_len_dia_ratio" type="real" title="Maximum Merge Factor" value="6.0" ver="1847" scope="Session">
                    <Description>This option specifies a factor that limits the distance between step hole/pocket feature faces. If the distance between two faces is smaller than or equal to the diameter times the specified value, the faces will be considered as belonging to the same feature.</Description>
                    <Real/>
                  </Pref>
                </Group>
                <Group name="User Defined (Legacy)">
                    <Pref name="CAM_get_cam_wth_udf" type="int" title="Merge CAM Data when Placing UDF" value="1" ver="3.0.0" scope="Session">
                        <Description>This switch tells whether or not to merge in the machining data with a UDF when the UDF is instanced.</Description>
                        <Toggle off="0"/>
