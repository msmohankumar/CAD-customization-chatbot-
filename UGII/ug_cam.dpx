                        <EnumElem name="No" value="0"/>
                        <EnumElem name="Yes" value="1"/>
                    </Data>
                </Pref>
                <Pref name="CAM_cls_typ" type="int" title="CLS Type" value="0" ver="3.0.0" scope="Session">
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Standard" value="0"/>
                        <EnumElem name="BCL" value="1"/>
                        <EnumElem name="ISO" value="2"/>
                    </Data>
                </Pref>
                <Pref name="CAM_cls_dec" type="int" title="Decimal Places" value="4" ver="3.0.0" scope="Session">
                    <Description>This option specifies the number of decimal places to be used when outputting CLSF files. This should be equal to or greater than the resolution your postprocessor will output.</Description>
                    <IntegerScale/>
