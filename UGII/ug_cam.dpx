            <Tab name="Tool Path" csh="PMAN_Manufacturing_Operation_Tool_Path">
                <Group name="Tool Path Replay">
                    <Pref name="CAM_oper_replay_refresh_before_each_path" type="int" title="Refresh before Each Path" value="1" ver="8.0.0" scope="Session">
                        <Description>This option determines whether or not the graphics window should be refreshed before replaying each operation when multiple operations have been selected.</Description>
                        <Toggle off="0"/>
