<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="docum.xsl"?>
<!--

-->
<PrefDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www-cyp.ugsolutions.com/assemblies/pman">
    <Application name="Manufacturing" csh="PMAN_Manufacturing" package_suppress="simcenter3d,simviewer,view,mechatronicsplayer,nx2dedit">
        <Category name="General" csh="PMAN_Manufacturing_General">
            <Tab name="Application Entry" csh="PMAN_Manufacturing_General_Application_Entry">
                <Group name="Part Load">
                    <Pref name="CAM_general_load_full_assembly" type="logical" title="Always Fully Load Assembly" value="true" ver="1953" scope="Session">
                        <Description>This option determines whether or not assemblies will be loaded fully when entering the Manufacturing application. When this option is on, it will always fully load the assembly regardless of the Assembly Load Options settings.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Software Updates" csh="PMAN_Manufacturing_General_Software_Updates">
                <Group name="Template Parts">
                    <Pref name="CAM_general_use_updated_templates" type="logical" title="Use Latest Updated Templates" value="true" ver="9.0.1" scope="Session">
                        <Description>This option determines whether or not template parts in the updates folder are used. This allows access to new functionality and critical fixes in maintenance releases. When this option is on, any templates found in the updates folder will be used instead of those in the resource template folders. When this option is off, templates installed in a maintenance release are ignored, and only the templates in the resource folder are used.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Library Files">
                    <Pref name="CAM_general_use_updated_libraries" type="logical" title="Use Latest Updated Libraries" value="true" ver="10.0.3" scope="Session">
                        <Description>This option determines whether or not library files in the updates folder are used. This includes tool, device, and machine libraries. This allows access to new content, functionality, and critical fixes in maintenance releases. When this option is on, libraries found in the updates folder will be used in addition to those in the resource folders. When this option is off, library files installed in the updates folder by a maintenance release are ignored, and only the files in the resource folder are used.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Online Services" csh="PMAN_Manufacturing_General_Online_Services">
                <Group name="Post Hub">
                    <Pref name="CAM_general_post_hub_download_page_url" type="utf8string" title="Download Page URL" value="https://posthub.sws.siemens.com" ver="1953" scope="Session">
                        <Description>This field specifies the download page URL that is used for Post Hub.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="CAM_general_post_hub_show_group" type="logical" title="Show Group in Dialog Box" value="true" ver="1926" scope="Session">
                        <Description>This option specifies whether the Post Hub group will display in the Postprocess dialog.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
        </Category>
        <Category name="User Interface" csh="PMAN_Manufacturing_User_Interface">
            <Tab name="General" csh="PMAN_Manufacturing_User_Interface_General">
                <Group name="Assisted Manufacturing">
                    <Pref name="CAM_enable_operation_creation_prediction" type="logical" title="Predict Operation Creation" value="true" ver="2007" scope="Session">
                        <Description>This option determines whether or not the operation creation will be saved and analyzed locally to predict operations to be created. When this option is off, the operation creation prediction and the corresponding user interface elements will be unavailable.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="CAM_enable_operation_item_usage_prediction" type="logical" title="Predict Operation Item Usage" value="true" ver="2212" scope="Session">
                        <Description>This option determines whether or not the operation item prediction will be saved and analyzed locally to predict operation items to be modified. When this option is off, the operation item prediction and the corresponding user interface elements will be unavailable.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="CAM_enable_feeds_items_usage_prediction" type="logical" title="Allow Feed and Speed Items on Prediction Page" value="false" ver="2306" scope="Session">
                        <Description>This option determines whether or not the Feed and Speed customizable item should be shown on the Prediction page of the operation dialog.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="CAM_operation_item_usage_number" type="int" title="Maximum Number of Predict Operation Items" value="5" ver="2212" scope="Session">
                        <Description>This option specifies the maximum number of predicted items shown on the Prediction page of the operation dialog.</Description>
                        <IntegerScale/>
                        <Data type="range">
                            <LowerBound value="3" include="1"/>
                            <UpperBound value="10" include="1"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Operation Navigator" csh="PMAN_Manufacturing_User_Interface_Operation_Navigator">
                <Group name="Colors">
                    <Pref name="CAM_ont_tl_chg_col" type="int" title="Tool Change" value="127" ver="1926" scope="Session">
                        <Description>This option specifies the font color of operations where a tool change occurs.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="CAM_ont_hil_col" type="int" title="Highlight" value="182" ver="1926" scope="Session">
                        <Description>This option specifies the font color used to highlight the active operation when using functions such as Visualize.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="CAM_ont_sup_path_col" type="int" title="Suppressed Path" value="87" ver="3.0.0" scope="Session">
                        <Description>This option specifies the font color of operations that have a suppressed tool path. Suppressed paths are not output and do not affect the IPW.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="CAM_ont_unused_item_col" type="int" title="Unused Items" value="93" ver="9.0.0" scope="Session">
                        <Description>This option specifies the font color of objects that are in the Unused Items group in any view.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="CAM_ont_parent_object_col" type="int" title="Parent Dependency Reference" value="152" ver="1926" scope="Session">
                        <Description>When an object is selected in the Operation Navigator, this option specifies the font color of the first-level parent objects which the selected object references.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="CAM_ont_child_object_col" type="int" title="Child Dependency Reference" value="105" ver="1953" scope="Session">
                        <Description>When an object is selected in the Operation Navigator, this option specifies the font color of the first-level child objects which reference the selected object.</Description>
                        <Color/>
                    </Pref>
                </Group>
                <Group name="Views">
                    <Pref name="CAM_ont_switch_to_match_create" type="int" title="Switch View to Match Create" value="0" ver="3.0.0" scope="Session">
                        <Description>This option determines whether or not the view of the Operation Navigator should switch to the corresponding view whenever creating a group (Program, Tool, Method or Geometry). This option has no affect when creating operations.</Description>
                        <Toggle off="0"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="1"/>
                            <EnumElem name="No" value="0"/>
                        </Data>
                    </Pref>
                    <Pref name="CAM_ont_condensed_tool_view" type="int" title="Condensed Tool View" value="0" ver="3.0.0" scope="Session">
                        <Description>This option determines whether or not the Tool View should condense so that the tree of carriers and pockets is not displayed.</Description>
                        <Toggle off="0"/>
                        <Data type="enum">
                            <EnumElem name="On" value="1"/>
                            <EnumElem name="Off" value="0"/>
                        </Data>
                    </Pref>
                    <Pref name="CAM_enable_details_view" type="logical" title="Show Details view" value="false" ver="1980" scope="Session">
                        <Description>This option determines whether or not the Details view will be visible in the Operation Navigator.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Selection">
                    <Pref name="CAM_ont_display_selected_item" type="logical" title="Display Selected Objects" value="true" ver="6.0.0" scope="Session">
                        <Description>This option will display graphical information about objects in the graphics window, when they are selected in the Operation Navigator.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="CAM_ont_display_operation_geometry" type="logical" title="Display Operation Geometry" value="true" ver="7.5.0" scope="Session">
                        <Description>When Display Selected Objects is on, this option will display the geometry with the tool path for operations when they are selected in the Operation Navigator. When this option is off, only the tool path is displayed.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="CAM_ont_display_selected_turning_operation_2d_ipw" type="logical" title="Display Turning IPW of Selected Objects" value="true" ver="6.0.0" scope="Session">
                        <Description>This option will display the 2D workpiece for turning operations in the graphics window, when they are selected in the Operation Navigator.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="CAM_ont_select_operation_in_all_views" type="logical" title="Select Operations in All Views" value="true" ver="6.0.0" scope="Session">
                        <Description>This option determines whether or not when an operation or operations are selected in a view, they will also be selected in the three other views.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Machine">
                    <Pref name="CAM_machine_show_retrieve_machine_information" type="int" title="Display Information when Machine is Replaced" value="1" ver="8.0.0" scope="Session">
                        <Description>This option will determine whether or not the information window will display changes made to objects in the tool view when the machine is replaced.</Description>
                        <Toggle off="0"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="1"/>
                            <EnumElem name="No" value="0"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>          
            <Tab name="Dialog Boxes" csh="PMAN_Manufacturing_User_Interface_Dialog_Boxes">
                <Group name="Visibility">
                <Pref name="CAM_dialog_show_pics" type="int" title="Show Pictures in Dialog Boxes" value="1" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not pictures are displayed in the Cutting Parameters and Non Cutting Moves dialog boxes.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
                <Pref name="CAM_create_dialog_show_pics" type="int" title="Show Picture Tooltips in Create Object Dialog Boxes" value="1" ver="8.5.0" scope="Session">
                    <Description>This option determines whether or not picture tooltips are displayed in the Create Operation dialog box.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
                <Pref name="CAM_dialog_show_find" type="logical" title="Show Find in Dialog Boxes" value="false" ver="11.0.1" scope="Session">
                    <Description>This option determines whether or not CAM dialog boxes should include the Find option at the top of the dialog. The Find option allows a search string to be entered which returns matches on labels within the dialog. Explorer style dialogs always include the Find option.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="CAM_dialog_create_show_tool_desc" type="logical" title="Show Tool Descriptions in Create Operation" value="true" ver="6.0.0" scope="Session">
                    <Description>This option will show the tool descriptions with the tool names when selecting a tool. This is used in the list of tool parents in the Location group in the Create Operation dialog box, and in the tool selection block in operation dialog boxes.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                </Group>
                <Group name="Machine Axis Positions Dialog">
                    <Pref name="CAM_show_machine_axis_positions_dialog" type="logical" title="Show Dialog" value="true" ver="9.0.0" scope="Session">
                        <Description>This option specifies that the Machine Axis Positions Dialog will display during Dynamic Machine Tool Positioning and ISV.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="CAM_machine_axis_ordering" type="int" title="Sort By" value="0" ver="11.0.0" scope="Session">
                        <Description>This option specifies how to sort the machine axes in the Machine Axis Positions dialog box.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="Axis Type/Name" value="0"/>
                            <EnumElem name="Axis Number" value="1"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Layout">
                <Pref name="CAM_update_customization" type="int" title="Always Update Dialog Boxes" value="0" ver="7.0.0" scope="Session">
                    <Description>This option determines whether or not an update of the dialog customization from the current template should take place for all objects in the work part every time manufacturing is entered. Having this option on may cause a delay when entering manufacturing.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
                <Pref name="CAM_vis_scr_itm" type="int" title="Number of Items before Scrolling" value="10" ver="3.0.0" scope="Session">
                    <Description>This option specifies the vertical size of the scrollable portion of a dialog box. When more than this number of items are displayed in a scrollable dialog box, the scroll bars will appear.</Description>
                    <IntegerScale/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                        <UpperBound value="50" include="1"/>
                    </Data>
                </Pref>
                </Group>
                <Group name="Icon Size">
                  <Pref name="CAM_icon_size_in_create_dialogs" type="int" title="Create Object Dialogs" value="2" ver="2212" scope="Session">
                    <Description>This option specifies the size of the icons in the tool palette shown in create object dialogs, such as the Create Operation dialog. Small style icons are 24x24 pixels, Medium style icons are 32x32 pixels, and Large style icons are 48x48 pixels. The Default size uses the actual size of the bitmap if a custom icon is provided, otherwise the standard icon size.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                      <EnumElem name="Small" value="0"/>
                      <EnumElem name="Medium" value="1"/>
                      <EnumElem name="Large" value="2"/>
                      <EnumElem name="Default" value="3"/>
                    </Data>
                  </Pref>
                </Group>
                <Group name="Other">
                <Pref name="CAM_run_wzd" type="int" title="Run Process Assistant" value="1" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not the Process Assistant functionality should be activated. When on, it causes the system to request user inputs in a specific step-by-step sequence if the WIZARD key word is defined in the configuration file.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
                </Group>
            </Tab>
            <Tab name="Synchronization" csh="PMAN_Manufacturing_User_Interface_Synchronization">
                <Group name="Time Chart Colors">
                    <Pref name="CAM_time_chart_event_type_rapid_col" type="int" title="Rapid Events" value="155" ver="3.0.0" scope="Session">
                        <Description>This option specifies the color of rapid events.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="CAM_time_chart_event_type_feed_col" type="int" title="Feed Events" value="97" ver="3.0.0" scope="Session">
                        <Description>This option specifies the color of feed events.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="CAM_time_chart_event_type_tool_change_col" type="int" title="Tool Change Events" value="124" ver="3.0.0" scope="Session">
                        <Description>This option specifies the color of tool change events.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="CAM_time_chart_event_type_dwell_col" type="int" title="Dwell Events" value="106" ver="6.0.0" scope="Session">
                        <Description>This option specifies the color of dwell events.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="CAM_time_chart_spindle_1_col" type="int" title="Spindle 1" value="117" ver="3.0.0" scope="Session">
                        <Description>This option specifies the color of spindle number 1.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="CAM_time_chart_spindle_2_col" type="int" title="Spindle 2" value="47" ver="3.0.0" scope="Session">
                        <Description>This option specifies the color of spindle number 2.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="CAM_time_chart_spindle_3_col" type="int" title="Spindle 3" value="56" ver="6.0.0" scope="Session">
                        <Description>This option specifies the color of spindle number 3.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="CAM_time_chart_spindle_4_col" type="int" title="Spindle 4" value="71" ver="6.0.0" scope="Session">
                        <Description>This option specifies the color of spindle number 4.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="CAM_time_chart_primary_spindle_col" type="int" title="Primary Spindle" value="130" ver="6.0.0" scope="Session">
                        <Description>This option specifies the color of the primary spindle.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="CAM_time_chart_sync_mark_col" type="int" title="Synchronization Marks" value="188" ver="6.0.0" scope="Session">
                        <Description>This option specifies the color of synchronization marks.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="CAM_time_chart_background_col" type="int" title="Background" value="87" ver="6.0.0" scope="Session">
                        <Description>This option specifies the color of the background.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="CAM_time_chart_legend_background_col" type="int" title="Legend Background" value="87" ver="6.0.0" scope="Session">
                        <Description>This option specifies the color of the legend background.</Description>
                        <Color/>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Palettes" csh="PMAN_Manufacturing_User_Interface_Palettes">
                <Pref name="CAM_palette_show_wizard_palette" type="logical" title="Show Manufacturing Wizards Palette" value="true" ver="4.0.0" scope="Session">
                    <Description>This option determines whether or not the Manufacturing Wizards palette is shown in the resource bar if a Manufacturing license is present.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="CAM_palette_show_cam_machine_samples_palette" type="logical" title="Show Manufacturing Machine Samples Palette" value="true" ver="12.0.1" scope="Session">
                    <Description>This option determines whether or not the Manufacturing Machine Samples palette is shown in the Resource Bar when in the Manufacturing application or when no part is displayed.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Roles" csh="PMAN_Manufacturing_User_Interface_Roles">
                <Pref name="CAM_change_config_from_role" type="logical" title="Change Configuration Based on Role" value="true" ver="6.0.0" scope="Session">
                    <Description>This option determines whether or not the CAM configuration is changed to the one included in the last selected role.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
            </Tab>
        </Category>
        <Category name="Operation" csh="PMAN_Manufacturing_Operation">
            <Tab name="General" csh="PMAN_Manufacturing_Operation_General">
                <Group name="Machining Data">
                    <Pref name="CAM_autoset_machining_data" type="int" title="Automatically Set in Operation" value="0" ver="3.0.0" scope="Session">
                        <Description>This sets the preference to automatically set the machining data in a milling or drilling operation, and update the settings whenever a relevant parameter changes.</Description>
                        <Toggle off="0"/>
                        <Data type="enum">
                            <EnumElem name="No" value="0"/>
                            <EnumElem name="Yes" value="1"/>
                        </Data>
                    </Pref>
                    <Pref name="CAM_always_set_stepover_type" type="logical" title="Always Change Stepover Type to Constant" value="true" ver="10.0.2" scope="Session">
                        <Description>This option specifies whether or not the Stepover type in a milling or drilling operation will always be changed to Constant when Set Machining Data is activated.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="No" value="false"/>
                            <EnumElem name="Yes" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="CAM_support_exact_match" type="logical" title="Only Exact Matches" value="false" ver="1953" scope="Session">
                        <Description>This option determines whether or not the machining data computation will be limited to exact matches of tool diameter and tool length.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Off" value="false"/>
                            <EnumElem name="On" value="true"/>
                        </Data>
                    </Pref>
                    <Pref name="Cut_feed_computation_accuracy_EU" type="int" title="Feed per Minute Decimal Places" value="1" ver="5.0.4" scope="Session">
                        <Description>This option sets the rounding accuracy for cut feed rate computation from the machining data library.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="0" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="Cut_feed_computation_accuracy_MU" type="int" title="Feed per Minute Decimal Places" value="1" ver="5.0.4" scope="Session">
                        <Description>This option sets the rounding accuracy for cut feed rate computation from the machining data library.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="0" include="1"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Preview">
                    <Pref name="CAM_floor_wall_mill_initial_preview_value" type="logical" title="Floor and Wall Milling Preview" value="false" ver="1899" scope="Session">
                        <Description>This is the initial setting of the dynamic preview toggle for Floor and Wall Milling operations. This is used only the first time an operation is created or edited. After that, the setting is remembered with dialog memory.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                           <EnumElem name="On" value="true"/>
                           <EnumElem name="Off" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="CAM_project_preview_vector_number" type="int" title="Projection Preview - Max Vectors to Display" value="50" ver="12.0.1" scope="Session">
                        <Description>This option specifies the maximum number of vectors to display for Surface Contour operations projection preview.</Description>
                        <Integer/>
                        <Data type="range">
                           <LowerBound value="1" include="1"/> 
                        </Data>
                    </Pref>
                </Group>
                <Group name="User Defined Operations">
                    <Pref name="CAM_env_var_prefix" type="string" title="User Defined Operation Prefix" value="" ver="3.0.0" scope="Session">
                        <Description>This option specifies the environment variable prefix for user-defined operations.</Description>
                        <String/>
                    </Pref>
                </Group>
                <Group name="Legacy">
                    <Pref name="CAM_opr_stepover_applied_on_plane_opt" type="logical" title="Allow On-plane Stepover Option in Area Milling" value="false" ver="1872" scope="Session">
                        <Description>This option determines whether or not the On-plane stepover option is available in Area Milling.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="CAM_opr_nonsteep_cut_patterns_opt" type="logical" title="Allow Legacy Non-steep Cut Patterns in Area Milling" value="false" ver="2007" scope="Session">
                        <Description>This option determines whether or not the radial and concentric non-steep cut patterns are available in Area Milling operations.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="CAM_opr_drive_method_opt" type="logical" title="Allow Legacy Drive Methods in Fixed and Variable Contour" value="false" ver="1872" scope="Session">
                        <Description>This option determines whether or not the Boundary, Spiral, and Radial Cut drive methods are available in Fixed and Variable Contour.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
