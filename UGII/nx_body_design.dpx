<PrefDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www-cyp.ugsolutions.com/assemblies/pman">
    <Application name="Vehicle Design - Body Design" csh="PMAN_Vehicle_Design_Body_Design" package_suppress="simcenter3d,simviewer,view,mechatronicsplayer,nx2dedit">
        <Category name="B-pillar" csh="PMAN_Vehicle_Design_Body_Design_B_Pillar">
            <Tab name="General" csh="PMAN_Vehicle_Design_Body_Design_B_Pillar_General">
                <Pref name="DDBPILLAR_BODY_TYPE" type="string" title="Body Type" value="Sedan" ver="4.0.0" scope="Session">
                    <Description>This field allows the vehicle body type to be specified.</Description>
                    <String/>
                </Pref>
                <Group name="Section Information">
                <Pref name="DDBPILLAR_SECTION_PARAMETER_MAX_COUNT" type="int" title="Maximum Number of Segments" value="8" ver="4.0.0" scope="Session">
                    <Description>This option specifies the maximum number of segments in the B-pillar cross section.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="DDBPILLAR_SECTION_REG_NAME" type="string" title="Registry Spreadsheet Name" value="bd_bpillar_cross_section_reg.xls" ver="4.0.0" scope="Session">
                    <Description>This field allows a spreadsheet file name to be specified. This spreadsheet will register all B-pillar cross sections.</Description>
                    <Widestring/>
                </Pref>
                </Group>
                <Pref name="DDBPILLAR_UPPER_OFFSET_MU" type="real" title="Upper Offset" value="36.0" unit="mm" ver="4.0.0" scope="Session">
                    <Description>This option specifies the upper offset to the beltline.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="DDBPILLAR_UPPER_OFFSET_EU" type="real" title="Upper Offset" value="1.42" unit="in" ver="4.0.0" scope="Session">
                    <Description>This option specifies the upper offset to the beltline.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="DDBPILLAR_LOWER_OFFSET_MU" type="real" title="Lower Offset" value="-178.0" unit="mm" ver="4.0.0" scope="Session">
                    <Description>This option specifies the lower offset to the beltline.</Description>
                    <Real/>
                    <Data type="range">
                        <UpperBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="DDBPILLAR_LOWER_OFFSET_EU" type="real" title="Lower Offset" value="-7.01" unit="in" ver="4.0.0" scope="Session">
                    <Description>This option specifies the lower offset to the beltline.</Description>
                    <Real/>
                    <Data type="range">
                        <UpperBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="DDBPILLAR_FACE_OFFSET_MU" type="real" title="Face Offset" value="28.0" unit="mm" ver="4.0.0" scope="Session">
                    <Description>This option specifies the offset between the B-pillar outer surface and the B and R surfaces.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="DDBPILLAR_FACE_OFFSET_EU" type="real" title="Face Offset" value="1.1" unit="in" ver="4.0.0" scope="Session">
                    <Description>This option specifies the offset between the B-pillar outer surface and the B and R surfaces.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="DATABASE_READ_MODE_IN_XLS" type="int" title="Format for Database (spreadsheet)" value="1" ver="4.0.0" scope="Session">
                    <Description>This option specifies which database that should be loaded.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="xs4" value="0"/>
                        <EnumElem name="xls" value="1"/>
                    </Data>
                </Pref>
            </Tab>
