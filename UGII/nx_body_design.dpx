        <Category name="Glass Drop" csh="PMAN_Vehicle_Design_Body_Design_Glass_Drop">
            <Tab name="General" csh="PMAN_Vehicle_Design_Body_Design_Glass_Drop_General">
                <Pref name="DDGLASS_HIDDEN_LAYER" type="int" title="Hidden Layer" value="255" ver="4.0.0" scope="Session">
                    <Description>This option specifies which layer some geometry will be put and hidden.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                        <UpperBound value="256" include="1"/>
                    </Data>
                </Pref>
                <Pref name="DDGLASS_THICKNESS_MU" type="real" title="Glass Thickness" value="4.0" unit="mm" ver="4.0.0" scope="Session">
                    <Description>This option specifies the thickness of the glass in order to construct the barrel surface as a thickened sheet.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="DDGLASS_THICKNESS_EU" type="real" title="Glass Thickness" value="0.16" unit="in" ver="4.0.0" scope="Session">
                    <Description>This option specifies the thickness of the glass in order to construct the barrel surface as a thickened sheet.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Barrel Surface" csh="PMAN_Vehicle_Design_Body_Design_Glass_Drop_Barrel_Surface">
                <Label label="The barrel surface is based on three section curves. The locations of these section curves are defined by the following options."/>
                <Pref name="DDGLASS_FIT_FRONT_OFFSET_MU" type="real" title="Forward Section" value="100.0" unit="mm" ver="4.0.0" scope="Session">
                    <Description>This option specifies the offset of the forward section curve from the front of the glass.</Description>
                    <Real/>
                </Pref>
                <Pref name="DDGLASS_FIT_FRONT_OFFSET_EU" type="real" title="Forward Section" value="3.94" unit="in" ver="4.0.0" scope="Session">
                    <Description>This option specifies the offset of the forward section curve from the front of the glass.</Description>
                    <Real/>
                </Pref>
                <Pref name="DDGLASS_FIT_MID_LOCATION_MU" type="real" title="Middle Section" value="3300.0" unit="mm" ver="4.0.0" scope="Session">
                    <Description>This option specifies the location of a section curve (measured longitudinally) through the styled glass surface. This should be approximately in the middle of the glass; sometimes referred to as the 3300 section.</Description>
                    <Real/>
                </Pref>
                <Pref name="DDGLASS_FIT_MID_LOCATION_EU" type="real" title="Middle Section" value="129.9" unit="in" ver="4.0.0" scope="Session">
                    <Description>This option specifies the location of a section curve (measured longitudinally) through the styled glass surface. This should be approximately in the middle of the glass; sometimes referred to as the 3300 section.</Description>
                    <Real/>
                </Pref>
                <Pref name="DDGLASS_FIT_REAR_OFFSET_MU" type="real" title="Rearward Section" value="50.0" unit="mm" ver="4.0.0" scope="Session">
                    <Description>This option specifies the offset of the rearward section curve from the rear of the glass.</Description>
                    <Real/>
                </Pref>
                <Pref name="DDGLASS_FIT_REAR_OFFSET_EU" type="real" title="Rearward Section" value="1.97" unit="in" ver="4.0.0" scope="Session">
                    <Description>This option specifies the offset of the rearward section curve from the rear of the glass.</Description>
                    <Real/>
                </Pref>
                <Separator/>
                <Pref name="DDGLASS_DROP_BARREL_TYPE" type="int" title="Barrel Surface Type" value="0" ver="4.0.0" scope="Session">
                    <Description>This option specifies how the barrel surface should be created. This also determines the way the glass will drop down. The Drop Range step of the wizard will only display if the second option is chosen.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="Revolved - Glass Drop Down by Motion" value="0"/>
                        <EnumElem name="Swept - Glass Drop Down by Translation" value="1"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Motion Analysis" csh="PMAN_Vehicle_Design_Body_Design_Glass_Drop_Motion_Analysis">
                <Pref name="DDGLASS_CHECK_OBJECT_NAME" type="string" title="Objects for Interference Checking" value="{BODY_0,BODY_1,BODY_2}" ver="4.0.0" scope="Session">
                    <Description>This field allows object names which will automatically be used for interference checking to be specified.</Description>
                    <Widestring/>
                </Pref>
            </Tab>
            <Tab name="Channels" csh="PMAN_Vehicle_Design_Body_Design_Glass_Drop_Channels">
                <Pref name="DDGLASS_CHANNEL_START" type="int" title="Start Point of Glass Channel" value="0" ver="4.0.0" scope="Session">
                    <Description>This option specifies the starting point of the glass channel. The length of the front and rear glass channels will extend from this point.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="At the Upper Path Point" value="0"/>
                        <EnumElem name="At the Beltline with Offset" value="1"/>
                    </Data>
                </Pref>
                <Pref name="DDGLASS_START_OFFSET_BLT_MU" type="real" title="Beltline Offset" value="10.0" unit="mm" ver="4.0.0" scope="Session">
                    <Description>This option specifies the offset from the beltline. The offset applies if the above option is set to 'At the Beltline with Offset'.</Description>
                    <Real/>
                </Pref>
                <Pref name="DDGLASS_START_OFFSET_BLT_EU" type="real" title="Beltline Offset" value="0.39" unit="in" ver="4.0.0" scope="Session">
                    <Description>This option specifies the offset from the beltline. The offset applies if the above option is set to 'At the Beltline with Offset'.</Description>
                    <Real/>
                </Pref>
                <Pref name="DDGLASS_FRONT_CHANNEL_COMPONENT" type="string" title="Front Glass Channel Component Name" value="front_channel" ver="4.0.0" scope="Session">
                    <Description>This field allows a component name for the front glass channel to be specified.</Description>
                    <Widestring/>
                </Pref>
                <Pref name="DDGLASS_REAR_CHANNEL_COMPONENT" type="string" title="Rear Glass Channel Component Name" value="rear_channel" ver="4.0.0" scope="Session">
                    <Description>This field allows a component name for the rear glass channel to be specified.</Description>
                    <Widestring/>
                </Pref>
            </Tab>
