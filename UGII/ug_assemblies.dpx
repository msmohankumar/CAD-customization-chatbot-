<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="docum.xsl"?>
<!--

-->
<PrefDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www-cyp.ugsolutions.com/assemblies/pman">
    <Application name="Assemblies" csh="PMAN_Assemblies" package_suppress="simviewer,mechatronicsplayer,nx2dedit">
        <Category name="General" csh="PMAN_Assemblies_General">
            <Tab name="Miscellaneous" csh="PMAN_Assemblies_General_Miscellaneous">
                <Label label="When Generating Missing Part Family Members"/>
                <Pref name="Assemblies_VerifyPFTWhileAutoRegenPFM" type="int" title="Check for Newer Revisions of the Template Part" value="0" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not the load operation should check if the part family member referenced by the assembly is generated from the version of the template which is configured based on the load options.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="Assemblies_UpdateSwitch" type="int" title="Display Update Report" value="0" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not an update report is automatically displayed when an assembly is loaded (if it has components that require update).</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_MemberSelect" type="int" title="Component Member Select" value="0" ver="3.0.0" scope="Session">
                    <Description>This option determines how components are selected with the Class Selection dialog box. When this option is on, the assembly component member is selected. When this option is off, the component itself is selected. Selecting the assembly component member for display changes (such as visibility and color changes) results in different display properties for the component member at the assembly level compared to the component part file itself.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="On" value="1"/>
                        <EnumElem name="Off" value="0"/>
                    </Data>
                    <Docum>Caution: Selecting the members of a component for display changes (such as visibility and color changes), rather than selecting the whole component will result in the part file increasing in size.</Docum>
                </Pref>
              <Pref name="Assemblies_RestoreWAVEMode" type="logical" title="Restore WAVE Mode" value="true" ver="11.0.0" scope="Session">
                <Description>This option determines whether or not an attempt to restore the state of the WAVE Mode option in the Assembly Navigator will be made on NX startup. The attempt will be successful if all of the following conditions are true.<Formatted>
1. This option is on.
2. The WAVE Mode option was on when last exiting NX.
3. A WAVE Control license is available.</Formatted></Description>
                <Toggle off="false"/>
                <Data type="enum">
                  <EnumElem name="Yes" value="true"/>
                  <EnumElem name="No" value="false"/>
                </Data>
              </Pref>
                <Pref name="Assemblies_DisplayReadOnlyNotification" type="logical" title="Display Message when Work Part is Read-only" value="false" ver="8.0.0" scope="Session">
                  <Description>This option determines whether or not a message is displayed when changing the work part to a part that is read-only. The message box provides an option to cancel the change.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="Yes" value="true"/>
                    <EnumElem name="No" value="false"/>
                  </Data>
                </Pref>
                <Pref name="Assemblies_DisplayReadOnly" type="int" title="Display Message when Modifying Read-only Parts" value="1" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not a message is displayed when modifying a read-only part.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_WarnOnPFMemberModification" type="logical" title="Display Message when Modifying Part Family Members" value="true" ver="9.0.0" scope="Session">
                    <Description>This option determines whether or not a message is displayed when modifying a part family member part.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_WarnReadOnlyOnSave" type="logical" title="Display Message when Saving Read-only Parts" value="true" ver="5.0.0" scope="Session">
                  <Description>This option determines whether or not a message is displayed when performing any of the Save operations when some of the parts are read-only.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_UpdateStructureOnExpand" type="int" title="Update Structure on Expand" value="1" ver="10.0.0" scope="Session">
                    <Description>This option determines whether or not the structure of a component should be updated from its immediate child components when that component is expanded in the Assembly Navigator.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
              <Pref name="Assemblies_RestoreShowComponentGroupsSetting" type="int" title="Restore Show Component Groups Setting" value="1" ver="5.0.1" scope="Session">
                <Description>This option determines whether or not an attempt to restore the state of the Show Component Groups option in the Assembly Navigator will be made on NX startup. The attempt will be successful if all of the following conditions are true.<Formatted>
1. This option is on.
2. The Show Component Groups option was on when last exiting NX.
3. An Advanced Assemblies license is available.</Formatted></Description>
                <Toggle off="0"/>
                <Data type="enum">
                  <EnumElem name="Yes" value="1"/>
                  <EnumElem name="No" value="0"/>
                </Data>
              </Pref>
              <Separator/>
              <Pref name="Assemblies_ProximityComponentGroupDefault" type="real" title="Component Group Proximity Distance" value="10.00" ver="6.0.0" scope="Session">
                <Description>This option specifies the initial proximity distance value for component groups.</Description>
                <Real/>
              </Pref>
                <Separator/>
                <Pref name="Assemblies_partDescriptiveName" type="string" title="Part Name Style Attribute" value="$FILENAME" ver="3.0.0" scope="Session">
                    <Description>This field allows a part attribute title to be entered. The value of this attribute will be displayed in dialog boxes and the graphics window title bar. Valid options include <Code>$FILENAME, $DESCRIPTION</Code>, and anything else deemed to be a part attribute title.</Description>
                    <Widestring/>
                    <Docum>The description which appears when creating a part while NX is connected to Teamcenter is accessed through part attribute <Code>DB_PART_DESC</Code>, not <Code>$DESCRIPTION</Code>.</Docum>
                </Pref>
                <Separator/>
                <Pref name="Assemblies_ClrAnlAttenColor" type="int" title="Interference Highlight Color" value="155" ver="3.0.0" scope="Session">
                    <Description>This option specifies the attention color used to highlight interferences as they are reported. This is used in the Assembly Sequencing, Reposition Component, and View Section (Cap Display) commands.</Description>
                    <Color/>
                </Pref>
                <Separator/>
                <Pref name="Assemblies_PromptForDeformParameters" type="logical" title="Display Deform Parameters on Update" value="false" ver="6.0.0" scope="Session">
                    <Description>This option determines whether or not the Deform Parameters dialog box will be presented when a Deform feature is updated through the Update Deform command.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
              <Separator/>
              <Pref name="Assemblies_PrepopulateSimplifyDialog" type="logical" title="Populate Simplify Assembly Dialog" value="true" ver="1953" scope="Session">
                <Description>This option determines whether or not the Simplify Assembly dialog is populated with visible bodies when invoked.</Description>
                <Toggle off="false"/>
                <Data type="enum">
                  <EnumElem name="Yes" value="true"/>
                  <EnumElem name="No" value="false"/>
                </Data>
              </Pref>
            </Tab>
            <Tab name="Work Part" csh="PMAN_Assemblies_General_Work_Part">
                <Pref name="Assemblies_ChangeWorkPartRefSet" type="int" title="Display using 'Entire Part' Reference Set" value="1" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not the action of changing work part will set the new work part to have the 'Entire Part' reference set. The previous reference set in use will be restored automatically when this component is no longer the work part.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_WarnAutoWorkPartChange" type="int" title="Inform on Automatic Change" value="0" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not an information message will be displayed when the work part changes automatically as a result of another assemblies operation.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
              <Pref name="Assemblies_WarnOnDelete" type="logical" title="Warn on Delete" value="false" ver="2212" scope="Session">
                <Description>This option determines whether or not a warning message is displayed when deleting components of an assembly.</Description>
                <Toggle off="false"/>
                <Data type="enum">
                  <EnumElem name="Yes" value="true"/>
                  <EnumElem name="No" value="false"/>
                </Data> 
              </Pref>
            </Tab>
            <Tab name="Component Operations" csh="PMAN_Assemblies_General_Component_Operations">
                <Group name ="General">
                    <Pref name ="Assemblies_CompOperationsLayer" type ="int" title ="Layer" value ="1" ver="6.0.0" scope ="Session">
                        <Description>This option specifies which layer option should be used while performing a component operation on an assembly.</Description>
                        <OptionMenu/>
                        <Data type ="enum">
                            <EnumElem name="Original" value="1"/>
                            <EnumElem name="Work" value="0"/>
                            <EnumElem name="As Specified" value="2"/>
                        </Data>                      
                    </Pref>
                </Group>
                <Group name="Add Component">
                  <Pref name="Assemblies_AddCompScatter" type="logical" title="Scatter" value="true" ver="1847" scope="Session">
                      <Description>When multiple components are added to an assembly at the same time in the Add Component dialog, this option determines whether or not to scatter their locations.</Description>
                      <Toggle off="false"/>
                      <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                      </Data>
                    </Pref>
                <Pref name="Assemblies_AddCompLayerOption" type="int" title="Layer" value="1" ver="5.0.0" scope="Session">
                    <Description>This option specifies which layer option will be used when adding components to an assembly.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Original" value="1"/>
                        <EnumElem name="Work" value="0"/>
                        <EnumElem name="As Specified" value="2"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_AddComponentCommandForFileNew" type="int" title="Command to Use in File New" value="1" ver="1980" scope="Session">
                    <Description>This option determines whether the Add Component command or the Assemble command is invoked when creating a new part using an Assemblies template in the File New dialog.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Add Component" value="0"/>
                        <EnumElem name="Assemble" value="1"/>
                    </Data>
                </Pref>
                </Group>
                <Group name="Drag and Drop">              
                <Pref name="Assemblies_OnDragAndDropOnGraphics" type="int" title="On Part in Graphics Window" value="2" ver="1980" scope="Session">
                    <Description>This option specifies which command dialog, if any, should be launched when one or more components are dragged and dropped onto the graphics area of an already displayed part. Use the Position Directly option to skip commands and position using the other options in this group.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                      <EnumElem name="Start Assemble" value="0"/>
                      <EnumElem name="Start Add Component" value="1"/>
                      <EnumElem name="Position Directly" value="2"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_AddCompPositioningTypeMultiSelect" type="int" title="Positioning" value="2" ver="5.0.0" scope="Session">
                    <Description>This option specifies which initial positioning type will be used when one or more components are dragged and dropped.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Absolute Origin" value="0"/>
                        <EnumElem name="Select Origin" value="1"/>
                        <EnumElem name="By Constraints" value="2"/>
                        <EnumElem name="Move" value="3"/>                  
                    </Data>
                </Pref>
                <Pref name="Assemblies_AddCompOnXYPlaneMultiSelect" type="logical" title="Position on XC-YC Plane" value="false" ver="10.0.0" scope="Session">
                    <Description>This option determines whether or not the selected components should be placed on the XC-YC plane, i.e. at ZC=0. This option is applicable when the "Positioning" option is either "Select Origin" or "Move".</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                      <EnumElem name="Yes" value="true"/>
                      <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_AddCompMultiSelectScatter" type="int" title="Scatter" value="1" ver="5.0.0" scope="Session">
                    <Description>This option determines whether or not multiple selected components should be scattered upon drag and drop.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_AddCompLayerOptionMultiSelect" type="int" title="Layer" value="1" ver="5.0.0" scope="Session">
                    <Description>This option specifies which initial layer option will be used when one or more components are dragged and dropped.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Original" value="1"/>
                        <EnumElem name="Work" value="0"/>
                        <EnumElem name="As Specified" value="2"/>
                    </Data>
                </Pref>
                </Group>
                <Group name="New Component">
                  <Pref name="Assemblies_CreateNewReferenceSet" type="string" title="Reference Set Name" value="" ver="6.0.2" scope="Session">
                    <Description>This field specifies the name of the reference set to which geometry selected for the new component will be added.</Description>
                    <Widestring/>
                  </Pref>
                </Group>
                <Group name="Replace Component">
                    <Pref name ="Assemblies_ReplaceCompName" type ="int" title ="Component Name" value ="2" ver="7.5.0" scope ="Session">
                        <Description>This option specifies which component name option should be used while performing a replacement operation on an assembly.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="Maintain" value="0"/>
                            <EnumElem name="Original" value="1"/>
                            <EnumElem name="As Specified" value="2"/>
                        </Data>
                    </Pref>
                    <Pref name ="Assemblies_ReplaceCompLayer" type ="int" title ="Layer" value ="0" ver="6.0.0" scope ="Session">
                        <Description>This option specifies which layer option should be used while performing a replacement operation on an assembly.</Description>
                        <OptionMenu/>
                        <Data type="enum">
                            <EnumElem name="Maintain" value="0"/>
                            <EnumElem name="Original" value="1"/>
                            <EnumElem name="Work" value="2"/>
                            <EnumElem name="As Specified" value="3"/>
                        </Data>
                    </Pref>
                    <Pref name="Assemblies_PreserveAttributes" type="logical" title="Preserve Component Attributes" value="false" ver="5.0.0" scope="Session">
                        <Description>This option determines whether or not component attributes are applied to the new component after a Replace Component or Open Component As operation.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                        <Docum>When this option is on, any component attributes in the component that the Replace Component or Open Component As command is performed on will be copied over to the new component. This is restricted to the assembly where the Replace Component or Open Component As command is performed. Component attributes that are inherited from the underlying part are always recreated after a Replace Component or Open Component As operation, so are not affected by this option's setting.</Docum>
                    </Pref>
                    <Pref name="Assemblies_ReplaceCompMaintainRelationships" type="logical" title="Maintain Relationships" value="true" ver="6.0.0" scope="Session">
                        <Description>This option determines whether or not to perform an associative replacement of components.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Command Part Template Type" csh="PMAN_Assemblies_General_Command_Part_Template_Type">
              <Pref name ="Assemblies_NewParentTemplateType" type ="int" title ="New Parent" value ="1" ver="1926" scope ="Session">
                  <Description>This option specifies the part template type to use for the New Parent command. The type refers to the application that the template belongs to (see the Type column in the File New dialog). If only one part template of the specified type exists, it will be used as the template for the New Parent command. If multiple exist, this option will be ignored and the File New dialog will be presented. Use the "Select from File New" option to always present the File New dialog.</Description>
                  <OptionMenu/>
                  <Data type ="enum">
                      <EnumElem name="Gateway" value="0"/>
                      <EnumElem name="Assemblies" value="1"/>
                      <EnumElem name="Select from File New" value="2"/>
                  </Data>
               </Pref>
               <Pref name ="Assemblies_NewAssemblyTemplateType" type ="int" title ="Assemble" value ="1" ver="1926" scope ="Session">
                   <Description>This option specifies the part template type to use for the Assemble command. The type refers to the application that the template belongs to (see the Type column in the File New dialog). If only one part template of the specified type exists, it will be used as the template for the Assemble command. If multiple exist, this option will be ignored and the File New dialog will be presented. Use the "Select from File New" option to always present the File New dialog.</Description>
                   <OptionMenu/>
                   <Data type ="enum">
                       <EnumElem name="Gateway" value="0"/>
                       <EnumElem name="Assemblies" value="1"/>
                       <EnumElem name="Select from File New" value="2"/>
                   </Data>
                </Pref>
                <Pref name ="Assemblies_NewComponentTemplateType" type ="int" title ="New Component" value ="1" ver="1926" scope ="Session">
                    <Description>This option specifies the part template type to use for the New Component and Simplify Assembly commands. The type refers to the application that the template belongs to (see the Type column in the File New dialog). If only one part template of the specified type exists, it will be used as the template for the New Component and Simplify Assembly commands. If multiple exist, this option will be ignored and the File New dialog will be presented. Use the "Select from File New" option to always present the File New dialog.</Description>
                    <OptionMenu/>
                    <Data type ="enum">
                        <EnumElem name="Gateway" value="0"/>
                        <EnumElem name="Model" value="1"/>
                        <EnumElem name="Assemblies" value="2"/>
                        <EnumElem name="Select from File New" value="3"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Interpart Modeling" csh="PMAN_Assemblies_General_Interpart_Modeling">
              <Pref name="Assemblies_AllowInterPart" type="int" title="Allow Associative Interpart Modeling" value="2" ver="6.0.0" scope="Session">
                 <Description>This option specifies if associative interpart modeling is allowed and if so, to what extent.<Formatted>
"Yes": Allows associative interpart modeling everywhere - all WAVE commands are available, the Create Interpart Link option on the Selection Bar for many of the modeling commands is active, and interpart expressions may be created.
"Yes, but Not within Commands": Provides all explicit associative interpart modeling capabilities, but deactivates the Create Interpart Link option for all modeling commands.
"No": Disallows associative interpart modeling everywhere.</Formatted></Description>
                  <RadioBox orientation="vertical"/>
                  <Data type="enum">
                      <EnumElem name="Yes" value="2"/>
                      <EnumElem name="Yes, but Not within Commands" value="1"/>
                      <EnumElem name="No" value="0"/>
                  </Data>
                </Pref>
                <Pref name="Assemblies_AllowPromotions" type="int" title="Allow Promote Body" value="1" ver="8.0.0" scope="Session">
                    <Description>This option determines whether or not the Promote Body command is available.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>                
              <Pref name="Assemblies_InitialUpdateDelay" type="int" title="Delay Link Update" value="0" ver="7.5.0" scope="Session">
                <Description>This option determines whether or not link update is delayed. When link update is delayed an action is required to cause update, such as "Update Links". Link update includes linked features, linked expressions, and linked PMI.</Description>
                <Toggle off="0"/>
                <Data type="enum">
                  <EnumElem name="Yes" value="1"/>
                  <EnumElem name="No" value="0"/>
                </Data>
              </Pref>
                <Pref name="Assemblies_InitialDelayConstraints" type="logical" title="Delay Assembly Constraints" value="false" ver="7.5.0" scope="Session">
                    <Description>This option determines whether or not update of assembly constraints should be delayed until the "Update Assembly Constraints" command is chosen.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_UpdateDelayScope" type="int" title="Scope of Delayed Update" value="0" ver="3.0.0" scope="Session">
                    <Description>This option specifies whether all parts loaded in the session or only those in the displayed assembly are updated when an update is triggered.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Displayed Assembly" value="1"/>
                        <EnumElem name="Session" value="0"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Instance Attributes" csh="PMAN_Assemblies_General_Instance_Attributes">
                <Group name="Attribute Control">
                    <Pref name="Assemblies_AllowCalloutAsPartAttribute" type="logical" title="Allow &apos;CALLOUT&apos; as a Part Attribute" value="false" ver="10.0.0" scope="Session">
                        <Description>This option allows the 'CALLOUT' instance attribute to be set as a part attribute.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="Assemblies_AllowCalloutOverride" type="logical" title="Allow &apos;CALLOUT&apos; Attribute Override" value="true" ver="10.0.0" scope="Session">
                        <Description>This option allows the 'CALLOUT' instance attribute to be overridden on a referencing component.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="Assemblies_CopyInstanceAttribute" type="logical" title="Copy Instance Attribute" value="true" ver="12.0.0" scope="Session">
                        <Description>This option determines whether or not to copy instance attributes on various component copy operations.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                </Group>
                <Group name="Attribute Title Aliases">
                    <Pref name="Assemblies_ReferenceComponentAttributeTitleAlias" type="utf8string" title="REFERENCE_COMPONENT" value="" ver="10.0.0" scope="Session">
                        <Description>This option specifies the attribute title alias for the system attribute with the title 'REFERENCE_COMPONENT'.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="Assemblies_DraftingComponentAttributeTitleAlias" type="utf8string" title="DRAFTING_COMPONENT" value="" ver="10.0.0" scope="Session">
                        <Description>This option specifies the attribute title alias for the system attribute with the title 'DRAFTING_COMPONENT'.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="Assemblies_PlistIgnoreMemberAttributeTitleAlias" type="utf8string" title="PLIST_IGNORE_MEMBER" value="" ver="10.0.0" scope="Session">
                        <Description>This option specifies the attribute title alias for the system attribute with the title 'PLIST_IGNORE_MEMBER'.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="Assemblies_PlistIgnoreSubAssemblyAttributeTitleAlias" type="utf8string" title="PLIST_IGNORE_SUBASSEMBLY" value="" ver="10.0.0" scope="Session">
                        <Description>This option specifies the attribute title alias for the system attribute with the title 'PLIST_IGNORE_SUBASSEMBLY'.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="Assemblies_DbSequenceNumberAttributeTitleAlias" type="utf8string" title="DB_SEQUENCE_NUMBER" value="" ver="10.0.0" scope="Session">
                        <Description>This option specifies the attribute title alias for the system attribute with the title 'DB_SEQUENCE_NUMBER'.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="Assemblies_CalloutAttributeTitleAlias" type="utf8string" title="CALLOUT" value="" ver="10.0.0" scope="Session">
                        <Description>This option specifies the attribute title alias for the system attribute with the title 'CALLOUT'.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="Assemblies_UgRefSetAttributeTitleAlias" type="utf8string" title="UG REF SET" value="" ver="10.0.0" scope="Session">
                        <Description>This option specifies the attribute title alias for the system attribute with the title 'UG REF SET'.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="Assemblies_UgGeometryAttributeTitleAlias" type="utf8string" title="UG GEOMETRY" value="" ver="10.0.0" scope="Session">
                        <Description>This option specifies the attribute title alias for the system attribute with the title 'UG GEOMETRY'.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="Assemblies_UgAltRepAttributeTitleAlias" type="utf8string" title="UG ALTREP" value="" ver="10.0.0" scope="Session">
                        <Description>This option specifies the attribute title alias for the system attribute with the title 'UG ALTREP'.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="Assemblies_DbCompHandleAttributeTitleAlias" type="utf8string" title="DB_COMP_HANDLE" value="" ver="10.0.0" scope="Session">
                        <Description>This option specifies the attribute title alias for the system attribute with the title 'DB_COMP_HANDLE'.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="Assemblies_DbQuantityAttributeTitleAlias" type="utf8string" title="DB_QUANTITY" value="" ver="10.0.0" scope="Session">
                        <Description>This option specifies the attribute title alias for the system attribute with the title 'DB_QUANTITY'.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="Assemblies_DbUnitsAttributeTitleAlias" type="utf8string" title="DB_UNITS" value="" ver="10.0.0" scope="Session">
                        <Description>This option specifies the attribute title alias for the system attribute with the title 'DB_UNITS'.</Description>
                        <Widestring/>
                    </Pref>
                    <Pref name="Assemblies_LWDwgGeometryOmittedAttributeTitleAlias" type="utf8string" title="LW_DWG_GEOMETRY_OMITTED" value="" ver="10.0.0" scope="Session">
