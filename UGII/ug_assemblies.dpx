                        <Color/>
                    </Pref>
                    <Pref name="Assemblies_OutdatedColor" type="int" title="Information Status" value="112" ver="7.5.2" scope="Session">
                        <Description>This option specifies the color of constraints that have an information status which are not ignored in the current arrangement.</Description>
                        <Color/>
                    </Pref>
                    <Pref name="Assemblies_DelayedColor" type="int" title="Update Delayed" value="6" ver="7.5.0" scope="Session">
                        <Description>This option specifies the color of constraints that are update delayed.</Description>
                        <Color/>
                    </Pref>
                </Group>
                <Group name="Decimal Places">
                    <Pref name="Assemblies_DistanceDecimalPlaces" type="int" title="Distance" value="3" ver="5.0.0" scope="Session">
                        <Description>This option specifies the number of digits to display after the decimal point on distance constraints.</Description>
                        <IntegerScale/>
                        <Data type="range">
                            <LowerBound value="0" include="1"/>
                            <UpperBound value="9" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="Assemblies_AngleDecimalPlaces" type="int" title="Angle" value="3" ver="5.0.0" scope="Session">
                        <Description>This option specifies the number of digits to display after the decimal point on angle constraints.</Description>
                        <IntegerScale/>
                        <Data type="range">
                            <LowerBound value="0" include="1"/>
                            <UpperBound value="9" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="Assemblies_DimensionlessDecimalPlaces" type="int" title="Dimensionless" value="3" ver="1872" scope="Session">
                        <Description>This option specifies the number of digits to display after the decimal point on dimensionless constraint values.</Description>
                        <IntegerScale/>
                        <Data type="range">
                            <LowerBound value="0" include="1"/>
                            <UpperBound value="9" include="1"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
            <Tab name="Solver" csh="PMAN_Assemblies_Positioning_Solver">
                <Pref name="Assemblies_LinearTolerance_MU" type="real" title="Linear Tolerance" value="1.0e-5" unit="mm" ver="5.0.0" scope="Session">
                    <Description>This option specifies the tolerance used by the solver to make comparisons involving lengths. For example, to determine whether two distances are equal.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="1.0e-8" include="1"/>
                        <UpperBound value="1.0e+2" include="1"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_LinearTolerance_EU" type="real" title="Linear Tolerance" value="3.937e-7" unit="in" ver="5.0.0" scope="Session">
                    <Description>This option specifies the tolerance used by the solver to make comparisons involving lengths. For example, to determine whether two distances are equal.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="1.0e-9" include="1"/>
                        <UpperBound value="1.0e+1" include="1"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_LinearTolerance_MCU" type="real" title="Linear Tolerance" value="1.0e-5" unit="μm" ver="2212" scope="Session">
                    <Description>This option specifies the tolerance used by the solver to make comparisons involving lengths. For example, to determine whether two distances are equal.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="1.0e-11" include="1"/>
                        <UpperBound value="1.0e+2" include="1"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_LinearTolerance_SIU" type="real" title="Linear Tolerance" value="1.0e-5" unit="m" ver="2212" scope="Session">
                    <Description>This option specifies the tolerance used by the solver to make comparisons involving lengths. For example, to determine whether two distances are equal.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="1.0e-8" include="1"/>
                        <UpperBound value="1.0e+3" include="1"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_AngularTolerance" type="real" title="Angular Tolerance" value="5.73e-10" unit="deg" ver="5.0.0" scope="Session">
                    <Description>This option specifies the tolerance used by the solver to make comparisons involving angles. For example, to determine whether two lines are parallel.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="1.0e-12" include="1"/>
                        <UpperBound value="1.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_AcceptTolerantCurves" type="int" title="Accept Tolerant Curves" value="0" ver="5.0.0" scope="Session">
                    <Description>This option determines whether or not curves that are approximately circular (within the Modeling Distance Tolerance) may be used in any constraint that can use a circle or circular arc.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Conversion" csh="PMAN_Assemblies_Positioning_Conversion">
                <Pref name="Assemblies_ConstraintConversion" type="int" title="Convert to Assembly Constraints on Part Load" value="0" ver="6.0.0" scope="Session">
                    <Description>This option determines whether or not mating conditions are converted to assembly constraints automatically on load.
Conversion which takes place on load is not validated.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
                <Group name ="Validate Constraint Conversion">
                    <Pref name="Assemblies_UseCustomMateconvTolerances" type="logical" title="Use Custom Limits" value="false" ver="6.0.1" scope="Session">
                        <Description>This option determines whether custom limits are used for checking component movement during constraint conversion. When this option is off, the system bases the limits on the Positioning Linear Tolerance option.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="Assemblies_MateconvHardTolerance_MU" type="real" title="Maximum Acceptable Component Movement" value="1.0e-3" unit="mm" ver="6.0.1" scope="Session">
                        <Description>During constraint conversion, if a component would move by more than this amount a warning will be issued and the constraint will be suppressed.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="1.0e-8" include="1"/>
                            <UpperBound value="1.0e+2" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="Assemblies_MateconvHardTolerance_EU" type="real" title="Maximum Acceptable Component Movement" value="3.937e-5" unit="in" ver="6.0.1" scope="Session">
                        <Description>During constraint conversion, if a component would move by more than this amount a warning will be issued and the constraint will be suppressed.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="1.0e-9" include="1"/>
                            <UpperBound value="1.0e+1" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="Assemblies_MateconvSoftTolerance_MU" type="real" title="Report Component Movements Greater Than" value="1.0e-5" unit="mm" ver="6.0.1" scope="Session">
                        <Description>During constraint conversion, if a component would move by more than this amount an informational message will be issued.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="1.0e-8" include="1"/>
                            <UpperBound value="1.0e+2" include="1"/>
                        </Data>
                    </Pref>
                    <Pref name="Assemblies_MateconvSoftTolerance_EU" type="real" title="Report Component Movements Greater Than" value="3.937e-7" unit="in" ver="6.0.1" scope="Session">
                        <Description>During constraint conversion, if a component would move by more than this amount an informational message will be issued.</Description>
                        <Real/>
                        <Data type="range">
                            <LowerBound value="1.0e-9" include="1"/>
                            <UpperBound value="1.0e+1" include="1"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
        </Category>
        <Category name="Site Standards" csh="PMAN_Assemblies_Site_Standards">
            <Tab name="Reference Sets" csh="PMAN_Assemblies_Site_Standards_Reference_Sets" package_suppress="view">
                <Label label="System Reference Sets"/>
                <Group name="Model Reference Set">
                    <Pref name="Assemblies_ModelReferenceSet" type="string" title="Name" value="MODEL" ver="3.0.0" scope="Session">
                        <Description>This field specifies the name of the reference set that contains the real model geometry in the part file.</Description>
                        <Widestring/>
                        <Docum>If a name is specified, a reference set with this name will be created and solid and sheet bodies will automatically be added to it. It will also be used to determine which geometry to consider when storing a bounding box or if generating a faceted representation when saving a part. If a name is not specified, or the part being saved does not contain the named reference set, all appropriate geometry in the part will be used for these operations.</Docum>
                    </Pref>
                    <Pref name="Assemblies_ModelReferenceSetContents" type="int" title="Contents" value="0" ver="7.5.0" scope="Session">
                        <Description>This option specifies which bodies are automatically added to the model reference set as they are created.</Description>
                        <RadioBox orientation="horizontal"/>
                        <Data type="enum">
                            <EnumElem name="Sheets and Solids" value="0"/>
                            <EnumElem name="Solids Priority" value="1"/>
                            <EnumElem name="No Automation" value="2"/>
                        </Data>
                        <Docum>With the Sheets and Solids option, both solid bodies and sheet bodies are added. With the Solids Priority option, solid bodies are given priority over sheet bodies. Sheet bodies are deemed to be primarily construction geometry and are only added if there are no solid bodies in the reference set. This second option maintains pre-NX 4 behavior. In both cases, stand alone facet bodies are included in the model reference set.</Docum>
                    </Pref>
                    <Pref name="Assemblies_ModelReferenceSet_IncludeWAVE" type="logical" title="Include WAVE Linked Bodies" value="true" ver="4.0.0" scope="Session">
                        <Description>This option determines whether or not WAVE linked bodies will be added to the Model reference set.</Description>
                        <Toggle off="false"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="true"/>
                            <EnumElem name="No" value="false"/>
                        </Data>
                    </Pref>
                    <Pref name="Assemblies_AutoAddNewToRefSet" type="int" title="Add Components Automatically" value="1" ver="3.0.0" scope="Session">
                        <Description>This option determines whether or not Model reference sets will be created with the property that new components added to the part subsequently will be added to these reference sets automatically. When this option is on and the content of the Model reference sets are automated, Model reference sets will also be created for assembly parts in which model geometry is created.</Description>
                        <Toggle off="0"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="1"/>
                            <EnumElem name="No" value="0"/>
                        </Data>
                        <Docum>When this option is on and assembly level model geometry is created in an existing assembly part, all of the existing components of the assembly will be added automatically to the Model reference sets that are created. The components will be added to the reference sets using the Model reference sets in the components if these are available.

When this option is on, Model reference sets in existing parts will be converted to have the property that components added to the part should be added to the reference set automatically.

When this option is off, newly created Model reference sets will not have the property that new components are added to them automatically and no attempt will be made to maintain Model reference sets for assembly parts automatically. This allows the pre-NX3 behavior of Model reference sets to be maintained.

To follow Assembly Modeling best practices this option should be set.
</Docum>
                    </Pref>
                </Group>
                <Pref name="Assemblies_LWRefSetRemoval" type="int" title="Lightweight Reference Set Removal" value="1" ver="7.5.0" scope="Session">
                    <Description>Lightweight reference sets are no longer needed or supported. They will be automatically removed when legacy parts are loaded except in cases where they have been manually edited. This option determines whether manually edited lightweight reference sets should also be deleted or converted to user-managed reference sets.</Description>
                    <RadioBox orientation="vertical"/>
                    <Data type="enum">
                        <EnumElem name="Remove All" value="1"/>
                        <EnumElem name="Convert to User Reference Sets" value="0"/>
                    </Data>
                    <Docum>When this option is set to "Remove All", the lightweight reference set in legacy parts will unconditionally be deleted at load time.

When the option is set to "Convert to User Reference Sets", the lightweight reference set in legacy parts will still be deleted at load time except under specific circumstances where it was previously manually edited. In these cases, the reference set will be kept as a user-managed reference set. For more detailed information regarding these special situations, refer to the technical documentation for Assemblies.
</Docum>
                </Pref>
                <Group name="Simplified Reference Set">
                    <Pref name="Assemblies_SimplifiedReferenceSet" type="string" title="Name" value="SIMPLIFIED" ver="2206" scope="Session">
                        <Description>This field specifies the name of a simplified reference set that will be created automatically when bodies are created by the Simplify Assembly, Wrap Assembly, and Linked Exterior commands.
If the reference set is created by the Simplify Assembly command, new components will not be added to the reference set by default.
If the same name is used for the simplified and the model reference sets, the simplified reference set will not be used.</Description>
                        <Widestring/>
                    </Pref>
                </Group>
                <Separator/>
                <Label label="User Reference Sets"/>
                <Group name="Additional Model Reference Sets">
                    <Pref name="Assemblies_AdditionalModelRefSets" type="string" title="Names" value="" ver="3.0.0" scope="Session">
                        <Description>This field can be used to specify a number of reference sets (delimited by spaces). These would be the names of additional reference sets that represent model geometry (for example if left-hand and right-hand parts are represented in the same part file).</Description>
                        <Widestring/>
                        <Docum>These reference sets are taken account of in the process of generating Teamcenter Visualization files (a Teamcenter Visualization file will be  generated to represent each reference set).
It is necessary to have specified the main Model Reference Set in order for this option to have any effect.</Docum>
                    </Pref>
                </Group>
                    <Pref name="Assemblies_AutoAddNewToUserRefSet" type="int" title="Add Components Automatically" value="1" ver="6.0.0" scope="Session">
                        <Description>This option determines whether or not user defined reference sets will be created with the property that new components added to the part subsequently will be added automatically, and all existing child components will be included.</Description>
                        <Toggle off="0"/>
                        <Data type="enum">
                            <EnumElem name="Yes" value="1"/>
                            <EnumElem name="No" value="0"/>
                        </Data>
                        <Docum>When this option is on, all of the existing child components of the assembly will be added automatically to the user defined reference sets that are created. The components will be added to the reference sets using the same named reference sets in the components if these are available, otherwise it will use the Entire Part reference set as default.

To follow Assembly Modeling best practices this option should be set.
</Docum>
                    </Pref>
            </Tab>
            <Tab name="Part Name Versions" csh="PMAN_Assemblies_Site_Standards_Part_Name_Versions">
                <Docum>The following rules are used to describe the part names the system will use when searching for the version of a part for an assembly. The syntax for the rules follows the Regular Expressions used by the UNIX system. Leaving the rules blank means the system does not recognize versions of files and will compare complete part names when searching for parts. Examples follow.
<Formatted>
EXAMPLE 1:

Full Part File Name Format:                         ([a-z_]+_)([0-9]+(\.[0-9]+)*)
Version Independent Section of the Part File Name:  \1
Part File Name Match:                               \1([0-9]+(\.[0-9]+)*)
Version Section of Part File Name Match:            \1
Version Type:                                       Military
</Formatted>
The "Full Part File Name Format" specifies part names of one or more alphabetic characters and underscores "_" followed by an underscore "_", then a number, then 0 or more sets of a period "." and a number.
<Formatted>
Example part names in this format are:

          a_1.prt
          bcd_1.6.prt
          efg_h_ijk_1.3.2.prt

</Formatted>
The "Version Independent Section of the Part File Name" indicates that the FIRST section of the "Full Part File Name Format" pattern (i.e., "([a-z_]+_)" ) is the independent section.
<Formatted>
Example part names without their versions in this format are:

          a_
          bcd_
          efg_h_ijk_

</Formatted>
The "Part File Name Match" rule indicates that valid versioned part names consist of the part name followed by a number, then 0 or more sets of a period "." and a number.

Example part names are the same as those under "Full Part File Name Format".

The "Version Section of Part File Name Match" indicates that the FIRST section of the "Part File Name Match" pattern (i.e., "([0-9]+(\.[0-9]+)*)" ) is the part version section.
<Formatted>
Example versions in this format are:

          1
          1.6
          1.3.2


EXAMPLE 2:

Full Part File Name Format:                         ([0-9\-]+)(o|([a-hj-np-z]|([a-hj-np-z][a-hj-np-z])))
Version Independent Section of the Part File Name:  \1
Part File Name Match:                               \1(o|([a-hj-np-z]|([a-hj-np-z][a-hj-np-z])))
Version Section of Part File Name Match:            \2
Version Type:                                       Alphabetic

</Formatted>
The "Full Part File Name Format" specifies part names of digits and dashes followed by either a letter o (for "original"), or a letter in the range a-z excluding i and o, or 2 letters in the range a-z excluding i's and o's.
<Formatted>
Example part names in this format are:

          053316o.prt
          053316h.prt
          053316cl.prt
          1-32e.prt
          68-9-00x.prt
          -32-56-kd.prt

</Formatted>
The "Version Independent Section of the Part File Name" indicates that the FIRST section of the "Full Part File Name Format" pattern (i.e., "([0-9\-]+)" ) is the independent section.
<Formatted>
Example part names without their versions in this format are:

          053316
          1-32
          68-9-00
          -32-56-

</Formatted>
The "Part File Name Match" rule indicates that valid versioned part names consist of the part name followed by either a letter o (for "original"), or a letter in the range a-z excluding i and o, or 2 letters in the range a-z excluding i's and o's.

Example part names are the same as those under "Full Part File Name Format".

The "Version Section of Part File Name Match" rule indicates that the SECOND section of the "Part File Name Match" pattern (i.e., "([a-hj-np-z]|([a-hj-np-z][a-hj-np-z]))") is the part version section.
<Formatted>
where:
        \1(o|([a-hj-np-z]|([a-hj-np-z][a-hj-np-z])))
          |^|------------------^-------------------|
           1               section 2
</Formatted>
From these rules, the "o" matches as being part of the version, but is not used for the purposes of comparison. Hence, when the version is extracted from a part with "o" as the revision, the system will get the empty string and all versions will match to be greater than this, meaning that "o" is the first version of the part.
<Formatted>
Example versions in this format are:

          o
          h
          cl
          e
          x
          kd</Formatted></Docum>
                <Label label="The following options are used to describe the part names the system will use when searching for the correct version of a part for an assembly. The syntax for the rules follows the Regular Expressions used by the UNIX system. Leaving the options blank means the system will not recognize versions of files and will compare complete part names when searching for parts."/>
                <Pref name="Assemblies_PartNameFormat" type="string" title="Full Part File Name Format" value="" ver="3.0.0" scope="Session">
                    <Description>This field specifies the general format for the full name of versioned part files.</Description>
                    <Widestring/>
                </Pref>
                <Pref name="Assemblies_PartNameSubstitution" type="string" title="Version Independent Section of the Part File Name" value="" ver="3.0.0" scope="Session">
                    <Description>This field specifies which section of the Full Part File Name Format that is the version independent section of the name. It consists of a back-slash (\) followed by the number of the section.</Description>
                    <String/>
                </Pref>
                <Pref name="Assemblies_PartNameMatch" type="string" title="Part File Name Match" value="" ver="3.0.0" scope="Session">
                    <Description>This field is used to match all versions of a part file. It consists of a back-slash (\) followed by the Version Independent Section of the Part File Name along with the section of the Full Part File Name format that specifies the pattern for the part file version.</Description>
                    <Widestring/>
                </Pref>
                <Pref name="Assemblies_VersionSubstitution" type="string" title="Version Section of Part File Name Match" value="" ver="3.0.0" scope="Session">
                    <Description>This field specifies which section of the Part File Name Match that is the version section of the name. It consists of a back-slash (\) followed by the number of the section.</Description>
                    <String/>
                </Pref>
                <Pref name="Assemblies_VersionType" type="int" title="Version Type" value="4" ver="3.0.0" scope="Session">
                    <Description>This option specifies the sorting order that will be used to determine the latest version of a part file.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Numeric" value="0"/>
                        <EnumElem name="Reverse Numeric" value="1"/>
                        <EnumElem name="Alphabetic" value="2"/>
                        <EnumElem name="Reverse Alphabetic" value="3"/>
                        <EnumElem name="Military" value="4"/>
                        <EnumElem name="Alphanumeric" value="5"/>
                    </Data>
                    <Docum>A valid option must be specified even if file versioning is not being used.
<Formatted>
Military:               1 &lt; 1.2 &lt; 3.2.1
Numeric:                2 &lt; 5 &lt; 11
Reverse Numeric:        15 &lt; 7 &lt; 2
Alphabetic:             a &lt; m &lt; z &lt; av &lt; ff
Reverse Alphabetic:     z &lt; a &lt; vv &lt; aa
Alphanumeric:           a2 &lt; a5 &lt; b2 &lt; b23 &lt; aa12 &lt; bc22</Formatted></Docum>
                </Pref>
            </Tab>
            <Tab name="True Shape" csh="PMAN_Assemblies_Site_Standards_True_Shape" package_suppress="view">
                <Docum>True Shape functionality divides space into a grid of cubic cells that are used to generate a coarse representation of the geometry in component parts. This data is used in spatial filtering operations. Specify the size of the cells that will be used in this true shape representation and the units that this cell size is expressed in here.

Note that an inappropriate cell size can negatively impact the performance and/or the quality of results achieved from True Shape spatial filtering. Please read the cell size definition guidelines in the True Shape Filtering portion of the technical documentation before defining a cell size.</Docum>
                <Pref name="Assemblies_TrueShapeCellSize" type="real" title="Cell Size" value="0.0" ver="3.0.0" scope="Session">
                    <Description>This option specifies the size of the cells to be used in True Shape operations. A value of 0.0 indicates that True Shape data should not be employed in evaluating spatial filtering operations and that True Shape data should not be generated on part save.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_TrueShapeCellSizeUnits" type="int" title="Cell Units" value="0" ver="3.0.0" scope="Session">
                    <Description>This option specifies the units of the cells used in True Shape representations. When this option is set to "Undefined", the cell size will be interpreted in the units specified by the "Defaults Units" option on the Gateway -&gt; General -&gt; Part tab.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Undefined" value="0"/>
                        <EnumElem name="Inches" value="2"/>
                        <EnumElem name="Millimeters" value="1"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_TrueShapeGenerate" type="int" title="Generate Component Shape Representations on Save" value="0" ver="3.0.0" scope="Session">
                    <Description>This option determines whether or not True Shape spatial filtering generates component shape representations (&lt;component name&gt;.tso) on part save. (Note that a non-zero Cell Size must also be defined in order for True Shape data to be generated.)</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_TrueShapeUtilize" type="int" title="Utilize Component Shape Representations in Filtering" value="0" ver="10.0.0" scope="Session">
                    <Description>This option determines whether or not the evaulation of spatial filtering operations will utilize True Shape representations. (Note that a non-zero Cell Size must also be defined in order for True Shape data to be utilized.)</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Lightweight Representations" csh="PMAN_Assemblies_Site_Standards_Lightweight_Representations" package_suppress="view">
                <Pref name="Assemblies_FacetToleranceOption" type="int" title="Lightweight Facet Tolerance" value="1" ver="7.5.0" scope="Session">
                    <Description>This option specifies how accurate newly created lightweight geometry should be. Smoother options result in better visualization, but require more memory and will result in larger file sizes.</Description>
                    <RadioBox orientation="vertical"/>
                    <Data type="enum">
                        <EnumElem name="Rough" value="0"/>
                        <EnumElem name="Standard" value="1"/>
                        <EnumElem name="Smooth" value="2"/>
                        <EnumElem name="Extra Smooth" value="3"/>
                        <EnumElem name="Custom" value="4"/>
                    </Data>
                </Pref>
                <Group name="Custom Tolerances">
                <Pref name="Assemblies_FacetRepTolerance" type="real" title="Distance" value="0.0" ver="3.0.0" scope="Session">
                    <Description>This option specifies the distance tolerance value to be used when creating lightweight representations. This value is interpreted in the units specified by the Defaults Units option on the Gateway -&gt; General -&gt; Part tab.

When a value of 0.0 is specified, the system will govern the tolerance, using a technique which sets the tolerance values according to the size of the body being faceted.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                    <Docum>This value will be used whenever a facet body is created, unless an alternative tolerance is specified through the Edit Facet Parameters command available from the Part Navigator context menu.</Docum>
                </Pref>
                    <Pref name="Assemblies_FacetAngleTolerance" type="real" title="Angle (recommended range: 5 - 60 degrees)" value="0.0" unit="deg" ver="5.0.0" scope="Session">
                        <Description>This option specifies the angle tolerance value to be used when creating new lightweight representations. The angle tolerance controls the maximum angle in degrees between facet edges used to approximate curves and the triangles used to approximate surfaces.

Smaller values will typically result in larger numbers of facets. Specifying a value less than 1 is likely to result in very large numbers of facets and excessive memory usage during the creation of a faceted representation.

When a value of 0.0 is specified, a system-defined default tolerance will be used.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                        <UpperBound value="90.0" include="1"/>
                    </Data>
                    <Docum>This value will be used whenever a new facet body is created. When the value of this option is set to 0.0, the system will use a predetermined angle tolerance value for the body being faceted.</Docum>
                </Pref>
                </Group>
                <Pref name="Assemblies_FacetRepLODGeneration" type="logical" title="Generate Levels of Detail" value="true" ver="5.0.0" scope="Session">
                    <Description>This option determines whether or not levels of detail are to be generated when creating lightweight representations.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_AutoLightweightGeneration" type="int" title="Automatic Lightweight Generation for" value="2" ver="7.5.0" scope="Session">
                    <Description>This option specifies which reference sets will be automatically populated with lightweight geometry for the solid geometry it contains.</Description>
                    <RadioBox orientation="vertical"/>
                    <Data type="enum">
                        <EnumElem name="All in Part" value="0"/>
                        <EnumElem name="All in Model Reference Set" value="1"/>
                        <EnumElem name="All in Model and User Reference Sets" value="2"/>
                        <EnumElem name="None" value="3"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Miscellaneous" csh="PMAN_Assemblies_Site_Standards_Miscellaneous" package_suppress="view">
                <Group name="Product Outlines">
                    <Pref name="Assemblies_ProductOutlineLayer" type="int" title="Layer" value="190" ver="3.0.0" scope="Session">
                        <Description>This option specifies what layer the product outline will be placed on when there is no <Code>PRODUCT_OUTLINE</Code> layer category in the part file.</Description>
                        <Integer/>
                        <Data type="range">
                            <LowerBound value="1" include="1"/>
                            <UpperBound value="256" include="1"/>
                        </Data>
                    </Pref>
                  <Pref name="Assemblies_ProductOutlineIsSelectable" type="int" title="Layer Setting when Shown" value="0" ver="6.0.0" scope="Session">
                      <Description>This option specifies the layer setting for the product outline layer when the menu bar option Show Product Outline is on.</Description>
                      <OptionMenu/>
                      <Data type="enum">
                          <EnumElem name="Visible Only" value="0"/>
                          <EnumElem name="Selectable" value="1"/>
                      </Data>
                  </Pref>
                </Group>
                <Group name="Assembly Mirror">
                    <Pref name="Assemblies_MirrorAbsoluteOriginLocation" type="logical" title="Mirror Absolute Origin Location" value="false" ver="12.0.2" scope="Session">
                      <Description>This option determines if the absolute origin location of all mirrored assemblies or components should be mirrored.</Description>
                      <Toggle off="false"/>
                      <Data type="enum">
                        <EnumElem name="Yes" value="true"/>
                        <EnumElem name="No" value="false"/>
                      </Data>
                    </Pref>
                    <Pref name="Assemblies_SymmetryPlaneName" type="string" title="Name of Symmetry Datum CSYS/Plane" value="Symmetry Override" ver="7.5.0" scope="Session">
                        <Description>This option specifies the name of the user defined datum CSYS or plane that will be used as a plane of symmetry.</Description>
                        <Widestring/>
                    </Pref>
                </Group>
                <Group name="Assembly Reflection">
                    <Pref name="Assemblies_ReflectionName" type="int" title="Reflected Components" value="0" ver="12.0.2" scope="Session">
                        <Description>This option specifies if components with reflection transforms should be displayed and if so, whether the Reflect Component command should be available, which allows new components with reflection transforms to be created.</Description>
                        <OptionMenu/>
                        <Data type ="enum">
                            <EnumElem name="Disallow" value="0"/>
                            <EnumElem name="Display Only" value="1"/>
                            <EnumElem name="Display and Create" value="2"/>
                        </Data>
                    </Pref>
                </Group>
            </Tab>
        </Category>
        <Category name="Synchronous Assemblies" csh="PMAN_Assemblies_Synchronous_Assemblies">
            <Tab name="Component Display" csh="PMAN_Assemblies_Synchronous_Assemblies_Component_Display" package_suppress="view">
                <Group name="Modeling Component">
                    <Pref name="Assemblies_ModelingComponentColor" type="int" title="Color" value="109" ver="2306" scope="Session">
                        <Description>This option specifies the color of modeling components that can change shape when using the Synchronous Assemblies Move command.</Description>
                        <Color/>
                    </Pref>
                </Group>
            </Tab>
        </Category>
    </Application>
</PrefDefinition>
