<PrefDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www-cyp.ugsolutions.com/assemblies/pman">
    <Application name="Analysis" csh="PMAN_Analysis" package_suppress="simviewer,nx2dedit">
        <Category name="Measurement" csh="PMAN_Analysis_Measurement">
            <Tab name="General" csh="PMAN_Analysis_Measurement_General">
              <Group name="Settings">
                <Label label="Results Filters"/>
                <Pref name="UG_measurementDistanceFilter" type="logical" title="Distance Measurements" value="true" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not distance measurements are displayed when measuring objects.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementLengthFilter" type="logical" title="Length Measurements" value="true" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not length measurements are displayed when measuring objects.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementAngleFilter" type="logical" title="Angle Measurements" value="true" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not angle measurements are displayed when measuring objects.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementFaceFilter" type="logical" title="Face Measurements" value="true" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not face measurements are displayed when measuring objects.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementBodyFilter" type="logical" title="Body Measurements" value="true" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not body measurements are displayed when measuring objects.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementExtremeFilter" type="logical" title="Extreme Measurements" value="false" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not extreme measurements are displayed when measuring objects.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementOtherFilter" type="logical" title="Other Measurements" value="false" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not other measurements are displayed when measuring objects.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Separator/>
                <Pref name="UG_measurementReferenceCSYS" type="int" title="Reference CSYS" value="0" ver="12.0.1" scope="Session">
                    <Description>This option specifies the reference CSYS used when measuring applicable objects.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Absolute - Work Part" value="0"/>
                        <EnumElem name="WCS" value="1"/>
                    </Data>
                </Pref>
                <Pref name="UG_measurementNXConsole" type="logical" title="Send Results to Console" value="false" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not measurement results are sent to the Console on OK or Apply.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="Yes" value="true"/>
                    <EnumElem name="No" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementNXInfoWindow" type="logical" title="View Results in Information Window" value="false" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not measurement results are shown in the Information window on OK or Apply.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="Yes" value="true"/>
                    <EnumElem name="No" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementInViewPlane" type="logical" title="Measure in View Plane" value="false" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not distances or angles are measured relative to the normal view plane of the screen.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="Yes" value="true"/>
                    <EnumElem name="No" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementEnableHints" type="logical" title="Present Hints" value="true" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not hints are presented if there are other possible measurements given the current selection context and options.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="Yes" value="true"/>
                    <EnumElem name="No" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementIndividualCoordinates" type="logical" title="Create Individual Coordinate Expressions" value="false" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not individual expressions for the X, Y, and Z coordinates are created in addition to a point expression.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="Yes" value="true"/>
                    <EnumElem name="No" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementUnits" type="int" title="Units" value="0" ver="2312" scope="Session">
                    <Description>This option specifies the units used during measurement.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="From Units Manager" value="0"/>
                        <EnumElem name="English(NX) - lbm/in/lbf" value="1"/>
                        <EnumElem name="English - lbm/ft/lbf" value="2"/>
                        <EnumElem name="Metric - g/mm/N" value="3"/>
                        <EnumElem name="Metric - g/cm/N" value="4"/>
                        <EnumElem name="Metric(SI) - kg/m/N" value="5"/>
                        <EnumElem name="Metric(NX) - kg/mm/N" value="6"/>
                        <EnumElem name="Metric(Micro) - mg/µm/µN" value="7" />
                    </Data>
                </Pref>
                <Pref name="UG_measurementAngularUnits" type="int" title="Angular Units" value="0" ver="12.0.1" scope="Session">
                    <Description>This option specifies the angular units used during measurement.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="From Units Manager" value="0"/>
                        <EnumElem name="Degrees" value="1"/>
                        <EnumElem name="Radians" value="2"/>
                    </Data>
                </Pref>
                <Pref name="UG_measurementDecimalPlaces" type="int" title="Decimal Places" value="0" ver="12.0.1" scope="Session">
                    <Description>This option specifies the number of decimal places displayed during measurement.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Dialog Box Decimal Places" value="0"/>
                        <EnumElem name="0" value="1"/>
                        <EnumElem name="1" value="2"/>
                        <EnumElem name="2" value="3"/>
                        <EnumElem name="3" value="4"/>
                        <EnumElem name="4" value="5"/>
                        <EnumElem name="5" value="6"/>
                        <EnumElem name="6" value="7"/>
                    </Data>
                </Pref>
                <Pref name="UG_measurementAllowReselectionOfObjects" type="logical" title="Allow Reselection of Objects" value="false" ver="2212" scope="Session">
                  <Description>This option determines whether or not objects that are already in the selection list can be reselected.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="Yes" value="true"/>
                    <EnumElem name="No" value="false"/>
                  </Data>
                </Pref>
              </Group>
              <Group name="Euler Angles">
                <Pref name="UG_measurementEulerAngles" type="int" title="Convention" value="10" ver="12.0.1" scope="Session">
                    <Description>This option specifies the convention used when computing Euler angles measurements.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="X-Y-Z" value="0"/>
                        <EnumElem name="X-Z-Y" value="1"/>
                        <EnumElem name="Y-X-Z" value="2"/>
                        <EnumElem name="Y-Z-X" value="3"/>
                        <EnumElem name="Z-X-Y" value="4"/>
                        <EnumElem name="Z-Y-X" value="5"/>
                        <EnumElem name="X-Y-X" value="6"/>
                        <EnumElem name="X-Z-X" value="7"/>
                        <EnumElem name="Y-X-Y" value="8"/>
                        <EnumElem name="Y-Z-Y" value="9"/>
                        <EnumElem name="Z-X-Z" value="10"/>
                        <EnumElem name="Z-Y-Z" value="11"/>
                    </Data>
                </Pref>              
              </Group>
              <Group name="Bounding Box Method">
                <Pref name="UG_measurementBBoxMethod" type="int" title="Convention" value="0" ver="1872" scope="Session">
                    <Description>This option specifies the method used when computing bounding box measurements.</Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Axis Aligned" value="0"/>
                        <EnumElem name="Best Fit" value="1"/>
                    </Data>
                </Pref>              
              </Group>
              <Group name="Section Inertia">
                 <Pref name="UG_measurementSectionInertiaType" type="int" title="Type" value="0" ver="2212" scope="Session">
                    <Description>This option specifies the method used to calculate the section inertia.
<Formatted>
Solid: Calculates the section inertia as a solid section. A solid analysis can only be performed for closed sections. 

Hollow: Calculates the section inertia as a hollow (sheet metal) section. If the sections are open, hollow analysis is performed automatically.
</Formatted></Description>
                    <OptionMenu/>
                    <Data type="enum">
                        <EnumElem name="Solid" value="0"/>
                        <EnumElem name="Hollow" value="1"/>
                    </Data>
                 </Pref>
                 <Pref name="UG_measurementSectionInertiaHollowThickness_EU" type="real" title="Thickness" value="0.00157" unit="in" ver="2212" scope="Session">
                    <Description>This option specifies the thickness used for to compute a hollow section inertia. The section is offset by half the value of the specified hollow thickness in both directions in the section plane, and the closed section is analyzed.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.000004" include="0"/>
                        <UpperBound value="39370" include="0"/>
                    </Data>
                 </Pref>
                 <Pref name="UG_measurementSectionInertiaHollowThickness_MU" type="real" title="Thickness" value="0.04" unit="mm" ver="2212" scope="Session">
                      <Description>This option specifies the thickness used for to compute a hollow section inertia. The section is offset by half the value of the specified hollow thickness in both directions in the section plane, and the closed section is analyzed.</Description>
                      <Real/>
                      <Data type="range">
                          <LowerBound value="0.0001" include="0"/>
                          <UpperBound value="1000000" include="0"/>
                      </Data>
                 </Pref>
               </Group>
            </Tab>
            <Tab name="Measurements" csh="PMAN_Analysis_Measurement_Measurements">
              <Group name="Distance">
                <Pref name="UG_measurementPointCoordinate" type="logical" title="Point Coordinates" value="true" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not coordinates are displayed when measuring points.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementMinimumDistance" type="logical" title="Minimum Distance" value="true" ver="2206" scope="Session"> 
                  <Description>This option determines whether or not minimum distance is displayed when measuring distances.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementMaximumDistance" type="logical" title="Maximum Distance" value="true" ver="2206" scope="Session">
                  <Description>This option determines whether or not maximum distance is displayed when measuring distances.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementMaximumCenterToEdge" type="logical" title="Maximum Center to Edge" value="true" ver="2206" scope="Session">
                  <Description>This option determines whether or not maximum center to edge distance is displayed when measuring distances.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementMaximumEdgeToCenter" type="logical" title="Maximum Edge to Center" value="true" ver="2206" scope="Session">
                  <Description>This option determines whether or not maximum edge to center distance is displayed when measuring distances.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementCenterToCenter" type="logical" title="Center to Center" value="true" ver="2206" scope="Session">
                  <Description>This option determines whether or not center to center distance is displayed when measuring distances.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementMinimumPerpendicular" type="logical" title="Minimum Perpendicular" value="true" ver="2206" scope="Session">
                  <Description>This option determines whether or not minimum perpendicular distance is displayed when measuring distances.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementMaximumPerpendicular" type="logical" title="Maximum Perpendicular" value="true" ver="2206" scope="Session">
                  <Description>This option determines whether or not maximum perpendicular distance is displayed when measuring distances.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                  <Pref name="UG_measurementMinimumDistanceArcExtension" type="logical" title="Minimum Distance to Arc Extension" value="true" ver="2306" scope="Session">
                  <Description>This option determines whether or not minimum distance to arc extension is displayed when measuring distances.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementMinimumPerpendicularArcExtension" type="logical" title="Minimum Perpendicular to Arc Extension" value="true" ver="2306" scope="Session">
                  <Description>This option determines whether or not minimum perpendicular distance to arc extension is displayed when measuring distances.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementMinimumProjectedClearance" type="logical" title="Minimum Projected Clearance" value="true" ver="2206" scope="Session">
                  <Description>This option determines whether or not minimum projected clearance distance is displayed when measuring distances.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementMaximumProjectedClearance" type="logical" title="Maximum Projected Clearance" value="true" ver="2206" scope="Session">
                  <Description>This option determines whether or not maximum projected clearance distance is displayed when measuring distances.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementMinimumProjectedContact" type="logical" title="Minimum Projected Contact" value="true" ver="2206" scope="Session">
                  <Description>This option determines whether or not minimum projected contact distance is displayed when measuring distances.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementMaximumProjectedContact" type="logical" title="Maximum Projected Contact" value="true" ver="2206" scope="Session">
                  <Description>This option determines whether or not maximum projected contact distance is displayed when measuring distances.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementMinimumProjectedOrthogonal" type="logical" title="Minimum Projected Orthogonal" value="true" ver="2206" scope="Session">
                  <Description>This option determines whether or not minimum projected orthogonal distance is displayed when measuring distances.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementMaximumProjectedOrthogonal" type="logical" title="Maximum Projected Orthogonal" value="true" ver="2206" scope="Session">
                  <Description>This option determines whether or not maximum projected orthogonal distance is displayed when measuring distances.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementThickness" type="logical" title="Thickness" value="true" ver="2206" scope="Session">
                  <Description>This option determines whether or not thickness is displayed when measuring and defining a point on face on a selected body.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>                
                <Pref name="UG_measurementDeltaValues" type="logical" title="X, Y, Z Delta Distances" value="false" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not X, Y, and Z delta distances are displayed when measuring between objects in 3D space or X and Y delta distances are displayed if measuring relative to the view plane of the screen.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementDisplayDistanceStartPoint" type="logical" title="Start Point Coordinates" value="false" ver="2206" scope="Session">
                    <Description>This option determines whether or not start point coordinate information is displayed when measuring distances.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="On" value="true"/>
                        <EnumElem name="Off" value="false"/>
                    </Data>
                </Pref>
                <Pref name="UG_measurementDisplayDistanceEndPoint" type="logical" title="End Point Coordinates" value="false" ver="2206" scope="Session">
                    <Description>This option determines whether or not end point coordinate information is displayed when measuring distances.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="On" value="true"/>
                        <EnumElem name="Off" value="false"/>
                    </Data>
                </Pref>
              </Group>
              <Group name="Curve/Edge">
                <Pref name="UG_measurementCurveLength" type="logical" title="Curve Length" value="true" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not curve length is displayed when measuring curves or edges.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementCurveRadius" type="logical" title="Radius" value="true" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not curve radius is displayed when measuring circular curves or edges.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                  <Pref name="UG_measurementCurveDiameter" type="logical" title="Diameter" value="true" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not curve diameter is displayed when measuring circular curves or edges.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementCurveStart" type="logical" title="Curve Start Coordinates" value="true" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not curve start point coordinates are displayed when measuring curves or edges.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementCurveEnd" type="logical" title="Curve End Coordinates" value="true" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not curve end point coordinates are displayed when measuring curves or edges.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementCurveCenter" type="logical" title="Arc Center Coordinates" value="true" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not arc center point coordinates are displayed when measuring circular curves or edges.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementCurveMinRadius" type="logical" title="Min Radius of Curvature" value="true" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not minimum radius of curvature is displayed when measuring non-linear curves or edges.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementPointsOnCurveLength" type="logical" title="Points on Curve Length" value="true" ver="2312" scope="Session">
                    <Description>This option determines whether or not curve length is displayed when measuring points on curve.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="On" value="true"/>
                        <EnumElem name="Off" value="false"/>
                    </Data>
                </Pref>
              </Group>
              <Group name="Angle">
                <Pref name="UG_measurementInnerAngle" type="logical" title="Inner Angle" value="true" ver="2206" scope="Session">
                  <Description>This option determines whether or not inner angle is displayed when measuring angles.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementOuterAngle" type="logical" title="Outer Angle" value="true" ver="2206" scope="Session">
                  <Description>This option determines whether or not outer angle is displayed when measuring angles.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementSupplementaryAngle" type="logical" title="Supplementary Angle" value="true" ver="2206" scope="Session">
                  <Description>This option determines whether or not supplementary angle is displayed when measuring angles.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementComplementaryAngle" type="logical" title="Complementary Angle" value="true" ver="2206" scope="Session">
                  <Description>This option determines whether or not complementary angle is displayed when measuring angles.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
              </Group>
              <Group name="Face">
                <Pref name="UG_measurementFaceArea" type="logical" title="Area" value="true" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not area is displayed when measuring faces.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementFacePerimeter" type="logical" title="Perimeter" value="true" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not perimeter is displayed when measuring faces.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementFaceRadius" type="logical" title="Radius" value="true" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not radius is displayed when measuring cylindrical faces.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementFaceDiameter" type="logical" title="Diameter" value="true" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not diameter is displayed when measuring cylindrical faces.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
                    <EnumElem name="Off" value="false"/>
                  </Data>
                </Pref>
                <Pref name="UG_measurementFaceCOG" type="logical" title="Centroid" value="true" ver="12.0.1" scope="Session">
                  <Description>This option determines whether or not centroid is displayed when measuring faces.</Description>
                  <Toggle off="false"/>
                  <Data type="enum">
                    <EnumElem name="On" value="true"/>
