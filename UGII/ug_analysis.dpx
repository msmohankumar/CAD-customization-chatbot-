                <Pref name="Analysis_PDA_NegativeSmallAngleFaceColor" type="int" title="Negative (&lt; Draft Angle Limit)" value="56" ver="6.0.0" scope="Session">
                    <Description>This option specifies the color of the negative faces which draft angles, relative the reverse mold release direction (i.e. the -Z direction), are less than the specified draft angle limit.</Description>
                    <Color/>
                </Pref>
                <Pref name="Analysis_PDA_NegativeLargeAngleFaceColor" type="int" title="Negative (&gt;= Draft Angle Limit)" value="140" ver="6.0.0" scope="Session">
