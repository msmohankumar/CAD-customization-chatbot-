                        <EnumElem name="On" value="1"/>
                        <EnumElem name="Off" value="0"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_ClrAnlMsgSoft" type="int" title="Soft" value="1" ver="3.0.0" scope="Session">
                    <Description>Two objects are situated such that the minimum distance between the bodies is less than or equal to the clearance zone, but the objects do not touch.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="On" value="1"/>
                        <EnumElem name="Off" value="0"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_ClrAnlMsgTouch" type="int" title="Touching" value="1" ver="3.0.0" scope="Session">
                    <Description>Two objects are touching but do not intersect each other.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="On" value="1"/>
                        <EnumElem name="Off" value="0"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_ClrAnlMsgContain" type="int" title="Containment" value="1" ver="3.0.0" scope="Session">
