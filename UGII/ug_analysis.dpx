            <Tab name="Report" csh="PMAN_Analysis_Assembly_Clearance_Report">
                <Label label="The following four options determine the types of interferences that will be reported during analysis or review."/>
                <Pref name="Assemblies_ClrAnlMsgHard" type="int" title="Hard" value="1" ver="3.0.0" scope="Session">
                    <Description>The objects intersect each other.</Description>
                    <Toggle off="0"/>
