              </Pref>
              <Pref name="Design_for_Sustainability_PermissionToEditFiles" type="logical" title="Permission to Edit Files" value="true" ver="2312" scope="Session">
                <Description>This option specifies whether the environmental material and mapping files can be edited using the Map Environmental Material dialog. When the option is off, the directory paths set by the customer defaults can only be viewed and the subtypes cannot be edited.</Description>
                <Toggle off="false"/>
                <Data type="enum">
                  <EnumElem name="Yes" value="true"/>
                  <EnumElem name="No" value="false"/>
                </Data>
              </Pref>
           </Tab>
           <Tab name="Calculate Scores" csh="PMAN_Analysis_Design_for_Sustainability_Calculate_Scores">
             <Group name="Subtype Score Options">
               <Pref name="Design_for_Sustainability_NominalMaterial" type="logical" title="Nominal" value="true" ver="2306" scope="Session">
                   <Description>This option specifies whether or not to calculate sustainability scores for the environmental materials according to the subtypes in the SustainabilityMapping.json file.</Description>
                   <Toggle off="false"/>
                   <Data type="enum">
                       <EnumElem name="Yes" value="true"/>
                       <EnumElem name="No" value="false"/>
                   </Data>
               </Pref>
               <Pref name="Design_for_Sustainability_RecycledMaterial" type="logical" title="Recycled" value="false" ver="2306" scope="Session">
                   <Description>This option specifies whether or not to calculate sustainability scores for the recycled environmental materials.</Description>
                   <Toggle off="false"/>
                   <Data type="enum">
                       <EnumElem name="Yes" value="true"/>
                       <EnumElem name="No" value="false"/>
                   </Data>
               </Pref>
               <Pref name="Design_for_Sustainability_RenewableMaterial" type="logical" title="Renewable" value="false" ver="2306" scope="Session">
                   <Description>This option specifies whether or not to calculate sustainability scores for the renewable environmental materials.</Description>
                   <Toggle off="false"/>
                   <Data type="enum">
                       <EnumElem name="Yes" value="true"/>
                       <EnumElem name="No" value="false"/>
                   </Data>
               </Pref>
             </Group>
             <Pref name="Design_for_Sustainability_KeepOnlySelectedSubtypeScore" type="int" title="Unselected Subtype Scores" value="1" ver="2312" scope="Session">
               <Description>This option specifies if unselected scores should be deleted or retained. Retained scores are not updated.</Description>
               <RadioBox orientation="horizontal"/>
               <Data type="enum">
                 <EnumElem name="Delete" value="0"/>
                 <EnumElem name="Retain" value="1"/>
               </Data>
             </Pref>
             <Pref name="Design_for_Sustainability_DisplayUpdatedScores" type="logical" title="Display Updated Scores" value="false" ver="2312" scope="Session">
               <Description>This option specifies whether or not to display sustainability scores for environmental materials in the Information window upon update.</Description>
               <Toggle off="false"/>
               <Data type="enum">
                 <EnumElem name="Yes" value="true"/>
                 <EnumElem name="No" value="false"/>
               </Data>
             </Pref>
          </Tab>
        </Category>

    </Application>
