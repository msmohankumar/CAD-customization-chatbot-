                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_ClrInterfRange1_MU" type="real" title="Upper Limit" value="1.0" unit="mm" ver="4.0.2" scope="Session">
                    <Description>This option specifies the upper distance limit for this range.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_ClrInterfRange1_MCU" type="real" title="Upper Limit" value="1.0" unit="μm" ver="2212" scope="Session">
                    <Description>This option specifies the upper distance limit for this range.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_ClrInterfRange1_SIU" type="real" title="Upper Limit" value="1.0" unit="m" ver="2212" scope="Session">
                    <Description>This option specifies the upper distance limit for this range.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_ClrInterfColor1" type="int" title="Color" value="147" ver="6.0.0" scope="Session">
                    <Description>This option specifies the color of the distance lines displayed for a soft interference at this range.</Description>
                    <Color/>
                </Pref>
                </Group>
                <Group name="Range 2">
                <Label label="Greater than range 1 and less than the following upper limit:"/>
                <Pref name="Assemblies_ClrInterfRange2_EU" type="real" title="Upper Limit" value="0.2" unit="in" ver="4.0.2" scope="Session">
