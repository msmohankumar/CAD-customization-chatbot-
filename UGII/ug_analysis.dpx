        <Category name="Assembly Clearance" csh="PMAN_Analysis_Assembly_Clearance" package_suppress="view,mechatronicsplayer">
            <Tab name="General" csh="PMAN_Analysis_Assembly_Clearance_General">
                <Docum>The following options define the settings for clearance analysis. These values are set only when a new clearance analysis data set is created. Existing data sets will not be affected.</Docum>
                <Pref name="Assemblies_ClrAnlMode" type="int" title="Calculation Settings" value="3" ver="3.0.0" scope="Part">
                    <Description>This option specifies the type of analysis to be used.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="Lightweight" value="6"/>
                        <EnumElem name="Exact if Loaded" value="3"/>
                        <EnumElem name="Exact" value="1"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_ClrAnlDefClrZone_MU" type="real" title="Clearance Zone" value="0.0" unit="mm" ver="3.0.0" scope="Part">
                    <Description>This option specifies a clearance zone for the objects in the display part. Objects that are closer to each other than the clearance zone, but not touching, will be reported as soft interferences.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_ClrAnlDefClrZone_EU" type="real" title="Clearance Zone" value="0.0" unit="in" ver="3.0.0" scope="Part">
                    <Description>This option specifies a clearance zone for the objects in the display part. Objects that are closer to each other than the clearance zone, but not touching, will be reported as soft interferences.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <!---->
                <Pref name="Assemblies_ClrAnlResetOutOfDatePair" type="int" title="Reset Out-of-date Interferences to Undetermined" value="0" ver="5.0.0" scope="Session">
                    <Description>This option determines whether or not out-of-date clearance pairs status should be reset back to undetermined.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="Yes" value="1"/>
                        <EnumElem name="No" value="0"/>
                    </Data>
                </Pref>
            </Tab>
            <Tab name="Exceptions" csh="PMAN_Analysis_Assembly_Clearance_Ignored_Pairs">
                <Group name="Dataset Options">
                <Pref name="Assemblies_ClrAnlExclWithinComp" type="int" title="Pairs within the Same Subassembly" value="0" ver="3.0.0" scope="Part">
                    <Description>This option determines whether or not object pairs that come from the same subassembly/component should be excluded from the analysis.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="On" value="1"/>
                        <EnumElem name="Off" value="0"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_ClrAnlExclWithinGroup" type="int" title="Pairs within the Same Group" value="0" ver="3.0.0" scope="Part">
                    <Description>This option determines whether or not object pairs that come from the same group should be excluded from the analysis.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="On" value="1"/>
                        <EnumElem name="Off" value="0"/>
                    </Data>
                </Pref>
                <Pref name="Assemblies_ClrAnlExclWithinPart" type="int" title="Pairs within the Same Part" value="0" ver="3.0.0" scope="Part">
                    <Description>This option determines whether or not object pairs that come from the same part should be excluded from the analysis.</Description>
                    <Toggle off="0"/>
                    <Data type="enum">
                        <EnumElem name="On" value="1"/>
                        <EnumElem name="Off" value="0"/>
                    </Data>
                </Pref>
                </Group>
                <Pref name="Assemblies_ClrAnlReCalcIgnOutOfDatePair" type="int" title="Reanalyze Out-of-date Ignored Pairs" value="0" ver="5.0.0" scope="Session">
                    <Description>This option determines whether or not ignored pairs should be reanalyzed during clearance analysis.</Description>
