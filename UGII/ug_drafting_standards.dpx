                <EnumElem name="Shaded" bitmap="shade.bmp" value="3"/>
              </Data>
            </Pref>
            <Pref name="Drafting_cursorTracking" type="int" title="Cursor Tracking" drftStd="yes" value="0" ver="3.0.0" scope="Part">
