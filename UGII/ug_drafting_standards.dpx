              <EnumElem name="Diameter" value="HOLE_DIAMETER"/>
              <EnumElem name="Diameter Fit" value="HOLE_DIAMETER_FIT"/>
              <EnumElem name="Depth" value="HOLE_DEPTH"/>
              <EnumElem name="Depth Fit" value="HOLE_DEPTH_FIT"/>
              <EnumElem name="Drill Type" value="HOLE_DRILL_TYPE"/>
              <EnumElem name="Drill Direction" value="HOLE_DRILL_DIRECTION"/>
            </Data>
          </Pref>
        </Tab>
        <Tab name="Hole Filters" csh="PMAN_Drafting_Standard_Table_Hole_Table_Hole_Filters">
          <Group name="Types to Include">
            <Pref name="DR_HOLEREPORT_TYPE1" type="logical" title="Through Holes" drftStd="yes" value="true" ver="9.0.0" scope="Part">
              <Description>This option determines whether or not to include through holes in the hole table.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="No" value="false"/>
                <EnumElem name="Yes" value="true"/>
              </Data>
            </Pref>
            <Pref name="DR_HOLEREPORT_TYPE2" type="logical" title="Blind Holes" drftStd="yes" value="true" ver="9.0.0" scope="Part">
              <Description>This option determines whether or not to include blind holes in the hole table.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="No" value="false"/>
                <EnumElem name="Yes" value="true"/>
              </Data>
            </Pref>
            <Pref name="DR_HOLEREPORT_TYPE3" type="logical" title="Counterbored Holes" drftStd="yes" value="true" ver="9.0.0" scope="Part">
              <Description>This option determines whether or not to include counterbored holes in the hole table</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="No" value="false"/>
                <EnumElem name="Yes" value="true"/>
              </Data>
            </Pref>
            <Pref name="DR_HOLEREPORT_TYPE4" type="logical" title="Countersunk Holes" drftStd="yes" value="true" ver="9.0.0" scope="Part">
              <Description>This option determines whether or not to include countersunk holes in the hole table.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="No" value="false"/>
                <EnumElem name="Yes" value="true"/>
              </Data>
            </Pref>
            <Pref name="DR_HOLEREPORT_TYPE5" type="logical" title="Threaded Holes" drftStd="yes" value="true" ver="9.0.0" scope="Part">
              <Description>This option determines whether or not to include threaded holes in the hole table.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="No" value="false"/>
                <EnumElem name="Yes" value="true"/>
              </Data>
            </Pref>
            <Pref name="DR_HOLEREPORT_TYPE6" type="logical" title="Combination Holes" drftStd="yes" value="true" ver="9.0.0" scope="Part">
              <Description>This option determines whether or not to include combo holes in the hole table.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="No" value="false"/>
                <EnumElem name="Yes" value="true"/>
              </Data>
            </Pref>
            <Pref name="DR_HOLEREPORT_TYPE7" type="logical" title="Wire EDM Start Holes" drftStd="yes" value="true" ver="9.0.0" scope="Part">
              <Description>This option determines whether or not to include wire EDM start holes in the hole table.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="No" value="false"/>
                <EnumElem name="Yes" value="true"/>
              </Data>
            </Pref>
            <Pref name="DR_HOLEREPORT_TYPE8" type="logical" title="Circles in Modeling Sketches" drftStd="yes" value="false" ver="9.0.0" scope="Part">
              <Description>This option determines whether or not to include circles in Modeling sketches in the hole table.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="No" value="false"/>
                <EnumElem name="Yes" value="true"/>
              </Data>
            </Pref>
            <Pref name="DR_HOLEREPORT_TYPE9" type="logical" title="Circles in Drafting Sketches" drftStd="yes" value="false" ver="9.0.0" scope="Part">
              <Description>This option determines whether or not to include circles in Drafting sketches in the hole table.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="No" value="false"/>
                <EnumElem name="Yes" value="true"/>
              </Data>
            </Pref>
            <Pref name="DR_HOLEREPORT_ONESIDE" type="int" title="Scope" drftStd="yes" value="2" ver="9.0.0" scope="Part">
              <Description>This option specifies which holes to include in the hole table.
<Formatted>
One-sided Holes: Only holes visible in the view are included.
Two-sided Holes: All holes are included.
</Formatted></Description>
              <RadioBox orientation="vertical"/>
              <Data type="enum">
                <EnumElem name="One-sided Holes" value="1"/>
                <EnumElem name="Two-sided Holes" value="2"/>
              </Data>
            </Pref>
            <Pref name="DR_HOLEREPORT_COUNTERSINK_ANGLE_MCU" type="real" title="Countersink Angle" drftStd="yes" value="90" unit="deg" ver="2306" scope="Part">
                <Description>This option specifies the countersink angle a hole must have to be identified as a countersunk hole.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="1"/>
                    <UpperBound value="360.0" include="1"/>
                </Data>
            </Pref>
            <Pref name="DR_HOLEREPORT_COUNTERSINK_ANGLE_MU" type="real" title="Countersink Angle" drftStd="yes" value="90" unit="deg" ver="9.0.0" scope="Part">
                <Description>This option specifies the countersink angle a hole must have to be identified as a countersunk hole.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="1"/>
                    <UpperBound value="360.0" include="1"/>
                </Data>
            </Pref>
            <Pref name="DR_HOLEREPORT_COUNTERSINK_ANGLE_SIU" type="real" title="Countersink Angle" drftStd="yes" value="90" unit="deg" ver="2306" scope="Part">
                <Description>This option specifies the countersink angle a hole must have to be identified as a countersunk hole.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="1"/>
                    <UpperBound value="360.0" include="1"/>
                </Data>
            </Pref>
            <Pref name="DR_HOLEREPORT_COUNTERSINK_ANGLE_EU" type="real" title="Countersink Angle" drftStd="yes" value="82" unit="deg" ver="9.0.0" scope="Part">
              <Description>This option specifies the countersink angle a hole must have to be identified as a countersunk hole.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.0" include="1"/>
                <UpperBound value="360.0" include="1"/>
              </Data>
            </Pref>
            <Pref name="DR_HOLEREPORT_INCLUDE_PARTIAL_HOLES" type="logical" title="Include Partial Holes" drftStd="yes" value="false" ver="9.0.0" scope="Part">
              <Description>This option determines whether or not holes that are less than 360 degrees will be selectable to include in the hole table.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="No" value="false"/>
                <EnumElem name="Yes" value="true"/>
              </Data>
            </Pref>
            <Pref name="DR_HOLEREPORT_PARTIAL_ANGLE" type="real" title="Partial Hole Angle" drftStd="yes" value="180" unit="deg" ver="9.0.0" scope="Part">
              <Description>This option specifies the partial hole angle. Only holes with an angle greater than or equal to this value can be included in the hole table.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="5.0" include="1"/>
                <UpperBound value="360.0" include="1"/>
              </Data>
            </Pref>
          </Group>
        </Tab>
        <Tab name="Hole Type Symbols" csh="PMAN_Drafting_Standard_Table_Hole_Table_Hole_Type_Symbols">
          <Pref name="DR_HOLEREPORT_TYPE1_SYMBOL" type="string" title="Through Hole" drftStd="yes" value="N" ver="9.0.0" scope="Session">
            <Description>This option specifies the symbol for a through hole.</Description>
            <String/>
          </Pref>
          <Pref name="DR_HOLEREPORT_TYPE2_SYMBOL" type="string" title="Blind Hole" drftStd="yes" value="B" ver="9.0.0" scope="Session">
            <Description>This option specifies the symbol for a blind hole.</Description>
            <String/>
          </Pref>
          <Pref name="DR_HOLEREPORT_TYPE3_SYMBOL" type="string" title="Counterbored Hole" drftStd="yes" value="CB" ver="9.0.0" scope="Session">
            <Description>This option specifies the symbol for a counterbored hole.</Description>
            <String/>
          </Pref>
          <Pref name="DR_HOLEREPORT_TYPE4_SYMBOL" type="string" title="Countersunk Hole" drftStd="yes" value="CS" ver="9.0.0" scope="Session">
            <Description>This option specifies the symbol for a countersunk hole.</Description>
            <String/>
          </Pref>
          <Pref name="DR_HOLEREPORT_TYPE5_SYMBOL" type="string" title="Threaded Hole" drftStd="yes" value="M" ver="9.0.0" scope="Session">
            <Description>This option specifies the symbol for a threaded hole.</Description>
            <String/>
          </Pref>
          <Pref name="DR_HOLEREPORT_TYPE6_SYMBOL" type="string" title="Combo Hole" drftStd="yes" value="COM" ver="9.0.0" scope="Session">
            <Description>This option specifies the symbol for a combo hole.</Description>
            <String/>
          </Pref>
          <Pref name="DR_HOLEREPORT_TYPE7_SYMBOL" type="string" title="Wire EDM Start Hole" drftStd="yes" value="W" ver="9.0.0" scope="Session">
            <Description>This option specifies the symbol for a wire EDM start hole.</Description>
            <String/>
          </Pref>
          <Pref name="DR_HOLEREPORT_TYPE8_SYMBOL" type="string" title="Sketch Hole" drftStd="yes" value="SK" ver="9.0.0" scope="Session">
            <Description>This option specifies the symbol for a circular sketch hole.</Description>
            <String/>
          </Pref>
        </Tab>
        <Tab name="Label Format" csh="PMAN_Drafting_Standard_Table_Hole_Table_Label_Format">
          <Pref name="DR_HOLEREPORT_LABEL" type="int" title="Index" drftStd="yes" value="1" ver="9.0.0" scope="Part">
            <Description>This option specifies how to compose the hole label.</Description>
            <OptionMenu/>
            <Data type="enum">
              <EnumElem name="Hole Type Symbol and Number" value="0"/>
              <EnumElem name="Number" value="1"/>
              <EnumElem name="Letter and Number" value="2"/>
            </Data>
          </Pref>
          <Pref name="DR_HOLEREPORT_LABEL_WITH_LEADER" type="logical" title="Show Hole Label with Leader" drftStd="yes" value="true" ver="9.0.0" scope="Part">
            <Description>This option determines whether or not hole labels should be displayed with leaders.</Description>
            <Toggle off="false"/>
            <Data type="enum">
              <EnumElem name="No" value="false"/>
              <EnumElem name="Yes" value="true"/>
            </Data>
          </Pref>
          <Pref name="DR_HOLEREPORT_RELATIVE_LOCATION" type="int" title="Relative Location" drftStd="yes" value="2" ver="9.0.0" scope="Part">
            <Description>This option specifies the location of the ID label relative to the hole.</Description>
            <OptionMenu/>
            <Data type="enum">
              <EnumElem name="Top Left" value="0"/>
              <EnumElem name="Top Middle" value="1"/>
              <EnumElem name="Top Right" value="2"/>
              <EnumElem name="Center Left" value="3"/>
              <EnumElem name="Center Middle" value="4"/>
              <EnumElem name="Center Right" value="5"/>
              <EnumElem name="Bottom Left" value="6"/>
              <EnumElem name="Bottom Middle" value="7"/>
              <EnumElem name="Bottom Right" value="8"/>
            </Data>
          </Pref>
          <Pref name="DR_HOLEREPORT_CHARACTERS_TO_SKIP" type="string" title="Characters to Skip" drftStd="yes" value="I,O,Q,S,X,Z" ver="9.0.0" scope="Part">
            <Description>This option specifies the characters to skip when creating new hole table labels. This is a comma separated list.</Description>
            <String/>
          </Pref>
        </Tab>
        <Tab name="Attributes" csh="PMAN_Drafting_Standard_Table_Hole_Table_Attributes">
          <Pref name="DR_HOLEREPORT_DIA_FIT" type="string" title="Diameter Fit" drftStd="yes" value="*D_FIT*, *D_TOL*" ver="9.0.0" scope="Session">
            <Description>For an attribute applied to the face of a hole to be interpreted as diameter fit, its name has to include one of the strings specified by this option. The value of the attribute can then be displayed in the hole table.</Description>
            <Widestring/>
          </Pref>
          <Pref name="DR_HOLEREPORT_DEP_FIT" type="string" title="Depth Fit" drftStd="yes" value="*T_FIT*, *T_TOL*" ver="9.0.0" scope="Session">
            <Description>For an attribute applied to the face of a hole to be interpreted as depth fit, its name has to include one of the strings specified by this option. The value of the attribute can then be displayed in the hole table.</Description>
            <Widestring/>
          </Pref>
          <Pref name="DR_HOLEREPORT_DRILLING" type="string" title="Drill Type" drftStd="yes" value="*DRILL*, *DRILL_TYPE*" ver="9.0.0" scope="Session">
            <Description>For an attribute applied to the face of a hole to be interpreted as drill type, its name has to include one of the strings specified by this option. The value of the attribute can then be displayed in the hole table.</Description>
            <Widestring/>
          </Pref>
          <Pref name="DR_HOLEREPORT_WIREEDM" type="string" title="Wire EDM Start Hole Type" drftStd="yes" value="PDW_WIRE" ver="9.0.0" scope="Session">
            <Description>This option specifies the attribute a sketch hole must have to be interpreted as a wire EDM start hole.</Description>
            <Widestring/>
          </Pref>
          <Pref name="DR_HOLEREPORT_IGNORED_HOLE" type="string" title="Ignored Hole" drftStd="yes" value="*IGNORED_HOLE*" ver="9.0.0" scope="Session">
            <Description>To exclude a hole from the hole table, its face has to have an attribute which name includes one of the strings specified by this option.</Description>
            <Widestring/>
          </Pref>
        </Tab>
      </SubCategory>
      <SubCategory name="Part Family Table" csh="PMAN_Drafting_Standard_Table_Part_Family_Table">
        <Tab name="Part Family Table" csh="PMAN_Drafting_Standard_Table_Part_Family_Table_Part_Family_Table">
          <Group name="Workflow">
            <Pref name="Drafting_partFamilyTableAllowManualRows" type="logical" title="Allow Manual Rows" drftStd="yes" value="true" ver="1980" scope="Part">
              <Description>This option determines whether or not manual rows are allowed to be inserted into a part family table.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="Yes" value="true"/>
                <EnumElem name="No" value="false"/>
              </Data>
