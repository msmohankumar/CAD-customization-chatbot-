                    <EnumElem name="6" value="6"/>
                </Data>
            </Pref>
          <Pref name="DR_HOLEREPORT_PRECISION_EU" type="int" title="Decimal Places" drftStd="yes" value="3" ver="9.0.0" scope="Part">
            <Description>This option specifies the number of decimal places for numerical values in the hole table.</Description>
            <OptionMenu/>
