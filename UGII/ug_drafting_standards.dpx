            <Pref name="Drafting_partFamilyTableAllowManualColumns" type="logical" title="Allow Manual Columns" drftStd="yes" value="true" ver="1980" scope="Part">
              <Description>This option determines whether or not manual columns are allowed to be inserted into a part family table.</Description>
