    <Category name="PMI" csh="PMAN_Drafting_Standard_PMI">
      <SubCategory name="GD&amp;T General" csh="PMAN_Drafting_Standard_PMI_GDT_General">
        <Tab name="Standard" csh="PMAN_Drafting_Standard_PMI_GDT_General_Standard">
          <Group name="Configurable Rules">
            <Pref name="Gdt_allowCircleU" type="int" title="Allow Circle U Symbol" drftStd="yes" value="0" ver="3.0.0" scope="Part">
              <Description>This option determines whether or not the circle U symbol will be available amongst the Profile Options (which are available only when creating or editing a Profile of a Line or a Profile of a Surface tolerance).</Description>
              <Toggle off="0"/>
              <Data type="enum">
                <EnumElem name="No" value="0"/>
                <EnumElem name="Yes" value="1"/>
              </Data>
              <Docum>The circle U symbol can be used to mark unequally disposed tolerance zones for the Unequally Disposed Bilateral profile type.</Docum>
            </Pref>
            <Pref name="Gdt_validDatumChar" type="string" title="Allowed Datum Letters" drftStd="yes" value="ABCDEFGHJKLMNPRSTUVWXYZ" ver="2206" scope="Part">
                <Description>This option specifies the English/ASCII label letters that are valid when creating datum identifiers.</Description>
                <String/>
            </Pref> 
            <Pref name="Gdt_invalidDatumChar" type="string" title="Excluded Datum Letters" drftStd="yes" value="IOQ" ver="3.0.0" scope="Part">
              <Description>This option specifies the label letters that are not valid when creating datum identifiers.</Description>
              <String/>
              <Docum>A maximum of 25 characters can be specified. The specified characters should not be separated (by for example inserting a space or a comma between characters). If the letters I, O, and Q are not specified, they will become legal to use as datum labels. This would not be standard compliant.</Docum>
            </Pref>
          </Group>
        </Tab>
        <Tab name="Region" csh="PMAN_Drafting_Standard_PMI_GDT_General_Region">
          <Pref name="Gdt_xhatchAngle" type="real" title="Hatch Angle" drftStd="yes" value="45.0" unit="deg" ver="3.0.0" scope="Session">
            <Description>This option specifies the angle of the crosshatch.</Description>
            <Real/>
          </Pref>
          <Pref name="Gdt_xhatchDistance_MCU" type="real" title="Hatch Distance" drftStd="yes" value="2.5" unit="μm" ver="2306" scope="Session">
            <Description>This option specifies the distance between crosshatch lines.</Description>
            <Real/>
            <Data type="range">
              <LowerBound value="0.0" include="0"/>
            </Data>
          </Pref>
            <Pref name="Gdt_xhatchDistance_MU" type="real" title="Hatch Distance" drftStd="yes" value="2.5" unit="mm" ver="3.0.0" scope="Session">
                <Description>This option specifies the distance between crosshatch lines.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                </Data>
            </Pref>
            <Pref name="Gdt_xhatchDistance_SIU" type="real" title="Hatch Distance" drftStd="yes" value="2.5" unit="m" ver="2306" scope="Session">
                <Description>This option specifies the distance between crosshatch lines.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                </Data>
            </Pref>
          <Pref name="Gdt_xhatchDistance_EU" type="real" title="Hatch Distance" drftStd="yes" value="0.1" unit="in" ver="3.0.0" scope="Session">
            <Description>This option specifies the distance between crosshatch lines.</Description>
            <Real/>
            <Data type="range">
              <LowerBound value="0.0" include="0"/>
            </Data>
          </Pref>
        </Tab>
      </SubCategory>
      <SubCategory name="Section View" csh="PMAN_Drafting_Standard_PMI_Section_view">
          <Tab name="Cutting Plane Symbol" csh="PMAN_Drafting_Standard_PMI_Section_View_CuttingPlaneSymbol">
              <Group name="Text">
                  <Pref name="Drafting_PMI_CPSText_AllowedText" type="utf8string" title="Allowed Letters" drftStd="yes" value="ABCDEFGHJKLMNPRSTUVWXYZ0123456789A_." ver="2206" scope="Part">
                      <Description>This option specifies the English/ASCII letter list that the cutting plane symbol text will increment through.</Description>
                      <Widestring/>
                  </Pref>
                  <Pref name="Drafting_PMI_CPSText_Excluded" type="utf8string" title="Excluded Letters" drftStd="yes" value="IOQ" ver="2206" scope="Part">
                      <Description>This option specifies the letters that are excluded from being used in a cutting plane symbol text.</Description>
                      <String/>
                  </Pref>   
              </Group>    
          </Tab>
      </SubCategory>
    </Category>   
  </Application>
