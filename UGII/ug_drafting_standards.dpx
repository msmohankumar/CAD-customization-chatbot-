              <EnumElem name="4" value="4"/>
              <EnumElem name="5" value="5"/>
              <EnumElem name="6" value="6"/>
            </Data>
          </Pref>
          <Pref name="DR_HOLEREPORT_DELETED_HOLES_DISPLAY" type="int" title="Deleted Holes Display" drftStd="yes" value="0" ver="9.0.0" scope="Part">
            <Description>This option specifies how to show rows that are deleted.</Description>
            <OptionMenu/>
            <Data type="enum">
              <EnumElem name="Strikethrough" value="0"/>
              <EnumElem name="Hide Text" value="1"/>
              <EnumElem name="Remove" value="2"/>
            </Data>
          </Pref>
          <Pref name="DR_HOLEREPORT_DEPTH_DATUM" type="int" title="Depth Measurement Datum" drftStd="yes" value="0" ver="9.0.0" scope="Part">
            <Description>This option specifies how to report the depth of holes.</Description>
            <OptionMenu/>
            <Data type="enum">
              <EnumElem name="Top /Bottom Face" value="0"/>
              <EnumElem name="Base Face" value="1"/>
              <EnumElem name="Step Face" value="2"/>
