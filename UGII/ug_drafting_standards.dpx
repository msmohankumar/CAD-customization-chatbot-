<PrefDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www-cyp.ugsolutions.com/assemblies/pman">
  <Application name="Drafting Standard" csh="PMAN_Drafting_Standard" package_suppress="simcenter3d,simviewer,view,mechatronicsplayer">
    <Category name="General" csh="PMAN_Drafting_Standard_General">
      <SubCategory name="Standard" csh="PMAN_Drafting_Standard_General_Standard">
        <Tab name="Standard" csh="PMAN_Drafting_Standard_General_Standard_Standard">
          <Group name="Standards">
            <Pref name="Drafting_centerLineDisplay" type="int" title="Centerline Display" drftStd="yes" value="0" ver="3.0.0" scope="Session">
              <Description>This option specifies what standard should be used for centerline display.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="Normal" value="0"/>
                <EnumElem name="China National Standard" value="1"/>
              </Data>
            </Pref>
            <Pref name="Drafting_SFsymbolStandard" type="int" title="Surface Finish" drftStd="yes" value="1" ver="7.5.0" scope="Part">
              <Description>This option specifies the standard for the Drafting surface finish symbol.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="ANSI/ASME" value="0"/>
                <EnumElem name="ISO 1992" value="1"/>
                <EnumElem name="JIS" value="2"/>
                <EnumElem name="DIN 1992" value="3"/>
                <EnumElem name="ISO 2002" value="4"/>
                <EnumElem name="DIN 2002" value="5"/>
                <EnumElem name="GB 131- 93" value="6"/>
                <EnumElem name="ESKD" value="7"/>
              </Data>
            </Pref>
            <Pref name="Drafting_weldSymbolStandard_EU" type="string" title="Weld" drftStd="yes" value="ANSI" ver="7.5.0" scope="Part">
              <Description>This option specifies the standard for weld symbols.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="ANSI/AWS" value="ANSI"/>
                <EnumElem name="ISO" value="ISO"/>
                <EnumElem name="DIN" value="DIN"/>
                <EnumElem name="JIS" value="JIS"/>
                <EnumElem name="ESKD" value="ESKD"/>
                <EnumElem name="GB" value="GB"/>
              </Data>
            </Pref>
            <Pref name="Drafting_weldSymbolStandard_MCU" type="string" title="Weld" drftStd="yes" value="ISO" ver="2306" scope="Part">
                <Description>This option specifies the standard for weld symbols.</Description>
                <OptionMenu/>
                <Data type="enum">
                    <EnumElem name="ANSI/AWS" value="ANSI"/>
                    <EnumElem name="ISO" value="ISO"/>
                    <EnumElem name="DIN" value="DIN"/>
                    <EnumElem name="JIS" value="JIS"/>
                    <EnumElem name="ESKD" value="ESKD"/>
                    <EnumElem name="GB" value="GB"/>
                </Data>
            </Pref>
            <Pref name="Drafting_weldSymbolStandard_MU" type="string" title="Weld" drftStd="yes" value="ISO" ver="7.5.0" scope="Part">
              <Description>This option specifies the standard for weld symbols.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="ANSI/AWS" value="ANSI"/>
                <EnumElem name="ISO" value="ISO"/>
                <EnumElem name="DIN" value="DIN"/>
                <EnumElem name="JIS" value="JIS"/>
                <EnumElem name="ESKD" value="ESKD"/>
                <EnumElem name="GB" value="GB"/>
              </Data>
            </Pref>
            <Pref name="Drafting_weldSymbolStandard_SIU" type="string" title="Weld" drftStd="yes" value="ISO" ver="2306" scope="Part">
                <Description>This option specifies the standard for weld symbols.</Description>
                <OptionMenu/>
                <Data type="enum">
                    <EnumElem name="ANSI/AWS" value="ANSI"/>
                    <EnumElem name="ISO" value="ISO"/>
                    <EnumElem name="DIN" value="DIN"/>
                    <EnumElem name="JIS" value="JIS"/>
                    <EnumElem name="ESKD" value="ESKD"/>
                    <EnumElem name="GB" value="GB"/>
                </Data>
            </Pref>
            <Pref name="Drafting_limitsAndFitsStandard" type="int" title="Limits and Fits" drftStd="yes" value="ISO" ver="8.0.0" scope="Part">
              <Description>This option specifies what Drawing Standard lookup table is used for Limits and Fits style tolerances.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="ISO" value="0"/>
                <EnumElem name="ANSI" value="1"/>
                <EnumElem name="JIS" value="2"/>
                <EnumElem name="DIN" value="3"/>
                <EnumElem name="ESKD" value="4"/>
                <EnumElem name="GB" value="5"/>
              </Data>
            </Pref>
          </Group>
          <Group name="Text Editor">
            <Pref name="Drafting_datumFeatureDisplay" type="int" title="Datum Symbol Display" drftStd="yes" value="0" ver="3.0.0" scope="Session">
              <Description>This option specifies what standard should be used for datum symbol display.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="Normal" value="0"/>
                <EnumElem name="China National Standard" value="1"/>
              </Data>
            </Pref>
            <Pref name="Drafting_checkStandard_EU" type="int" title="Tolerancing Standard" drftStd="yes" value="0" ver="7.5.0" scope="Session">
              <Description>This option specifies the tolerancing standard that should be used. The standard chosen controls the availability of geometric tolerancing callouts and some of the datum leader types. It also controls the standard that a validation of the feature control frame syntax should be checked against (applies to ANSI and ISO only).</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="ANSI Y14.5M - 1982" value="0"/>
                <EnumElem name="ASME Y14.5M - 1994" value="1"/>
                <EnumElem name="ISO 1101 - 1983" value="2"/>
                <EnumElem name="None" value="3"/>
                <EnumElem name="ASME Y14.5 - 2009" value="4"/>
              </Data>
              <Docum>Checking the feature control frame syntax against the ASME standard is only available through the Geometric Tolerancing module.</Docum>
            </Pref>
            <Pref name="Drafting_checkStandard_MCU" type="int" title="Tolerancing Standard" drftStd="yes" value="2" ver="2306" scope="Session">
                <Description>This option specifies the tolerancing standard that should be used. The standard chosen controls the availability of geometric tolerancing callouts and some of the datum leader types. It also controls the standard that a validation of the feature control frame syntax should be checked against (applies to ANSI and ISO only).</Description>
                <OptionMenu/>
                <Data type="enum">
                    <EnumElem name="ANSI Y14.5M - 1982" value="0"/>
                    <EnumElem name="ASME Y14.5M - 1994" value="1"/>
                    <EnumElem name="ISO 1101 - 1983" value="2"/>
                    <EnumElem name="None" value="3"/>
                    <EnumElem name="ASME Y14.5 - 2009" value="4"/>
                </Data>
                <Docum>Checking the feature control frame syntax against the ASME standard is only available through the Geometric Tolerancing module.</Docum>
            </Pref>
            <Pref name="Drafting_checkStandard_MU" type="int" title="Tolerancing Standard" drftStd="yes" value="2" ver="7.5.0" scope="Session">
              <Description>This option specifies the tolerancing standard that should be used. The standard chosen controls the availability of geometric tolerancing callouts and some of the datum leader types. It also controls the standard that a validation of the feature control frame syntax should be checked against (applies to ANSI and ISO only).</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="ANSI Y14.5M - 1982" value="0"/>
                <EnumElem name="ASME Y14.5M - 1994" value="1"/>
                <EnumElem name="ISO 1101 - 1983" value="2"/>
                <EnumElem name="None" value="3"/>
                <EnumElem name="ASME Y14.5 - 2009" value="4"/>
              </Data>
              <Docum>Checking the feature control frame syntax against the ASME standard is only available through the Geometric Tolerancing module.</Docum>
            </Pref>
            <Pref name="Drafting_checkStandard_SIU" type="int" title="Tolerancing Standard" drftStd="yes" value="2" ver="2306" scope="Session">
                <Description>This option specifies the tolerancing standard that should be used. The standard chosen controls the availability of geometric tolerancing callouts and some of the datum leader types. It also controls the standard that a validation of the feature control frame syntax should be checked against (applies to ANSI and ISO only).</Description>
                <OptionMenu/>
                <Data type="enum">
                    <EnumElem name="ANSI Y14.5M - 1982" value="0"/>
                    <EnumElem name="ASME Y14.5M - 1994" value="1"/>
                    <EnumElem name="ISO 1101 - 1983" value="2"/>
                    <EnumElem name="None" value="3"/>
                    <EnumElem name="ASME Y14.5 - 2009" value="4"/>
                </Data>
                <Docum>Checking the feature control frame syntax against the ASME standard is only available through the Geometric Tolerancing module.</Docum>
            </Pref>
          </Group>
          <Group name="Legacy GD&amp;&amp;T">
            <Pref name="Gdt_standard" type="int" title="Tolerancing Standard" drftStd="yes" value="2" ver="1847" scope="Part">
              <Description>This option specifies the tolerancing standard to be used by legacy checked GD&amp;&amp;T. This option will not be used when creating PMI.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="ANSI Y14.5M - 1982" value="1"/>
                <EnumElem name="ASME Y14.5M - 1994" value="2"/>
                <EnumElem name="ISO 1101 - 1983" value="3"/>
                <EnumElem name="ASME Y14.5 - 2009" value="4"/>
              </Data>
            </Pref>
          </Group>
        </Tab>
      </SubCategory>
      <SubCategory name="Workflow" csh="PMAN_Drafting_Standard_General_Workflow">
          <Tab name="Workflow" csh="PMAN_Drafting_Standard_General_Workflow_Workflow">
              <Group name="Update">
                  <Pref name="Drafting_delayDrawingUpdate" type="int" title="Delay Drawing Update" drftStd="yes" value="1" ver="1847" scope="Part">
                      <Description>This option determines whether or not the updating of drawing views and annotations during system initiated drawing updates should be suppressed.</Description>
                      <Toggle off="0"/>
                      <Data type="enum">
                          <EnumElem name="Yes" value="1"/>
                          <EnumElem name="No" value="0"/>
                      </Data>
                  </Pref>
              </Group>
          </Tab>
      </SubCategory>
    </Category>
    <Category name="Common" csh="PMAN_Drafting_Standard_Common">
      <SubCategory name="Lettering" csh="PMAN_Drafting_Standard_Common_Lettering">
        <Tab name="Lettering" csh="PMAN_Drafting_Standard_Common_Lettering_Lettering">
          <Group name="Alignment">
            <Pref name="Drafting_entitySite" type="int" title="Text Alignment" drftStd="yes" value="5" ver="3.0.0" scope="Part">
              <Description>This option specifies a position relative to the text which is used when positioning the text on the drawing.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="Top Left" bitmap="align_pos_topleft.bmp" value="1"/>
                <EnumElem name="Top Center" bitmap="align_pos_topcenter.bmp" value="2"/>
                <EnumElem name="Top Right" bitmap="align_pos_topright.bmp" value="3"/>
                <EnumElem name="Middle Left" bitmap="align_pos_midleft.bmp" value="4"/>
                <EnumElem name="Middle Center" bitmap="align_pos_midcenter.bmp" value="5"/>
                <EnumElem name="Middle Right" bitmap="align_pos_midright.bmp" value="6"/>
                <EnumElem name="Bottom Left" bitmap="align_pos_bottomleft.bmp" value="7"/>
                <EnumElem name="Bottom Center" bitmap="align_pos_bottomcenter.bmp" value="8"/>
                <EnumElem name="Bottom Right" bitmap="align_pos_bottomright.bmp" value="9"/>
              </Data>
            </Pref>
            <Pref name="Drafting_horizontalTextJust" type="int" title="Text Justification" drftStd="yes" value="1" ver="3.0.0" scope="Part">
              <Description>This option specifies the horizontal text justification.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="Left" bitmap="alignleft.bmp" value="1"/>
                <EnumElem name="Center" bitmap="align_center.bmp" value="2"/>
                <EnumElem name="Right" bitmap="alignright.bmp" value="3"/>
              </Data>
            </Pref>
          </Group>
          <Group name="Text Parameters">
            <Pref name="Drafting_draftTextColor" type="int" title="Color" drftStd="yes" value="216" ver="8.0.0" scope="Part">
              <Description>This option specifies the color of general text.</Description>
              <Color/>
            </Pref>
            <Pref name="Drafting_draftTextFont" type="string" title="Font" drftStd="yes" value="Arial" ver="8.0.0" scope="Part">
              <Description>This option specifies the character font for general text. Any font present in the directory pointed to by the UGII_CHARACTER_FONT_DIR environment variable can be specified here.</Description>
              <String/>
            </Pref>
            <Pref name="Drafting_draftTextFontStyle" type="string" title="Font Style" drftStd="yes" value="0" ver="8.5.0" scope="Part">
              <Description>This option specifies the font style for general text.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="Regular" value="0"/>
                <EnumElem name="Bold" value="1"/>
                <EnumElem name="Italic" value="2"/>
                <EnumElem name="Bold Italic" value="3"/>
              </Data>
            </Pref>
            <Pref name="Drafting_draftTextWidth" type="int" title="Width" drftStd="yes" value="3" ver="8.5.0" scope="Part">
              <Description>This option specifies the line width for the font for general text.</Description>
              <AnnotationTextWidth/>
            </Pref>
            <Pref name="Drafting_draftTextCharSize_EU" type="real" title="Height" drftStd="yes" value="0.125" unit="in" ver="3.0.3" scope="Part">
              <Description>This option specifies the character height for general text.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.0" include="0"/>
              </Data>
            </Pref>
            <Pref name="Drafting_draftTextCharSize_MCU" type="real" title="Height" drftStd="yes" value="3.175" unit="μm" ver="2306" scope="Part">
              <Description>This option specifies the character height for general text.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.0" include="0"/>
              </Data>
            </Pref>
            <Pref name="Drafting_draftTextCharSize_MU" type="real" title="Height" drftStd="yes" value="3.175" unit="mm" ver="3.0.3" scope="Part">
                <Description>This option specifies the character height for general text.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                </Data>
            </Pref>
            <Pref name="Drafting_draftTextCharSize_SIU" type="real" title="Height" drftStd="yes" value="3.175" unit="m" ver="2306" scope="Part">
                <Description>This option specifies the character height for general text.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                </Data>
            </Pref>
            <Pref name="Drafting_draftTextSpaceFactor" type="real" title="NX Font Gap Factor" drftStd="yes" value="1.0" ver="3.0.0" scope="Part">
              <Description>This option specifies the NX Font spacing between the characters in the general text as a multiple of the standard character spacing for the current font.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.0" include="1"/>
              </Data>
            </Pref>
            <Pref name="Drafting_standardDraftTextSpaceFactor" type="real" title="Standard Font Gap Factor" drftStd="yes" value="0.0" ver="8.0.0" scope="Part">
              <Description>This option specifies the Standard Font spacing between the characters in the general text as a multiple of the standard character spacing for the current font.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.0" include="1"/>
              </Data>
            </Pref>
            <Pref name="Drafting_draftTextAspectRatio" type="real" title="Text Aspect Ratio" drftStd="yes" value="1.0" ver="3.0.0" scope="Part">
              <Description>This option specifies the aspect ratio at which general text is displayed in relation to the original size of the current font. The aspect ratio is the ratio of the character's width to its height.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.0" include="0"/>
              </Data>
            </Pref>
            <Pref name="Drafting_draftTextSymbolAspectRatio" type="real" title="Symbol Aspect Ratio" drftStd="yes" value="1.0" ver="11.0.0" scope="Part">
                <Description>This option specifies the aspect ratio at which the symbols are displayed in relation to the original size of the current symbol. The aspect ratio is the ratio of the symbol's width to its height. Only symbols created using the NX control characters are supported.</Description>
                <Real/>
                <Data type="range">
                    <LowerBound value="0.0" include="0"/>
                </Data>
            </Pref>
            <Pref name="Drafting_draftTextLineFactor" type="real" title="Line Gap Factor" drftStd="yes" value="1.0" ver="3.0.0" scope="Part">
              <Description>This option specifies the line spacing value for general text as a multiple of the standard spacing for the current font.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.0" include="0"/>
              </Data>
            </Pref>
            <Pref name="Drafting_draftingAidAngle" type="real" title="Lettering Angle" drftStd="yes" value="0.0" unit="deg" ver="3.0.0" scope="Part">
              <Description>This option specifies the angle of general text (notes and labels) and balloons.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.0" include="1"/>
                <UpperBound value="360.0" include="1"/>
              </Data>
            </Pref>
          </Group>
          <Group name="Tolerance Frame">
            <Pref name="Drafting_frameSize" type="real" title="Height Factor" drftStd="yes" value="2.0" ver="3.0.0" scope="Part">
              <Description>This option specifies the size of the geometric tolerance symbol frame relative to the character size.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="1.0" include="0"/>
              </Data>
            </Pref>
          </Group>
          <Group name="Symbol">
            <Pref name="Drafting_symbolTextFont" type="string" title="Symbol Font File" drftStd="yes" value="NX ANSI Symbols" ver="8.5.0" scope="Part">
              <Description>This option specifies the symbol font file used for symbols if the text type is of standard font.</Description>
              <String/>
            </Pref> 
          </Group>
          <Group name="Vertical Text">
            <Pref name="Drafting_verticaltextdirection" type="int" title="Multiline Text Display Format" drftStd="yes" value="0" ver="2312" scope="Session">
              <Description>This option specifies how multiline text is displayed when vertical text is used.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="Right to Left" value="0"/>
                <EnumElem name="Left to Right" value="1"/>
              </Data>
            </Pref>
          </Group>
        </Tab>
      </SubCategory>
      <SubCategory name="Line/Arrow" csh="PMAN_Drafting_Standard_Common_Line_Arrow">
        <Tab name="Arrowhead" csh="PMAN_Drafting_Standard_Common_Line_Arrow_Arrowhead">
          <Group name="Workflow">
            <Pref name="Drafting_automaticOrientation" type="logical" title="Automatic Orientation" drftStd="yes" value="false" ver="10.0.0" scope="Part">
              <Description>This option determines whether or not the arrow orientation for dimensions will be automatically determined or based on the arrowhead orientation defaults.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="No" value="false"/>
                <EnumElem name="Yes" value="true"/>
              </Data>
            </Pref>
          </Group>
          <Group name="Leader and Dimension Side 1">
            <Pref name="Drafting_arrowhead1Type" type="int" title="Type" drftStd="yes" value="1" ver="6.0.0" scope="Part">
              <Description>This option specifies the type of arrowhead for the first dimension arrow. This option also specifies the arrowhead for other drafting aids.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="Closed Arrow" bitmap="ui_pref_left_closed_arrow.bmp" value="1"/>
                <EnumElem name="Closed Solid Arrow" bitmap="ui_pref_left_closed_solid_arrow.bmp" value="8"/>
                <EnumElem name="Open Arrow" bitmap="ui_pref_left_open_arrow.bmp" value="2"/>
                <EnumElem name="Top Open Arrow" bitmap="ui_pref_left_top_open_arrow.bmp" value="14"/>
                <EnumElem name="Bottom Open Arrow" bitmap="ui_pref_left_bot_open_arrow.bmp" value="15"/>
                <!--  <EnumElem name="Filled Arrow" bitmap="ui_pref_left_filled_arrow.bmp" value="0"/> -->
                <!--  <EnumElem name="Top Filled Arrow" bitmap="ui_pref_left_top_filled_arrow.bmp" value="-4"/> -->
                <!--  <EnumElem name="Bottom Filled Arrow" bitmap="ui_pref_left_bot_filled_arrow.bmp" value="-5"/> -->
                <EnumElem name="Closed Double Arrow" bitmap="ui_pref_left_closed_double_arrow.bmp" value="9"/>
                <EnumElem name="Closed Double Solid Arrow" bitmap="ui_pref_left_closed_double_solid_arrow.bmp" value="10"/>
                <EnumElem name="Open Double Arrow" bitmap="ui_pref_left_open_double_arrow.bmp" value="11"/>
                <!--  <EnumElem name="Filled Double Arrow" bitmap="ui_pref_left_filled_double_arrow.bmp" value="-2"/> -->
                <EnumElem name="None" bitmap="ui_pref_left_none.bmp" value="6"/>
                <EnumElem name="Origin Symbol" bitmap="ui_pref_left_origin.bmp" value="5"/>
                <EnumElem name="Cross" bitmap="ui_pref_left_crossed.bmp" value="3"/>
                <EnumElem name="Integral" bitmap="ui_pref_left_integral.bmp" value="12"/>
                <EnumElem name="Dot" bitmap="ui_pref_left_dot.bmp" value="4"/>
                <!--  <EnumElem name="Filled Dot" bitmap="ui_pref_left_filled_dot.bmp" value="-1"/> -->
                <EnumElem name="Box" bitmap="ui_pref_left_box.bmp" value="13"/>
                <!--  <EnumElem name="Filled Box" bitmap="ui_pref_left_filled_box.bmp" value="-3"/> -->
              </Data>
            </Pref>
            <Pref name="Drafting_arrowHead1Color_EU" type="int" title="Color" drftStd="yes" value="216" ver="8.0.0" scope="Part">
              <Description>This option specifies the color of the first arrowhead.</Description>
              <Color/>
            </Pref>
            <Pref name="Drafting_arrowHead1Color_MCU" type="int" title="Color" drftStd="yes" value="216" ver="2306" scope="Part">
              <Description>This option specifies the color of the first arrowhead.</Description>
              <Color/>
            </Pref>
            <Pref name="Drafting_arrowHead1Color_MU" type="int" title="Color" drftStd="yes" value="216" ver="8.0.0" scope="Part">
                <Description>This option specifies the color of the first arrowhead.</Description>
                <Color/>
            </Pref>
            <Pref name="Drafting_arrowHead1Color_SIU" type="int" title="Color" drftStd="yes" value="216" ver="2306" scope="Part">
                <Description>This option specifies the color of the first arrowhead.</Description>
                <Color/>
            </Pref>
            <Pref name="Drafting_arrowHead1Font" type="int" title="Style" drftStd="yes" value="1" ver="1980" scope="Part">
              <Description>This option specifies the line style for the first arrowhead.</Description>
              <GeneralLineFont LineFontOptionMaskString="MBDLineFonts"/>
            </Pref>
            <Pref name="Drafting_arrowHead1Width" type="int" title="Width" drftStd="yes" value="9" ver="10.0.0" scope="Part">
              <Description>This option specifies the line width for the first arrowhead.</Description>
              <AnnotationLineWidth allowWideLineWidth="true"/>
            </Pref>
            <Pref name="Drafting_textArrowPlacementSideOne" type="int" title="Orientation" drftStd="yes" value="2" ver="10.0.0" scope="Part">
              <Description>This option specifies the arrowhead orientation for leaders and dimension line side 1 when automatic orientation is off.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="Manual - Arrows In" bitmap="manplacearrin.bmp" value="2"/>
                <EnumElem name="Manual - Arrows Out" bitmap="manplacearrout.bmp" value="3"/>
              </Data>
            </Pref>
          </Group>
          <Group name="Dimension Side 2">
            <Pref name="Drafting_arrowhead2Type" type="int" title="Type" drftStd="yes" value="1" ver="6.0.0" scope="Part">
              <Description>This option specifies the type of arrowhead for the second dimension arrow.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="Closed Arrow" bitmap="ui_pref_right_closed_arrow.bmp" value="1"/>
                <EnumElem name="Closed Solid Arrow" bitmap="ui_pref_right_closed_solid_arrow.bmp" value="8"/>
                <EnumElem name="Open Arrow" bitmap="ui_pref_right_open_arrow.bmp" value="2"/>
                <EnumElem name="Top Open Arrow" bitmap="ui_pref_right_top_open_arrow.bmp" value="14"/>
                <EnumElem name="Bottom Open Arrow" bitmap="ui_pref_right_bot_open_arrow.bmp" value="15"/>
                <!-- <EnumElem name="Filled Arrow" bitmap="ui_pref_right_filled_arrow.bmp" value="0"/> -->
                <!--  <EnumElem name="Top Filled Arrow" bitmap="ui_pref_right_top_filled_arrow.bmp" value="-4"/> -->
                <!--  <EnumElem name="Bottom Filled Arrow" bitmap="ui_pref_right_bot_filled_arrow.bmp" value="-5"/> -->
                <EnumElem name="Closed Double Arrow" bitmap="ui_pref_right_closed_double_arrow.bmp" value="9"/>
                <EnumElem name="Closed Double Solid Arrow" bitmap="ui_pref_right_closed_double_solid_arrow.bmp" value="10"/>
                <EnumElem name="Open Double Arrow" bitmap="ui_pref_right_open_double_arrow.bmp" value="11"/>
                <!-- <EnumElem name="Filled Double Arrow" bitmap="ui_pref_right_filled_double_arrow.bmp" value="-2"/> -->
                <EnumElem name="None" bitmap="ui_pref_right_none.bmp" value="6"/>
                <EnumElem name="Origin Symbol" bitmap="ui_pref_right_origin.bmp" value="5"/>
