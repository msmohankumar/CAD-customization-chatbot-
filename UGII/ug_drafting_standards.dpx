              <ViewLineWidth allowOriginalWidth="true" allowWideLineWidth="true"/>
            </Pref>
            <Pref name="Drafting_ShowSmoothEdges" type="logical" title="Show Smooth Edges" drftStd="yes" value="true" ver="11.0.0" scope="Part">
              <Description>This option determines whether or not smooth edges should be displayed for secondary geometry. Smooth edges are those whose adjacent faces have the same surface tangent at the edge where they meet. Smooth edges will be displayed with the same color, line style and width as secondary geometry visible and hidden lines.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="Yes" value="true"/>
                <EnumElem name="No" value="false"/>
              </Data>
            </Pref>
            <Pref name="Drafting_ShowVirtualIntersections" type="logical" title="Show Virtual Intersections" drftStd="yes" value="false" ver="11.0.0" scope="Part">
              <Description>This option determines whether or not virtual intersection lines should be displayed for secondary geometry. Virtual intersection lines are lines that show where blended faces theoretically intersect. Virtual intersections will be displayed with the same color, line style and width as secondary geometry visible and hidden lines.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="Yes" value="true"/>
                <EnumElem name="No" value="false"/>
              </Data>
            </Pref>            
          </Group>
          <Group name="Secondary Geometry Hidden by Primary Geometry">
            <Pref name="Drafting_SecondaryHiddenByPrimaryVisibleLineColor" type="int" title="Visible Line Color" drftStd="yes" value="125" ver="11.0.0" scope="Part">
              <Description>This option specifies the color of secondary geometry that is hidden by primary geometry.</Description>
              <ColorOption/>
              <Data type="color">
                <EnumElem name="Original" value="0"/>
              </Data>
            </Pref>
            <Pref name="Drafting_SecondaryHiddenByPrimaryVisibleLineFont" type="int" title="Visible Line Style" drftStd="yes" value="0" ver="1847" scope="Part">
              <Description>This option specifies the line style for secondary geometry that is hidden by primary geometry.</Description>
              <GeneralLineFont allowDefaultFont="true" showDefaultAsOriginalFont="true" allowInvisibleFont="true" LineFontOptionMaskString="GeneralLineFonts" LineFontDefaultValue="8"/>
            </Pref>
            <Pref name="Drafting_SecondaryHiddenByPrimaryVisibleLineWidth" type="int" title="Visible Line Width" drftStd="yes" value="8" ver="11.0.0" scope="Part">
              <Description>This option specifies the line width for secondary geometry that is hidden by primary geometry.</Description>
              <ViewLineWidth allowOriginalWidth="true" allowWideLineWidth="true"/>
            </Pref>
            <Pref name="Drafting_SecondaryHiddenByPrimaryHiddenLineColor" type="int" title="Hidden Line Color" drftStd="yes" value="125" ver="11.0.0" scope="Part">
              <Description>This option specifies the color of hidden secondary geometry that is hidden by primary geometry.</Description>
              <ColorOption/>
              <Data type="color">
                <EnumElem name="Original" value="0"/>
              </Data>
            </Pref>
            <Pref name="Drafting_SecondaryHiddenByPrimaryHiddenLineFont" type="int" title="Hidden Line Style" drftStd="yes" value="0" ver="1847" scope="Part">
              <Description>This option specifies the line style for hidden secondary geometry that is hidden by primary geometry.</Description>
              <GeneralLineFont allowDefaultFont="true" showDefaultAsOriginalFont="true" allowInvisibleFont="true" LineFontOptionMaskString="GeneralLineFonts" LineFontDefaultValue="8"/>
            </Pref>
            <Pref name="Drafting_SecondaryHiddenByPrimaryHiddenLineWidth" type="int" title="Hidden Line Width" drftStd="yes" value="8" ver="11.0.0" scope="Part">
              <Description>This option specifies the line width for hidden secondary geometry that is hidden by primary geometry.</Description>
              <ViewLineWidth allowOriginalWidth="true" allowWideLineWidth="true"/>
            </Pref>            
          </Group>
          <Group name="Primary Geometry Hidden by Secondary Geometry">
            <Pref name="Drafting_PrimaryHiddenBySecondaryVisibleLineColor" type="int" title="Visible Line Color" drftStd="yes" value="216" ver="11.0.0" scope="Part">
              <Description>This option specifies the color of primary geometry that is hidden by secondary geometry.</Description>
              <ColorOption/>
              <Data type="color">
                <EnumElem name="Original" value="0"/>
              </Data>
            </Pref>
            <Pref name="Drafting_PrimaryHiddenBySecondaryVisibleLineFont" type="int" title="Visible Line Style" drftStd="yes" value="2" ver="1847" scope="Part">
              <Description>This option specifies the line style for primary geometry that is hidden by secondary geometry.</Description>
              <GeneralLineFont allowDefaultFont="true" showDefaultAsOriginalFont="true" allowInvisibleFont="true" LineFontOptionMaskString="GeneralLineFonts" LineFontDefaultValue="8"/>
            </Pref>
            <Pref name="Drafting_PrimaryHiddenBySecondaryVisibleLineWidth" type="int" title="Visible Line Width" drftStd="yes" value="8" ver="11.0.0" scope="Part">
              <Description>This option specifies the line width for primary geometry that is hidden by secondary geometry.</Description>
              <ViewLineWidth allowOriginalWidth="true" allowWideLineWidth="true"/>
            </Pref>
            <Pref name="Drafting_PrimaryHiddenBySecondaryHiddenLineColor" type="int" title="Hidden Line Color" drftStd="yes" value="216" ver="11.0.0" scope="Part">
              <Description>This option specifies the color of hidden primary geometry that is hidden by secondary geometry.</Description>
              <ColorOption/>
              <Data type="color">
                <EnumElem name="Original" value="0"/>
              </Data>
            </Pref>
            <Pref name="Drafting_PrimaryHiddenBySecondaryHiddenLineFont" type="int" title="Hidden Line Style" drftStd="yes" value="2" ver="1847" scope="Part">
              <Description>This option specifies the line style for hidden primary geometry that is hidden by secondary geometry.</Description>
              <GeneralLineFont allowDefaultFont="true" showDefaultAsOriginalFont="true" allowInvisibleFont="true" LineFontOptionMaskString="GeneralLineFonts" LineFontDefaultValue="8"/>
            </Pref>
            <Pref name="Drafting_PrimaryHiddenBySecondaryHiddenLineWidth" type="int" title="Hidden Line Width" drftStd="yes" value="8" ver="11.0.0" scope="Part">
              <Description>This option specifies the line width for hidden primary geometry that is hidden by secondary geometry.</Description>
              <ViewLineWidth allowOriginalWidth="true" allowWideLineWidth="true"/>
            </Pref>
          </Group>          
        </Tab>
        <Tab name="Threads" csh="PMAN_Drafting_Standard_View_Common_Threads">
          <Group name="Display">
            <Pref name="Drafting_threadMethod_EU" type="int" title="Type" drftStd="yes" value="1" ver="6.0.0" scope="Part">
              <Description>This option specifies the standard for thread representations.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="None" value="0"/>
                <EnumElem name="Simplified-Full Arc" value="1"/>
                <EnumElem name="Simplified-3/4 Arc" value="4"/>
                <EnumElem name="Schematic" value="2"/>
                <EnumElem name="Detailed-Full Arc" value="3"/>
                <EnumElem name="Detailed-3/4 Arc" value="5"/>
              </Data>
            </Pref>
            <Pref name="Drafting_threadMethod_MU" type="int" title="Type" drftStd="yes" value="4" ver="6.0.0" scope="Part">
              <Description>This option specifies the standard for thread representations.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="None" value="0"/>
                <EnumElem name="Simplified-Full Arc" value="1"/>
                <EnumElem name="Simplified-3/4 Arc" value="4"/>
                <EnumElem name="Schematic" value="2"/>
                <EnumElem name="Detailed-Full Arc" value="3"/>
                <EnumElem name="Detailed-3/4 Arc" value="5"/>
              </Data>
            </Pref>
          </Group>
          <Group name="Pitch">
            <Pref name="Drafting_threadMinAppPitch" type="real" title="Minimum" drftStd="yes" value="0.0" ver="3.0.0" scope="Part">
              <Description>This option specifies the minimum distance (in drawing coordinates) to which two points which represent crests of a thread are drawn. It is also used to control the minimum distance between a thread and it's defining geometry such as edges, silhouettes, or section edges of the cylindrical face or chamfered face.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.0" include="1"/>
                <UpperBound value="360.0" include="1"/>
              </Data>
            </Pref>
          </Group>
          <Group name="Appearance">
            <Pref name="Drafting_overrideVisibleThreadColor" type="int" title="Visible Color" drftStd="yes" value="0" ver="6.0.0" scope="Part">
              <Description>This option specifies the color of visible thread curves.</Description>
              <ColorOption/>
              <Data type="color">
                <EnumElem name="No" value="0"/>
              </Data>
            </Pref>
            <Pref name="Drafting_threadTrueHiddenLine" type="int" title="Render True Hidden Line" drftStd="yes" value="1" ver="7.5.5" scope="Part">
              <Description>This option determines whether or not true hidden line rendering will be performed on symbolic threads.</Description>
              <Toggle off="0"/>
              <Data type="enum">
                <EnumElem name="On" value="1"/>
                <EnumElem name="Off" value="0"/>
              </Data>
            </Pref>
          </Group>
        </Tab>
        <Tab name="PMI" csh="PMAN_Drafting_Standard_View_Common_PMI">
          <Group name="Reference Sets">
            <Pref name="Drafting_inheritPmiReferenceSetBehavior" type="int" title="PMI to Inherit" drftStd="yes" value="1" ver="10.0.0" scope="Part">
              <Description> When inheriting PMI onto a drawing, this option determines which PMI objects are inherited based on whether or not the objects they reference are included in the active reference set.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="None" value="0"/>
                <EnumElem name="Partial" value="1"/>
                <EnumElem name="All" value="2"/>
              </Data>
            </Pref>
          </Group>
          <Group name="Inherited">
            <Pref name="Drafting_inheritPmi" type="int" title="PMI" drftStd="yes" value="0" ver="5.0.0" scope="Part">
              <Description>This option determines whether any PMI are inherited from the model view, and if so, if all PMI from the model view should be inherited, or only those whose planes are parallel to the drawing sheet should get inherited.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="None" value="0"/>
                <EnumElem name="From Model View" value="1"/>
                <EnumElem name="Aligned to Drawing (View Only)" value="2"/>
                <EnumElem name="Aligned to Drawing (Entire Part)" value="3"/>
              </Data>
            </Pref>
            <Pref name="Drafting_inheritGdt" type="int" title="Tolerance Features" drftStd="yes" value="0" ver="4.0.0" scope="Part">
              <Description>This option specifies how GDT will be inherited onto an imported view.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="None" value="0"/>
                <EnumElem name="In Drawing Plane" value="1"/>
                <EnumElem name="From Model View" value="2"/>
              </Data>
            </Pref>
            <Pref name="Drafting_inheritPmiOntoDrawing" type="logical" title="Show PMI on Drawing" drftStd="yes" value="false" ver="5.0.0" scope="Part">
              <Description>This option controls whether PMI are inherited within a view or onto the drawing sheet.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="No" value="false"/>
                <EnumElem name="Yes" value="true"/>
              </Data>
            </Pref>
            <Pref name="Drafting_pmiDimensionFromRevolved" type="logical" title="Show PMI Dimensions from Revolved Sketches" drftStd="yes" value="false" ver="9.0.0" scope="Part">
              <Description>When a sketch has been used to create a Revolve feature, this option determines whether or not PMI dimensions from the sketch should be inherited to a view that is parallel to the axis of the Revolve feature (but not necessarily parallel to the sketch plane).</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="No" value="false"/>
                <EnumElem name="Yes" value="true"/>
              </Data>
            </Pref>
            <Pref name="Drafting_inheritPmiRearrange" type="logical" title="Rearrange Inherited PMI" drftStd="yes" value="true" ver="5.0.0" scope="Part">
              <Description>This option controls whether or not the system will rearrange PMI after they have been inherited on a drawing to minimize their overlap with other PMI and with the geometry.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="No" value="false"/>
                <EnumElem name="Yes" value="true"/>
              </Data>
            </Pref>
          </Group>
          <Group name="Section View">
            <Pref name="Drafting_crosshatchInheritedPMILightweightSectionView" type="logical" title="Inherit Crosshatch" drftStd="yes" value="false" ver="11.0.0" scope="Part">
              <Description>This option controls whether crosshatch from PMI Section View should be inherited to drawing.</Description>
              <Toggle off="false"/>
              <Data type="enum">
                <EnumElem name="No" value="false"/>
                <EnumElem name="Yes" value="true"/>
              </Data>
            </Pref>
            <Pref name="Drafting_compoundBoxLightweightSectionView" type="int" title="Allow Compound Box Type" drftStd="yes" value="0" ver="11.0.0" scope="Session">
              <Description>This option allows multiple Box Type PMI Section Views to be combined into one drafting view.</Description>
              <Toggle off="0"/>
              <Data type="enum">
                <EnumElem name="On" value="1"/>
                <EnumElem name="Off" value="0"/>
              </Data>
            </Pref>
          </Group>
        </Tab>
        <Tab name="Shading" csh="PMAN_Drafting_Standard_View_Common_Shading">
          <Group name="Format">
            <Pref name="Drafting_viewRenderingStyle" type="int" title="Rendering Style" drftStd="yes" value="0" ver="5.0.0" scope="Part">
              <Description>This option specifies the general display style preference for new member views.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="Fully Shaded" value="2"/>
                <EnumElem name="Wireframe" value="0"/>
                <EnumElem name="Partially Shaded" value="1"/>
              </Data>
            </Pref>
            <Pref name="Drafting_overrideVisibleWireframeColor" type="int" title="Visible Wireframe Color" drftStd="yes" value="0" ver="5.0.0" scope="Part">
              <Description>This option specifies the color of visible wireframe objects in shaded member views.</Description>
              <ColorOption/>
              <Data type="color">
                <EnumElem name="No" value="0"/>
              </Data>
            </Pref>
            <Pref name="Drafting_overrideHiddenWireframeColor" type="int" title="Hidden Wireframe Color" drftStd="yes" value="0" ver="5.0.0" scope="Part">
              <Description>This option specifies the color of hidden wireframe objects in shaded member views.</Description>
              <ColorOption/>
              <Data type="color">
                <EnumElem name="No" value="0"/>
              </Data>
            </Pref>
            <Pref name="Drafting_shadedCutFaceColor" type="int" title="Shaded Cut Face Color" drftStd="yes" value="96" ver="8.0.0" scope="Part">
              <Description>This option specifies the color of shaded cut faces.</Description>
              <ColorOption/>
              <Data type="color">
                <EnumElem name="Original" value="0"/>
              </Data>
            </Pref>
            <Pref name="Drafting_viewShadedTwoSidedLight" type="int" title="Use Two-sided Light" drftStd="yes" value="1" ver="5.0.0" scope="Part">
              <Description>This option determines whether or not lighting should be applied to both the front and backsides of faces. Two-sided light is useful for objects such as sheet bodies, which without this feature could generate an invalid display.</Description>
              <Toggle off="0"/>
              <Data type="enum">
                <EnumElem name="On" value="1"/>
                <EnumElem name="Off" value="0"/>
              </Data>
              <Docum>When this option is on, lighting from a light source is applied to the front or the back face, depending on which side is exposed to the light source. When this option is off, lighting is not applied to the back of a face even if the back is exposed to the light source.</Docum>
            </Pref>
            <Pref name="Drafting_viewShadedShininess" type="real" title="Shininess" drftStd="yes" value="0.4" ver="7.5.4" scope="Part">
              <Description>This option specifies the intensity of highlights added to shaded surfaces, making them appear shiny.</Description>
              <RealScale precision="2"/>
              <Data type="range">
                <LowerBound value="0.0" include="1" name="None"/>
                <UpperBound value="1.0" include="1" name="Maximum"/>
              </Data>
            </Pref>
          </Group>
          <Group name="Tolerance">
            <Pref name="Drafting_viewShadedTolerance" type="int" title="Shaded Tolerance" drftStd="yes" value="1" ver="5.0.0" scope="Part">
              <Description>This option specifies the shading tolerance preference for new member views.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="Coarse" value="0"/>
                <EnumElem name="Standard" value="1"/>
                <EnumElem name="Fine" value="2"/>
                <EnumElem name="Extra Fine" value="3"/>
                <EnumElem name="Ultra Fine" value="4"/>
                <EnumElem name="Customize" value="5"/>
              </Data>
            </Pref>
            <Pref name="Drafting_viewShadedEdgeTolerance" type="real" title="Edge" drftStd="yes" value="0.005" ver="5.0.0" scope="Part">
              <Description>This option controls the faceted representation by considering the curved edge approximation of the object. This option has been found to be very useful at sealing the gaps between faces. The recommended value is 0.005. A smaller tolerance value results in a finer display, but also causes slower performance. The value is interpreted to be in the units of the part. The default value is specified in inches.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.0" include="1"/>
                <UpperBound value="1000.0" include="1"/>
              </Data>
            </Pref>
            <Pref name="Drafting_viewShadedFaceTolerance" type="real" title="Face" drftStd="yes" value="0.005" ver="5.0.0" scope="Part">
              <Description>This option controls the faceted representation of the object by considering the surface approximation. The recommended value is 0.005. A smaller tolerance value results in a finer display, but also causes slower performance. The value is interpreted to be in the units of the part. The default value is specified in inches.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.0" include="1"/>
                <UpperBound value="1000.0" include="1"/>
              </Data>
            </Pref>
            <Pref name="Drafting_viewShadedAngleTolerance" type="real" title="Angle" drftStd="yes" value="15.0" unit="deg" ver="5.0.0" scope="Part">
              <Description>This option is beneficial for faces with high curvature (sharp corners). Recommended setting is 3-15 degrees.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="1.0" include="1"/>
                <UpperBound value="90.0" include="1"/>
              </Data>
            </Pref>
            <Pref name="Drafting_shadedViewFacetTolerance" type="real" title="Shaded View Facet Tolerance" drftStd="yes" value="1.00" ver="7.5.0" scope="Session">
              <Description>This option specifies the facet tolerance used for CGM creation of shaded drawing views.</Description>
              <RealScale precision="2"/>
              <Data type="range">
                <LowerBound value="0.01" include="1" name="0.01 Smooth"/>
                <UpperBound value="2.00" include="1" name="Coarse 2.00"/>
              </Data>
            </Pref>
          </Group>
        </Tab>
        <Tab name="Smooth Edges" csh="PMAN_Drafting_Standard_View_Common_Smooth_Edges">
          <Group name="Format">
            <Pref name="Drafting_smoothEdge" type="int" title="Show Smooth Edges" drftStd="yes" value="1" ver="3.0.0" scope="Part">
              <Description>This option determines whether or not smooth edges should be displayed. Smooth edges are those whose adjacent faces have the same surface tangent at the edge where they meet.</Description>
              <Toggle off="0"/>
              <Data type="enum">
                <EnumElem name="On" value="1"/>
                <EnumElem name="Off" value="0"/>
              </Data>
            </Pref>
            <Pref name="Drafting_smoothEdgeColor" type="int" title="Color" drftStd="yes" value="135" ver="6.0.0" scope="Part">
              <Description>This option specifies the color of smooth edges.</Description>
              <ColorOption/>
              <Data type="color">
                <EnumElem name="Original" value="0"/>
              </Data>
            </Pref>
            <Pref name="Drafting_smoothEdgeFont" type="int" title="Style" drftStd="yes" value="0" ver="1847" scope="Part">
              <Description>This option specifies the line style for smooth edges. The option Original means that the line style is inherited from the line style of the model.</Description>
              <GeneralLineFont allowDefaultFont="true" showDefaultAsOriginalFont="true" LineFontOptionMaskString="GeneralLineFonts" LineFontDefaultValue="0"/>
            </Pref>
            <Pref name="Drafting_smoothEdgeWidth" type="int" title="Width" drftStd="yes" value="0" ver="8.5.0" scope="Part">
              <Description>This option specifies the line width for smooth edges. The option Original means that the line width is inherited from the line width of the model.</Description>
              <ViewLineWidth allowOriginalWidth="true" allowWideLineWidth="true"/>
            </Pref>
            <Pref name="Drafting_smoothEdgeGapping" type="int" title="Show End Gaps" drftStd="yes" value="0" ver="3.0.0" scope="Part">
              <Description>This option determines whether or not the ends of smooth edges should be displayed.</Description>
              <Toggle off="0"/>
              <Data type="enum">
                <EnumElem name="On" value="1"/>
                <EnumElem name="Off" value="0"/>
              </Data>
            </Pref>
            <Pref name="Drafting_smoothEdgeGapSize_MU" type="real" title="End Gap Size" drftStd="yes" value="3.0" unit="mm" ver="3.0.0" scope="Part">
              <Description>This option specifies the end gap length.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.0" include="1"/>
              </Data>
            </Pref>
            <Pref name="Drafting_smoothEdgeGapSize_EU" type="real" title="End Gap Size" drftStd="yes" value="0.125" unit="in" ver="3.0.0" scope="Part">
              <Description>This option specifies the end gap length.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.0" include="1"/>
              </Data>
            </Pref>
            <Pref name="Drafting_smoothEdgeTolerance" type="int" title="Override Angle Tolerance" drftStd="yes" value="0" ver="7.5.0" scope="Part">
              <Description>An edge is determined to be smooth if the adjacent faces are tangent within an angle tolerance. This option determines whether or not the system default angle tolerance for smooth edges should be overridden by the below specified Angle Tolerance.</Description>
              <Toggle off="0"/>
              <Data type="enum">
                <EnumElem name="Yes" value="1"/>
                <EnumElem name="No" value="0"/>
              </Data>
            </Pref>
            <Pref name="Drafting_smoothEdgeToleranceValue" type="real" title="Angle Tolerance" drftStd="yes" value="1.0" unit="deg" ver="7.5.0" scope="Part">
              <Description>This option specifies the angle tolerance to be used to identify smooth edges.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.0" include="1"/>
                <UpperBound value="90.0" include="1"/>
              </Data>
            </Pref>
          </Group>
        </Tab>
        <Tab name="View Label" csh="PMAN_Drafting_Standard_View_Common_View_Label">
          <Group name="Format">
            <Pref name="Drafting_vwLabelInitialLetter" type="utf8string" title="Letter" drftStd="yes" value="A" ver="6.0.0" scope="Part">
              <Description>This option specifies the first view label letter. The letters automatically increment. After the last letter (as specified by the next option), the letters increment based on the value of the Secondary Indexing option.</Description>
              <String/>
            </Pref>
            <Pref name="Drafting_vwLabelSecondaryIndexingMode" type="int" title="Secondary Indexing" drftStd="yes" value="1" ver="8.5.0" scope="Part">
              <Description>This option specifies how view letters will be generated once all the available letters are used. The Alphabetic option will begin again at the beginning of the available letters. The numeric option will begin at 1.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="Alphabetic" value="0"/>
                <EnumElem name="Numeric" value="1"/>
              </Data>
            </Pref>
            <Pref name="Drafting_vwLabelSecondaryIndexingAlignment" type="int" title="Secondary Indexing Alignment" drftStd="yes" value="1" ver="8.5.0" scope="Session">
              <Description>This option specifies where the secondary index for a view letter is located in relation to the primary letter.</Description>
              <OptionMenu/>
              <Data type="enum">
                <EnumElem name="Inline" value="0"/>
                <EnumElem name="Subscript" value="1"/>
              </Data>
            </Pref>
            <Pref name="Drafting_vwLabelSecondaryIndexingSubscrpiptSizeFactor" type="real" title="Subscript Size Factor" drftStd="yes" value="0.5" ver="8.5.0" scope="Session">
              <Description>This option specifies the character size of the secondary index subscript letter relative to the character size of the current font.</Description>
              <Real/>
              <Data type="range">
                <LowerBound value="0.01" include="1"/>
                <UpperBound value="1.0" include="1"/>
              </Data>
            </Pref>
          </Group>
          <Group name="Label">
            <Pref name="Drafting_vwLabelIncludedLetters" type="utf8string" title="Allowed Letters" drftStd="yes" value="ABCDEFGHJKLMNPRSTUVWXYZ" ver="6.0.0" scope="Part">
              <Description>This option specifies the English/ASCII letter list that the view label letters will increment through. Numbers can be included in this list.</Description>
              <Widestring/>
            </Pref>
            <Pref name="Drafting_vwLabelMaxLetters" type="int" title="Maximum Number of Letters" drftStd="yes" value="3" ver="3.0.0" scope="Part">
              <Description>This option specifies the maximum number of letters allowed in a view label.</Description>
              <Integer/>
            </Pref>
            <Pref name="Drafting_vwLabelMinLetters" type="int" title="Minimum Number of Letters" drftStd="yes" value="1" ver="3.0.0" scope="Part">
              <Description>This option specifies the minimum number of letters required in a view label.</Description>
              <Integer/>
            </Pref>
            <Pref name="Drafting_vwLabelExcludedLetters" type="utf8string" title="Excluded Letters" drftStd="yes" value="IOQ" ver="6.0.0" scope="Part">
              <Description>This option specifies the letters that are excluded from being used in a view label.</Description>
              <String/>
            </Pref>
            <Pref name="Drafting_vwLabelExcludedLetterstoBegin" type="utf8string" title="Excluded Letters for Start" drftStd="yes" value="" ver="11.0.1" scope="Part">
