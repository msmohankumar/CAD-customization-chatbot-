          </Pref>
        </Tab>
        <Tab name="Content" csh="PMAN_Drafting_Standard_Table_Hole_Table_Content">
          <Pref name="DR_HOLEREPORT_DETAIL" type="list_sel_reorder_value" title="Content and Order" drftStd="yes" value="&quot;HOLE_DIAMETER:1&quot;\&quot;HOLE_DIAMETER_FIT:1&quot;\&quot;HOLE_DEPTH:1&quot;\&quot;HOLE_DEPTH_FIT:1&quot;\&quot;HOLE_DRILL_TYPE:1&quot;\&quot;HOLE_DRILL_DIRECTION:1&quot;" ver="9.0.0" scope="Part">
