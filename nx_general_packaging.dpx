        <Category name="Direct Field of View" csh="PMAN_Vehicle_Design_General_Packaging_Direct_Field_of_View">
            <Tab name="General" csh="PMAN_Vehicle_Design_General_Packaging_Direct_Field_of_View_General">
                <Pref name="VD_DFV_EYE_TYPE" type="int" title="Driver Type" value="0" ver="4.0.0" scope="Session">
                    <Description>This option specifies the driver type. This defines the eye position.<Formatted>
A Group of Drivers: The analysis is based on an Eyellipse.
An Individual Driver: The analysis is based on two eye points.</Formatted></Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="A Group of Drivers" value="0"/>
                        <EnumElem name="An Individual Driver" value="1"/>
                    </Data>
                </Pref>
                <Pref name="VD_DFV_LIMIT_TYPE" type="int" title="View Limitation Type" value="0" ver="4.0.0" scope="Session">
                    <Description>This option specifies how the driver's view is limited.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="By an Aperture (Through a Window)" value="0"/>
                        <EnumElem name="By Head Turn and Eye Rotation" value="1"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="VD_DFV_EYE_ROTATION_TYPE" type="int" title="Eye Rotation" value="0" ver="4.0.0" scope="Session">
                    <Description>This option specifies the angles for the eye rotation.<Formatted>
Maximum: Predefined angles for a maximum field of rotation (30 degrees left, 30 degrees right, 45 degrees up, and 60 degrees down).
Easy: Predefined angles for an easy field of rotation (15 degrees left, 15 degrees right, 15 degrees up, and 15 degrees down).
Customize: Allows angles for the eye rotation parameters to be entered.</Formatted></Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Maximum" value="0"/>
                        <EnumElem name="Easy" value="1"/>
                        <EnumElem name="Customize" value="2"/>
                    </Data>
                </Pref>
                <Label label="The following options specify customized eye rotation angles."/>
                <Pref name="VD_DFV_EYE_ROTATION_ANG_LEFT" type="real" title="Left" value="30.0" unit="deg" ver="4.0.0" scope="Session">
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                        <UpperBound value="90.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="VD_DFV_EYE_ROTATION_ANG_RIGHT" type="real" title="Right" value="60.0" unit="deg" ver="4.0.0" scope="Session">
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                        <UpperBound value="90.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="VD_DFV_EYE_ROTATION_ANG_UP" type="real" title="Up" value="17.0" unit="deg" ver="4.0.0" scope="Session">
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                        <UpperBound value="90.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="VD_DFV_EYE_ROTATION_ANG_DOWN" type="real" title="Down" value="4.0" unit="deg" ver="4.0.0" scope="Session">
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                        <UpperBound value="90.0" include="1"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="VD_DFV_HEAD_TURN_TYPE" type="int" title="Head Turn" value="0" ver="4.0.0" scope="Session">
                    <Description>This option specifies the angles for the head turn.<Formatted>
Maximum: Predefined angles for maximum head turn (60 degrees left and 60 degrees right).
Easy: Predefined angles for an easy head turn (45 degrees left and 45 degrees right).
Customize: Allows angles for the head turn parameters to be entered.
No: No head turn will be included in the analysis.</Formatted></Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Maximum" value="0"/>
                        <EnumElem name="Easy" value="1"/>
                        <EnumElem name="Customize" value="2"/>
                        <EnumElem name="No" value="3"/>
                    </Data>
                </Pref>
                <Label label="The following options specify customized head turn angles."/>
                <Pref name="VD_DFV_HEAD_TURN_ANG_LEFT" type="real" title="Left" value="0.0" unit="deg" ver="4.0.0" scope="Session">
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                        <UpperBound value="90.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="VD_DFV_HEAD_TURN_ANG_RIGHT" type="real" title="Right" value="0.0" unit="deg" ver="4.0.0" scope="Session">
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                        <UpperBound value="90.0" include="1"/>
                    </Data>
                </Pref>
            </Tab>
