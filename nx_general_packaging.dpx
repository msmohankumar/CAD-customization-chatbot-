        <Category name="Reflect Data" csh="PMAN_Vehicle_Design_General_Packaging_Reflect_Data">
            <Tab name="All" csh="PMAN_Vehicle_Design_General_Packaging_Reflect_Data_All">
              <Pref name="VD_RD_REFLECTION_METHOD" type="int" title="Reflection Type" value="0" ver="11.0.1" scope="Session">
                <Description>This option specifies which reflection calculation type to use - direct reflection or reverse reflection.</Description>
                <RadioBox orientation="horizontal"/>
                <Data type="enum">
                  <EnumElem name="Direct" value="0"/>
                  <EnumElem name="Reverse" value="1"/>
                </Data>
              </Pref>
                <Group name="Transparent Material Properties">
                <Pref name="VD_RD_REFRACTION_INDEX" type="real" title="Index of Refraction " value="1.5" ver="4.0.0" scope="Session">
                    <Description>This option specifies the ratio of the speed of light in a vacuum to the speed of light in the medium of consideration (windshields).</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="1.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="VD_RD_THICKNESS_MU" type="real" title="Thickness" value="5.0" unit="mm" ver="4.0.0" scope="Session">
                    <Description>This option specifies the thickness of the object defined by the target faces (windshields). A value greater than zero means that reflections are displayed on both the front and rear faces of the object.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="VD_RD_THICKNESS_EU" type="real" title="Thickness" value="0.2" unit="in" ver="4.0.0" scope="Session">
                    <Description>This option specifies the thickness of the object defined by the target faces (windshields). A value greater than zero means that reflections are displayed on both the front and rear faces of the object.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                </Group>
                <Group name="Computation Accuracy">
                <Pref name="VD_RD_RESOLUTION" type="int" title="Resolution" value="1" ver="4.0.0" scope="Session">
                    <Description>This option specifies the resolution setting of the calculation. This setting determines the number of sample points used to produce the reflected curve, but does not affect the accuracy of any given reflected point. A high resolution may produce a smoother curve, but requires more processing time.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Low" value="0"/>
                        <EnumElem name="Medium" value="1"/>
                        <EnumElem name="High" value="2"/>
                        <EnumElem name="Customized" value="3"/>
                    </Data>
                </Pref>
                <Pref name="VD_RD_DISTANCE_TOLERANCE_MU" type="real" title="Distance Tolerance" value="0.01" unit="mm" ver="1980" scope="Session">
                    <Description>This option specifies the distance tolerance in the Reflection Data calculation.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="VD_RD_DISTANCE_TOLERANCE_EU" type="real" title="Distance Tolerance" value="0.0004" unit="in" ver="1980" scope="Session">
                    <Description>This option specifies the distance tolerance in the Reflection Data calculation.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="VD_RD_ANGULAR_TOLERANCE" type="real" title="Angular Tolerance" value="0.05" unit="deg" ver="1980" scope="Session">
                    <Description>This option specifies the angular tolerance in the Reflection Data calculation.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="0"/>
                    </Data>
                </Pref>
                <Pref name="VD_RD_ACCURANCY" type="int" title="Customized Accuracy" value="20" ver="4.0.0" scope="Session">
                    <Description>This option specifies the accuracy of the calculation (applies only if 'Resolution' is set to 'Customized'). The value represents the number of sample points used to produce the reflected curve, but does not affect the accuracy of any given reflected point. A high resolution may produce a smoother curve, but requires more processing time.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="2" include="1"/>
                    </Data>
                </Pref>
               </Group>
               <Group name="Reflect Data">
                <Pref name="VD_RD_REFLECT_DATA" type="logical" title="Create" value="true" ver="4.0.0" scope="Session">
                    <Description>This option determines whether or not the reflected data should be created.</Description>
                    <Toggle off="false"/>
