            <Tab name="Layer" csh="PMAN_Vehicle_Design_General_Packaging_General_Layer">
                <Pref name="VD_USE_WORK_LAYER" type="logical" title="Use Work Layer" value="true" ver="4.0.0" scope="Session">
                    <Description>This option determines whether or not the work layer should be used for the objects created by General Packaging. When this option is off, the objects are placed on layers as specified by the following options.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Separator/>
                <Pref name="VD_SOLID_LAYER" type="int" title="Solid and Sheet Bodies" value="1" ver="4.0.0" scope="Session">
                    <Description>This option specifies which layer the solid body and sheet body should be placed on.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                        <UpperBound value="256" include="1"/>
                    </Data>
                </Pref>
                <Pref name="VD_WIREFRAME_LAYER" type="int" title="Wireframe Geometries" value="2" ver="4.0.0" scope="Session">
                    <Description>This option specifies which layer the wireframe geometry should be placed on. The wireframe geometry includes curves, points, coordinate system, datums etc.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                        <UpperBound value="256" include="1"/>
                    </Data>
                </Pref>
                <Pref name="VD_ANNOTATION_LAYER" type="int" title="Annotations" value="3" ver="4.0.0" scope="Session">
                    <Description>This option specifies which layer the annotation should be placed on.</Description>
                    <Integer/>
                    <Data type="range">
                        <LowerBound value="1" include="1"/>
                        <UpperBound value="256" include="1"/>
                    </Data>
                </Pref>
            </Tab>
