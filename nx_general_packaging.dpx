        <Category name="Windshield Vision Zones" csh="PMAN_Vehicle_Design_General_Packaging_Windshield_Vision_Zones">
            <Tab name="General" csh="PMAN_Vehicle_Design_General_Packaging_Windshield_Vision_Zones_General">
                <Pref name="VD_WS_STANDARD" type="int" title="Standard" value="0" ver="2306" scope="Session">
                    <Description>This option specifies the windshield standard to be applied.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="SAE (Class A) - US Standard" value="0"/>
                        <EnumElem name="SAE (Class B) - US Standard" value="1"/>
                        <EnumElem name ="ECE - European Standard" value="2"/>
                        <EnumElem name ="FMVSS - Standard No. 104" value="3"/>
                        <EnumElem name ="GB - Chinese Standard" value="4"/>
                        <EnumElem name ="UMTRI" value="5"/>
                        <EnumElem name ="MLIT 45" value="6"/>
                        <EnumElem name ="AIS 019" value="7"/>
                        <EnumElem name ="User Defined" value="8"/>
                    </Data>
                </Pref>
                <Pref name="VD_WS_VEHICLE_TYPE" type="int" title="Vehicle Classification (F Dimension)" value="0" ver="4.0.0" scope="Session">
                    <Description>This option specifies the detailed vehicle classification. This option applies to the SAE (Class B) standard only.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="Truck, CBE and CAE (0-1020 mm)" value="0"/>
                        <EnumElem name="Truck, CBE and CAE (1020-1270 mm)" value="1"/>
                        <EnumElem name="Truck, CBE and CAE (1270 mm and greater)" value="2"/>
                        <EnumElem name="Buses, CBE-School and Commercial (1270-1520 mm)" value="3"/>
                        <EnumElem name="Buses, Forward Control, School and Commercial (1270-1520 mm)" value="4"/>
                        <EnumElem name="Forward Control or Multipurpose (All)" value="5"/>
                        <EnumElem name="Light-Duty Utility Vehicle (All)" value="6"/>
                        <EnumElem name="Van, Multistop (Open)" value="7"/>
                        <EnumElem name="Trucks, COE (1020 mm and greater)" value="8"/>
                    </Data>
                </Pref>
                <Pref name="VD_WS_VEHICLE_OVERALL_WIDTH" type="int" title="Overall Width (W116)" value="0" ver="4.0.0" scope="Session">
                    <Description>This option specifies the range of the body width dimension of the vehicle. This dimension is measured as the lateral distance between the widest points on the body-in-white. Excluded from this measurement are mirrors, door handles, marker and signal lamps, molding, appliques, and cladding. Running boards may be excluded if they do not extend beyond the mirrors. This option applies to the FMVSS standard only.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="Metric: W116 &lt; 1520 mm, English: W116 &lt; 60 in" value="0"/>
                        <EnumElem name="Metric: 1520 mm &lt;= W116 &lt; 1630 mm, English: 60 in &lt;= W116 &lt; 64 in" value="1"/>
                        <EnumElem name="Metric: 1630 mm &lt;= W116 &lt; 1730 mm, English: 64 in &lt;= W116 &lt; 68 in" value="2"/>
                        <EnumElem name="Metric: W116 &gt;= 1730 mm, English: W116 &gt;= 68 in" value="3"/>
                    </Data>
                </Pref>
                <Pref name="VD_WS_INPUT_METHOD" type="int" title="V-Points Method" value="2" ver="1953" scope="Session">
                    <Description>This option specifies which method to use to define V-points (vision points), which are required for the ECE standard.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="Use Hip Point and Back Angle" value="0"/>
                        <EnumElem name="Use Existing V-Points" value="1"/>
                        <EnumElem name="Use Existing ECE Points Feature" value="2"/>
                    </Data>
                </Pref>
                <Pref name="VD_WS_WINDSHIELD_TYPE" type="int" title="Windshield Type" value="0" ver="4.0.0" scope="Session">
                    <Description>This option specifies if the windshield consists of a single piece or multiple pieces. This option applies to the SAE (Class B) standard only.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="One-piece" value="0"/>
                        <EnumElem name="Multi-piece" value="1"/>
                    </Data>
                </Pref>
                <Separator/>
                <Image bitmap="illu_dark_band_offset.bmp"/>
                <Pref name="VD_WS_DARK_BAND_OFFSET_MU" type="real" title="(OF) Shade Band Offset" value="25.4" unit="mm" ver="4.0.0" scope="Session">
                    <Description>This option specifies the offset value used to create offset curves (OF). These serve as the limit for the test areas and wiped area. The offset curves are calculated from the shade (dark) band curves (SB) which represent the inside perimeter of the shade band on the windshield.</Description>
                    <Real/>
                    <Data type="range">
                        <LowerBound value="0.0" include="1"/>
                    </Data>
                </Pref>
                <Pref name="VD_WS_DARK_BAND_OFFSET_EU" type="real" title="(OF) Shade Band Offset" value="1.0" unit="in" ver="4.0.0" scope="Session">
                    <Description>This option specifies the offset value used to create offset curves (OF). These serve as the limit for the test areas and wiped area. The offset curves are calculated from the shade (dark) band curves (SB) which represent the inside perimeter of the shade band on the windshield.</Description>
