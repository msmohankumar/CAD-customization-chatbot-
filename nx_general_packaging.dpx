        <Category name="Vehicle Packaging" csh="PMAN_Vehicle_Design_General_Packaging_Vehicle_Packaging">
            <Tab name="Create Options" csh="PMAN_Vehicle_Design_General_Packaging_Vehicle_Packaging_Create_Options">
                <Pref name="VD_CREATE_OPTION_POSTURE_CHECKER" type="logical" title="Posture Checkers" value="true" ver="4.0.0" scope="Session">
                    <Description>This option determines whether or not the posture checkers should be created.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Pref name="VD_CREATE_OPTION_REAR_VIEW" type="logical" title="Rear View Dimensions" value="true" ver="4.0.0" scope="Session">
                    <Description>This option determines whether or not the rear view dimensions should be defined and the corresponding criteria geometry should be created. The rear view dimensions includes shoulder room, elbow room, hip room, entrance height, etc.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Group name="Side View">
                <Pref name="VD_CREATE_OPTION_FLOOR_COVERING" type="logical" title="Floor Covering Dimensions" value="true" ver="4.0.0" scope="Session">
                    <Description>This option determines whether or not the floor covering dimensions should be defined and the corresponding criteria geometry should be created. The floor covering dimensions includes depressed floor covering thickness, undepressed floor covering thickness, etc.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
