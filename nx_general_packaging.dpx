        <Category name="H-point Design Tool" csh="PMAN_Vehicle_Design_General_Packaging_H_Point_Design_Tool">
            <Tab name="All" csh="PMAN_Vehicle_Design_General_Packaging_H_Point_Design_Tool_All">
                <Pref name="VD_HPD_LEG_LENGTH_TYPE" type="int" title="Leg Lengths" value="0" ver="4.0.0" scope="Session">
                    <Description>This option specifies the leg length type.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="SgRP" value="0"/>
                        <EnumElem name="Mid Size Male (for Study Only)" value="1"/>
                        <EnumElem name="Other (for Study Only)" value="2"/>
                    </Data>
                    <Docum>SgRP and Mid Size Male values are fixed. SgRP is the only Leg Length type that meets SAE standards.</Docum>
                </Pref>
                <Pref name="VD_HPD_OCC_POSITION_METHOD" type="int" title="Position the Accelerator Pedal Shoe" value="0" ver="2312" scope="Session">
                    <Description>This option specifies how to position the pedal shoe (applies only if 'Leg Lengths' is set to 'SgRP'). The first option locks the shoe plane angle and heel point position the SgRP is moved. The second option locks the shoe plane angle and/or ankle angle.</Description>
                    <RadioBox/>
                    <Data type="enum">
                        <EnumElem name="By Pedal Geometry or Shoe Plane Angle" value="0"/>
                        <EnumElem name="By Reverse Engineering from SgRP Position" value="1"/>
                        <EnumElem name="By Reverse Engineering from Pedal, Floor and Seat Track Travel Line" value="2"/>
                    </Data>
