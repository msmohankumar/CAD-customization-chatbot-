        <Category name="Vehicle Coordinate System" csh="PMAN_Vehicle_Design_General_Packaging_Vehicle_Coordinate_System">
            <Tab name="General" csh="PMAN_Vehicle_Design_General_Packaging_Vehicle_Coordinate_System_General">
                <Pref name="VEHICLE_CSYS_SHOW_REMINDER" type="logical" title="Show Confirmation Message for Automatic Creation" value="true" ver="4.0.2" scope="Session">
                    <Description>This option controls whether or not the confirmation message box should be shown when Vehicle Coordinate System is automatically created on the fly without using specific definition UI.</Description>
                    <Toggle off="false"/>
                    <Data type="enum">
                        <EnumElem name="No" value="false"/>
                        <EnumElem name="Yes" value="true"/>
                    </Data>
                </Pref>
                <Pref name="VEHICLE_CSYS_TYPE" type="int" title="Vehicle Coordinate System Type" value="0" ver="4.0.2" scope="Session">
                    <Description>This option specifies the type of the vehicle coordinate system which will be used for general packaging modules and body design modules.</Description>
                    <RadioBox orientation="horizontal"/>
                    <Data type="enum">
                        <EnumElem name="Pre-defined Type I (Front to Rear Direction = +X, Ground to Roof Direction = +Z, Origin = absolute zero)" bitmap="abs_csys.bmp" value="0"/>
                        <EnumElem name="Pre-defined Type II (Front to Rear Direction = +Y, Ground to Roof Direction = +Z, Origin = absolute zero)" bitmap="rotated_csys.bmp" value="1"/>
                        <EnumElem name="User Defined (Front to Rear Direction = user defined, Ground to Roof Direction = user defined , Origin = user defined)" bitmap="user_csys.bmp" value="2"/>
                    </Data>
                </Pref>   
                <Label label="The values in the following options will be used when the type of Vehicle Coordinate System is User Defined."/>
                <Group name="User Defined - Front to Rear Direction">
                <Pref name="VEHICLE_CSYS_ALONG_VEC_X" type="real" title="I" value="1.0" ver="4.0.2" scope="Session">
                    <Description>This option specifies the Cartesian I value for the direction from the front of the vehicle to the rear of the vehicle.</Description>
                    <Real/>
                </Pref>
                <Pref name="VEHICLE_CSYS_ALONG_VEC_Y" type="real" title="J" value="0.0" ver="4.0.2" scope="Session">
                    <Description>This option specifies the Cartesian J value for the direction from the front of the vehicle to the rear of the vehicle.</Description>
                    <Real/>
                </Pref>
                <Pref name="VEHICLE_CSYS_ALONG_VEC_Z" type="real" title="K" value="0.0" ver="4.0.2" scope="Session">
                    <Description>This option specifies the Cartesian K value for the direction from the front of the vehicle to the rear of the vehicle.</Description>
                    <Real/>
                </Pref>
                </Group>                
                <Group name="User Defined - Ground to Roof Direction">
                <Label label="Ground to roof direction will be orthogonalized based on front to rear direction if it is not perpendicular to front to rear direction."/>
                <Pref name="VEHICLE_CSYS_UP_VEC_X" type="real" title="I" value="0.0" ver="4.0.2" scope="Session">
                    <Description>This option specifies the Cartesian I value for the direction from the ground to the roof of the vehicle.</Description>
                    <Real/>
                </Pref>
                <Pref name="VEHICLE_CSYS_UP_VEC_Y" type="real" title="J" value="0.0" ver="4.0.2" scope="Session">
                    <Description>This option specifies the Cartesian J value for the direction from the ground to the roof of the vehicle.</Description>
                    <Real/>
                </Pref>
                <Pref name="VEHICLE_CSYS_UP_VEC_Z" type="real" title="K" value="1.0" ver="4.0.2" scope="Session">
                    <Description>This option specifies the Cartesian K value for the direction from the ground to the roof of the vehicle.</Description>
                    <Real/>
                </Pref>
                </Group>                
                <Group name="User Defined - Origin">
                <Pref name="VEHICLE_CSYS_ORIGIN_X_MU" type="real" title="X" value="0.0" unit="mm" ver="4.0.2" scope="Session">
                    <Description>This option specifies the X value of the vehicle coordinate system origin relative to the absolute coordinate system.</Description>
                    <Real/>
                </Pref>
                <Pref name="VEHICLE_CSYS_ORIGIN_X_EU" type="real" title="X" value="0.0" unit="in" ver="4.0.2" scope="Session">
                    <Description>This option specifies the X value of the vehicle coordinate system origin relative to the absolute coordinate system.</Description>
                    <Real/>
                </Pref>
                <Pref name="VEHICLE_CSYS_ORIGIN_Y_MU" type="real" title="Y" value="0.0" unit="mm" ver="4.0.2" scope="Session">
                    <Description>This option specifies the Y value of the vehicle coordinate system origin relative to the absolute coordinate system.</Description>
                    <Real/>
                </Pref>
                <Pref name="VEHICLE_CSYS_ORIGIN_Y_EU" type="real" title="Y" value="0.0" unit="in" ver="4.0.2" scope="Session">
                    <Description>This option specifies the Y value of the vehicle coordinate system origin relative to the absolute coordinate system.</Description>
                    <Real/>
                </Pref>
                <Pref name="VEHICLE_CSYS_ORIGIN_Z_MU" type="real" title="Z" value="0.0" unit="mm" ver="4.0.2" scope="Session">
                    <Description>This option specifies the Z value of the vehicle coordinate system origin relative to the absolute coordinate system.</Description>
                    <Real/>
                </Pref>
                <Pref name="VEHICLE_CSYS_ORIGIN_Z_EU" type="real" title="Z" value="0.0" unit="in" ver="4.0.2" scope="Session">
                    <Description>This option specifies the Z value of the vehicle coordinate system origin relative to the absolute coordinate system.</Description>
                    <Real/>
                </Pref>
                </Group>                
            </Tab>                           
